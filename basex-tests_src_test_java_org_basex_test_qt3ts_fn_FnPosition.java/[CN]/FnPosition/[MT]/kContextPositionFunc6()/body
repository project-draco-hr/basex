{
  final XQuery query=new XQuery("empty((1, 2, 3, current-time(), current-date(), 6, 7, 8) [0 eq position()])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
