{
  final XQuery query=new XQuery("fn:count(//center//@center-attr-2)",ctx);
  try {
    query.context(node(file("prod/AxisStep/Tree1Child.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
