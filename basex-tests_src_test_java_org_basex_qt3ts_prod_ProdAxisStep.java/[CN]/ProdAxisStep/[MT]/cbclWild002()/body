{
  final XQuery query=new XQuery("\n" + "        declare namespace foo=\"http://localhost/\";\n" + "        <a><foo:b/><foo:c/></a>/foo:b/self::foo:*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<foo:b xmlns:foo=\"http://localhost/\"/>",false));
}
