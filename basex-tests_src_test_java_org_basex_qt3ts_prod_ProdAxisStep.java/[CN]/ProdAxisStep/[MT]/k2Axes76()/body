{
  final XQuery query=new XQuery("<a> <b c=\"\"/> <d/> </a>/descendant-or-self::node()[last()]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<d/>",false));
}
