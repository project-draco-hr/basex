{
  final XQuery query=new XQuery("<a> { <a b=\"blah\"/>/@*/self::attribute(b) } </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a b=\"blah\"/>",false));
}
