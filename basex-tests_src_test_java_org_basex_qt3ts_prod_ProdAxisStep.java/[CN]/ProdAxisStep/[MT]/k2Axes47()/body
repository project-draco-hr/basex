{
  final XQuery query=new XQuery("declare variable $myVar := <e/>; empty($myVar/descendant-or-self::text())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
