{
  final XQuery query=new XQuery("declare variable $myVar := <e/>; empty(fn:root($myVar/(/)/..))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPST0005") || error("XPDY0050")));
}
