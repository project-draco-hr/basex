{
  final XQuery query=new XQuery("\n" + "        declare namespace ns1 = \"http://www.example.org/ns1\"; \n" + "        declare namespace ns2 = \"http://www.example.org/ns2\"; \n"+ "        declare construction strip; \n"+ "        let $element as element(*, xs:untyped) := <e>test</e> \n"+ "        let $element as element(*, xs:untyped) := $element/self::* \n"+ "        return count($element)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
