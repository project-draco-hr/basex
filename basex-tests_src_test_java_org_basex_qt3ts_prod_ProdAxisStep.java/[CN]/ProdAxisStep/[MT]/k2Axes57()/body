{
  final XQuery query=new XQuery("declare variable $var := <a> <b/> <c/> <d/> </a>; ($var/*, $var/*)/.",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<b/><c/><d/>",false));
}
