{
  final XQuery query=new XQuery("declare variable $var := document{<e><f>f's value</f></e>}; $var/(/)/(/)//f",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<f>f's value</f>",false));
}
