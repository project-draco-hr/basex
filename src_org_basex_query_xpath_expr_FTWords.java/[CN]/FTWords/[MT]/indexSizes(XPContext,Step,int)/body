{
  fto.sb.init(token);
  fto.sb.fz=fto.is(FTOpt.FZ);
  fto.sb.wc=fto.is(FTOpt.WC);
  fto.sb.st=fto.is(FTOpt.ST);
  fto.sb.cs=fto.is(FTOpt.CS);
  fto.sb.dc=fto.is(FTOpt.DC);
  int i=0;
  while (fto.sb.more()) {
    final int n=ctx.item.data.nrIDs(fto.sb);
    if (n == 0)     return 0;
    i=Math.max(i,n);
  }
  return i;
}
