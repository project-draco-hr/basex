{
  final XQuery query=new XQuery("processing-instruction pi {'&#x20;&#x0A;&#x0D;&#x09;text'}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<?pi text?>",false));
}
