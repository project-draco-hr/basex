{
  final XQuery query=new XQuery("local-name(processing-instruction {\" name \"} {\"content\"})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"name"));
}
