{
  checkWrite(ctx);
  final boolean all=expr.length == 2 && checkBln(expr[1],ctx);
  final Data data=data(0,ctx);
  try {
    if (all) {
      OptimizeAll.optimizeAll(data,ctx.context,null);
    }
 else {
      Optimize.optimize(data,data.meta.prop);
    }
  }
 catch (  final Exception ex) {
    DBERR.thrw(input,ex);
  }
  return null;
}
