{
switch (def) {
case EVENT:
    return event(ctx);
case OPENID:
  return open(ctx,true);
case OPENPRE:
return open(ctx,false);
case SYSTEM:
return system(ctx);
case INFO:
return info(ctx);
case ADD:
return add(ctx);
case DELETE:
return delete(ctx);
case RENAME:
return rename(ctx);
case REPLACEDOC:
return replace(ctx);
case OPTIMIZE:
return optimize(ctx);
default :
return super.item(ctx,ii);
}
}
