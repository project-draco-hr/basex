{
  return new Iter(){
    final Iter ir=ctx.iter(expr[0]);
    @Override public Item next() throws QueryException {
      final Item it=ir.next();
      if (it == null)       return null;
      final DBNode node=checkDBNode(it);
      return Int.get(id ? node.data.id(node.pre) : node.pre);
    }
  }
;
}
