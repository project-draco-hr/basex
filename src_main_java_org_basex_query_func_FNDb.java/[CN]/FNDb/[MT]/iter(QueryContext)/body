{
switch (sig) {
case _DB_OPEN:
    return open(ctx).iter();
case _DB_TEXT:
  return text(ctx);
case _DB_ATTRIBUTE:
return attribute(ctx);
case _DB_FULLTEXT:
return fulltext(ctx);
case _DB_LIST:
return list(ctx);
case _DB_NODE_ID:
return node(ctx,true);
case _DB_NODE_PRE:
return node(ctx,false);
default :
return super.iter(ctx);
}
}
