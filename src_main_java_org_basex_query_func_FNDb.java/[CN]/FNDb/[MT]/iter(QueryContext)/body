{
switch (def) {
case OPEN:
    return open(ctx);
case TEXTIDX:
  return textIndex(ctx);
case ATTRIDX:
return attributeIndex(ctx);
case FTIDX:
return fulltextIndex(ctx);
case LIST:
return list(ctx);
case NODEID:
return node(ctx,true);
case NODEPRE:
return node(ctx,false);
default :
return super.iter(ctx);
}
}
