{
switch (def) {
case DBOPEN:
    return open(ctx).iter();
case DBTEXT:
  return text(ctx);
case DBATTR:
return attribute(ctx);
case DBFULLTEXT:
return fulltext(ctx);
case DBLIST:
return list(ctx);
case DBNODEID:
return node(ctx,true);
case DBNODEPRE:
return node(ctx,false);
default :
return super.iter(ctx);
}
}
