{
  final boolean up=def == Function.DBADD || def == Function.DBDELETE || def == Function.DBRENAME || def == Function.DBREPLACE || def == Function.DBOPTIMIZE || def == Function.DBPUT;
  return u == Use.CTX && (def == Function.DBTEXT || def == Function.DBATTR || def == Function.DBFULLTEXT || def == Function.DBEVENT || up) || u == Use.UPD && up || super.uses(u);
}
