{
  if (rec || ((InsertInto)p).rec)   return;
  final InsertInto i=(InsertInto)p;
  final ANodeList newInsert=i.insert;
  if (i.last) {
    for (int j=0; j < newInsert.size(); j++)     insert.add(newInsert.get(j));
  }
 else {
    for (int j=0; j < insert.size(); j++)     newInsert.add(insert.get(j));
    insert=newInsert;
  }
}
