{
  final XQuery query=new XQuery("<elem attr=\"{(1)}\">{(<child/>),(<child/>)}</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem attr=\"1\"><child/><child/></elem>",false));
}
