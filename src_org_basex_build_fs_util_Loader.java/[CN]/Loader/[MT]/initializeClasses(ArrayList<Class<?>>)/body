{
  int counter=0;
  for (  final Class<?> c : classes) {
    try {
      Class.forName(c.getName(),true,ClassLoader.getSystemClassLoader());
      counter++;
    }
 catch (    final Throwable t) {
      Main.errln("Failed to load class (%)",t.getMessage());
      break;
    }
  }
  return counter;
}
