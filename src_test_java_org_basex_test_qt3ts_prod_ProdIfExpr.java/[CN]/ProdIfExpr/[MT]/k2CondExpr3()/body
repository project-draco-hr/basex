{
  final XQuery query=new XQuery("declare default function namespace \"http://www.example.com/\"; declare function unordered() { 1 }; unordered()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
