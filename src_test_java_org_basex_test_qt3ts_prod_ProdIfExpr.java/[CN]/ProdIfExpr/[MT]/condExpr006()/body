{
  final XQuery query=new XQuery("<out>{ if (1 != 0) then if (4 != 5) then 1 else 2 else 3 }</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>1</out>",false));
}
