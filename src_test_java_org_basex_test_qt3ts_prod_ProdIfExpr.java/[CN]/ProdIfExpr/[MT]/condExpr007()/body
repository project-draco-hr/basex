{
  final XQuery query=new XQuery("if (//MissingNode) then <elem3/> else if (/Root/Customers[@CustomerID='ALFKI']//Country = \"Germany\") then <elem1/> else <elem2/>",ctx);
  query.context(node(file("prod/AxisStep/nw_Customers.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem1/>",false));
}
