{
  final XQuery query=new XQuery("(every $s in tokenize('??', ',') satisfies matches($s, '^(?:\\P{P}*)$')) and (every $s in tokenize('\u203f\uff65\u301c\u301c\uff0d\u301d\u301d\uff62\u301e\u301e\uff63????\u2039????\u203a????\uff64', ',') satisfies not(matches($s, '^(?:\\P{P}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
