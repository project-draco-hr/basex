{
  final XQuery query=new XQuery("(every $s in tokenize('???\u17ff,????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u17b4\u17b5??????????????????????????????????????????????????????????????????????????????????????????\u17d4\u17d5\u17d6???\u17d8\u17d9\u17da?????????\u17de\u17df??????????????????????????????\u17ea\u17eb\u17ec\u17ed\u17ee\u17ef??????????????????????????????\u17fa\u17fb\u17fc\u17fd\u17fe\u17ff', ',') satisfies matches($s, '^(?:\\p{IsKhmer}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsKhmer}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
