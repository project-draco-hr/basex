{
  final XQuery query=new XQuery("(every $s in tokenize('\u1680\u169f,\u1680??????????????????????????????????????????????????????????????????????????????\u169b\u169c\u169d\u169e\u169f', ',') satisfies matches($s, '^(?:\\p{IsOgham}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsOgham}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
