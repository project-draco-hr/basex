{
  tba.delete(nodesPerBlock,nodesPerBlock);
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodesPerBlock);
  assertEntrysEqual(2 * nodesPerBlock,nodesPerBlock,size - 2 * nodesPerBlock);
  closeAndReload();
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodesPerBlock);
  assertEntrysEqual(2 * nodesPerBlock,nodesPerBlock,size - 2 * nodesPerBlock);
}
