{
  tba.insert(3,getTestEntries(1));
  assertEquals(size + 1,tba.size());
  assertEntrysEqual(0,0,4);
  assertAreInserted(4,1);
  assertEntrysEqual(4,5,size - 4);
  closeAndReload();
  assertEquals(size + 1,tba.size());
  assertEntrysEqual(0,0,4);
  assertAreInserted(4,1);
  assertEntrysEqual(4,5,size - 4);
}
