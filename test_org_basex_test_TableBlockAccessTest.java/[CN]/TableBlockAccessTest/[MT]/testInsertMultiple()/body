{
  tba.insert(3,getTestEntries(3));
  assertEquals(size + 3,tba.size());
  assertEntrysEqual(0,0,4);
  assertAreInserted(4,3);
  assertEntrysEqual(4,7,size - 4);
  closeAndReload();
  assertEquals(size + 3,tba.size());
  assertEntrysEqual(0,0,4);
  assertAreInserted(4,3);
  assertEntrysEqual(4,7,size - 4);
}
