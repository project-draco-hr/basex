{
  if (qc.scoring) {
    double s=0;
    boolean f=false;
    for (    final Expr e : exprs) {
      final Item it=e.ebv(qc,info);
      f|=it.bool(ii);
      s+=it.score();
    }
    return Bln.get(f,Scoring.avg(s,exprs.length));
  }
  for (  final Expr e : exprs) {
    if (e.ebv(qc,info).bool(ii))     return Bln.TRUE;
  }
  return Bln.FALSE;
}
