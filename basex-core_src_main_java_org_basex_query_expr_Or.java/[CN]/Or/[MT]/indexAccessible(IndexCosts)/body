{
  int is=0;
  Expr[] ex={};
  boolean ia=true;
  for (  final Expr expr : exprs) {
    if (expr.indexAccessible(ic) && !ic.seq) {
      if (ic.costs() == 0)       continue;
      is+=ic.costs();
    }
 else {
      ia=false;
    }
    ex=Array.add(ex,expr);
  }
  ic.costs(is);
  exprs=ex;
  return ia;
}
