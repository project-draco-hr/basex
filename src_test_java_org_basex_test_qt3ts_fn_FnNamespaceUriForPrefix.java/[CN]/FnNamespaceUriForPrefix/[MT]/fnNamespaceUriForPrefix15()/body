{
  final XQuery query=new XQuery("fn:string(fn:namespace-uri-for-prefix(fn:substring-after(\"abcp1\",\"abc\"),<anElement xmlns:p1 = \"http://www.example.com/examples\">some context</anElement>))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.com/examples"));
}
