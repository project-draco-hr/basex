{
  final XQuery query=new XQuery("declare namespace p = \"http://www.example.com/B\"; namespace-uri-for-prefix(\"\", <e xmlns=\"http://www.example.com/A\" xmlns:A=\"http://www.example.com/C\"> <b xmlns:B=\"http://www.example.com/C\" xmlns=\"http://www.example.com/B\"/> </e>/p:b)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.com/B"));
}
