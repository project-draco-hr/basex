{
  final XQuery query=new XQuery("namespace-uri-for-prefix(\"\", exactly-one(\n" + "                      <e xmlns=\"http://www.example.com/A\" xmlns:A=\"http://www.example.com/C\"> <b xmlns:B=\"http://www.example.com/C\" xmlns=\"\"/> </e>/b)\n" + "                      )",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,""));
}
