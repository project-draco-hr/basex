{
  final XQuery query=new XQuery("\n" + "        let $var := <anElement xmlns:p1 = \"http://www.example.com/examples\">some context</anElement> \n" + "        return fn:count(fn:namespace-uri-for-prefix(\"p2\",$var))\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("0"));
}
