{
  final XQuery query=new XQuery("declare default element namespace \"http://www.example.com/defaultspace\"; \n" + "        let $var := <anElement xmlns:p1 = \"http://www.example.com/examples\">some context</anElement> \n" + "        return fn:string(fn:namespace-uri-for-prefix(\"\",$var))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.com/defaultspace"));
}
