{
  check(FLCREATEDIR);
  query(FLCREATEDIR.args(PATH1));
  query(FLCREATEDIR.args(PATH1));
  query(FLCREATEDIR.args(PATH3));
  query(FLDELETE.args(PATH1));
  query(FLWRITE.args(PATH1,"()"));
  error(FLCREATEDIR.args(PATH1),Err.FILEEXISTS);
  error(FLCREATEDIR.args(PATH3),Err.FILEEXISTS);
  query(FLDELETE.args(PATH1));
}
