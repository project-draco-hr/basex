{
  final String fun=check(Function.ISFILE);
  query(fun + "('" + Prop.TMP+ "')","false");
  query("file:write('" + PATH1 + "', ())");
  query(fun + "('" + PATH1+ "')","true");
  query("file:delete('" + PATH1 + "')");
  query("file:create-directory('" + PATH1 + "')");
  query(fun + "('" + PATH1+ "')","false");
  query("file:delete('" + PATH1 + "')");
}
