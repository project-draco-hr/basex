{
  final XQuery query=new XQuery("for $x in /works/employee[fn:position() mod 2 eq 1][fn:position() lt 5] return (fn:data($x/empnum), fn:data($x/pnum))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"E1 P1 E1 P3 E1 P5 E2 P1"));
}
