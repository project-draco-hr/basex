{
  final XQuery query=new XQuery("fn:count((//integer[fn:true()]))",ctx);
  try {
    query.context(node(file("docs/atomicns.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
