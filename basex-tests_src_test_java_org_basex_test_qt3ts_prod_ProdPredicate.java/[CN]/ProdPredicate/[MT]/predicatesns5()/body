{
  final XQuery query=new XQuery("(//integer[fn:true() or fn:true()])",ctx);
  try {
    query.context(node(file("docs/atomicns.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<integer>12678967543233</integer>",false));
}
