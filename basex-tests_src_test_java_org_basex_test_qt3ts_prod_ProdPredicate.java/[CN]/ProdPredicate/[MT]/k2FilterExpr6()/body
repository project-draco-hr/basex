{
  final XQuery query=new XQuery("let $d := document { <root><child type=\"\"/></root> } return $d//*[let $i := @type return $d//*[$i]]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<child type=\"\"/>",false));
}
