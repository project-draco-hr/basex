{
  final XQuery query=new XQuery(" format-date($d1, '[Y][EN]', 'en', (), ()), format-date($d2, '[Y][EN]', 'en', (), ()) ",ctx);
  try {
    query.bind("d1",new XQuery("xs:date('1990-12-01')",ctx).value());
    query.bind("d2",new XQuery("xs:date('-0055-12-01')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertDeepEq("\"1990AD\", \"55BC\"") || assertDeepEq("\"1990CE\", \"55BCE\"") || assertDeepEq("\"1990A.D.\", \"55B.C.\"")|| assertDeepEq("\"1990C.E.\", \"55B.C.E.\"")));
}
