{
  final XQuery query=new XQuery("string-join( for $z in -12 to +12 return format-date(adjust-date-to-timezone($t,\n" + "         $z*xs:dayTimeDuration('PT1H')), '[ZZ]'), ' ')",ctx);
  try {
    query.bind("t",new XQuery("xs:date('1985-03-01Z')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n         \tY X W V U T S R Q P O N Z A B C D E F G H I K L M\n\t\t "));
}
