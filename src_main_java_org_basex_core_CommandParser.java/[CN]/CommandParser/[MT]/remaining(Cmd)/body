{
  final StringBuilder sb=new StringBuilder();
  consumeWS();
  while (more())   sb.append(consume());
  String arg=finish(cmd,sb);
  if (arg != null) {
    if (arg.startsWith("\""))     arg=arg.substring(1);
    if (arg.endsWith("\""))     arg=arg.substring(0,arg.length() - 1);
  }
  return arg;
}
