{
  final String token=name(null);
  if (!(gui && token != null && token.length() <= 1)) {
    try {
      final String t=token == null ? "NULL" : token.toUpperCase(Locale.ENGLISH);
      return Enum.valueOf(cmp,t);
    }
 catch (    final IllegalArgumentException ex) {
    }
  }
  final Enum<?>[] alt=list(cmp,token);
  if (token == null) {
    throw par == null ? error(list(alt),CMDNO) : help(list(alt),par);
  }
  final byte[] name=lc(token(token));
  final Levenshtein ls=new Levenshtein();
  for (  final Enum<?> s : list(cmp,null)) {
    final byte[] sm=lc(token(s.name().toLowerCase(Locale.ENGLISH)));
    if (ls.similar(name,sm,0) && Cmd.class.isInstance(s))     throw error(list(alt),CMDSIMILAR,name,sm);
  }
  throw par == null ? error(list(alt),CMDWHICH,token) : help(list(alt),par);
}
