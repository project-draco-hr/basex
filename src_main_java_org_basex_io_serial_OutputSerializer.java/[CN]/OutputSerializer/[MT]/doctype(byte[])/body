{
  if (level != 0 || docsys == null)   return;
  if (sep)   indent();
  print(DOCTYPE);
  if (dt == null)   print(M_HTML);
 else   print(dt);
  if (docpub != null) {
    print(' ' + PUBLIC + " \""+ docpub+ '"');
  }
 else {
    print(' ' + SYSTEM);
  }
  print(" \"" + docsys + '"');
  print(ELEM_C);
  print(nl);
  docsys=null;
  sep=true;
}
