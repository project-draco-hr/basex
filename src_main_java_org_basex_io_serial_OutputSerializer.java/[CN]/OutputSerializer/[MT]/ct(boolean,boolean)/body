{
  if (ct != 1)   return false;
  ct++;
  if (empty)   finishOpen();
  level++;
  startOpen(META);
  attribute(HTTPEQUIV,token(CONTENT_TYPE));
  final String m=media.isEmpty() ? TEXT_HTML : media;
  attribute(CONTENT,new TokenBuilder(m).add(CHARSET).addExt(encoding).finish());
  if (html) {
    print(ELEM_C);
  }
 else {
    print(' ');
    print(ELEM_SC);
  }
  level--;
  if (empty)   finishClose();
  return true;
}
