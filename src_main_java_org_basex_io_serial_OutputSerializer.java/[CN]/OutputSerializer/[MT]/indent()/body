{
  if (!indent)   return;
  if (item) {
    item=false;
  }
 else {
    if (!suppress.isEmpty() && !tags.isEmpty()) {
      for (      final byte[] s : suppress) {
        if (tags.contains(s))         return;
      }
    }
    print(nl);
    final int ls=level * indents;
    for (int l=0; l < ls; ++l)     print(tab);
  }
}
