import BaseX, time
start = time.clock()
cmd = 'xquery 1 to 10'
try:
    session = BaseX.BaseX('localhost', 1984, 'admin', 'admin')
    if session.execute(cmd):
        print session.result()
    else:
        print session.info()
    session.close()
    time = ((time.clock() - start) * 1000)
    print time, 'ms.'
except IOError as e:
    print e
