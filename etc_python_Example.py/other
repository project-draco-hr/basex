import BaseXClient, time
start = time.clock()
cmd = 'xquery 1 to 10'
try:
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    if session.execute(cmd):
        print session.result()
    else:
        print session.info()
    time = ((time.clock() - start) * 1000)
    print time, 'ms.'
    if session.loadDoc('example.xml', None, 'python'):
        print session.result()
    else:
        print session.info()
    session.close()
except IOError as e:
    print e
