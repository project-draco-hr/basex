{
  query("for $i in " + _MAP_KEYS.args(_MAP_NEW.args(" for $i in 1 to 3 return " + _MAP_ENTRY.args("$i","$i+1"))) + " order by $i return $i","1 2 3");
  query("let $map := " + _MAP_NEW.args(" for $i in 1 to 3 return " + _MAP_ENTRY.args("$i","$i + 1")) + "for $k in "+ _MAP_KEYS.args("$map")+ " order by $k return "+ _MAP_GET.args("$map","$k"),"2 3 4");
}
