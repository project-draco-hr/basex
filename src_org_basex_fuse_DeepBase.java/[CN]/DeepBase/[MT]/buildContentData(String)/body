{
  MemData md=new MemData(64,data.tags,data.atts,data.ns,data.path);
  try {
    MemBuilder mb=new MemBuilder();
    mb.init(md);
    Prop.fscont=true;
    Prop.fsmeta=true;
    FSParser p=new FSParser(backingstore + path);
    mb.build(p,"tmp_memdata4file");
    return mb.finish();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return md;
}
