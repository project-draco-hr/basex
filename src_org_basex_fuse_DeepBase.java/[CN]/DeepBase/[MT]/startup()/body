{
  try {
    debug("[DeepBase.startup]");
    Prop.read();
    data=Open.open(DBNAME);
  }
 catch (  FileNotFoundException e) {
    try {
      data=CreateDB.xml(IO.get("deepfs.xml"),DBNAME);
      startup();
    }
 catch (    IOException ex) {
      ex.printStackTrace();
      System.exit(1);
    }
  }
catch (  IOException e) {
    e.printStackTrace();
    System.exit(1);
  }
}
