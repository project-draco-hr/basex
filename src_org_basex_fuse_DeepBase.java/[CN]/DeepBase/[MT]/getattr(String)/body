{
  debug("[DeepBase.getattr]",path);
  try {
    final Nodes nodes=query(xpath(path));
    debug("[DeepBase.getattr]","found " + nodes.size);
    if (nodes.size > 0) {
      int pre=nodes.nodes[0];
      debug("[DeepBase.getattr]","pre " + pre + "id "+ data.id(pre));
      return data.id(pre);
    }
  }
 catch (  QueryException e) {
    e.printStackTrace();
  }
  return -1;
}
