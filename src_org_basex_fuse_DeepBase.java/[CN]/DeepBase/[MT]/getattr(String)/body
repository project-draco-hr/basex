{
  debug("[DeepBase.getattr]",path);
  try {
    final Nodes n=query(xpath(path));
    debug("[DeepBase.getattr]","found " + n.size);
    if (n.size > 0) {
      int pre=n.nodes[0];
      debug("[DeepBase.getattr]","pre " + pre + " id "+ data.id(pre));
      return data.id(pre);
    }
  }
 catch (  QueryException e) {
    e.printStackTrace();
  }
  return -1;
}
