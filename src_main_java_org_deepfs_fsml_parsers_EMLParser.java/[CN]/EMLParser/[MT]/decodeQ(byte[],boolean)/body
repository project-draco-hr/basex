{
  final TokenBuilder tmp=new TokenBuilder();
  final int len=text.length;
  for (int i=0; i < len; ++i) {
    final byte c=text[i];
    if (c == '=') {
      if (i + 2 >= len)       break;
      if (text[++i] == 0xA)       continue;
      final int n1=hex2num(text[i]);
      final int n2=hex2num(text[++i]);
      addByte(tmp,n1 << 4 | n2,utf);
    }
 else     addByte(tmp,c,utf);
  }
  return tmp.finish();
}
