{
  final XQuery query=new XQuery("fn:insert-before( (\"a\", \"b\", \"c\"),1, \"z\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"z\", \"a\", \"b\", \"c\""));
}
