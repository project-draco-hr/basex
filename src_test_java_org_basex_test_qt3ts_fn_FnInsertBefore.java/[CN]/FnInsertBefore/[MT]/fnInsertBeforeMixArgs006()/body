{
  final XQuery query=new XQuery("fn:insert-before( (\"a\", \"b\", \"c\"),2, xs:integer(\"100\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"a\", 100, \"b\", \"c\""));
}
