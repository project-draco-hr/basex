{
  final XQuery query=new XQuery("fn:insert-before( (\"a\", \"b\", \"c\"),4, xs:date(\"1993-03-31\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"a\", \"b\", \"c\", xs:date('1993-03-31')"));
}
