{
  final XQuery query=new XQuery("fn:zero-or-one(xs:nonPositiveInteger(\"-475688437271870490\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-475688437271870490"));
}
