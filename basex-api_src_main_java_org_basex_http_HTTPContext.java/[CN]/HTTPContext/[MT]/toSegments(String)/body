{
  final StringList sl=new StringList();
  if (path != null) {
    final TokenBuilder tb=new TokenBuilder();
    for (int s=0; s < path.length(); s++) {
      final char ch=path.charAt(s);
      if (ch == '/') {
        if (tb.isEmpty())         continue;
        sl.add(tb.toString());
        tb.reset();
      }
 else {
        tb.add(ch);
      }
    }
    if (!tb.isEmpty())     sl.add(tb.toString());
  }
  return sl.toArray();
}
