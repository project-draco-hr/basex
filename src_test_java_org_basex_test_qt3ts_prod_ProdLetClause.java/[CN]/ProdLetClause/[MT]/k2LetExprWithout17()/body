{
  final XQuery query=new XQuery("declare function local:function() { let $b := (i/a) return () }; empty(local:function())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPDY0002") || error("XPST0005")));
}
