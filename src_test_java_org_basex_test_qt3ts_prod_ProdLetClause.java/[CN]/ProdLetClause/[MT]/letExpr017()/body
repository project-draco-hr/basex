{
  final XQuery query=new XQuery("let $x := (<a> <b> <c> 123 </c> </b> </a>) return $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a><b><c> 123 </c></b></a>",false));
}
