{
  final XQuery query=new XQuery("let $var as xs:boolean := if (fn:true()) then fn:true() else fn:false() return $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
