{
  final XQuery query=new XQuery("let $a := <elem/> let $b := <elem2/> return ($a,$b)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem/><elem2/>",false));
}
