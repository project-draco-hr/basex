{
  final XQuery query=new XQuery("\"a&#xD;aa&#xD;&#xA;a&#xD;&#xA;\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(((serializationMatches("a&#xD;aa&#xD;","i") || serializationMatches("a&#13;aa&#13;","")) && serializationMatches("\\r?\\n.*\\r?\\n","")));
}
