{
  print(' ');
  print(n);
  final byte[] val=escape && URIS.id(concat(lc(tag),COLON,lc(n))) != 0 ? escape(v) : v;
  print(ATT1);
  for (int k=0; k < val.length; k+=cl(val,k)) {
    final int ch=cp(val,k);
switch (ch) {
case '"':
      print(E_QU);
    break;
case 0x9:
case 0xA:
  hex(ch);
break;
default :
ch(ch);
}
}
print(ATT2);
}
