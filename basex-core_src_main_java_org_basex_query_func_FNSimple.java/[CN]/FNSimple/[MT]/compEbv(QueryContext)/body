{
  if (exprs.length == 0)   return this;
  final Expr e=exprs[0];
  Expr ex=this;
  if (func == Function.BOOLEAN) {
    if (!e.type().mayBeNumber())     ex=e;
  }
 else   if (func == Function.EXISTS) {
    if (e.type().type.isNode())     ex=e;
  }
  if (ex != this)   qc.compInfo(QueryText.OPTWRITE,this);
  return ex;
}
