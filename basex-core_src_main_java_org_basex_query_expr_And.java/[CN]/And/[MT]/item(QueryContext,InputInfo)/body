{
  if (qc.scoring) {
    double s=0;
    for (    final Expr e : exprs) {
      final Item it=e.ebv(qc,info);
      if (!it.bool(info))       return Bln.FALSE;
      s+=it.score();
    }
    return Bln.get(true,Scoring.avg(s,exprs.length));
  }
  for (  final Expr e : exprs) {
    if (!e.ebv(qc,info).bool(info))     return Bln.FALSE;
  }
  return Bln.TRUE;
}
