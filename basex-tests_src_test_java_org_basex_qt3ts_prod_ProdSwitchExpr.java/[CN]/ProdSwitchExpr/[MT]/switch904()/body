{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := 2; \n" + "        <out>{ switch ($in) \n"+ "            case 1 return \"Moo\" \n"+ "            case 5 return \"Meow\" \n"+ "            case 3 return \"Quack\" \n"+ "            case ($in to 4) return \"Oink\" }</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
