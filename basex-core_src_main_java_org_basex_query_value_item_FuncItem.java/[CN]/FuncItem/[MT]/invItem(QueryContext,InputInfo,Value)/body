{
  final Value cv=qc.value;
  final long ps=qc.pos, sz=qc.size;
  try {
    qc.value=ctxVal;
    qc.pos=pos;
    qc.size=size;
    for (int i=0; i < params.length; i++)     qc.set(params[i],args[i],ii);
    return expr.item(qc,ii);
  }
  finally {
    qc.value=cv;
    qc.pos=ps;
    qc.size=sz;
  }
}
