{
  final StringBuilder sb=new StringBuilder();
  if (!(query instanceof Str))   sb.append("{ ");
  sb.append(query.toString());
  if (!(query instanceof Str))   sb.append(" }");
  if (occ != null) {
    sb.append(QueryTokens.OCCURS + " " + occ[0]+ " "+ QueryTokens.TO+ " "+ occ[1]+ " "+ QueryTokens.TIMES);
  }
  return sb.toString();
}
