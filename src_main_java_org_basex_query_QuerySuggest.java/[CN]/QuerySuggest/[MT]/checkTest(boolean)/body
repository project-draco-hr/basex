{
  if (tag == null)   return;
  final ArrayList<PathNode> tmp=new ArrayList<PathNode>();
  boolean s=false;
  for (  final PathNode p : all) {
    final byte[] nm=p.token(data);
    if (startsWith(nm,tag)) {
      if (!eq || eq(nm,tag))       tmp.add(p);
      s|=!eq(tag,nm);
    }
  }
  show=tag.length == 0 || s;
  curr=tmp;
}
