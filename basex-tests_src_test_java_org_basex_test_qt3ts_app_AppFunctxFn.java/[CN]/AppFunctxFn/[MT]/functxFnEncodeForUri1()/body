{
  final XQuery query=new XQuery("(encode-for-uri( 'Sales % Numbers.pdf'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Sales%20%25%20Numbers.pdf"));
}
