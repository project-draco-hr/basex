{
  final XQuery query=new XQuery("let $ordDoc := (/) return (min($ordDoc//item/@color/string(.)))",ctx);
  try {
    query.context(node(file("app/FunctxFn/functx_order.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"beige"));
}
