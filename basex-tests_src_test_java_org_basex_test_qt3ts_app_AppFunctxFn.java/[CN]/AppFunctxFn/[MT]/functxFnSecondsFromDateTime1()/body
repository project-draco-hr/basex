{
  final XQuery query=new XQuery("(seconds-from-dateTime( xs:dateTime('2006-08-15T10:30:23.5')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"23.5"));
}
