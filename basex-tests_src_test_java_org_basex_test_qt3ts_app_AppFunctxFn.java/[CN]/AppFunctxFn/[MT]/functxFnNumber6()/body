{
  final XQuery query=new XQuery("let $priceDoc := (/) return ($priceDoc// prod/price[number() >\n" + "         35])",ctx);
  try {
    query.context(node(file("app/FunctxFn/functx_prices.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<price currency=\"USD\">69.99</price><price currency=\"USD\">39.99</price>",false));
}
