{
  final XQuery query=new XQuery("(string-to-codepoints('abc'), string-to-codepoints('a'), string-to-codepoints(''))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"97 98 99 97"));
}
