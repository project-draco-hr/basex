{
  final XQuery query=new XQuery("(avg( (xs:yearMonthDuration('P4M'), xs:yearMonthDuration('P6M') ) ))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P5M"));
}
