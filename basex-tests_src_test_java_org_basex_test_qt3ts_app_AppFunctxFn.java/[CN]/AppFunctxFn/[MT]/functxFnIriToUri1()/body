{
  final XQuery query=new XQuery("(iri-to-uri( 'http://datypic.com/Sales Numbers.pdf'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://datypic.com/Sales%20Numbers.pdf"));
}
