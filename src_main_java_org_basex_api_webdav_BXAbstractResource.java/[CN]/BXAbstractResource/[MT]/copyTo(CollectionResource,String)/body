{
  Session s=null;
  try {
    s=session.login();
    if (target instanceof BXRootResource)     copyToRoot(s,name);
 else     if (target instanceof BXFolder)     copyTo(s,(BXFolder)target,name);
  }
 catch (  final Exception ex) {
    error(ex);
  }
 finally {
    try {
      if (s != null)       s.close();
    }
 catch (    final IOException e) {
      error(e);
    }
  }
}
