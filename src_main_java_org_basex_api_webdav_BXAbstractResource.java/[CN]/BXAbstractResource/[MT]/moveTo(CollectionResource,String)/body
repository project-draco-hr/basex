{
  try {
    final Session s=factory.login(user,pass);
    try {
      if (target instanceof BXAllDatabasesResource)       moveToRoot(s,name);
 else       if (target instanceof BXFolder)       moveTo(s,(BXFolder)target,name);
    }
  finally {
      s.close();
    }
  }
 catch (  final Exception ex) {
    handle(ex);
  }
}
