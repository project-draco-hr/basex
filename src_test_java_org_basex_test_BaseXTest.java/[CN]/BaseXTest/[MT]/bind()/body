{
  equals("1","-ba=1","-q$a");
  equals("2","-ba=1","-bb=1","-q$a+$b");
  equals("3","-ba=1","-bb=2","-q$a+$b");
  INPUT.write(token("$a"));
  equals("4","-ba=4",INPUT.toString());
  equals("5,6;7'","-ba=5,6;7'","-q$a");
  equals("8","-b{}a=8","-q$a");
  equals("9","-b{URI}a=9","-qdeclare namespace a='URI'; $a:a");
  equals("12","-ba=1","-q$a","-ba=2","-q$a");
}
