{
  IN.write(token("$a"));
  new BaseX("-ba=1 -o" + OUT + " "+ IN);
  assertEquals("1",string(OUT.read()));
  IN.write(token("$a + $b"));
  new BaseX("-ba=1 -bb=2 -o" + OUT + " "+ IN);
  assertEquals("3",string(OUT.read()));
  new BaseX("-ba=1,b=2 -o" + OUT + " "+ IN);
  assertEquals("3",string(OUT.read()));
}
