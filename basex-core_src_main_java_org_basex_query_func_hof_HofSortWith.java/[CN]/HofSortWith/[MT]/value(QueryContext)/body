{
  final Value v=exprs[0].value(qc);
  final Comparator<Item> cmp=getComp(1,qc);
  if (v.size() < 2)   return v;
  final ValueBuilder vb=v.cache();
  try {
    Arrays.sort(vb.items(),0,(int)vb.size(),cmp);
  }
 catch (  final QueryRTException ex) {
    throw ex.getCause();
  }
  return vb.value();
}
