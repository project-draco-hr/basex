{
  if (hash == o.hash)   return key.sameKey(o.key,ii) ? this : new TrieList(hash,key,value,o.key,o.value);
  final TrieNode[] ch=new TrieNode[KIDS];
  final int k=key(hash,l), ok=key(o.hash,l), nu;
  if (k == ok) {
    ch[k]=add(o,l + 1,ii);
    nu=1 << k;
  }
 else {
    ch[k]=this;
    ch[ok]=o;
    nu=1 << k | 1 << ok;
  }
  return new TrieBranch(ch,nu,2);
}
