{
  final XQuery query=new XQuery("fn:lower-case(fn:string(fn:contains(fn:document-uri(fn:doc($uri)),$uri)))",ctx);
  query.context(node(file("docs/works-mod.xml")));
  query.bind("uri",new XQuery("'http://www.w3.org/fots/docs/works-mod.xml'",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"true") || assertStringValue(false,"false")));
}
