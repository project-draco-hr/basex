{
  final XQuery query=new XQuery("fn:substring-before(fn:string(fn:contains((/)/document-uri(),\"works-mod\")),\"e\")",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"tru"));
}
