{
  final XQuery query=new XQuery("fn:contains((fn:doc($uri))/document-uri(),$uri) or ((fn:doc($uri))/document-uri() = \"\")",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    query.bind("uri",new XQuery("'http://www.w3.org/fots/docs/works-mod.xml'",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || assertBoolean(false)));
}
