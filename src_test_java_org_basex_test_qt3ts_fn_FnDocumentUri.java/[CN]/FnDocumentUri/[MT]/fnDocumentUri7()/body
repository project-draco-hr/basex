{
  final XQuery query=new XQuery("fn:count(fn:document-uri(processing-instruction {\"PITarget\"} {\"PIContent\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
