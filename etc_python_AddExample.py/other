import BaseXClient, time
try:
    start = time.clock()
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    print session.execute('create db testDB')
    print session.info()
    session.add('World.xml', '', '<x>Hello World!</x>')
    print session.info()
    session.close()
    time = ((time.clock() - start) * 1000)
    print time, 'ms.'
except IOError as e:
    print e
