{
  final XQuery query=new XQuery("/works[1]/employee[1]/empnum[1] >> <a>50000</a>",ctx);
  try {
    query.context(node(file("docs/works.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || assertBoolean(true)));
}
