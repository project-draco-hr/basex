{
  final XQuery query=new XQuery("fn:matches(\"a\", \"a[^b]\"), fn:matches(\"a \", \"a[^b]\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("false(), true()"));
}
