{
  final XQuery query=new XQuery("fn:matches(\"hello world\", \"\\p{ I s B a s i c L a t i n }+\", \"x\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
