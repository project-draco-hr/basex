{
  final XQuery query=new XQuery("typeswitch(current-time()) case node() return 0 case xs:integer return 3 case xs:anyAtomicType return true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
