{
  final XQuery query=new XQuery("\n" + "\t for $x in (<e/>, 1, \"x\") return\n" + "\t typeswitch ($x) \n"+ "\t case $i as xs:integer | xs:boolean | element() return 1\n"+ "\t default $v return 0\n"+ "\t ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
