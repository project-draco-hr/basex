{
  final XQuery query=new XQuery("\n" + "\t typeswitch (47) \n" + "\t case $i as xs:integer | xs:string return $i\n"+ "\t case $i as xs:integer return $i\n"+ "\t default $v return 0\n"+ "\t ",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertEq("47") && assertType("xs:integer")));
}
