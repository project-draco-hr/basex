{
  final XQuery query=new XQuery("\n" + "\t typeswitch (xs:integer(47)) \n" + "\t case $i as xs:decimal | xs:integer return $i\n"+ "\t case $i as xs:decimal return $i\n"+ "\t default $v return 0\n"+ "\t ",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertEq("47") && assertType("xs:decimal")));
}
