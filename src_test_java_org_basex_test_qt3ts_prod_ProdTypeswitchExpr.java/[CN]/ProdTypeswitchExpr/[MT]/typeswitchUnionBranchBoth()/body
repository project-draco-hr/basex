{
  final XQuery query=new XQuery("\n" + "\t typeswitch (xs:integer(47)) \n" + "\t case $i as xs:decimal | xs:integer return $i\n"+ "\t default $v return 0\n"+ "\t ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("47") && assertType("xs:decimal")));
}
