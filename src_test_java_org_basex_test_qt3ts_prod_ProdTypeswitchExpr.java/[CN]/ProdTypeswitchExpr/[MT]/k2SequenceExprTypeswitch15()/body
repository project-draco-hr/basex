{
  final XQuery query=new XQuery("<e/>/(typeswitch (self::node()) case $i as xs:integer return $i default $v return $v)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e/>",false));
}
