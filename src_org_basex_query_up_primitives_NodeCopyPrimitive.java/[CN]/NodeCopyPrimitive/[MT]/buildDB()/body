{
  final SeqIter seq=new SeqIter();
  if (c.size() == 0)   return null;
  final Iterator<Iter> it=c.iterator();
  while (it.hasNext()) {
    final Iter ni=it.next();
    ni.reset();
    Item i=ni.next();
    while (i != null) {
      seq.add(i);
      i=ni.next();
    }
  }
  return UpdateFunctions.buildDB(seq,((DBNode)node).data);
}
