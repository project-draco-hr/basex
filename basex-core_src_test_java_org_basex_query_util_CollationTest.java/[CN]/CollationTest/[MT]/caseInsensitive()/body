{
  final String prolog="declare default collation " + "'http://www.w3.org/2005/xpath-functions/collation/html-ascii-case-insensitive'; ";
  query(prolog + "'A'='A'",true);
  query(prolog + "'A'='a'",true);
  query(prolog + "'a'='A'",true);
  query(prolog + "'\u00c4'='A\u00e4'",false);
  query(prolog + "compare('a', 'A')","0");
}
