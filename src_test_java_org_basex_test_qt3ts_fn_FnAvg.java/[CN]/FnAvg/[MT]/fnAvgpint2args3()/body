{
  final XQuery query=new XQuery("fn:avg((xs:positiveInteger(\"999999999999999999\"),xs:positiveInteger(\"1\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("500000000000000000"));
}
