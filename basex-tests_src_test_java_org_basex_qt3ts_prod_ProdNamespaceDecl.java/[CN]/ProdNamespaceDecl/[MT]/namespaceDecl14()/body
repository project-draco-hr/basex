{
  final XQuery query=new XQuery("declare namespace XML = \"http://www.example.com/examples\"; let $var := <XML:someElement>some context</XML:someElement> return $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<XML:someElement xmlns:XML=\"http://www.example.com/examples\">some context</XML:someElement>",false));
}
