{
  final XQuery query=new XQuery("\n" + "        xquery version \"3.0\";\n" + "      \tdeclare function local:square($i as xs:integer) as xs:integer { $i*$i };\n"+ "        function-lookup(QName(\"http://www.w3.org/2005/xquery-local-functions\", \"square\"), 1)(13)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("169"));
}
