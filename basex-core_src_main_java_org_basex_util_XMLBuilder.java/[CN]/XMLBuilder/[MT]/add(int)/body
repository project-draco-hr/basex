{
  if (cp < ' ' && cp != '\n' && cp != '\t' || cp >= 0x7F && cp < 0xA0) {
    addHex(cp);
  }
 else   if (cp == '&') {
    cache.add(E_AMP);
  }
 else   if (cp == '>') {
    cache.add(E_GT);
  }
 else   if (cp == '<') {
    cache.add(E_LT);
  }
 else   if (cp == 0x2028) {
    cache.add(E_2028);
  }
 else {
    cache.add(cp);
  }
}
