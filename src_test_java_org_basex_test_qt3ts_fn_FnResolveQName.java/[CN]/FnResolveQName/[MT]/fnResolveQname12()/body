{
  final XQuery query=new XQuery("fn:string(fn:namespace-uri-from-QName(fn:resolve-QName(xs:string(\"p1:anElement\"), <anElement xmlns:p1=\"http://example.com/examples\">Some content</anElement>)))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://example.com/examples"));
}
