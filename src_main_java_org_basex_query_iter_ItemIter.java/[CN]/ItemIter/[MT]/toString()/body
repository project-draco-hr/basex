{
  final StringBuilder sb=new StringBuilder();
  if (size != 1)   sb.append("(");
  for (int v=0; v != size; ++v) {
    sb.append((v != 0 ? ", " : "") + item[v]);
    if (sb.length() > 15 && v + 1 != size) {
      sb.append(", ...");
      break;
    }
  }
  if (size != 1)   sb.append(")");
  return sb.toString();
}
