{
  for (int n=0; n < TESTS; n++) {
    final int j=n;
    Performance.sleep(rand.nextInt(500));
    new Thread(){
      @Override public void run(){
        try {
          final int t=rand.nextInt(2);
          sessions.get(j).execute(q[t]);
          tdone++;
        }
 catch (        final IOException ex) {
          fail(ex.toString());
        }
      }
    }
.start();
  }
  while (tdone < TESTS)   Performance.sleep(200);
}
