{
  final XQuery query=new XQuery("(: Name: Constr-ws-nobound-3 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve leading tab :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>\tx</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("string-to-codepoints(string($result))[1] = 9"));
}
