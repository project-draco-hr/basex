{
  final XQuery query=new XQuery("(: Name: Constr-ws-adjchref-3 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve tab adjacent to character reference :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>\t&#x30;\t</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (9, 48, 9))"));
}
