{
  final XQuery query=new XQuery("(: Name: Constr-ws-genenclexpr-2 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve enclosed exp line feed :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>{\"\n"+ "\"}</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (10))"));
}
