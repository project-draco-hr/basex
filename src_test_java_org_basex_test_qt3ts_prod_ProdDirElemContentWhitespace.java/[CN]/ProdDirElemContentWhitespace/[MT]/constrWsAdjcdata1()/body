{
  final XQuery query=new XQuery("(: Name: Constr-ws-adjcdata-1 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve space adjacent to cdata section :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem> <![CDATA[]]> </elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>  </elem>",false));
}
