{
  final XQuery query=new XQuery("declare boundary-space strip; <elem>{1} </elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>1</elem>",false));
}
