{
  final XQuery query=new XQuery("(: Name: Constr-ws-adjcdata-3 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve tab adjacent to cdata section :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>\t<![CDATA[]]>\t</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (9, 9))"));
}
