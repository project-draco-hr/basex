{
  final XQuery query=new XQuery("(: Name: Constr-ws-adjcdata-2 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve line feed adjacent to cdata section :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>\n"+ "<![CDATA[]]>\n"+ "</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (10, 10))"));
}
