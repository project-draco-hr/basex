{
  final XQuery query=new XQuery("declare boundary-space strip; <elem>&#xA;</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (10))"));
}
