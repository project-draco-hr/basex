{
  final XQuery query=new XQuery("declare boundary-space strip; <elem xml:space=\"preserve\"> </elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem xml:space=\"preserve\"/>",false));
}
