{
  final XQuery query=new XQuery("(: Name: Constr-ws-enclexpr-24 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve whitespace tab between child elements :)\n"+ "\n"+ "declare boundary-space preserve;\n"+ "<elem>\t\t\t<a/>\t\t\t<b/>\t\t\t</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>\t\t\t<a/>\t\t\t<b/>\t\t\t</elem>",false));
}
