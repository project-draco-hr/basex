{
  final XQuery query=new XQuery(queryFile(file("prod/DirElemContent.whitespace/Constr-ws-enclexpr-24.xq")),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>\t\t\t<a/>\t\t\t<b/>\t\t\t</elem>",false));
}
