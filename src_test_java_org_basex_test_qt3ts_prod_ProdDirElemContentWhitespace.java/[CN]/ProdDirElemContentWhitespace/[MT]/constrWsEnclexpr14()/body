{
  final XQuery query=new XQuery("declare boundary-space preserve; <elem>\n" + "\n" + "\n"+ "{1}</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>\n\n\n1</elem>",false));
}
