{
  final XQuery query=new XQuery("(: Name: Constr-ws-genenclexpr-3 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve enclosed exp tab :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>{\"\t\"}</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (9))"));
}
