{
  final XQuery query=new XQuery("(: Name: Constr-ws-nobound-2 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve leading line feed :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>\n"+ "x</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("string-to-codepoints(string($result))[1] = 10"));
}
