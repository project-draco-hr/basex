{
  final XQuery query=new XQuery("(: Name: Constr-ws-enclexpr-22 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve whitespace space between child elements :)\n"+ "\n"+ "declare boundary-space preserve;\n"+ "<elem>   <a/>   <b/>   </elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>   <a/>   <b/>   </elem>",false));
}
