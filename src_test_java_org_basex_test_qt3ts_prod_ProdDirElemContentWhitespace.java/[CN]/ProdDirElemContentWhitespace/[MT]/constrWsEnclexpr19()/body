{
  final XQuery query=new XQuery("(: Name: Constr-ws-enclexpr-19 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve whitespace space between enclosed expression and close tag :)\n"+ "\n"+ "declare boundary-space preserve; \n"+ "<elem>{1}   </elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>1   </elem>",false));
}
