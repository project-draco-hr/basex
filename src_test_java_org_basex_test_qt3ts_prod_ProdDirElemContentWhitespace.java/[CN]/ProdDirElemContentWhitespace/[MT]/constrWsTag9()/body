{
  final XQuery query=new XQuery("declare boundary-space preserve; <elem>   \t\n" + "      \t    <a>          \t         \n" + "\t\t\n"+ "\t\n"+ "        </a>\t\n"+ "<a>        \t     </a>             <a>\t  <b>\n"+ "\n"+ "         \t </b>\n"+ "\n"+ "  </a>\t\n"+ "\n"+ "\t\t\n"+ "      </elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>   \t\n      \t    <a>          \t         \n\t\t\n\t\n        </a>\t\n<a>        \t     </a>             <a>\t  <b>\n\n         \t </b>\n\n  </a>\t\n\n\t\t\n      </elem>",false));
}
