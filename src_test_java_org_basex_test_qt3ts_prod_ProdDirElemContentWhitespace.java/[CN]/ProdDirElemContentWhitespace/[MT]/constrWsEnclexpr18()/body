{
  final XQuery query=new XQuery("(: Name: Constr-ws-enclexpr-18 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve whitespace tab between enclosed expressions :)\n"+ "\n"+ "declare boundary-space preserve; \n"+ "<elem>{1}\t\t\t{2}</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>1\t\t\t2</elem>",false));
}
