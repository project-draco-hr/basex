{
  final XQuery query=new XQuery("(: Name: Constr-ws-enclexpr-15 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve whitespace tab between open tag and enclosed expression :)\n"+ "\n"+ "declare boundary-space preserve; \n"+ "<elem>\t\t\t{1}</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem>\t\t\t1</elem>",false));
}
