{
  final XQuery query=new XQuery("(: Name: Constr-ws-nobound-5 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve trailing line feed :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>x\n"+ "</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (120, 10))"));
}
