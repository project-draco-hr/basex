{
  final XQuery query=new XQuery("(: Name: Constr-ws-adjchref-2 :)\n" + "(: Written by: Andreas Behm :)\n" + "(: Description: preserve line feed adjacent to character reference :)\n"+ "\n"+ "declare boundary-space strip;\n"+ "<elem>\n"+ "&#x30;\n"+ "</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (10, 48, 10))"));
}
