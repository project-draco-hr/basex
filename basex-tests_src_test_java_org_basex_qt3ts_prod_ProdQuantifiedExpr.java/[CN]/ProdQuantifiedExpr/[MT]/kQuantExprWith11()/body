{
  final XQuery query=new XQuery("every $a as empty-sequence() in (1, 2), $b as xs:integer in $a satisfies $b",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
