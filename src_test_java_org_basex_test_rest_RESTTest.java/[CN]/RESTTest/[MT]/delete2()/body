{
  put(DB,null);
  post(DB + "/a",stream("<a/>"));
  post(DB + "/a",stream("<a/>"));
  post(DB + "/b",stream("<b/>"));
  assertContains(delete(DB + "/a"),"2 document");
  assertContains(delete(DB + "/b"),"1 document");
  assertContains(delete(DB + "/b"),"0 document");
  assertEquals(delete(DB).trim(),Util.info(DBDROPPED,DB));
  try {
    delete(DB);
    fail("Error expected.");
  }
 catch (  final FileNotFoundException ex) {
  }
}
