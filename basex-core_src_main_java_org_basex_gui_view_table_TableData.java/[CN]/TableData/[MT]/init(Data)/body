{
  roots=new TokenList();
  for (  final byte[] k : data.paths.desc(EMPTY,true,true)) {
    int c=0;
    for (    final byte[] kk : data.paths.desc(k,true,false)) {
      final Names nm=startsWith(kk,'@') ? data.attrNames : data.elemNames;
      if (nm.stat(nm.id(delete(kk,'@'))).isLeaf())       ++c;
    }
    if (c > 2)     roots.add(k);
  }
  init(data,-1);
}
