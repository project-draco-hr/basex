{
  parseAttrs(multipart,attrs);
  if (attrs.get(MEDIATYPE) == null)   REQINV.thrw(ii,"Attribute media-type of http:multipart is mandatory");
  final AxisMoreIter i=multipart.children();
  if (contItems == null) {
    for (ANode n; (n=i.next()) != null; )     parts.add(parsePart(n,null,ii));
  }
 else {
    for (ANode n; (n=i.next()) != null; )     parts.add(parsePart(n,contItems.next(),ii));
  }
}
