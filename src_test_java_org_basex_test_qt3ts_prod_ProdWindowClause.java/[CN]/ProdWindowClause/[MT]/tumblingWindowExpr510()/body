{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w in (1 to 10)\n" + "          start $s when true()\n"+ "          end $e when true()\n"+ "          return <window>{$w}</window>\n"+ "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<window>1</window><window>2</window><window>3</window><window>4</window><window>5</window><window>6</window><window>7</window><window>8</window><window>9</window><window>10</window>",false));
}
