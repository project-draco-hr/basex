{
  final XQuery query=new XQuery("\n" + "          declare function local:window()\n" + "          {\n"+ "            for tumbling window $w in (1 to 10)\n"+ "            start $s when true()\n"+ "            end $e when $e - $s eq 3\n"+ "            return <window>{$w}</window>\n"+ "          };\n"+ "          \n"+ "          local:window()\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window>1 2 3 4</window><window>5 6 7 8</window><window>9 10</window>",false));
}
