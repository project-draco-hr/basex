{
  final XQuery query=new XQuery("\n" + "          for $w at $y in (1 to 2)\n" + "          for tumbling window $w in (2, 4, 6, 8, 10, 12, 14)\n"+ "          start $first when $first mod $y = 0\n"+ "          return <window>{ $y }</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window>1</window><window>1</window><window>1</window><window>1</window><window>1</window><window>1</window><window>1</window><window>2</window><window>2</window><window>2</window><window>2</window><window>2</window><window>2</window><window>2</window>",false));
}
