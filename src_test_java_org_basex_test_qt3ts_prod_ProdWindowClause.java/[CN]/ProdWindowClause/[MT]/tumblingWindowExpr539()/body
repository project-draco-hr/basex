{
  final XQuery query=new XQuery("\n" + "          for $i in 1 to 3\n" + "          count $r\n"+ "          for tumbling window $w in (1 to 10)\n"+ "          start $s when true()\n"+ "          end $e when $e - $s eq 2\n"+ "          where $w = $r + 1\n"+ "          return <window num=\"{$r}\">{$w}</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window num=\"1\">1 2 3</window><window num=\"2\">1 2 3</window><window num=\"3\">4 5 6</window>",false));
}
