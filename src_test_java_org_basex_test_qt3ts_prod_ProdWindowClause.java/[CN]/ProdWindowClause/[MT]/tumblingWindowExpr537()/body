{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w in (1 to 10)\n" + "          start $s when true()\n"+ "          end $e when $e - $s eq 2\n"+ "          count $r\n"+ "          return <window num=\"{$r}\">{$w}</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window num=\"1\">1 2 3</window><window num=\"2\">4 5 6</window><window num=\"3\">7 8 9</window><window num=\"4\">10</window>",false));
}
