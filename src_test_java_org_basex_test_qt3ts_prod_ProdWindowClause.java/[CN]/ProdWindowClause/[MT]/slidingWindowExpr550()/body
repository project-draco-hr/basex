{
  final XQuery query=new XQuery("\n" + "          for sliding window $w in (1 to 3)\n" + "          start when true()\n"+ "          end when false()\n"+ "          return <window>{$w}</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window>1 2 3</window><window>2 3</window><window>3</window>",false));
}
