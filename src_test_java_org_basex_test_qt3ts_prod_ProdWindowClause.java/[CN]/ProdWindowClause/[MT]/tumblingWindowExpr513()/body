{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w in (1 to 10)\n" + "          start $s at $x when true()\n"+ "          only end $e at $y when $y - $x eq 2\n"+ "          return <window>{$w}</window>\n"+ "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<window>1 2 3</window><window>4 5 6</window><window>7 8 9</window>",false));
}
