{
  final XQuery query=new XQuery("\n" + "          for sliding window $w in (1 to 10)\n" + "          start $s at $x previous $sp next $sn when true()\n"+ "          end $e at $y previous $ep next $en when false() \n"+ "          where count($w) eq 10 and $x eq 1 and empty($sp) and $sn eq 2 and $e eq 10 and $y eq 10 and $ep eq 9 and empty($en)\n"+ "          return true()\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
