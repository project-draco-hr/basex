{
  final XQuery query=new XQuery("\n" + "          for sliding window $w in (1 to 10)\n" + "          start $s when true()\n"+ "          end $e when $e - $s eq 2\n"+ "          count $r\n"+ "          where $r le 2\n"+ "          return <window num=\"{$r}\">{$w}</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window num=\"1\">1 2 3</window><window num=\"2\">2 3 4</window>",false));
}
