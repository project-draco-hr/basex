{
  final XQuery query=new XQuery("for sliding window $w in (1, 2, 3, 4) \n" + "      start $s at $ps previous $pps when fn:true()\n" + "      only end $s at $ps previous $pps when $ps - $ps eq 1\n"+ "      return $w",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XQST0103"));
}
