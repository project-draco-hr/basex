{
  final XQuery query=new XQuery("\n" + "          for sliding window $w in (1 to 10)\n" + "          start $s when true()\n"+ "          end $e when $e - $w eq 2\n"+ "          return <window>{$w}</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XPST0008"));
}
