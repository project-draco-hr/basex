{
  final XQuery query=new XQuery("\n" + "        <o>{\n" + "          for tumbling window $w in (2, 4, 6, 8, 10, 12, 14)\n"+ "          start $first when $first mod 3 = 0\n"+ "          return <window>{ $w }</window>\n"+ "        }</o>  \n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<o><window>6 8 10</window><window>12 14</window></o>",false));
}
