{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w in (1 to 10)\n" + "          start $s when true()\n"+ "          only end $e when $e - $s eq 2\n"+ "          count $r\n"+ "          return\n"+ "            <window num=\"{$r}\">{\n"+ "              for $i in $w\n"+ "              order by $i descending\n"+ "              return $i\n"+ "            }</window>\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<window num=\"1\">3 2 1</window><window num=\"2\">6 5 4</window><window num=\"3\">9 8 7</window>",false));
}
