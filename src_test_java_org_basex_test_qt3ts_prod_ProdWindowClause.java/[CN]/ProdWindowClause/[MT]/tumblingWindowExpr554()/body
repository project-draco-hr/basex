{
  final XQuery query=new XQuery("\n" + "          declare variable $local:foo as xs:integer* := 1 to 5;\n" + "          \n"+ "          declare function local:window()\n"+ "          {\n"+ "            for tumbling window $w1 in $local:foo\n"+ "            start $s when true()\n"+ "            end $e when $e - $s eq 3\n"+ "            for tumbling window $w2 in $w1\n"+ "            start $s when true()\n"+ "            end $e when true()\n"+ "            return\n"+ "            <window>{\n"+ "              fn:distinct-values($w1[.=$w2])\n"+ "            }</window>\n"+ "          };\n"+ "          \n"+ "          local:window()\n"+ "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<window>1</window><window>2</window><window>3</window><window>4</window><window>5</window>",false));
}
