{
  final XQuery query=new XQuery("for sliding window $w in (1, 2, 3, 4) \n" + "      start $s at $s previous $s when fn:true()\n" + "      only end $s at $s previous $s when $s - $s eq 1\n"+ "      return $w",ctx);
  final QT3Result res=result(query);
  result=res;
  test((error("XQST0089") || error("XQST0103")));
}
