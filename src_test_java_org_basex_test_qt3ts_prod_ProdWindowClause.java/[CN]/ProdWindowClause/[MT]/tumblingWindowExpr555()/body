{
  final XQuery query=new XQuery("\n" + "          declare variable $local:foo as xs:integer* := 1 to 10;\n" + "          \n"+ "          declare function local:window()\n"+ "          {\n"+ "            for tumbling window $w in $local:foo\n"+ "            start $s when false()\n"+ "            end $e when false()\n"+ "            return\n"+ "              <window>{$w}</window>\n"+ "          };\n"+ "          \n"+ "          local:window()\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEmpty());
}
