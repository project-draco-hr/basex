{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w1 in\n" + "            for tumbling window $w2 in (1 to 10)\n"+ "            start $s when true()\n"+ "            only end $e when $e - $s eq 2\n"+ "            return $w2\n"+ "          start $s when true()\n"+ "          end $e when $e - $s eq 2\n"+ "          return <window>{$w1}</window>\n"+ "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<window>1 2 3</window><window>4 5 6</window><window>7 8 9</window>",false));
}
