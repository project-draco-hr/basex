{
  final XQuery query=new XQuery("\n" + "          for sliding window $w in (1, 2, 3, 4, 14, 13, 12, 11) \n" + "          start $s when fn:true()\n"+ "          only end $e when $e eq $s + 10\n"+ "          return string-join($w!string(), ' ')\n"+ "        ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertDeepEq("\"1 2 3 4 14 13 12 11\", \"2 3 4 14 13 12\", \"3 4 14 13\", \"4 14\""));
}
