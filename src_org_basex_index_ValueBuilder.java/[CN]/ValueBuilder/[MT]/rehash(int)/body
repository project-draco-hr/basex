{
  final int l=bucket.length;
  if (s == l)   return;
  final int[] tmp=new int[s];
  for (int i=0; i < l; i++) {
    int tid=bucket[i];
    while (tid != 0) {
      final int pp=hash(pos[tid]) & s - 1;
      final int nx=next[tid];
      next[tid]=tmp[pp];
      tmp[pp]=tid;
      tid=nx;
    }
  }
  bucket=tmp;
  next=Array.extend(next);
  ids=Array.extend(ids);
  pos=Array.extend(pos);
}
