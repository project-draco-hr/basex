{
  final XQuery query=new XQuery("serialize(doc('serialize/serialize-008-src.xml'),\n" + "            doc('serialize/serialize-008-params.xml')/params/*)",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertQuery("matches($result,'\\n\\s+<title>')") && assertQuery("matches($result,'\\n\\s+<p>')") && assertQuery("not(matches($result,'\\n\\s+<code>'))")));
}
