{
  final XQuery query=new XQuery("if (implicit-timezone() eq xs:dayTimeDuration('-PT9M'))\n" + "            then xs:gYear(\"2008\") ne xs:gYear(\"2008-09:01\")\n" + "            else xs:gYear(\"2008\") ne xs:gYear(\"2008-09:00\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
