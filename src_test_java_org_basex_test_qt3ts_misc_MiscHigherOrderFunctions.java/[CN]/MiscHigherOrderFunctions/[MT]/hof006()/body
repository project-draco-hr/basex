{
  final XQuery query=new XQuery("\n" + "      declare default function namespace \"http://example.com/hof-006\"; \n" + "      declare function g($x as xs:integer) as xs:integer { $x + 1 }; \n"+ "      let $f := g#1 return $f(21)\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("22"));
}
