{
  final XQuery query=new XQuery("\n" + "for $f in (concat(\"one \", ?, \" three\"), substring-before(\"one two three\", ?), matches(?, \"t.*o\"), xs:NCName(?))\n" + "return $f(\"two\")\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertDeepEq("\"one two three\", \"one \", true(), xs:NCName(\"two\")"));
}
