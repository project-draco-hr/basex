{
  final XQuery query=new XQuery("let $f := concat#1234, $n := function-name($f) \n" + "        return (local-name-from-QName($n), namespace-uri-from-QName($n), function-arity($f))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"concat http://www.w3.org/2005/xpath-functions 1234"));
}
