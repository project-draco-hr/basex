{
  final XQuery query=new XQuery("let $f := concat#123456, $n := function-name($f) \n" + "        return (local-name-from-QName($n), namespace-uri-from-QName($n), function-arity($f))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"concat http://www.w3.org/2005/xpath-functions 123456"));
}
