{
  final XQuery query=new XQuery("\n" + "        let $f := function($a as xs:string, $b as xs:string) { \n" + "            starts-with($a, $b) and ends-with($a, $b)}(?, 'a') \n"+ "        return <out>{$f('abracadabra')}</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>true</out>",false));
}
