{
  final XQuery query=new XQuery("(let $a := 92, $b := true() return function($c) { $a, $b, $c }) instance of function(item()*) as item()*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
