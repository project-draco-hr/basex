{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) as xs:integer { $x + 1 }; \n" + "      \tlocal:f#1 eq 3\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOTY0013"));
}
