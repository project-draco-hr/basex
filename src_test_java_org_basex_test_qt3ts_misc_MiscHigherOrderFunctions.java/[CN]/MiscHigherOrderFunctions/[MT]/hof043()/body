{
  final XQuery query=new XQuery("\n" + "      declare function local:round($x as xs:double) as xs:double \n" + "      \t{ fn:floor($x) }; \n"+ "      declare function local:ops() as (function(xs:double) as xs:double)* \n"+ "      \t{ (abs#1, local:round#1, function($x){$x+1}, round-half-to-even(?, 2)) }; \n"+ "      string-join(for $f in local:ops() return string($f(xs:untypedAtomic('123.456'))), '~')\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"123.456~123~124.456~123.46"));
}
