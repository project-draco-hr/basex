{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) as xs:integer { $x + 1 }; \n" + "      \tdeep-equal((1,2,3,4,local:f#1), (1,2,3,4,local:f#1))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOTY0015"));
}
