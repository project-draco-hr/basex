{
  final XQuery query=new XQuery("let $f := fn:substring-before#2(?, '-') return <out>{$f('the-end-of-the-world')}</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>the</out>",false));
}
