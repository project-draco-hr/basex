{
  final XQuery query=new XQuery("\n" + "      declare function local:scramble($x as function(xs:string) as xs:string, $y as xs:string) as xs:string { $x($y) }; \n" + "      local:scramble(function($x){translate($x, \"abcdefghijklmnopqrstuvwxyz\", \"nopqrstuvwxyzabcdefghijklm\")}, \"john\")\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"wbua"));
}
