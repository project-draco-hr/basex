{
  final XQuery query=new XQuery("\n" + "         let $f := <b/>/name#0\n" + "         return <a/>/$f()\n"+ "      \n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("\"b\""));
}
