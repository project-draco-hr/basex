{
  final XQuery query=new XQuery("let $f := concat#8 return $f('a','b','c','d','e','f','g','h')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abcdefgh"));
}
