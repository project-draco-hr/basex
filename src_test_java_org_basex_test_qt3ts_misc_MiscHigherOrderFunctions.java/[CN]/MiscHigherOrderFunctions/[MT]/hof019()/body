{
  final XQuery query=new XQuery("\n" + "      declare function local:scramble($x as function(*), $y as xs:string) as xs:string { $x($y) }; \n" + "      declare function local:rot13($x as xs:string) as xs:string { translate($x, \"abcdefghijklmnopqrstuvwxyz\", \"nopqrstuvwxyzabcdefghijklm\") }; \n"+ "      local:scramble(local:rot13#1, \"mike\")\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"zvxr"));
}
