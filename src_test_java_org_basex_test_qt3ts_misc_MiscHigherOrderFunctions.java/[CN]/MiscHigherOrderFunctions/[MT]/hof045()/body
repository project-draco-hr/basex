{
  final XQuery query=new XQuery("\n" + "      declare function local:round($x as xs:double, $algorithm as (function(xs:double) as xs:double)) as xs:double { $algorithm($x) }; \n" + "      declare variable $roundToCeiling := local:round(?, ceiling#1); $roundToCeiling(12.4)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("13"));
}
