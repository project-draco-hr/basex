{
  final XQuery query=new XQuery("\n" + "      declare function local:case($x as function(*), $y as xs:string) as xs:string { $x($y) }; \n" + "      local:case(upper-case#1, \"Mike\"), local:case(lower-case#1, \"Mike\")\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"MIKE mike"));
}
