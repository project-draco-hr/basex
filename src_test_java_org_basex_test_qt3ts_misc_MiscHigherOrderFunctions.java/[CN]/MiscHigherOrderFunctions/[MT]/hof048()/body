{
  final XQuery query=new XQuery("\n" + "        declare function local:do() as xs:integer { (local:f#1)(5) }; \n" + "        declare function local:f($x as xs:integer) as xs:integer { $x + 1 }; \n"+ "        local:do()",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("6"));
}
