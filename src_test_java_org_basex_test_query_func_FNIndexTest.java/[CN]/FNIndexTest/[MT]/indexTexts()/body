{
  check(_INDEX_TEXTS);
  String entries=_INDEX_TEXTS.args(DB);
  query("count(" + entries + ")",5);
  query("exists(" + entries + "/self::value)","true");
  query(entries + "/@count = 1","true");
  query(entries + "/@count != 1","false");
  entries=_INDEX_TEXTS.args(DB,"X");
  query("count(" + entries + ")",1);
}
