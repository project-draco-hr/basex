{
  final XQuery query=new XQuery("xs:duration(\"P1Y2M3DT10H30M23S\") cast as xs:dayTimeDuration",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P3DT10H30M23S"));
}
