{
  final XQuery query=new XQuery("not(xs:NOTATION(\"prefix:local\") castable as xs:NOTATION)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPST0017") || error("XPST0080")));
}
