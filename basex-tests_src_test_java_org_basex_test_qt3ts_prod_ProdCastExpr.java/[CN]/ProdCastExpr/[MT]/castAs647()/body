{
  final XQuery query=new XQuery("xs:string(2.123456789123456789) cast as xs:decimal",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"2.123456789123456789") || error("FOCA0006")));
}
