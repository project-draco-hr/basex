{
  final int i=id(tok.get());
  if (i > 0) {
    final int cnt=len[i];
    if (cnt > 0) {
      final int[] pres=ids[i];
      return new IndexIterator(){
        private int p;
        @Override public boolean more(){
          return p < cnt;
        }
        @Override public int next(){
          return pres[p++];
        }
        @Override public double score(){
          return -1;
        }
        @Override public int size(){
          return cnt;
        }
      }
;
    }
  }
  return IndexIterator.EMPTY;
}
