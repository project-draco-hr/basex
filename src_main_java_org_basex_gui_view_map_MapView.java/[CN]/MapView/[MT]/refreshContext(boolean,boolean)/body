{
  final Nodes context=gui.context.current();
  final int hist=gui.notify.hist;
  final boolean page=!more && rectHist[hist + 1] != null && rectHist[hist + 1].pre == 0 || more && (context.size() != 1 || focused == null || context.pres[0] != focused.pre);
  if (page)   focused=new MapRect(0,0,getWidth(),1);
  zoom(more,quick);
}
