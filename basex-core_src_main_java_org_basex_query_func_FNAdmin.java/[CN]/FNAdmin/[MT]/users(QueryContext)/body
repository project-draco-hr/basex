{
  final ValueBuilder vb=new ValueBuilder();
  for (  final User u : expr.length == 0 ? ctx.context.users.users(null) : checkData(ctx).meta.users.users(ctx.context.users)) {
    vb.add(new FElem(USER).add(u.name).add(PERMISSION,u.perm.toString().toLowerCase(Locale.ENGLISH)).add(PASSWORD,u.password));
  }
  return vb;
}
