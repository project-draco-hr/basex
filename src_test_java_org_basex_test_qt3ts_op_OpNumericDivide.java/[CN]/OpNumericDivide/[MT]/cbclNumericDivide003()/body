{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:double) as xs:double { $arg * $arg }; \n" + "      \tlocal:square(7) div 1e0\n"+ "      \t",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"49"));
}
