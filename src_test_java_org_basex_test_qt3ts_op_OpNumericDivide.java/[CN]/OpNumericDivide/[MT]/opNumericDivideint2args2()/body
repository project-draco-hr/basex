{
  final XQuery query=new XQuery("fn:round-half-to-even((xs:int(\"-1873914410\") div xs:int(\"-2147483648\")),10)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0.8726093965"));
}
