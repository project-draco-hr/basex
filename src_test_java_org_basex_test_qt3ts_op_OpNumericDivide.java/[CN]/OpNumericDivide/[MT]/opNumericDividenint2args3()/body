{
  final XQuery query=new XQuery("xs:negativeInteger(\"-1\") div xs:negativeInteger(\"-999999999999999999\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0.000000000000000001") || assertEq("0.000000000000000001000000000000000001") || assertEq("0")));
}
