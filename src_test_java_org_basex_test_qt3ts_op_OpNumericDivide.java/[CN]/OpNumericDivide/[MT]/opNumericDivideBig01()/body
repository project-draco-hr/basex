{
  final XQuery query=new XQuery("(10000000000000000000000000000.0 div 0.1) cast as xs:string",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOAR0002") || assertStringValue(false,"100000000000000000000000000000")));
}
