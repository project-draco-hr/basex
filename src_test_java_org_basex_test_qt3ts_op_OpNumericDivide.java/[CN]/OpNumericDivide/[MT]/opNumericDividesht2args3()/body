{
  final XQuery query=new XQuery("xs:short(\"32767\") div xs:short(\"-32768\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("-0.999969482421875") || assertEq("-0.999969482421")));
}
