{
  if (root == null || !root.iterable())   return false;
  final int sl=steps.length;
  for (int s=0; s < sl; ++s) {
switch (((Step)steps[s]).axis) {
case ANC:
case ANCORSELF:
case PREC:
case PRECSIBL:
case PARENT:
      return false;
case DESC:
case DESCORSELF:
case FOLL:
case FOLLSIBL:
    return s + 1 == sl || s + 2 == sl && ((Step)steps[s + 1]).axis == ATTR;
default :
}
}
return true;
}
