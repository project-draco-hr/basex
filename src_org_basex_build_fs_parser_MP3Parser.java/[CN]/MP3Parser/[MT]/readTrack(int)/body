{
  byte[] value=readText(s);
  if (value == null)   return null;
  int size=value.length;
  int i=0;
  while (i < size && (value[i] < '0' || value[i] > '9')) {
    value[i]=0;
    i++;
  }
  if (i >= size - 1)   return null;
  while (i < size && value[i] >= '0' && value[i] <= '9')   i++;
  while (i < size)   value[i++]=0;
  return value;
}
