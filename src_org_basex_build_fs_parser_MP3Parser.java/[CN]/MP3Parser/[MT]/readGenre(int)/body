{
  byte[] value=readText(s);
  int id;
  if (!Token.ws(value)) {
    if (value[0] == '(') {
      int limit=1;
      while (value[limit] >= '0' && value[limit] <= '9' && limit < s)       limit++;
      id=Token.toInt(value,1,limit);
    }
 else     id=Token.toInt(value);
    return id == Integer.MIN_VALUE ? value : getGenre(id);
  }
  return Token.EMPTY;
}
