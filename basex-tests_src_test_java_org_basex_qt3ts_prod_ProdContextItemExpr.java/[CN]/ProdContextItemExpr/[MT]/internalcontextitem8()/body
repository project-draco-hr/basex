{
  final XQuery query=new XQuery("(fn:false(),fn:true(),fn:false())[fn:not(xs:boolean(.))]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"false false"));
}
