{
  final XQuery query=new XQuery("<elem mark=\"w0\">{//west/@west-attr-1, //west/@west-attr-2}</elem>",ctx);
  query.context(node(file("prod/AxisStep/TopMany.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem mark=\"w0\" west-attr-1=\"w1\" west-attr-2=\"w2\"/>",false));
}
