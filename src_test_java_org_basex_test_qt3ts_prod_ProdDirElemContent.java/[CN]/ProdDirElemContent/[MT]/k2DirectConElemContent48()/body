{
  final XQuery query=new XQuery("<e xmlns=\"http://www.example.com/\"> <a xmlns=\"\"/> </e>, <e xmlns=\"http://www.example.com/\"> <a xmlns=\"\"/> </e>/count(in-scope-prefixes(a)), <e xmlns=\"http://www.example.com/\"> <a xmlns=\"\"> <b xmlns=\"\"/> </a> </e>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns=\"http://www.example.com/\"><a xmlns=\"\"/></e>1<e xmlns=\"http://www.example.com/\"><a xmlns=\"\"><b/></a></e>",false));
}
