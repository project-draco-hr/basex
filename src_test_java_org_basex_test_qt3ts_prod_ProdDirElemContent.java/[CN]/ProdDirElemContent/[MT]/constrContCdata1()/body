{
  final XQuery query=new XQuery("<elem><![CDATA[cdata&<>'\"&lt;&#x20;]]></elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>cdata&amp;&lt;&gt;'\"&amp;lt;&amp;#x20;</elem>",false));
}
