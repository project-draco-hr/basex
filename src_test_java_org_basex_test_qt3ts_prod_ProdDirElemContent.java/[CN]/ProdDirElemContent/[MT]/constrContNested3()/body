{
  final XQuery query=new XQuery("<elem><!----><!--content--></elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem><!----><!--content--></elem>",false));
}
