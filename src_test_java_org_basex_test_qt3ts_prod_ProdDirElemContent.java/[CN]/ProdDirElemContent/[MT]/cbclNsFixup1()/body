{
  final XQuery query=new XQuery(" \n" + "      \tlet $x := <ns:foo xmlns:ns=\"http://www.w3.org/foo\" ns:attr=\"foo\" /> \n" + "      \treturn let $y := <ns:foo xmlns:ns=\"http://www.w3.org/bar\" ns:attr=\"bar\" /> \n"+ "      \treturn let $z := <root> { $x/@*, $y/@* } </root> \n"+ "      \treturn count(distinct-values(in-scope-prefixes($z)))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("3"));
}
