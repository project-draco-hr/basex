{
  final XQuery query=new XQuery("declare copy-namespaces no-preserve, inherit; <y xmlns:inherit=\"http://www.example.com/inherit\">{(/)}</y>/x/z",ctx);
  try {
    query.context(node(file("prod/DirElemContent/nsmode.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<z xmlns:inherit=\"http://www.example.com/inherit\"/>",false));
}
