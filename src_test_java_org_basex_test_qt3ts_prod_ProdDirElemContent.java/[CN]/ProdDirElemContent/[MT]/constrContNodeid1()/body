{
  final XQuery query=new XQuery("for $x in <a/>, $y in <elem>{$x}</elem> return exactly-one($y/a) is $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
