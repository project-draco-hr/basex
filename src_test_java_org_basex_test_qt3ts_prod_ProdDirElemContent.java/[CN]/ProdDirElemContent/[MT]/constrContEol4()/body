{
  final XQuery query=new XQuery("<codepoints>{string-to-codepoints(<elem>&#xD;</elem>)}</codepoints>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<codepoints>13</codepoints>",false));
}
