{
  if (current.preRight == null) {
    current.preRight=newnode;
    newnode.preParent=current;
    preInsertCase1(newnode);
    return;
  }
  MapNode node=current.preRight;
  while (node.preLeft != null) {
    node.s+=newnode.s;
    node=node.preLeft;
  }
  node.preLeft=newnode;
  node.s+=newnode.s;
  newnode.preParent=node;
  preInsertCase1(newnode);
}
