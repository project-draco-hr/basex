{
  final XQuery query=new XQuery("string(exactly-one(<e xml:id=\" ab c d \"/>/@*))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"ab c d") || error("XQDY0091")));
}
