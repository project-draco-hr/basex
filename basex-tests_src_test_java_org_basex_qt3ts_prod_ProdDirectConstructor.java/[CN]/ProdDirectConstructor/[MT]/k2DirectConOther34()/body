{
  final XQuery query=new XQuery("<?validchar content a b c asdada dad ?> eq \"content a b c asdada dad \"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
