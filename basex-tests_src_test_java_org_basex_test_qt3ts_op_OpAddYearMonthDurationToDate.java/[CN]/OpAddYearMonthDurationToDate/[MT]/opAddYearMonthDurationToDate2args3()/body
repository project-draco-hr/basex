{
  final XQuery query=new XQuery("xs:date(\"2030-12-31Z\") + xs:yearMonthDuration(\"P0Y0M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2030-12-31Z"));
}
