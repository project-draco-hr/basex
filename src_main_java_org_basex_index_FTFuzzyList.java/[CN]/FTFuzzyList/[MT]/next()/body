{
  if (tp[tp.length - 1] == ptok)   return EMPTY;
  if (tp[ntl] == ptok || ntl == 0) {
    ctl++;
    while (tp[ctl] == -1)     ctl++;
    ntl=ctl + 1;
    while (tp[ntl] == -1)     ntl++;
  }
  if (ctl == tp.length)   return EMPTY;
  final byte[] tok=str.readBytes(ptok,ctl);
  size=str.read4(str.pos() + 5);
  ptok=(int)str.pos();
  return tok;
}
