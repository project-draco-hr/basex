{
  FTItem it=null;
  for (  final FTExpr e : expr) {
    final FTItem i=e.atomic(ctx);
    if (it != null) {
      it.all=it.all.and(i.all);
      it.score(i.score() == 0 ? 0 : ctx.score.and(i.score(),it.score()));
    }
 else {
      it=i;
    }
    if (it.score() == 0)     break;
  }
  return it;
}
