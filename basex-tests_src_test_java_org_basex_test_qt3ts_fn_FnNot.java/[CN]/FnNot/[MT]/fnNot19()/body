{
  final XQuery query=new XQuery("fn:concat(xs:string(fn:not(\"true\")),xs:string(fn:not(\"true\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("'falsefalse'"));
}
