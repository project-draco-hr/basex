{
  ser.openElement(this,VAR,token(var.toString()));
  if (pos != null)   ser.attribute(POS,token(pos.toString()));
  if (score != null)   ser.attribute(Token.token(SCORE),token(score.toString()));
  if (mark != null)   ser.attribute(MRK,token(mark.toString()));
  expr.plan(ser);
  ser.closeElement();
}
