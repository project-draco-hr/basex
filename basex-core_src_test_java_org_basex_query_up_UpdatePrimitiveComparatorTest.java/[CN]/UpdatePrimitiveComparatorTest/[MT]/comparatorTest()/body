{
  final Data d=data(TESTDOCUMENT);
  compare(new UpdatePrimitive[]{new InsertIntoAsFirst(3,d,null,null),new InsertBefore(3,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertInto(2,d,null,null),new InsertInto(3,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertInto(2,d,null,null),new InsertAfter(3,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertAfter(2,d,null,null),new InsertInto(3,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertInto(2,d,null,null),new InsertAfter(3,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertIntoAsFirst(5,d,null,null),new InsertBefore(5,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertInto(4,d,null,null),new InsertInto(5,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertInto(4,d,null,null),new InsertAfter(5,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertAfter(4,d,null,null),new InsertInto(5,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertAfter(7,d,null,null),new InsertInto(7,d,null,null)});
  compare(new UpdatePrimitive[]{new InsertInto(7,d,null,null),new InsertInto(4,d,null,null)});
  compare(new UpdatePrimitive[]{new DeleteNode(9,d,null),new DeleteNode(8,d,null)});
