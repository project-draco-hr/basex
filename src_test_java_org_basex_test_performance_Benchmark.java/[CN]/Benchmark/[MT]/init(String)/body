{
  out.println("=== " + Util.name(this) + " Test ===");
  if (!parseArguments(args))   return false;
  server=!local && !BaseXServer.ping(LOCALHOST,context.mprop.num(MainProp.SERVERPORT)) ? new BaseXServer("") : null;
  session=local ? new LocalSession(context) : new ClientSession(context,"admin","admin");
  session.execute(new Set(Prop.QUERYINFO,true));
  drop();
  return true;
}
