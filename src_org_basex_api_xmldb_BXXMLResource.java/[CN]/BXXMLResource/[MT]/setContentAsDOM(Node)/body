{
  String test="";
  try {
    TransformerFactory.newInstance().newTransformer().transform(new DOMSource(cont),new StreamResult(test));
  }
 catch (  TransformerConfigurationException e) {
    e.printStackTrace();
  }
catch (  TransformerException e) {
    e.printStackTrace();
  }
catch (  TransformerFactoryConfigurationError e) {
    e.printStackTrace();
  }
  content=test;
}
