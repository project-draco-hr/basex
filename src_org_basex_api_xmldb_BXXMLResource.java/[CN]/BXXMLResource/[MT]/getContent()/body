{
  if (content == null) {
    try {
      final CachedOutput out=new CachedOutput();
      new XMLSerializer(out).xml(data,pre);
      content=out.toString();
    }
 catch (    final IOException ex) {
      throw new XMLDBException(ErrorCodes.VENDOR_ERROR,ex.getMessage());
    }
  }
  return content;
}
