{
  if (content == null) {
    try {
      final CachedOutput out=new CachedOutput();
      final boolean chop=nodes.data.meta.chop;
      nodes.serialize(new XMLSerializer(out,false,chop));
      content=out.toString();
    }
 catch (    final Exception ex) {
      BaseX.debug(ex);
    }
  }
  return content;
}
