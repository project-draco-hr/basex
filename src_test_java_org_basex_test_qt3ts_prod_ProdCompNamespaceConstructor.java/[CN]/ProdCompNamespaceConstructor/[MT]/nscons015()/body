{
  final XQuery query=new XQuery("\n" + "        declare namespace z=\"http://www.zorba-xquery.com/\";\n" + "        <z:e>{ namespace { <a/>/* } { \"http://www.w3.org/\" } }</z:e>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<z:e xmlns:z=\"http://www.zorba-xquery.com/\" xmlns=\"http://www.w3.org/\" />",false));
}
