{
  final XQuery query=new XQuery("\n" + "        serialize( element e { namespace z { \"http://www.zorba-xquery.com/\" } } )\n" + "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertQuery("contains($result,'xmlns:z')") && assertQuery("contains($result,'\"http://www.zorba-xquery.com/\"')")));
}
