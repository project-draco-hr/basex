{
  final XQuery query=new XQuery("\n" + "        let $elem := <e>{ namespace z { \"http://www.zorba-xquery.com/\" } }</e>\n" + "        return\n"+ "          element { resolve-QName(\"z:f\", $elem) } {}\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<z:f xmlns:z=\"http://www.zorba-xquery.com/\" />",false));
}
