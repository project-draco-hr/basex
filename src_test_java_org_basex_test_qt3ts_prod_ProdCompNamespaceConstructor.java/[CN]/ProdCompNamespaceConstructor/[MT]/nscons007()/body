{
  final XQuery query=new XQuery("\n" + "        declare variable $s := \"http://saxon.sf.net/\"; \n" + "        declare variable $xsl := \"http://www.w3.org/1999/XSL/Transform\"; \n"+ "        declare variable $xmlns := \"xmlns\"; \n"+ "        <e> { namespace saxon {$s}, attribute a {23}, namespace xsl {$xsl}, namespace xmlns {$s}, element f {42} }</e>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XQDY0101"));
}
