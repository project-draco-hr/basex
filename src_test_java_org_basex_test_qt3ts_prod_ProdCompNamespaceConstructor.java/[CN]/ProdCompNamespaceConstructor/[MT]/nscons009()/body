{
  final XQuery query=new XQuery("\n" + "        declare variable $p1 := \"http://example.com/one\"; \n" + "        declare variable $p2 := \"http://example.com/two\"; \n"+ "        <e> { namespace p {$p1}, namespace p {$p2}, element f {42} }</e>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XQDY0102"));
}
