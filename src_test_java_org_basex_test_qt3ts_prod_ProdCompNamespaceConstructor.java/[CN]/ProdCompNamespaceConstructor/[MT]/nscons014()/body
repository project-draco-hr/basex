{
  final XQuery query=new XQuery("\n" + "        let $pre := \"z\",\n" + "            $uri := \"http://www.zorba-xquery.com/\"\n"+ "        return\n"+ "          <e>{ namespace { $pre } { $uri } }</e>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns:z=\"http://www.zorba-xquery.com/\"/>",false));
}
