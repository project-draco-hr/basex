{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:decimal) as xs:decimal { $arg * $arg }; \n" + "      \tlocal:square(4294967296.0) idiv 0.0000000000005 gt 0",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOAR0002")));
}
