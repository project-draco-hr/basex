{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:float) as xs:float { $arg * $arg }; \n" + "      \tlocal:square(xs:float(1e30)) idiv xs:float(5e-30)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOAR0002"));
}
