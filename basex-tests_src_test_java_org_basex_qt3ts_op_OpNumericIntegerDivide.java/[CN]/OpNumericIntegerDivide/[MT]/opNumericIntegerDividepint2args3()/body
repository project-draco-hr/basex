{
  final XQuery query=new XQuery("xs:positiveInteger(\"999999999999999999\") idiv xs:positiveInteger(\"1\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("999999999999999999"));
}
