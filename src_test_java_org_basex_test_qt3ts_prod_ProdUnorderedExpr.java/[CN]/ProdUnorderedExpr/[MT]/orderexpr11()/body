{
  final XQuery query=new XQuery("ordered {//part[@partof < 2] intersect //part[@partid = 1 or @partid > 2] }",ctx);
  query.context(node(file("docs/partlist.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<part partid=\"1\" partof=\"0\" name=\"engine\"/><part partid=\"3\" partof=\"1\" name=\"piston\"/>",false));
}
