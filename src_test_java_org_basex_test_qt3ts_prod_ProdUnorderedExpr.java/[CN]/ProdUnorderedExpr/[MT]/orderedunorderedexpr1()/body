{
  final XQuery query=new XQuery("ordered {if (fn:true()) then (0,1,2,3,4) else (\"A\",\"B\",\"C\")}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0 1 2 3 4"));
}
