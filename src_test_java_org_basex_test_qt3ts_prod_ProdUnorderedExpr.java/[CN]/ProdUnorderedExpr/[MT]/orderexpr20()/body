{
  final XQuery query=new XQuery("unordered { for $i in (//part[@partid = 1], //part[@partid = 2]), $j in (//part[@partof = $i/@partid]) where ($i/@partid + $j/@partid) < 7 return $i/@partid + $j/@partid }",ctx);
  query.context(node(file("docs/partlist.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"4 6") || assertStringValue(false,"6 4")));
}
