{
  final XQuery query=new XQuery("ordered {typeswitch(123) case $i as xs:string return (\"a\",\"b\",\"c\") case $i as xs:double return (\"a\",\"b\",\"c\") case $i as xs:integer return (1,2,3,4) default return (\"a\",\"b\",\"c\") }",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1 2 3 4"));
}
