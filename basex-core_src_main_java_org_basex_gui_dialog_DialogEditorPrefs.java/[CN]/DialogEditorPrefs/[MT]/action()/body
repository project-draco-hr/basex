{
  margin.setEnabled(showmargin.isSelected());
  indent.setEnabled(spaces.isSelected());
  showmargin.assign();
  invisible.assign();
  shownl.assign();
  numbers.assign();
  markline.assign();
  files.assign();
  spaces.assign();
  auto.assign();
  saverun.assign();
  parseproj.assign();
  showHidden.assign();
  return margin.assign() & indent.assign();
}
