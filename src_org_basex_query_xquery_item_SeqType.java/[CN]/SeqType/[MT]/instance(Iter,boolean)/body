{
  Item it=iter.next();
  if (it == null)   return type == Type.EMP || occ % 2 != 0;
  if (occ < 2)   return iter.next() == null && (!f && it.u() && !type.node() || it.type.instance(type));
  do {
    if ((f || !it.u() || type.node()) && !it.type.instance(type))     return false;
  }
 while ((it=iter.next()) != null);
  return true;
}
