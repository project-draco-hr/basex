{
  for (int o=0; o < K_N_OPT.length; o++)   if (options.contains(K_N_OPT[o]))   nprops.put(N_OPT[o],toInt(options.get(K_N_OPT[o])));
  for (int o=0; o < K_B_OPT.length; o++)   if (options.contains(K_B_OPT[o]))   nprops.put(B_OPT[o],eq(options.get(K_B_OPT[o]),TRUE));
  for (int o=0; o < K_S_OPT.length; o++)   if (options.contains(K_S_OPT[o]))   nprops.put(S_OPT[o],string(options.get(K_S_OPT[o])));
}
