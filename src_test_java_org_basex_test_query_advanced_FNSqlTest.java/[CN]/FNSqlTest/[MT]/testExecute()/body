{
  query("let $conn := sql:connect('jdbc:test') " + "return sql:execute($conn, 'UPDATE')","");
  final Item[] tuples=new Item[2];
  final NodeCache attrs1=new NodeCache();
  attrs1.add(new FAttr(new QNm(token("bigdec"),EMPTY),new Jav(new BigDecimal(1)).atom(null),null));
  attrs1.add(new FAttr(new QNm(token("bool"),EMPTY),new Jav(new Boolean(true)).atom(null),null));
  final FElem tuple1=new FElem(new QNm(token("sql:tuple"),SQLURI),null,attrs1,null,new Atts().add(SQL,SQLURI),null);
  tuples[0]=tuple1;
  final NodeCache attrs2=new NodeCache();
  attrs2.add(new FAttr(new QNm(token("bigdec"),EMPTY),new Jav(new BigDecimal(2)).atom(null),null));
  attrs2.add(new FAttr(new QNm(token("bool"),EMPTY),new Jav(new Boolean(false)).atom(null),null));
  final FElem tuple2=new FElem(new QNm(token("sql:tuple"),SQLURI),null,attrs2,null,new Atts().add(SQL,SQLURI),null);
  tuples[1]=tuple2;
  final ItemCache result=new ItemCache(tuples,2);
  query("let $conn := sql:connect('jdbc:test')" + "return sql:execute($conn, 'SELECT')",result.toString().replaceAll("(\\r|\\n) *",""));
}
