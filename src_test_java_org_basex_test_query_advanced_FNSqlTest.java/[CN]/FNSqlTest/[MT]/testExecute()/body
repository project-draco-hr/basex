{
  query("let $conn := sql:connect('jdbc:test') " + "return sql:execute($conn, 'UPDATE')","");
  final ItemCache result=new ItemCache();
  final NodeCache attrs1=new NodeCache();
  attrs1.add(new FAttr(new QNm(token("bigdec"),EMPTY),new Jav(new BigDecimal(1)).atom(null),null));
  attrs1.add(new FAttr(new QNm(token("bool"),EMPTY),new Jav(new Boolean(true)).atom(null),null));
  final FElem tuple1=new FElem(new QNm(token("tuple"),SQLURI),null,attrs1,null,new Atts().add(SQL,SQLURI),null);
  result.add(tuple1);
  final NodeCache attrs2=new NodeCache();
  attrs1.add(new FAttr(new QNm(token("bigdec"),EMPTY),new Jav(new BigDecimal(2)).atom(null),null));
  attrs1.add(new FAttr(new QNm(token("bool"),EMPTY),new Jav(new Boolean(false)).atom(null),null));
  final FElem tuple2=new FElem(new QNm(token("tuple"),SQLURI),null,attrs2,null,new Atts().add(SQL,SQLURI),null);
  result.add(tuple2);
  System.out.println(result.toString().replaceAll("(\\r|\\n) *",""));
  query("let $conn := sql:connect('jdbc:test')" + "return sql:execute($conn, 'SELECT')",result);
}
