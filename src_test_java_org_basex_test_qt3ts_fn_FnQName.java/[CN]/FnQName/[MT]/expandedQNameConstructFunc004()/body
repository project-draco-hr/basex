{
  final XQuery query=new XQuery("element {fn:QName((), \"person\")}{ \"test\" }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<person>test</person>",false));
}
