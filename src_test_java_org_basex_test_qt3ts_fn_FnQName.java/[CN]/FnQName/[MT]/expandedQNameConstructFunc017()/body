{
  final XQuery query=new XQuery("element {fn:QName( \"http://www.example.com/example\", string((//FolderName)[2]) )}{ \"test\" }",ctx);
  query.context(node(file("prod/ForClause/fsx.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<Folder00000000001 xmlns=\"http://www.example.com/example\">test</Folder00000000001>",false));
}
