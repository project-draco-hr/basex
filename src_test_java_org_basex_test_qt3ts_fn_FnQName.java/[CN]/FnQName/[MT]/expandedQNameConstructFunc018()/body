{
  final XQuery query=new XQuery("element {fn:QName( concat('http://www.example.com/', string((//FolderName)[2])), \"people\" )}{ \"test\" }",ctx);
  query.context(node(file("prod/ForClause/fsx.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<people xmlns=\"http://www.example.com/Folder00000000001\">test</people>",false));
}
