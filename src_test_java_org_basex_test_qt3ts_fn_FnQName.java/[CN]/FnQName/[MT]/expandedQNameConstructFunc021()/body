{
  final XQuery query=new XQuery("declare namespace ht=\"http://www.example.com/example\"; element {fn:QName( \"http://www.example.com/example\", \"ht2:person\" )}{ \"test\" }",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<ht2:person xmlns:ht2=\"http://www.example.com/example\">test</ht2:person>",false));
}
