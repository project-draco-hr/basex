{
  QueryProcessor qp=new QueryProcessor("http:send-request(" + "<http:request method='put'>" + "<http:body media-type='text/xml'><ToBeDeleted/></http:body>"+ "</http:request>, '" + RESTURL + "')",LCONTEXT);
  qp.execute();
  qp.close();
  qp=new QueryProcessor("http:send-request(" + "<http:request method='delete' status-only='true'/>, '" + RESTURL + "')",LCONTEXT);
  checkResponse(qp.execute(),HttpURLConnection.HTTP_OK,1);
  qp.close();
}
