{
  int[] holes=null;
  if (!wsConsume(PAR2)) {
    int i=0;
    do {
      if (wsConsume(PLHOLDER)) {
        holes=holes == null ? new int[]{i} : Array.add(holes,i);
      }
 else {
        final Expr e=single();
        if (e == null)         throw error(FUNCMISS_X,name);
        args.add(e);
      }
      i++;
    }
 while (wsConsume(COMMA));
    if (!wsConsume(PAR2))     throw error(FUNCMISS_X,name);
  }
  return holes;
}
