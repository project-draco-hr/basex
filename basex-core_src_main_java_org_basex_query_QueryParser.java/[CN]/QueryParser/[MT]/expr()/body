{
  final Expr e=single();
  if (e == null) {
    if (more())     return null;
    throw alter == null ? error(NOEXPR) : error();
  }
  if (!wsConsume(COMMA))   return e;
  final ExprList el=new ExprList(e);
  do   add(el,single());
 while (wsConsume(COMMA));
  return new List(info(),el.array());
}
