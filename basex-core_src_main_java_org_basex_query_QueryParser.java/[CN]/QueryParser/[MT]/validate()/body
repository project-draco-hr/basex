{
  final int i=pos;
  if (!wsConsumeWs(VALIDATE))   return;
  boolean brace=true;
  if (consume(BRACE1)) {
    brace=false;
  }
 else   if (consume(TYPE)) {
    final QNm qnm=eQName(QNAMEINV,SKIPCHECK);
    names.add(new QNmCheck(qnm));
  }
 else   if (!consume(STRICT) && !consume(LAX)) {
    pos=i;
    return;
  }
  if (brace)   wsCheck(BRACE1);
  check(single(),NOVALIDATE);
  wsCheck(BRACE2);
  throw error(IMPLVAL);
}
