{
  skipWs();
  final Expr name;
  final byte[] str=ncName(null);
  if (str.length == 0) {
    if (!wsConsume(CURLY1))     return null;
    name=check(expr(),PIWRONG);
    wsCheck(CURLY2);
  }
 else {
    name=Str.get(str);
  }
  if (!wsConsume(CURLY1))   return null;
  final Expr e=expr();
  wsCheck(CURLY2);
  return new CPI(sc,info(),name,e == null ? Empty.SEQ : e);
}
