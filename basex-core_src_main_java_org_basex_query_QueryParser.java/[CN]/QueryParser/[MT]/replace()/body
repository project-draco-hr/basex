{
  final int i=pos;
  if (!wsConsumeWs(REPLACE))   return null;
  final boolean v=wsConsumeWs(VALUEE);
  if (v) {
    wsCheck(OF);
    wsCheck(NODE);
  }
 else   if (!wsConsumeWs(NODE)) {
    pos=i;
    return null;
  }
  final Expr t=check(single(),INCOMPLETE);
  wsCheck(WITH);
  final Expr r=check(single(),INCOMPLETE);
  ctx.updating();
  return new Replace(sc,info(),t,r,v);
}
