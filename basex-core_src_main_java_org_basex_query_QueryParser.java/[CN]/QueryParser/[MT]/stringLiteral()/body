{
  skipWs();
  final char del=curr();
  if (!quote(del))   throw error(NOQUOTE,found());
  consume();
  tok.reset();
  while (true) {
    while (!consume(del)) {
      if (!more())       throw error(NOQUOTE,found());
      entity(tok);
    }
    if (!consume(del))     break;
    tok.add(del);
  }
  return tok.toArray();
}
