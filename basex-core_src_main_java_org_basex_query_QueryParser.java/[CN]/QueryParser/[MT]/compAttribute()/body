{
  skipWs();
  final Expr name;
  final QNm qn=eQName(null,SKIPCHECK);
  if (qn != null) {
    name=qn;
    qnames.add(qn,false);
  }
 else {
    if (!wsConsume(CURLY1))     return null;
    name=check(expr(),NOATTNAME);
    wsCheck(CURLY2);
  }
  if (!wsConsume(CURLY1))   return null;
  final Expr e=expr();
  wsCheck(CURLY2);
  return new CAttr(sc,info(),true,name,e == null ? Empty.SEQ : e);
}
