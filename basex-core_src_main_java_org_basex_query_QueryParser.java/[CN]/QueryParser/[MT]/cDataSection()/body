{
  final TokenBuilder tb=new TokenBuilder();
  while (true) {
    while (not(']')) {
      char ch=consume();
      if (ch == '\r') {
        ch='\n';
        if (curr(ch))         consume();
      }
      tb.add(ch);
    }
    consume();
    if (curr(']') && next() == '>') {
      pos+=2;
      return tb.finish();
    }
    tb.add(']');
  }
}
