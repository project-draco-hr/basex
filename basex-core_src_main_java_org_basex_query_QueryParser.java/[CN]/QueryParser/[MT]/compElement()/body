{
  skipWs();
  final Expr name;
  final QNm qn=eQName(null,SKIPCHECK);
  if (qn != null) {
    name=qn;
    qnames.add(qn);
  }
 else {
    if (!wsConsume(CURLY1))     return null;
    name=check(expr(),NOELEMNAME);
    wsCheck(CURLY2);
  }
  if (!wsConsume(CURLY1))   return null;
  final Expr e=expr();
  wsCheck(CURLY2);
  return new CElem(sc,info(),name,null,e == null ? new Expr[0] : new Expr[]{e});
}
