{
  wsCheck(BRACE1);
  final ExprList el=new ExprList();
  if (!wsConsume(BRACE2)) {
    do {
      add(el,check(single(),INVMAPKEY));
      if (!wsConsume(ASSIGN))       check(':');
      add(el,check(single(),INVMAPVAL));
    }
 while (wsConsume(COMMA));
    wsCheck(BRACE2);
  }
  return el.finish();
}
