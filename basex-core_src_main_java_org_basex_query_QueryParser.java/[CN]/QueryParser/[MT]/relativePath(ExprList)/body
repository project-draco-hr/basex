{
  while (true) {
    if (consume('/')) {
      if (consume('/')) {
        add(el,Step.get(info(),Axis.DESCORSELF,Test.NOD));
        checkAxis(Axis.DESC);
      }
 else {
        checkAxis(Axis.CHILD);
      }
    }
 else {
      return;
    }
    mark();
    final Expr st=step();
    if (st == null)     throw error(PATHMISS_X,found());
    add(el,st);
  }
}
