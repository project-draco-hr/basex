{
  Test tp=null;
switch (t) {
case DOC:
    tp=documentTest();
  break;
case ELM:
tp=elementTest();
break;
case ATT:
tp=attributeTest();
break;
case PI:
tp=piTest();
break;
case SCE:
case SCA:
tp=schemaTest();
break;
default :
break;
}
wsCheck(PAR2);
if (tp != null) return tp;
tp=Test.get(t);
if (tp == Test.NSP && !sc.xquery3) error(NSNOTALL);
return tp;
}
