{
  final Expr e=comparison();
  if (e != null) {
    if (wsConsumeWs(UPDATE)) {
      final InputInfo ii=info();
      final int s=localVars.openScope();
      qc.updating();
      final Expr m=check(single(),COPYEXPR);
      localVars.closeScope(s);
      return new TransformWith(ii,e,m);
    }
  }
  return e;
}
