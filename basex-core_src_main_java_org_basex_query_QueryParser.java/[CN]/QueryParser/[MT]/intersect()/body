{
  Expr e=instanceoff();
  boolean lastIs=false;
  ExprList el=null;
  while (true) {
    boolean is=wsConsumeWs(INTERSECT);
    if (!is && !wsConsumeWs(EXCEPT))     break;
    if ((is != lastIs) && el != null) {
      e=intersectExcept(lastIs,el);
      el=null;
    }
    lastIs=is;
    if (el == null)     el=new ExprList(e);
    add(el,instanceoff());
  }
  return el != null ? intersectExcept(lastIs,el) : e;
}
