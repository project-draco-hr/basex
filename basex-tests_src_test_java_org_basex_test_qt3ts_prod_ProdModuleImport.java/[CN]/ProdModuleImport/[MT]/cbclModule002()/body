{
  final XQuery query=new XQuery("\n" + "      \timport module namespace lib=\"http://www.xqsharp.com/test/baseuri-lib\"; \n" + "      \tdeclare base-uri \"http://www.example.org/wrong/\"; \n"+ "      \tbase-uri($lib:node/node())\n"+ "      ",ctx);
  try {
    query.addModule("http://www.xqsharp.com/test/baseuri-lib",file("prod/ModuleImport/baseuri-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.org/correct/"));
}
