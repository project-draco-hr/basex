{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"http://www.w3.org/Test&#x20;&#x20;&#x20;&#x20;Modules/test\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/Test Modules/test",file("prod/ModuleImport/module-uris2-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result>ok</result>",false));
}
