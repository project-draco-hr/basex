{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"&#x20;&#x9;&#xA;&#xD;http://www.w3.org/TestModules/test&#x20;&#x9;&#xA;&#xD;\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/test",file("prod/ModuleImport/module-uris1-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result>ok</result>",false));
}
