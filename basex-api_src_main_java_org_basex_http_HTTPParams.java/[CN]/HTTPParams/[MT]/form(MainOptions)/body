{
  if (form == null) {
    form=new HashMap<>();
    final String type=MimeTypes.type(http.contentType());
    if (MimeTypes.MULTIPART_FORM_DATA.equals(type)) {
      addMultipart(MimeTypes.parameters(type),options);
    }
 else     if (MimeTypes.APP_FORM_URLENCODED.equals(type)) {
      addURLEncoded();
    }
  }
  return form;
}
