{
  if (form == null) {
    form=new HashMap<>();
    final MediaType mt=http.contentType();
    if (MimeTypes.MULTIPART_FORM_DATA.equals(mt.type())) {
      addMultipart(mt,options);
    }
 else     if (MimeTypes.APP_FORM_URLENCODED.equals(mt.type())) {
      addURLEncoded();
    }
  }
  return form;
}
