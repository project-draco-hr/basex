{
  if (form == null) {
    form=new HashMap<String,Value>();
    final String ct=http.contentType();
    if (MimeTypes.MULTIPART_FORM_DATA.equals(ct)) {
      addMultipart(form,http.contentTypeExt());
    }
 else     if (MimeTypes.APP_FORM_URLENCODED.equals(ct)) {
      addURLEncoded(form);
    }
  }
  return form;
}
