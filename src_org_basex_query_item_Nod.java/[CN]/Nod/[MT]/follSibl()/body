{
  return new NodeIter(){
    /** 
 * Iterator. 
 */
    private NodeIter ir;
    @Override public Nod next() throws QueryException {
      if (ir == null) {
        final Nod r=parent();
        if (r == null)         return null;
        ir=r.child();
        Nod n;
        while ((n=ir.next()) != null && !n.is(Nod.this))         ;
      }
      return ir.next();
    }
  }
;
}
