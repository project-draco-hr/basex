{
  final XQuery query=new XQuery("element {fn:QName( \"http://www.example.com/example\", \"\" )}{ \"test\" }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOCA0002"));
}
