{
  final XQuery query=new XQuery("element {fn:QName(\"http://www.example.com/example\", \"ht:person\")}{ \"test\" }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<ht:person xmlns:ht=\"http://www.example.com/example\">test</ht:person>",false));
}
