{
  out.println("=== " + Util.name(this) + " Test ===");
  if (!parseArguments(args))   return false;
  final Context ctx=new Context();
  start=!local && !BaseXServer.ping(LOCALHOST,ctx.prop.num(Prop.SERVERPORT));
  if (start)   new BaseXServer("");
  session=local ? new LocalSession(ctx) : new ClientSession(ctx,"admin","admin");
  session.execute(new Set(Prop.QUERYINFO,true));
  drop();
  return true;
}
