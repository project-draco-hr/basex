{
  final XQuery query=new XQuery(" (xs:time(\"08:00:00+09:00\"),xs:time(\"08:00:00+10:00\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"08:00:00+09:00 08:00:00+10:00"));
}
