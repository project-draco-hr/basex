{
  final XQuery query=new XQuery("//book/price, (), (1)",ctx);
  try {
    query.context(node(file("docs/bib.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<price>65.95</price><price>65.95</price><price>39.95</price><price>129.95</price>1",false));
}
