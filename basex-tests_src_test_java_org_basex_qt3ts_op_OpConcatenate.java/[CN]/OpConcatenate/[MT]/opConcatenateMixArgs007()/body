{
  final XQuery query=new XQuery("xs:anyURI(\"www.example.com\") , xs:decimal(\"1.01\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"www.example.com\", 1.01"));
}
