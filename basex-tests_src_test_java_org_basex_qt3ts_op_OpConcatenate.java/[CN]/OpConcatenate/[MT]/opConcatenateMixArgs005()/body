{
  final XQuery query=new XQuery("xs:string(\"a\") , xs:anyURI(\"www.example.com\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"a\", \"www.example.com\""));
}
