{
  final XQuery query=new XQuery("() , xs:string(\" \") , xs:decimal(\"2.000000000000002\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"  2.000000000000002"));
}
