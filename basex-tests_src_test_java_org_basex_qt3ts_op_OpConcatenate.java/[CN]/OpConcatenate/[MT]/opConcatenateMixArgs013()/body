{
  final XQuery query=new XQuery("xs:date(\"1993-03-31\") , xs:boolean(\"true\"), xs:string(\"abc\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1993-03-31 true abc"));
}
