{
  final XQuery query=new XQuery("xs:dateTime(\"1972-12-31T00:00:00Z\") , (())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1972-12-31T00:00:00Z"));
}
