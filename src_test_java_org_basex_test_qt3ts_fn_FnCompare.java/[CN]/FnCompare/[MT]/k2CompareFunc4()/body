{
  final XQuery query=new XQuery("let $vA := (\"B STRING\", current-time())[1] treat as xs:string, \n" + "        $vB  := (\"b string\", current-time())[1] treat as xs:string\n" + "        return compare(lower-case($vA), lower-case($vB))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("0"));
}
