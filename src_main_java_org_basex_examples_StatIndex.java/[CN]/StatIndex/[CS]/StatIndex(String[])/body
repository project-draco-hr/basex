{
  if (!parseArguments(args))   return;
  final TokenList tl=new TokenList();
  tl.add("Document");
  tl.add("$path");
  tl.add("$text");
  tl.add("$attr");
  tl.add("$fuzzy");
  tl.add("$trie");
  tl.add("ltext");
  tl.add("lattr");
  tl.add("lfuzzy");
  tl.add("ltrie");
  table.header=tl;
  if (db != null) {
    analyze(db);
  }
 else {
    for (    final String d : List.list(ctx)) {
      System.out.println("- " + d);
      analyze(d);
    }
  }
  out.print(Token.string(table.finish()));
}
