{
  final Args arg=new Args(args);
  boolean ok=true;
  try {
    while (arg.more() && ok) {
      if (arg.dash()) {
        ok=false;
      }
 else {
        db=arg.string();
      }
    }
  }
 catch (  final Exception ex) {
    ok=false;
  }
  if (!ok) {
    Main.outln("Usage: " + Main.name(this) + " [options] [db?]"+ NL+ "  db  database to be parsed");
  }
  return ok;
}
