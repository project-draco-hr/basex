{
  final Type tr=r.type, ti=i.type;
  if (ti.isUntyped())   return DBL;
  if (!(i instanceof ANum))   throw FUNCMP.get(info,this,tr,ti);
  if (tr == ti)   return tr;
  if (tr == DBL || ti == DBL)   return DBL;
  if (tr == FLT || ti == FLT)   return FLT;
  if (tr == DEC || ti == DEC)   return DEC;
  return ITR;
}
