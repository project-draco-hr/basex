{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}escape-html-uri#1('http://www.example.com/')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/"));
}
