{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}format-date#5(xs:date('2012-01-01Z'), '[Y]', 'en', (), ())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2012"));
}
