{
  if (key == null) {
    if (CHECK && !check.isEmpty()) {
      for (      final String s : check.keySet())       errln("%." + SUFFIX + ": '%' not used",language,s);
    }
    return null;
  }
  final String val=TETXTS.get(key);
  if (val == null) {
    if (!TETXTS.isEmpty())     errln("%." + SUFFIX + ": '%' missing",language,key);
    return '[' + key + ']';
  }
  if (CHECK)   check.remove(key);
  return val;
}
