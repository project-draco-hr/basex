{
  if (key == null) {
    if (CHECK && check.size() != 0) {
      final Iterator<String> it=check.keySet().iterator();
      while (it.hasNext())       Util.errln("%." + SUFFIX + ": '%' not used",Prop.language,it.next());
    }
    return null;
  }
  final String val=TETXTS.get(key);
  if (val == null) {
    if (TETXTS.size() != 0)     Util.errln("%." + SUFFIX + ": '%' missing",Prop.language,key);
    return "[" + key + "]";
  }
  if (CHECK)   check.remove(key);
  return val;
}
