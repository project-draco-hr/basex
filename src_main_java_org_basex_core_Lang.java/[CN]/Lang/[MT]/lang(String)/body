{
  if (key == null) {
    if (CHECK && check.size() != 0) {
      for (      final String s : check.keySet())       Util.errln("%." + SUFFIX + ": '%' not used",Util.language,s);
    }
    return null;
  }
  final String val=TETXTS.get(key);
  if (val == null) {
    if (TETXTS.size() != 0)     Util.errln("%." + SUFFIX + ": '%' missing",Util.language,key);
    return '[' + key + ']';
  }
  if (CHECK)   check.remove(key);
  return val;
}
