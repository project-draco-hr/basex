{
  if (key == null) {
    for (    final String s : CHECK.keySet()) {
      Util.errln("%." + SUFFIX + ": '%' not defined in Text class",Prop.language,s);
      Util.stack(10);
      System.exit(0);
    }
    return null;
  }
  final String val=TEXTS.get(key);
  if (val == null) {
    Util.errln("%." + SUFFIX + ": '%' missing in Text class",Prop.language,key);
    Util.stack(10);
    System.exit(0);
    return '[' + key + ']';
  }
  CHECK.remove(key);
  return val;
}
