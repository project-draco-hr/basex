{
  final XQuery query=new XQuery("fn:filter( function($arg) { if ($arg eq 100) then 0 else fn:true()}, 1 to 10)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertCount(10));
}
