{
  final XQuery query=new XQuery("\n" + "      declare option prohibit-feature \"all-extensions static-typing\";\n" + "      ()\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEmpty() || error("XQST0128")));
}
