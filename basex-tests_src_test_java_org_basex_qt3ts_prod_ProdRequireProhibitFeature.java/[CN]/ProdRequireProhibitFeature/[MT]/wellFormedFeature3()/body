{
  final XQuery query=new XQuery("\n" + "      declare option require-feature \"123\";\n" + "      ()\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0122"));
}
