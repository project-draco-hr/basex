{
  final XQuery query=new XQuery("\n" + "      import module namespace module = \"http://www.w3.org/XQueryTest/RequireProhibitFeature\";\n" + "      declare option require-feature \"module higher-order-function\";\n"+ "      let $f := function() { () } return $f()\n"+ "    ",ctx);
