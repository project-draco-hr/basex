{
  final XQuery query=new XQuery("\n" + "      declare option require-feature \"higher-order-function unknown\";\n" + "      let $f := function() { () } return $f()\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQST0123")));
}
