{
  final XQuery query=new XQuery("\n" + "      declare option prohibit-feature \"all-optional-features\";\n" + "      declare option require-feature \"higher-order-function\";\n"+ "      1 instance of function(*)\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
