{
  final XQuery query=new XQuery("\n" + "      1 instance of function(*)\n" + "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || error("XQST0129")));
}
