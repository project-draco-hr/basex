{
  for (  final PathNode n : node.ch) {
    if (axis == Axis.DESC || axis == Axis.DESCORSELF) {
      add(n,nodes,name,kind);
    }
    if (kind == -1 && (n.kind != Data.ATTR ^ axis == Axis.ATTR) || kind == n.kind && (name == 0 || name == n.name)) {
      if (!nodes.contains(n))       nodes.add(n);
    }
  }
}
