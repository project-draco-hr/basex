{
  final XQuery query=new XQuery("fn:replace(\"This is a characte\",\"This is a characte\",\"This is a characte\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"This is a characte"));
}
