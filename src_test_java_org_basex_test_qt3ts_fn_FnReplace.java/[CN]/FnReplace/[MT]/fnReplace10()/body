{
  final XQuery query=new XQuery("fn:replace(\"abcd\", \"(ab)|(a)\", \"[1=$1][2=$2]\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[1=ab][2=]cd"));
}
