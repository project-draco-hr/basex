{
  rectangles.add(r);
  final int x=r.x + layout.x;
  final int y=r.y + layout.y;
  final int w=r.w - layout.w;
  final int h=r.h - layout.h;
  if ((w >= o || h >= o) && w > 0 && h > 0 && !ViewData.isLeaf(data,r.pre)) {
    final MapList ch=children(r.pre);
    if (ch.size != 0)     rectangles.addAll(algo.calcMap(new MapRect(x,y,w,h,l.list[ns],r.level + 1),ch,ch.weights,0,ch.size - 1,level + 1));
  }
}
