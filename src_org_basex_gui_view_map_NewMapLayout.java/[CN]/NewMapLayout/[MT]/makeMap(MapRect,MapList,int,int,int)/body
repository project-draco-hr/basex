{
  if (ne - ns == 1) {
    r.pre=l.list[ns];
    putRect(r,level);
  }
 else {
    if (level == 0) {
      rectangles.addAll(new SplitAlgo().calcMap(r,l,l.weights,ns,ne,level));
    }
 else {
      ArrayList<MapRect> rects=algo.calcMap(r,l,l.weights,ns,ne,level);
      for (      MapRect rect : rects) {
        putRect(rect,rect.level);
      }
    }
  }
}
