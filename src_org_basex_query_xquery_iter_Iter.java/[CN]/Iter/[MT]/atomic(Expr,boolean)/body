{
  long s=size();
  if (s != -1) {
    if (s == 1)     return next();
    if (s == 0) {
      if (!empty)       Err.empty(expr);
      return null;
    }
    seqErr(next(),next(),next(),expr);
  }
  final Item it=next();
  if (it == null) {
    if (!empty)     Err.empty(expr);
    return null;
  }
  final Item n=next();
  if (n != null)   seqErr(it,n,next(),expr);
  return it;
}
