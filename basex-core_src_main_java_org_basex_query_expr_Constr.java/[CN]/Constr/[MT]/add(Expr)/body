{
  final int s=ctx.sc.ns.size();
  try {
    for (    final Expr e : expr) {
      more=false;
      final Iter iter=ctx.iter(e);
      for (Item ch; (ch=iter.next()) != null && add(ch); )       ;
    }
    if (!text.isEmpty())     children.add(new FTxt(text.finish()));
    return this;
  }
  finally {
    ctx.sc.ns.size(s);
  }
}
