{
  runQuery("geo:envelope(<gml:LinearRing><gml:coordinates>1,1 20,1 50,30 1,1" + "</gml:coordinates></gml:LinearRing>)","<gml:Polygon srsName=\"0\"><gml:outerBoundaryIs>" + "<gml:LinearRing srsName=\"0\"><gml:coordinates>" + "1.0,1.0 50.0,1.0 50.0,30.0 1.0,30.0 1.0,1.0</gml:coordinates>"+ "</gml:LinearRing></gml:outerBoundaryIs></gml:Polygon>");
  runError("geo:envelope(text {'a'})",FUNCMP.qname());
