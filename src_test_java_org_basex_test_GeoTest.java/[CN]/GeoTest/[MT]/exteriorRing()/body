{
  runQuery("geo:exteriorRing(<gml:Polygon><gml:outerBoundaryIs><gml:LinearRing>" + "<gml:coordinates>11,11 18,11 18,18 11,18 11,11</gml:coordinates>" + "</gml:LinearRing></gml:outerBoundaryIs></gml:Polygon>)","<gml:LineString xmlns:gml=\"http://www.opengis.net/gml\">" + "<gml:coordinates>11.0,11.0 18.0,11.0 18.0,18.0 11.0,18.0 11.0,11.0" + "</gml:coordinates></gml:LineString>");
  runError("geo:exteriorRing(" + "<gml:Point><gml:coordinates>2.0,3.0</gml:coordinates></gml:Point>)",GeoErrors.qname(3));
  runError("geo:exteriorRing(" + "<gml:unknown><gml:coordinates>1,1</gml:coordinates></gml:unknown>)",GeoErrors.qname(1));
  runError("geo:exteriorRing()",XPARGS.qname());
  runError("geo:exteriorRing(text {'a'})",FUNCMP.qname());
}
