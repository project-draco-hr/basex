{
  runQuery("geo:boundary(<gml:Polygon><gml:outerBoundaryIs><gml:LinearRing>" + "<gml:coordinates>11,11 18,11 18,18 11,18 11,11</gml:coordinates>" + "</gml:LinearRing></gml:outerBoundaryIs></gml:Polygon>)","<gml:LineString xmlns:gml=\"http://www.opengis.net/gml\">" + "<gml:coordinates>11.0,11.0 18.0,11.0 18.0,18.0 11.0,18.0 " + "11.0,11.0</gml:coordinates></gml:LineString>");
