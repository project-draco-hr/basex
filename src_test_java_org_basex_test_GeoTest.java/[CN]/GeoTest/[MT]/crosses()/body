{
  runQuery("geo:crosses(" + "<gml:Point><gml:coordinates>10,11</gml:coordinates></gml:Point>, " + "<gml:LineString><gml:coordinates>0,0 2,2</gml:coordinates></gml:LineString>)","false");
  runError("geo:crosses(a, text {'a'})",NOCTX.qname());
  runError("geo:crosses(<gml:Point><gml:coordinates>10,10 12,11" + "</gml:coordinates></gml:Point>, " + "<gml:LinearRing><gml:coordinates>1,1 2,1 5,3 1,1</gml:coordinates>"+ "</gml:LinearRing>)",GeoErrors.qname(2));
  runError("geo:crosses(<gml:Point><gml:coordinates>1,1</gml:coordinates></gml:Point>)",FUNCARGS.qname());
}
