{
  final XQuery query=new XQuery("normalize-unicode((\"a string\", error()), \"NFC\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOER0000") || error("XPTY0004")));
}
