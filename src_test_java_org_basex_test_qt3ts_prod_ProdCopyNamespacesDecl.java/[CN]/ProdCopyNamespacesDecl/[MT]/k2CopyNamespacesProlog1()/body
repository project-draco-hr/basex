{
  final XQuery query=new XQuery("declare copy-namespaces no-preserve, inherit; <doc> <a>{<b xmlns:p=\"http://example.com/\"/>}</a> <a><b xmlns:p=\"http://example.com/\"/></a> </doc>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<doc><a><b/></a><a><b xmlns:p=\"http://example.com/\"/></a></doc>",false));
}
