{
  final XQuery query=new XQuery("declare copy-namespaces no-preserve,inherit; <a> { <b>{()}</b> } </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a><b/></a>",false));
}
