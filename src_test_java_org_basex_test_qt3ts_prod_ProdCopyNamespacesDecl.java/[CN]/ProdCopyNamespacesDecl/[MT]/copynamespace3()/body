{
  final XQuery query=new XQuery("\n" + "        declare namespace foo = \"http://example.org\"; \n" + "        declare copy-namespaces preserve,no-inherit; \n"+ "        let $existingElement := <existingElement xmlns=\"http://www.existingnamespace.com\">{\"Existing Content\"}</existingElement> \n"+ "        let $new := <foo:newElement xmlns = \"http://www.mynamespace.com\">{$existingElement}</foo:newElement> \n"+ "        return in-scope-prefixes(exactly-one($new/child::node()))\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertPermutation("\"\", \"xml\""));
}
