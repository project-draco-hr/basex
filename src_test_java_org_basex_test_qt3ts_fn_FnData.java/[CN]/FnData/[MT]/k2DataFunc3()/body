{
  final XQuery query=new XQuery("\n" + "      \tlet $x := <e><f>1</f></e>\n" + "      \treturn $x/data()\n"+ "      \t\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"1") && assertType("xs:untypedAtomic")));
}
