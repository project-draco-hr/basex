{
  final int c=bfc.get();
switch (c) {
case ENC_ISO_8859_1:
    return "ISO-8859-1";
case ENC_UTF_8:
  return "UTF-8";
case ENC_UTF_16_NO_BOM:
deepFile.debug("MP3Parser: Unsupported text encoding (UTF-16 without BOM)");
return null;
case ENC_UTF_16_WITH_BOM:
return "UTF-16";
default :
bfc.skip(-1);
return "";
}
}
