{
  createAtomicUpdates(preparePrimitives()).execute(true);
  Collections.sort(dbops);
  for (  final BasicOperation d : dbops)   d.apply();
  for (  final Put p : puts.values())   p.apply();
  if (data.inMemory() && !data.meta.original.isEmpty() && data.meta.options.get(MainOptions.WRITEBACK)) {
    try {
      Export.export(data,data.meta.original,null);
    }
 catch (    final IOException ex) {
      throw UPPUTERR.get(null,data.meta.original);
    }
  }
}
