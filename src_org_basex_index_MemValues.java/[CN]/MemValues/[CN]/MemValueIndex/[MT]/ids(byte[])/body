{
  final int i=id(key);
  if (i == 0)   return IndexIterator.EMPTY;
  return new IndexIterator(){
    int p=-1;
    @Override public boolean more(){
      return ++p < len[i];
    }
    @Override public int next(){
      return ids[i][p];
    }
    @Override public double score(){
      return -1;
    }
  }
;
}
