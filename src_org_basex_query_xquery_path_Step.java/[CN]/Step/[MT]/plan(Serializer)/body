{
  ser.startElement(this);
  ser.attribute(AXIS,Token.token(axis.name));
  ser.attribute(TEST,Token.token(test.toString()));
  ser.attribute(NS,timer());
  if (expr.length != 0) {
    ser.finishElement();
    for (    Expr e : expr)     e.plan(ser);
    ser.closeElement();
  }
 else {
    ser.emptyElement();
  }
}
