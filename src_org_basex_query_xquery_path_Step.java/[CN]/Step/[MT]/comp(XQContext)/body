{
  super.comp(ctx);
  if (expr.length == 0)   return new SimpleIterStep(axis,test,expr);
  final boolean last=expr[0] instanceof Fun && ((Fun)expr[0]).func == FunDef.LAST;
  final boolean num=expr[0].n();
  return expr.length > 1 || (!last && !num && uses(Using.POS)) ? this : new IterStep(axis,test,expr,last,num);
}
