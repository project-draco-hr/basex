{
  super.comp(ctx);
  final boolean last=expr[0] instanceof Fun && ((Fun)expr[0]).func == FunDef.LAST;
  final boolean num=expr[0].n();
  return expr.length > 1 || (!last && !num && uses(Using.POS)) ? this : expr.length == 0 ? new SimpleIterStep(axis,test,expr) : new IterStep(axis,test,expr,last,num);
}
