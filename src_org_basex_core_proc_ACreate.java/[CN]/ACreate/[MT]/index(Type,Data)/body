{
  if (d instanceof MemData)   return;
  IndexBuilder b=null;
switch (i) {
case TXT:
    b=new ValueBuilder(d,true);
  break;
case ATV:
b=new ValueBuilder(d,false);
break;
case FTX:
b=FTBuilder.get(d,d.meta.wildcards);
break;
default :
break;
}
d.closeIndex(i);
d.meta.dirty=true;
progress(b);
d.setIndex(i,b.build());
}
