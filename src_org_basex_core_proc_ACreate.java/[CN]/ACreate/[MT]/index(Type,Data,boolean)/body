{
  if (d instanceof MemData)   return;
  IndexBuilder b=null;
switch (i) {
case TXT:
    b=new ValueBuilder(d,true,o);
  break;
case ATV:
b=new ValueBuilder(d,false,o);
break;
case FTX:
b=d.meta.ftfz ? new FTFuzzyBuilder(d,o) : new FTTrieBuilder(d,o);
break;
default :
break;
}
d.closeIndex(i);
progress(b);
d.setIndex(i,b.build());
}
