{
  checkCreate(ctx);
switch (sig) {
case _SQL_INIT:
    return init(ctx);
case _SQL_CONNECT:
  return connect(ctx);
case _SQL_PREPARE:
return prepare(ctx);
case _SQL_CLOSE:
return close(ctx);
case _SQL_COMMIT:
return commit(ctx);
case _SQL_ROLLBACK:
return rollback(ctx);
default :
return super.item(ctx,ii);
}
}
