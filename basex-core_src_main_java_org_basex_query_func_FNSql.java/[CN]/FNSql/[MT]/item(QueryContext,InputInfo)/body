{
  checkCreate(qc);
switch (func) {
case _SQL_INIT:
    return init(qc);
case _SQL_CONNECT:
  return connect(qc);
case _SQL_PREPARE:
return prepare(qc);
case _SQL_CLOSE:
return close(qc);
case _SQL_COMMIT:
return commit(qc);
case _SQL_ROLLBACK:
return rollback(qc);
default :
return super.item(qc,ii);
}
}
