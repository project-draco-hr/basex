{
  final Iter iter=arg.length != 0 ? arg[0] : null;
switch (func) {
case TRUE:
    return Bln.TRUE.iter();
case FALSE:
  return Bln.FALSE.iter();
case BOOL:
return Bln.get(ebv(iter)).iter();
case NOT:
return Bln.get(!ebv(iter)).iter();
case EMPTY:
return Bln.get(iter.next() == null).iter();
case EXISTS:
return Bln.get(iter.next() != null).iter();
case ZEROONE:
Item it=iter.next();
if (it == null) return Iter.EMPTY;
if (iter.next() != null) Err.or(ZEROONE);
return it.iter();
case EXONE:
it=iter.next();
if (it == null || iter.next() != null) Err.or(EXONE);
return it.iter();
case ONEMORE:
final Iter ir=SeqIter.get(iter);
if (ir.size() < 1) Err.or(ONEMORE);
return ir;
case UNORDER:
return iter;
default :
BaseX.notexpected(func);
return null;
}
}
