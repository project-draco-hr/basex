{
  check(_ARCHIVE_DELETE);
  query("let $a := " + _FILE_READ_BINARY.args(ZIP) + "let $b := "+ _ARCHIVE_DELETE.args("$a","infos/stopWords")+ "let $c := "+ _ARCHIVE_ENTRIES.args("$b")+ "return count($c)",4);
  query("let $a := " + _FILE_READ_BINARY.args(ZIP) + "let $b := "+ _ARCHIVE_ENTRIES.args("$a")+ "let $c := "+ _ARCHIVE_DELETE.args("$a","($b/text())[position() > 1]")+ "return count(archive:entries($c))","1");
}
