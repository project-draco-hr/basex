{
switch (func) {
case CURRDATE:
    return currDate(ctx).iter();
case CURRDTM:
  return currDTM(ctx).iter();
case CURRTIME:
return currTIM(ctx).iter();
case IMPLZONE:
return implZone().iter();
case COLLAT:
return ctx.baseURI.resolve(ctx.collation).iter();
case STBASEURI:
return ctx.baseURI != Uri.EMPTY ? ctx.baseURI.iter() : Iter.EMPTY;
default :
BaseX.notexpected(func);
return null;
}
}
