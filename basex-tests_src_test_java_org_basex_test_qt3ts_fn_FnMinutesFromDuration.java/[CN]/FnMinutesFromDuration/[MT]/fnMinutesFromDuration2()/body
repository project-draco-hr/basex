{
  final XQuery query=new XQuery("fn:minutes-from-duration(xs:dayTimeDuration(\"-P5DT12H30M\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-30"));
}
