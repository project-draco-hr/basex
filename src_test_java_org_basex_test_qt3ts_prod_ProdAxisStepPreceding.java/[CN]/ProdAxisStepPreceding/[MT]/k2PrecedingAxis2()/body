{
  final XQuery query=new XQuery("declare variable $i := <root> <child> <child2> <child3> <leaf/> </child3> <following/> </child2> <following/> </child> <following/> </root>; empty(root($i)//leaf/preceding::node())",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
