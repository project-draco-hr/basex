{
  final XQuery query=new XQuery("<result> { (//node())[last()]/preceding::node(), empty((//node())[last()]/preceding::node()) } </result>",ctx);
  try {
    query.context(node(file("prod/AxisStep.preceding/AttributesAndElements.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result>true</result>",false));
}
