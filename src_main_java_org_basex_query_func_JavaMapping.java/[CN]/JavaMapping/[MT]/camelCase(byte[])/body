{
  final TokenBuilder tb=new TokenBuilder();
  boolean dash=false;
  for (int p=0; p < ln.length; p+=cl(ln,p)) {
    final int ch=cp(ln,p);
    if (dash) {
      tb.add(Character.toUpperCase(ch));
      dash=false;
    }
 else {
      dash=ch == '-';
      if (!dash)       tb.add(ch);
    }
  }
  return tb.toString();
}
