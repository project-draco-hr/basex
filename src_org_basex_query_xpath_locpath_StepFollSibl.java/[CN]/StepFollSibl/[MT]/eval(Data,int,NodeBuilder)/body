{
  int kind=data.kind(p);
  if (kind == Data.ATTR)   return;
  final int par=data.parent(p,kind);
  final int size=par + data.size(par,data.kind(par));
  int pre=p + data.size(p,kind);
  while (pre != size) {
    kind=data.kind(pre);
    test.eval(data,pre,kind,t);
    pre+=data.size(pre,kind);
  }
}
