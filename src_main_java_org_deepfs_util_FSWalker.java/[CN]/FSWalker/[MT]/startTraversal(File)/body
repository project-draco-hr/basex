{
  final File[] files=d.listFiles();
  if (files == null)   return;
  for (  final File f : files) {
    if (!valid(f)) {
      symLinkVisit(f);
    }
 else     if (f.isDirectory()) {
      preDirectoryVisit(f);
      levelUpdate(++level);
      startTraversal(f);
      levelUpdate(--level);
      postDirectoryVisit(f);
    }
 else {
      regularFileVisit(f);
    }
  }
}
