{
  ii.tn++;
  DNode dn=(DNode)ctx.coll(null).next();
  FTTokenizer fto=new FTTokenizer(val);
  int i=0;
  while (fto.more()) {
    final int n=dn.data.nrIDs(fto);
    if (n == 0) {
      ii.indexSize=0;
      return Bln.FALSE;
    }
    i=Math.max(i,n);
  }
  ii.indexSize=i;
  return new FTIndex(val,ii.lt,ii.tn);
}
