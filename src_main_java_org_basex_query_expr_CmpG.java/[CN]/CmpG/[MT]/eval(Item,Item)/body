{
  if (!(a instanceof FItem || b instanceof FItem)) {
    final Type ta=a.type, tb=b.type;
    if (ta == tb || ta.isUntyped() || tb.isUntyped() || a instanceof ANum && b instanceof ANum || a instanceof AStr && b instanceof AStr)     return op.op.eval(info,a,b);
  }
  throw XPTYPECMP.thrw(info,a.type,b.type);
}
