{
  if (args.length != 2) {
    help(new String[]{"help","stat"});
    return;
  }
  final Stat stat=new Stat();
  int rc=fs.stat(args[1],stat);
  if (rc == -1)   System.err.printf("stat failed.\n");
  PrintStream ps=new PrintStream(System.out);
  stat.printFields("deepshell: ",ps);
  ps.flush();
}
