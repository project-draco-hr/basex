{
  try {
    final Nodes n=new QueryProcessor("/",fs.getContext()).queryNodes();
    final PrintOutput out=new PrintOutput(System.out);
    final XMLSerializer xml=new XMLSerializer(out,false,true);
    n.serialize(xml);
    out.flush();
    xml.close();
  }
 catch (  final Exception ex) {
    ex.printStackTrace();
  }
}
