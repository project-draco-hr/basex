{
  ok(new CreateDB(NAME),context);
  ok(new Check(NAME),CONTEXT2);
  noCloseOk(new Add(FN,FILE));
  ok(new Check(NAME),CONTEXT2);
  noCloseOk(new Replace(FN,"<x/>"));
  ok(new Check(NAME),CONTEXT2);
  noCloseOk(new Rename(FN,FN));
  ok(new Check(NAME),CONTEXT2);
  noCloseOk(new Store(NAME2,"<x/>"));
  ok(new Check(NAME),CONTEXT2);
  noCloseOk(new Delete(FN));
}
