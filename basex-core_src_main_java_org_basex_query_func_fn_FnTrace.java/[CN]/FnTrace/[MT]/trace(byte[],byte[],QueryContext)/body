{
  final TokenBuilder tb=new TokenBuilder();
  if (label != null)   tb.add(label);
  final String info=tb.add(value).toString();
  if (qc.listen != null || qc.context.listener != null) {
    qc.evalInfo(info);
    if (qc.listen != null)     qc.listen.info(info);
  }
 else {
    Util.errln(info);
  }
}
