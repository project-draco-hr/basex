{
  final TokenBuilder tb=new TokenBuilder();
  if (label != null)   tb.add(label);
  final String info=tb.add(value).toString();
  final InfoListener il=qc.listener;
  if (il != null || qc.context.listener != null) {
    qc.evalInfo(info);
    if (il != null)     il.info(info);
  }
 else {
    Util.errln(info);
  }
}
