{
  final XQuery query=new XQuery("comment {comment {'one', comment {'two'}}, 'three', comment {'four'}}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<!--one two three four-->",false));
}
