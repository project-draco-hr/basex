{
  final XQuery query=new XQuery("<elem>{comment {'one'}}<a>{comment {'two'}}</a>{comment {'three'}}</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem><!--one--><a><!--two--></a><!--three--></elem>",false));
}
