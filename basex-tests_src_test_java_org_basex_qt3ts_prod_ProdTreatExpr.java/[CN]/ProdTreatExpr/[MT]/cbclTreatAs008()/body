{
  final XQuery query=new XQuery("subsequence((1 to 10)[. mod 2 = 0] treat as xs:integer+,1,2)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2 4"));
}
