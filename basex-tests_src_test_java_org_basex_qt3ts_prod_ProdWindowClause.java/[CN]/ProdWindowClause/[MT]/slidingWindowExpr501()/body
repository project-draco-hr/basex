{
  final XQuery query=new XQuery("for sliding window $w in (1, 2, 3, 4) \n" + "      start at $s when fn:true()\n" + "      end at $e when $e - $s eq 1\n"+ "      return $w",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("(1, 2, 2, 3, 3, 4, 4)"));
}
