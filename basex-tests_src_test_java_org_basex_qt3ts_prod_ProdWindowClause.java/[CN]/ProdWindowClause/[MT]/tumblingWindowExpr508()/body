{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w as xs:integer+ in (1, 2, \"london\", 3, 4.1, \"paris\")\n" + "          start $start when $start instance of xs:integer\n"+ "          only end next $beyond when $beyond instance of xs:string\n"+ "          return string-join($w!string(), ' ')\n"+ "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
