{
  final Session ss=session();
  if (ss == null)   return false;
  final boolean ok=ss.execute(pr,out);
  if (pr instanceof Exit)   return true;
  if (v || !ok) {
    final String inf=ss.info();
    if (!inf.isEmpty()) {
      if (!ok) {
        error(null,inf);
      }
 else {
        out(inf);
      }
    }
  }
  return ok;
}
