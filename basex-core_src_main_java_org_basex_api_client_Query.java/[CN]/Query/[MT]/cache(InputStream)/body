{
  cache=new TokenList();
  types=new ByteList();
  final ByteList bl=new ByteList();
  for (int t; (t=input.read()) > 0; ) {
    final DecodingInput di=new DecodingInput(input);
    for (int b; (b=di.read()) != -1; )     bl.add(b);
    cache.add(bl.toArray());
    types.add(t);
    bl.reset();
  }
  pos=0;
}
