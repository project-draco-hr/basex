{
  final XQuery query=new XQuery("analyze-string(\"banana\", \"(?:b(an)*a)\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<fn:analyze-string-result xmlns:fn=\"http://www.w3.org/2005/xpath-functions\"><fn:match>ban<fn:group nr=\"1\">an</fn:group>a</fn:match></fn:analyze-string-result>",true));
}
