{
  final XQuery query=new XQuery("analyze-string(\"banana\", \"custard\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<fn:analyze-string-result xmlns:fn=\"http://www.w3.org/2005/xpath-functions\"><fn:non-match>banana</fn:non-match></fn:analyze-string-result>",true));
}
