{
  final XQuery query=new XQuery("analyze-string(\"banana\", \"a(n)\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<fn:analyze-string-result xmlns:fn=\"http://www.w3.org/2005/xpath-functions\"><fn:non-match>b</fn:non-match><fn:match>a<fn:group nr=\"1\">n</fn:group></fn:match><fn:match>a<fn:group nr=\"1\">n</fn:group></fn:match><fn:non-match>a</fn:non-match></fn:analyze-string-result>",true));
}
