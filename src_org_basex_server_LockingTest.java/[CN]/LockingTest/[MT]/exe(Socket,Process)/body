{
  final AbstractProcess proc=new ClientProcessNew(s,p);
  try {
    final boolean ok=proc.execute(null);
    if (ok && p.printing()) {
      final PrintOutput out=new PrintOutput(System.out);
      proc.output(out);
      out.close();
    }
  }
 catch (  final FileNotFoundException ex) {
    ex.printStackTrace();
  }
catch (  final IOException ex) {
    ex.printStackTrace();
  }
}
