{
  if (expr.length == 2)   checkColl(expr[1],ctx);
  return new Iter(){
    final Iter iter=expr[0].iter(ctx);
    final ItemSet map=new ItemSet();
    @Override public Item next() throws QueryException {
      while (true) {
        Item i=iter.next();
        if (i == null)         return null;
        i=FNGen.atom(i);
        if (map.index(i))         return i;
      }
    }
  }
;
}
