{
  if (args.length == 2)   checkColl(args[1],ctx);
  return new Iter(){
    final Iter iter=args[0].iter(ctx);
    final ItemSet map=new ItemSet();
    @Override public Item next() throws QueryException {
      Item i;
      while ((i=iter.next()) != null) {
        i=FNGen.atom(i);
        if (map.index(i))         return i;
      }
      return null;
    }
  }
;
}
