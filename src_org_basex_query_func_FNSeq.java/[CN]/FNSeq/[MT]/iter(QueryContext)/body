{
  final Iter[] arg=new Iter[expr.length];
  for (int a=0; a < expr.length; a++)   arg[a]=ctx.iter(expr[a]);
switch (func) {
case INDEXOF:
    return indexOf(ctx);
case DISTINCT:
  return distinct(ctx);
case INSBEF:
return insbef(ctx);
case REVERSE:
return reverse(ctx);
case REMOVE:
return remove(ctx);
case SUBSEQ:
return subseq(ctx);
default :
return super.iter(ctx);
}
}
