{
  final XQuery query=new XQuery("format-dateTime($t,'[Y0001]-[M01]-[D01]T[H01]:[m01]:[s01].[f001]')",ctx);
  try {
    query.bind("t",new XQuery("xs:dateTime('2003-09-07T09:15:06.456')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2003-09-07T09:15:06.456"));
}
