{
  final XQuery query=new XQuery("for $i in 1 to 48,\n" + "                $d in $t + xs:yearMonthDuration('P1M')*$i\n" + "            return concat(\"[\", $d, \": \", format-dateTime($d, '[w]', (), 'ISO', ()))",ctx);
  try {
    query.bind("t",new XQuery("xs:dateTime('2005-12-01T12:00:00')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n            [2006-01-01T12:00:00: 0 [2006-02-01T12:00:00: 1 [2006-03-01T12:00:00: 1 [2006-04-01T12:00:00: 0 \n            [2006-05-01T12:00:00: 1 [2006-06-01T12:00:00: 1 [2006-07-01T12:00:00: 0 [2006-08-01T12:00:00: 1 \n            [2006-09-01T12:00:00: 0 [2006-10-01T12:00:00: 0 [2006-11-01T12:00:00: 1 [2006-12-01T12:00:00: 0 \n            [2007-01-01T12:00:00: 1 [2007-02-01T12:00:00: 1 [2007-03-01T12:00:00: 1 [2007-04-01T12:00:00: 0 \n            [2007-05-01T12:00:00: 1 [2007-06-01T12:00:00: 0 [2007-07-01T12:00:00: 0 [2007-08-01T12:00:00: 1 \n            [2007-09-01T12:00:00: 0 [2007-10-01T12:00:00: 1 [2007-11-01T12:00:00: 1 [2007-12-01T12:00:00: 0 \n            [2008-01-01T12:00:00: 1 [2008-02-01T12:00:00: 0 [2008-03-01T12:00:00: 0 [2008-04-01T12:00:00: 1 \n            [2008-05-01T12:00:00: 1 [2008-06-01T12:00:00: 0 [2008-07-01T12:00:00: 1 [2008-08-01T12:00:00: 0 \n            [2008-09-01T12:00:00: 1 [2008-10-01T12:00:00: 1 [2008-11-01T12:00:00: 0 [2008-12-01T12:00:00: 1 \n            [2009-01-01T12:00:00: 1 [2009-02-01T12:00:00: 0 [2009-03-01T12:00:00: 0 [2009-04-01T12:00:00: 1 \n            [2009-05-01T12:00:00: 0 [2009-06-01T12:00:00: 1 [2009-07-01T12:00:00: 1 [2009-08-01T12:00:00: 0 \n            [2009-09-01T12:00:00: 1 [2009-10-01T12:00:00: 1 [2009-11-01T12:00:00: 0 [2009-12-01T12:00:00: 1\n         "));
}
