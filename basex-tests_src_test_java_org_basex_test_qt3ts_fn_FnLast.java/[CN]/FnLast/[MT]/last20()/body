{
  final XQuery query=new XQuery("for $h in (/works/employee[12]) return $h/overtime[position() or fn:true()]/*/string()",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"Monday\", \"Tuesday\""));
}
