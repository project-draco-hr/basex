{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:generate($arg as xs:integer?) as xs:integer? { $arg }; \n" + "      \t( if (local:generate(0) = 0) then () else 1, for $x in local:generate(0) return -$x)[last()]\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0"));
}
