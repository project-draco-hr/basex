{
  final XQuery query=new XQuery("\n" + "        declare function local:index-of($seq as xs:integer*, $item as xs:integer?) as xs:float* { \n" + "          if (empty($item)) \n"+ "            then -1\n"+ "            else for $x at $p in $seq return if ($x eq $item) then $p else () \n"+ "        };\n"+ "        local:index-of(1 to 10, 3)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"3"));
}
