{
  final XQuery query=new XQuery("\n" + "        declare function local:f($v as xs:double*) as xs:double+ { if (empty($v)) then 0 else $v };\n" + "        declare function local:g($v as xs:double*) as xs:double+ { local:f($v) };\n"+ "        local:g((1,2,3))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2 3"));
}
