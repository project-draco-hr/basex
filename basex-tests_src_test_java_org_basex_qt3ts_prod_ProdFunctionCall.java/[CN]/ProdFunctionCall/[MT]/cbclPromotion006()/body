{
  final XQuery query=new XQuery("\n" + "        declare function local:f($arg as xs:anyAtomicType?) { $arg };\n" + "        local:f(index-of((1,2,3,2),2))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
