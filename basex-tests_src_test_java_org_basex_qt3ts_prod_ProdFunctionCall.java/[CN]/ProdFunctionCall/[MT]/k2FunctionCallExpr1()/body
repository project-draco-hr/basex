{
  final XQuery query=new XQuery("\n" + "        declare function local:bar($c, $d, $e, $f, $g, $h, $i, $j, $a, $b) { 1 }; \n" + "        declare function local:moo($k) { $k }; \n"+ "        local:moo(1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
