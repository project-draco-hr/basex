{
  final XQuery query=new XQuery("function-lookup(fn:QName('http://www.w3.org/2005/xpath-functions', 'unparsed-text-lines'), 2)('http://www.w3.org/fots/unparsed-text/text-plain-utf-8.txt', 'utf-8')",ctx);
  try {
    query.context(node(file("fn/function-lookup/function-lookup.xml")));
    query.addCollection("",new String[]{file("fn/function-lookup/collection-1.xml"),file("fn/function-lookup/collection-2.xml")});
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("xs:string+"));
}
