{
  final XQuery query=new XQuery("function-lookup(fn:QName('http://www.w3.org/2005/xpath-functions', 'substring-before'), 3)('string', 'ing', 'http://www.w3.org/2005/xpath-functions/collation/codepoint')",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"str"));
}
