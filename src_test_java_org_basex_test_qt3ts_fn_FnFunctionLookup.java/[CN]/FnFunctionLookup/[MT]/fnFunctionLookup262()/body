{
  final XQuery query=new XQuery("function-lookup(fn:QName('http://www.w3.org/2005/xpath-functions', 'in-scope-prefixes'), 1)(/root)",ctx);
  query.context(node(file("fn/function-lookup/function-lookup.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"xml"));
}
