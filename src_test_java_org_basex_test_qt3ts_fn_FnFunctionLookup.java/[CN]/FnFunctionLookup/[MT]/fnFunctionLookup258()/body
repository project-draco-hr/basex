{
  final XQuery query=new XQuery("function-lookup(fn:QName('http://www.w3.org/2005/xpath-functions', 'namespace-uri-from-QName'), 1)(fn:QName('http://www.example.org', 'foo:bar'))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.org"));
}
