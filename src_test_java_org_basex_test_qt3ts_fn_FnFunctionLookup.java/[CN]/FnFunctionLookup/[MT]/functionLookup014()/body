{
  final XQuery query=new XQuery("\n" + "        let $f := function-lookup(QName(\"http://exslt.org/dates-and-times\", \"month-abbreviation\"), 1)\n" + "        return if (exists($f)) then $f(\"2012-02-28\") else \"not-available\"\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"not-available") || assertStringValue(false,"Feb")));
}
