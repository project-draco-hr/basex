{
  final XQuery query=new XQuery("function-lookup(fn:QName('http://www.w3.org/2005/xpath-functions', 'adjust-dateTime-to-timezone'), 2)(xs:dateTime(\"1970-01-01T00:00:00Z\"),xs:dayTimeDuration(\"-PT10H\"))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1969-12-31T14:00:00-10:00"));
}
