{
  final XQuery query=new XQuery("('Ingratitude, ' ||  'thou ' ||  'marble-hearted' ||  ' fiend!')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Ingratitude, thou marble-hearted fiend!"));
}
