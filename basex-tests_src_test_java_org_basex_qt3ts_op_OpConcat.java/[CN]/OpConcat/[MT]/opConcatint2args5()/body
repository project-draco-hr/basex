{
  final XQuery query=new XQuery("(xs:int(\"-2147483648\")||xs:int(\"2147483647\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-21474836482147483647"));
}
