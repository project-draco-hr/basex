{
  while (true) {
    s();
    if (markupDecl())     continue;
    if (!consume(COND))     return;
    s();
    final boolean incl=consume(INCL);
    if (!incl)     check(IGNO);
    s();
    check('[');
    if (incl) {
      extSubsetDecl();
      check(CONE);
    }
 else {
      int c=1;
      while (c != 0) {
        if (consume(COND))         ++c;
 else         if (consume(CONE))         --c;
 else         if (consume() == 0)         error(INVEND);
      }
    }
  }
}
