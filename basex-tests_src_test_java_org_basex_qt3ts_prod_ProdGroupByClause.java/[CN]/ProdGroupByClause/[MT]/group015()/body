{
  final XQuery query=new XQuery("\n" + "          for $x in (true(), \"true\", xs:QName(\"true\"))\n" + "          group by $x\n"+ "          return $x\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("true true true",false));
}
