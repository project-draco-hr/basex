{
  final StringBuilder qs=new StringBuilder("xquery\n");
  qs.append("declare namespace rdf = " + "\"http://www.w3.org/1999/02/22-rdf-syntax-ns#\";\n");
  qs.append("declare namespace core = " + "\"http://rdfs.cyim.com/CoreContent#\";\n");
  qs.append("declare namespace wfdata = " + "\"http://rdfs.cyim.com/WorkflowData#\";\n");
  qs.append("insert node ").append("<wfdata:Workflow xmlns:wfdata=" + "\"http://rdfs.cyim.com/WorkflowData#\">\n").append("  <wfdata:raiseOn>2011-11-01T11:34:08Z</wfdata:raiseOn>\n").append("</wfdata:Workflow>\n").append("into /rdf:RDF/*[@rdf:about=\"" + uri + "\"] ");
  session.execute(qs.toString());
}
