{
  byte ch;
  if (pp != 0) {
    ch=last[(lp + pp++) & 0x0F];
  }
 else {
    ch=input.readByte();
    last[lp++]=ch;
    lp&=0x0F;
    if (ch == '\n') {
      col=1;
      line++;
    }
 else {
      col++;
    }
  }
  if (ch < ' ' && ch > 0 && !whitespace(ch))   error(XMLCHAR,(char)ch,ch);
  return ch;
}
