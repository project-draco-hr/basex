{
  if (!consume(ENCOD))   return null;
  s();
  check('=');
  s();
  final TokenBuilder enc=new TokenBuilder();
  final byte d=qu();
  byte ch=nextChar();
  if (letter(ch) && ch != '_') {
    while (letterOrDigit(ch) || ch == '.' || ch == '-') {
      enc.add(ch);
      ch=nextChar();
    }
    prev(1);
  }
  check((char)d);
  if (enc.size == 0)   error(DECLENCODE,enc);
  return string(enc.finish());
}
