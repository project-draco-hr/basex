{
  Object[] item={usern,false,false,false,false,crypt.encrypt(pass)};
  ArrayList<Object[]> list=getList();
  if (list == null)   list=new ArrayList<Object[]>();
  boolean in=false;
  for (  Object[] s : list) {
    if (s[0].equals(usern))     in=true;
  }
  if (!in)   list.add(item);
  writeList(list);
}
