{
  if (tok.range())   return idRange((RangeToken)tok);
  final long pos=get(tok.text);
  if (pos == 0)   return Array.NOINTS2;
  final int ds=idxl.readNum(pos);
  final int[] ids=new int[ds];
  int p=0;
  for (int d=0; d < ds; d++) {
    ids[d]=p + idxl.readNum();
    p=ids[d];
  }
  return new int[][]{ids};
}
