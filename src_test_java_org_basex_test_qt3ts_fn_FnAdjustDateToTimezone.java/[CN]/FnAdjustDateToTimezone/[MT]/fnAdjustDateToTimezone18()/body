{
  final XQuery query=new XQuery("let $tz := xs:dayTimeDuration(\"PT10H\") \n" + "         return fn:adjust-date-to-timezone(xs:date(\"2002-03-07Z\"),$tz) - xs:date(\"2006-03-07Z\")\n" + "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"-P1461DT10H"));
}
