{
  final XQuery query=new XQuery("adjust-date-to-timezone(current-date(), implicit-timezone()) eq current-date()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
