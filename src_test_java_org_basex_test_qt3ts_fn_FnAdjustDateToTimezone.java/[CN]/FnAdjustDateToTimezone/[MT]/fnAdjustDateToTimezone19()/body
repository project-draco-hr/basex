{
  final XQuery query=new XQuery("let $tz := xs:dayTimeDuration(\"PT10H\") \n" + "         return fn:adjust-date-to-timezone(xs:date(\"2004-03-07Z\"),$tz) - xs:date(\"2001-03-07Z\")\n" + "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"P1095DT14H"));
}
