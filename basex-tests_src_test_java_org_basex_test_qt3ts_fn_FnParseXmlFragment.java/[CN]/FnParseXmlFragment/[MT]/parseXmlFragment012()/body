{
  final XQuery query=new XQuery("parse-xml-fragment(codepoints-to-string((38, 35, 51, 56, 59)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"&"));
}
