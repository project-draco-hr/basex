{
  final Value val;
  if (values == null || values.size() == 0) {
    val=rxp.value;
  }
 else {
    final ValueBuilder vb=new ValueBuilder();
    for (    final Object o : values) {
      vb.add(o instanceof Value ? (Value)o : new Atm(o.toString()));
    }
    val=vb.value();
  }
  bind(rxp.name,args,val);
}
