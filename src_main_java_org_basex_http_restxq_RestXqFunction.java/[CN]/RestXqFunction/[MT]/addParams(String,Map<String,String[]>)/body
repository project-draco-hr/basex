{
  for (  final String nv : body.split("&")) {
    final String[] parts=nv.split("=",2);
    if (parts.length < 2)     continue;
    try {
      params.put(parts[0],new String[]{URLDecoder.decode(parts[1],Token.UTF8)});
    }
 catch (    final Exception ex) {
      Util.notexpected(ex);
    }
  }
}
