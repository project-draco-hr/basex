{
  query(transform("<a xmlns='x'/>","insert node <a xmlns='y'/> into $input"),"<a xmlns='x'><a xmlns='y'/></a>");
  create(2);
  query("insert node <a xmlns='y'/> into doc('d2')//*:x");
  assertEquals(NL + "  Pre[1] xmlns=\"xx\"" + NL+ "    Pre[2] xmlns=\"y\"",context.data().nspaces.toString());
}
