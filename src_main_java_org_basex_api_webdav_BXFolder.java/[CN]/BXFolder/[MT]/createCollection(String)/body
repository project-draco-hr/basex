{
  final String newFolder=path + DIRSEP + folder;
  try {
    ClientSession cs=login(user,pass);
    try {
      cs.execute(new Open(db));
      cs.execute(new Add("<empty/>","EMPTY.xml",newFolder));
    }
  finally {
      cs.close();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return new BXFolder(db,newFolder,user,pass);
}
