{
  Session s=null;
  try {
    s=session.login();
    deleteDummy(s,db,path);
    final String newFolder=path + SEP + folder;
    createDummy(s,db,newFolder);
    return new BXFolder(db,newFolder,session);
  }
 catch (  final Exception ex) {
    throw error(ex);
  }
 finally {
    try {
      if (s != null)       s.close();
    }
 catch (    final IOException e) {
      error(e);
    }
  }
}
