{
  try {
    ser.openElement(token(s2));
    for (int i=0; i < atts.getLength(); ++i)     ser.attribute(token(atts.getQName(i)),token(atts.getValue(i)));
    for (    final Entry<String,String> e : ns.entrySet())     ser.attribute(concat(XMLNSC,token(e.getKey())),token(e.getValue()));
  }
 catch (  final IOException ex) {
    throw new SAXException(ex);
  }
}
