  check("declare function local:id($x) as item() { $x };" + "let $x := <x/> " + "for $a in 1 to 2 "+ "for $b as element(x) in local:id($x) "+ "return $b","<x/>" + Prop.NL + "<x/>","//(For | Let)[starts-with(Var/@name, '$b')] << //For[Var/@name eq '$a']","every $let in //Let, $for in //For satisfies $let << $for");
