{
  check("for $i in 0 to 3, $j in 0 to 3 where (<x/>)[$i + $j] " + "let $foo := $i * $i return $foo * $foo","0 1","every $let in //Let satisfies $let << exactly-one(//Where)");
  check("<x/>/(for $i in 1 to 3 let $x := .  where $x return $x)","<x/>","//GFLWOR/*[1] is //Let");
  check("for $len in 1 to 3 " + "for sliding window $w in 1 to 3 start at $p when true() only " + "end at $q when $q - $p + 1 eq $len "+ "let $x := $len div 2 "+ "return count($w) div ($x + $x)","1 1 1 1 1 1","//For << //Let and //Let << //Window");
}
