{
  check("for $i at $p in 1 to 1000 " + "let $sum := sum(1 to $i * $i) " + "where $i lt 5 "+ "return $sum","1 10 45 136","//For << //Where and //Where << //Let");
  check("for $len in 1 to 3 " + "for sliding window $w in 1 to 3 start at $p when true() only " + "end at $q when $q - $p + 1 eq $len where $len > 2 return <window>{$w}</window>","<window>1 2 3</window>","//For << //Window and exists(//For/Filter)");
}
