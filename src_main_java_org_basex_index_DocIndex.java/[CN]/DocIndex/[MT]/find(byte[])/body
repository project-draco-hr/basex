{
  int l=0, h=pathorder.length - 1;
  while (l <= h) {
    int m=l + h >>> 1;
    final int c=diff(paths.get(pathorder[m]),v);
    if (c == 0) {
      while (m > 0 && eq(paths.get(pathorder[m - 1]),v))       --m;
      return m;
    }
    if (c < 0)     l=m + 1;
 else     h=m - 1;
  }
  return l;
}
