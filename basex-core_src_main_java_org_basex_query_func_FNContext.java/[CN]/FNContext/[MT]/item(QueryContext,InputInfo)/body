{
switch (func) {
case CURRENT_DATE:
    return qc.initDateTime().date;
case CURRENT_DATETIME:
  return qc.initDateTime().dtm;
case CURRENT_TIME:
return qc.initDateTime().time;
case IMPLICIT_TIMEZONE:
return qc.initDateTime().zone;
case DEFAULT_COLLATION:
final Collation coll=sc.collation;
return Uri.uri(coll == null ? QueryText.COLLATIONURI : coll.uri());
case STATIC_BASE_URI:
final Uri uri=sc.baseURI();
return uri == Uri.EMPTY ? null : uri;
default :
return super.item(qc,ii);
}
}
