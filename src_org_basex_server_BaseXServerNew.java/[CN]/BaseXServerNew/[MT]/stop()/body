{
  for (int i=0; i < sessions.size(); i++) {
    try {
      sessions.get(i).dis.close();
      int tmp=i;
      BaseX.outln("Client " + (tmp + 1) + " has logged out.");
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  try {
    sessionListener.thread=null;
    inputListener.thread=null;
    serverSocket.close();
    new Exit().execute(null);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
