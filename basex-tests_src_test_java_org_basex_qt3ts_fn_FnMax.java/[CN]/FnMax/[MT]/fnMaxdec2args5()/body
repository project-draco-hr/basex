{
  final XQuery query=new XQuery("fn:max((xs:decimal(\"-999999999999999999\"),xs:decimal(\"999999999999999999\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("999999999999999999"));
}
