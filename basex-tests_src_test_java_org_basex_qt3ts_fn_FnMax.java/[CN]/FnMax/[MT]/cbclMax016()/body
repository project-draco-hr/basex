{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) { \n" + "      \t\tif ($x = 3) then xs:duration(\"P1Y\") else xs:yearMonthDuration(\"P11M\") \n"+ "      \t}; \n"+ "      \tmax(for $x in (1,2,3) return local:f($x))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FORG0006"));
}
