{
  final XQuery query=new XQuery("max((xs:date('1066-10-02'), xs:date('1588-08-08'), xs:date('2011-06-29')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2011-06-29"));
}
