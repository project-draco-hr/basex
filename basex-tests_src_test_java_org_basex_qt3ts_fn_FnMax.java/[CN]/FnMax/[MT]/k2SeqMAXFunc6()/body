{
  final XQuery query=new XQuery("max(xs:anyURI(\"str1\"), \"max://example.com/UNSUPPORTED_COLLATION\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("str1") || error("FOCH0002")));
}
