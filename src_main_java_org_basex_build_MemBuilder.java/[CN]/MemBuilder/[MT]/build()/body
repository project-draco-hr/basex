{
  data=new MemData(tags,atts,ns,path,prop);
  meta=data.meta;
  meta.name=name;
  meta.textindex=true;
  meta.attrindex=true;
  meta.ftindex=false;
  final IO file=parser.src;
  meta.original=file != null ? file.path() : "";
  meta.filesize=file != null ? file.length() : 0;
  meta.time=System.currentTimeMillis();
  parse();
  data.init();
  path.finish(data);
  return data;
}
