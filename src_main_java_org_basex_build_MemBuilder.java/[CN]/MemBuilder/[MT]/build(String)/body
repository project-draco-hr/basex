{
  data=new MemData(tags,atts,ns,path,prop);
  meta=data.meta;
  meta.name=name;
  meta.textindex=true;
  meta.attrindex=true;
  meta.ftindex=false;
  final IO file=parser.src;
  meta.path=file != null ? file.path() : "";
  meta.filesize=file != null ? file.length() : 0;
  meta.time=file != null ? file.date() : System.currentTimeMillis();
  parse(name);
  data.init();
  return data;
}
