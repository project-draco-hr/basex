{
  data=new MemData(path,ns,parser.prop);
  final MetaData md=data.meta;
  md.name=name;
  md.createtext=true;
  md.createattr=true;
  md.textindex=true;
  md.attrindex=true;
  final IO file=parser.src;
  md.original=file != null ? file.path() : "";
  md.filesize=file != null ? file.length() : 0;
  md.time=file != null ? file.timeStamp() : System.currentTimeMillis();
  meta=data.meta;
  tags=data.tagindex;
  atts=data.atnindex;
  path.data(data);
}
