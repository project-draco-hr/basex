{
  if (ctx.item == null)   Err.or(XPNOCTX,this);
  final SeqIter ir=new SeqIter();
  final Iter iter=ctx.item.iter();
  Item i;
  while ((i=iter.next()) != null) {
    if (!i.node())     Err.or(XPNOCTX,this);
    Node n=(Node)i;
    while (n.parent() != null)     n=n.parent();
    ir.add(n);
  }
  return ir;
}
