{
  final TokenBuilder tb=new TokenBuilder();
  final byte[] v=Token.token(x + 1);
  tb.add(v.length == 1 ? Token.concat(Token.SPACE,v) : v);
  tb.add(": " + name() + "(");
  for (int e=0; e < arg.length; e++) {
    if (e != 0)     tb.add(",");
    tb.add(Token.token(arg[e].x + 1));
  }
  tb.add(")\n");
  if (tbl.size != 0)   tb.add(pf.dump(tbl));
  return tb.toString();
}
