{
  final XQuery query=new XQuery("\n" + "        \tparse-xml-fragment(unparsed-text(\"../docs/atomic.xml\"))/*\n" + "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("element(*,xs:untyped)"));
}
