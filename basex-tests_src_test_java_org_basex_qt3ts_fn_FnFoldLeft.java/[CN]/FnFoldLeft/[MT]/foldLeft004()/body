{
  final XQuery query=new XQuery("fold-left(function($a, $b) { $a and $b }, false(), (true(), false(), false()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
