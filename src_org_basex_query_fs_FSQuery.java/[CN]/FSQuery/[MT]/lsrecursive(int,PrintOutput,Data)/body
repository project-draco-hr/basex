{
  int[] allDir=FSUtils.getAllDir(data,pre);
  int[] dir=FSUtils.getAllOfDir(data,pre);
  for (  int j : dir) {
    out.print(FSUtils.getName(data,j));
    out.print("\t");
  }
  out.print(NL);
  for (int i=0; allDir[i] != 0 && i < allDir.length; i++) {
    pwd(out,allDir[i]);
    out.print(NL);
    lsrecursive(allDir[i],out,data);
  }
}
