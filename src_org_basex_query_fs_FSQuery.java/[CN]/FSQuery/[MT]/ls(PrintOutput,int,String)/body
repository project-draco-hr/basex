{
  final Data data=context.data();
  int n=FSUtils.goToDir(data,pre,path);
  if (n == -1) {
    out.println("ls: " + path + " No such file or directory");
    return;
  }
  int[] dir=FSUtils.getAllOfDir(data,n);
  for (  int i : dir) {
    out.print(FSUtils.getName(data,i));
    out.print("\t");
  }
}
