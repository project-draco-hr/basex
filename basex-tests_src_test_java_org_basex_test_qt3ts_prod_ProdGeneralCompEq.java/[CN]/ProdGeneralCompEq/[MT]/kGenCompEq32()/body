{
  final XQuery query=new XQuery("remove((6, \"a string\"), 2) = xs:untypedAtomic(\"6\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPTY0004")));
}
