{
  final XQuery query=new XQuery("let $x := codepoints-to-string(12) return ($x = $x)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOCH0001")));
}
