{
  final XQuery query=new XQuery("\n" + "        declare function local:foo() {\n" + "          $x\n"+ "        };\n"+ "        declare variable $x external := 5;\n"+ "        local:foo()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("5"));
}
