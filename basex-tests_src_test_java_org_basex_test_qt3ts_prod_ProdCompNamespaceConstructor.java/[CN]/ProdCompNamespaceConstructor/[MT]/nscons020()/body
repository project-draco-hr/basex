{
  final XQuery query=new XQuery("\n" + "        let $uri := \"http://www.w3.org/2000/xmlns/\"\n" + "        return\n"+ "          <e>{ namespace x { $uri } }</e>\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQDY0101"));
}
