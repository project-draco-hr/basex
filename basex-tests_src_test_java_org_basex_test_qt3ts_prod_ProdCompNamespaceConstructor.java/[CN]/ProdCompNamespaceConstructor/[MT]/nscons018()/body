{
  final XQuery query=new XQuery("\n" + "        let $pre := 1,\n" + "            $uri := \"http://www.zorba-xquery.com/\"\n"+ "        return\n"+ "          <e>{ namespace { $pre } { $uri } }</e>\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
