{
  final XQuery query=new XQuery("map:collation(map:new(map{\"a\":=1,\"b\":=2}, \"http://www.w3.org/2005/xpath-functions/collation/codepoint\"))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.w3.org/2005/xpath-functions/collation/codepoint"));
}
