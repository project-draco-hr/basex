{
  final XQuery query=new XQuery("map:collation(map:entry(\"a\",\"x\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.w3.org/2005/xpath-functions/collation/codepoint"));
}
