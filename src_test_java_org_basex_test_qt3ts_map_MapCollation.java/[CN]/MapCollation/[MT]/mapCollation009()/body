{
  final XQuery query=new XQuery("map:collation(map:new(map{\"a\":=1,\"b\":=2}, \"xpath-functions/collation/codepoint\"))",ctx);
  try {
    query.baseURI("http://www.w3.org/2005/");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.w3.org/2005/xpath-functions/collation/codepoint"));
}
