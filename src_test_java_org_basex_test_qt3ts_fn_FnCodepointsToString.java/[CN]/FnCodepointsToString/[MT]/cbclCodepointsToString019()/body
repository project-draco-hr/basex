{
  final XQuery query=new XQuery("for $x in 65 to 75 return string-length(codepoints-to-string($x to $x+10))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"11 11 11 11 11 11 11 11 11 11 11"));
}
