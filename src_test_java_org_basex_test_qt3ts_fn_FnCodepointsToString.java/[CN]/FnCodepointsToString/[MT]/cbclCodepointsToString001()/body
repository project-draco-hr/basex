{
  final XQuery query=new XQuery("\n" + "        declare function local:test($test as xs:integer) as xs:integer? { \n" + "          if ($test = 1) then ( 0 ) else if ($test = 2) then ( 9 ) else if ($test = 3) then ( 13 ) else if ($test = 4) then ( 16 ) else () \n"+ "        }; \n"+ "        fn:codepoints-to-string( local:test(1) to 32 )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOCH0001"));
}
