{
  final XQuery query=new XQuery("fn:codepoints-to-string(xs:integer(fn:avg((65,32,83,116,114,105,110,103))))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"["));
}
