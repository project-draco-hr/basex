{
  final XQuery query=new XQuery("xs:string(fn:codepoints-to-string((65,32,83,116,114,105,110,103)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"A String"));
}
