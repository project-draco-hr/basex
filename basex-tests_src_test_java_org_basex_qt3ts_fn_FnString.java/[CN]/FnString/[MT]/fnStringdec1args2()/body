{
  final XQuery query=new XQuery("fn:string(xs:decimal(\"617375191608514839\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"617375191608514839"));
}
