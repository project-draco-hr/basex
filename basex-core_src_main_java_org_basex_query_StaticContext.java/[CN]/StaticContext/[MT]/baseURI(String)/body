{
  final IO base=IO.get(uri);
  final String url;
  if (uri.isEmpty()) {
    url="";
  }
 else   if (base instanceof IOContent) {
    url=uri;
  }
 else   if (baseURI == Uri.EMPTY) {
    url=base.url();
  }
 else {
    url=baseIO().merge(uri).url();
  }
  baseURI=Uri.uri(uri.endsWith(".") || uri.endsWith("/") ? url + '/' : url);
}
