{
  final IO base=IO.get(uri);
  String url;
  if (uri.isEmpty()) {
    url="";
  }
 else   if (base instanceof IOContent) {
    url=uri;
  }
 else   if (baseURI == Uri.EMPTY) {
    url=base.url();
  }
 else {
    url=baseIO().merge(uri).url();
  }
  if (!url.endsWith("/") && (uri.endsWith(".") || uri.endsWith("/")))   url+='/';
  baseURI=Uri.uri(url);
}
