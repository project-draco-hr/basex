{
  final byte[] str=checkStr(arg[0]);
  Norm norm=null;
  if (arg.length == 2) {
    final byte[] n=uc(trim(checkStr(arg[1])));
    for (    final Norm f : Norm.values())     if (eq(f.name,n))     norm=f;
    if (norm == null)     Err.or(NORMUNI,n);
  }
 else {
    norm=Norm.C;
  }
  return Str.iter(str);
}
