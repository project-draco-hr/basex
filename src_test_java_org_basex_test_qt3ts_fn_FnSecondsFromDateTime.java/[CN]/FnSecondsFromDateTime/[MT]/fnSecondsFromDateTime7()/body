{
  final XQuery query=new XQuery("fn:seconds-from-dateTime(xs:dateTime(\"1999-05-31T23:59:59-05:00\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"59"));
}
