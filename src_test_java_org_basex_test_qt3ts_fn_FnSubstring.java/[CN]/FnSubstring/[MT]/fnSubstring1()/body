{
  final XQuery query=new XQuery("fn:substring(\"motor car\", 6)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false," car") && assertType("xs:string")));
}
