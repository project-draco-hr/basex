{
  final XQuery query=new XQuery("for $h in (/works[1]/employee[1]) return fn:count(($h/self::employee[1000]))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
