{
  final int tl=token.length;
  boolean f=false;
  for (  final int t : token) {
    if (t >= 0 && t <= ' ' || digit(t))     continue;
    f=t == 'e' || t == 'E' || t == '.' || t == '-' || t == '+';
    if (!f)     return Double.NaN;
  }
  if (f || tl > 9)   return dbl(token);
  final int d=toInt(token);
  return d == Integer.MIN_VALUE ? Double.NaN : d;
}
