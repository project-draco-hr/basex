{
  final XQuery query=new XQuery("fn:string((xs:dayTimeDuration(\"P10DT10H11M\")) div 2.0) and fn:false()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
