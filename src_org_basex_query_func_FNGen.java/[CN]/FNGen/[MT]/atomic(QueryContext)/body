{
  final Iter iter=ctx.iter(args[0]);
switch (func) {
case DOC:
    Item it=iter.next();
  return it == null ? null : it.type == Type.DOC ? it : ctx.doc(checkStr(it),false);
case DOCAVAIL:
it=iter.next();
if (it != null) {
final byte[] file=checkStr(it);
try {
  ctx.doc(file,false);
  return Bln.TRUE;
}
 catch (final QueryException e) {
}
}
return Bln.FALSE;
default :
return super.atomic(ctx);
}
}
