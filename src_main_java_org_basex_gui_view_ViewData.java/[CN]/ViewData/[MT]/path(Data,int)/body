{
  if (data == null)   return Token.EMPTY;
  int p=pre;
  int k=data.kind(p);
  final IntList il=new IntList();
  while (k != Data.DOC) {
    il.add(p);
    p=data.parent(p,k);
    k=data.kind(p);
  }
  final TokenBuilder tb=new TokenBuilder();
  tb.add("doc(\"");
  tb.add(content(data,p,true));
  tb.add("\")");
  for (int i=il.size() - 1; i >= 0; i--) {
    tb.add('/');
    tb.add(content(data,il.get(i),true));
  }
  return tb.finish();
}
