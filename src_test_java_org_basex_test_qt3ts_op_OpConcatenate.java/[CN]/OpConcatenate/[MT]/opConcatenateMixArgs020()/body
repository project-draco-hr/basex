{
  final XQuery query=new XQuery("//book/price, //book/title",ctx);
  query.context(node(file("docs/bib.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<price>65.95</price><price>65.95</price><price>39.95</price><price>129.95</price><title>TCP/IP Illustrated</title><title>Advanced Programming in the Unix environment</title><title>Data on the Web</title><title>The Economics of Technology and Content for Digital TV</title>",false));
}
