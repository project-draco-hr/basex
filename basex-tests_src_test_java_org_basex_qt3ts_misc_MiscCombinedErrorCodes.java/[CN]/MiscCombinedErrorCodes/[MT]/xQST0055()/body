{
  final XQuery query=new XQuery("declare copy-namespaces preserve,inherit; declare copy-namespaces preserve,no-inherit; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0055"));
}
