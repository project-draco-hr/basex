{
  final XQuery query=new XQuery("xs:integer(xs:double(1e308))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOCA0003") || (assertType("xs:integer") && assertQuery("string-length(string($result)) = (308, 309)") && assertQuery("starts-with(string($result), '1000000000') or starts-with(string($result), '999999999')"))));
}
