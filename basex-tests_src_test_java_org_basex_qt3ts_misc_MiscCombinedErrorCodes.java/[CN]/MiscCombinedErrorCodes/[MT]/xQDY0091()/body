{
  final XQuery query=new XQuery("<e xml:id=\" ab c d \"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQDY0091") || assertSerialization("<e xml:id=\"ab c d\"/>",false)));
}
