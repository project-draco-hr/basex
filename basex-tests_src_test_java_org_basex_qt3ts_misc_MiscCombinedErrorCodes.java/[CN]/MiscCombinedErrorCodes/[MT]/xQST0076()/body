{
  final XQuery query=new XQuery("for $x in ('a', 'b', 'c') order by $x collation 'http://www.w3.org/' return $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0076"));
}
