{
  final XQuery query=new XQuery("import schema namespace foo = \"http://www.example.org/\" at \"XQST0059.xsd\"; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0059"));
}
