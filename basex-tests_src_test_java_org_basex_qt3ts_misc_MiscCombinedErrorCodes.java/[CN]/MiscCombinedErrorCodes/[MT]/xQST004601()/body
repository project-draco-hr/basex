{
  final XQuery query=new XQuery("base-uri(<a xml:base=\"%gg\" />)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQST0046") || error("FORG0001") || assertStringValue(false,"%gg")));
}
