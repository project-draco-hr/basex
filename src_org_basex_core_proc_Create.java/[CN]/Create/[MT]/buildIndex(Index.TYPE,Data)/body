{
switch (i) {
case TXT:
    buildIndex(i,new ValueBuilder(true),d,Prop.allInfo ? CREATETEXT : null);
  break;
case ATV:
buildIndex(i,new ValueBuilder(false),d,Prop.allInfo ? CREATEATTR : null);
break;
case WRD:
buildIndex(i,new WordBuilder(),d,Prop.allInfo ? CREATEWORD : null);
break;
case FUY:
buildIndex(i,new FuzzyBuilder(),d,Prop.allInfo ? CREATEFT : null);
break;
case FTX:
buildIndex(i,new FTBuilder(),d,Prop.allInfo ? CREATEFT : null);
}
}
