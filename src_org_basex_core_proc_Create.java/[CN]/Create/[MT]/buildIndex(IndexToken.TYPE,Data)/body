{
switch (i) {
case TXT:
    buildIndex(i,new ValueBuilder(true),d,Prop.allInfo ? CREATETEXT : null);
  break;
case ATV:
buildIndex(i,new ValueBuilder(false),d,Prop.allInfo ? CREATEATTR : null);
break;
case FTX:
buildIndex(i,d.meta.ftfuzzy ? new FZBuilder() : new FTBuilder(),d,Prop.allInfo ? CREATEFT : null);
break;
default :
break;
}
}
