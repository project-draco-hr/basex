{
  final XQuery query=new XQuery("declare variable $x := (xs:string(\"a\") , xs:anyURI(\"www.example.com\")) ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"a www.example.com"));
}
