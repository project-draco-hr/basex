{
  final XQuery query=new XQuery("declare variable $x := 'This is a \"String\"' ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"This is a \"String\""));
}
