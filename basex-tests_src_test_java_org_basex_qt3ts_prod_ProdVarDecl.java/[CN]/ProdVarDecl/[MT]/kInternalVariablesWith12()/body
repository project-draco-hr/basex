{
  final XQuery query=new XQuery("declare variable $var1 := local:myFunc(); declare function local:myFunc() { 1 }; $var1 eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
