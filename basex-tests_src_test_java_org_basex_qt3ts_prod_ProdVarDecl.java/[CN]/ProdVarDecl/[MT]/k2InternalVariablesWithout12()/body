{
  final XQuery query=new XQuery("declare variable $local:myVar := local:thisFunctionDoesNotExist(); $local:myVar",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0017"));
}
