{
  kind=Kind.values()[in.readNum()];
  if (kind == Kind.INT || kind == Kind.DBL) {
    min=toDouble(in.readBytes());
    max=toDouble(in.readBytes());
  }
 else   if (kind == Kind.CAT) {
    cats=new Set();
    final int cl=in.readNum();
    for (int i=0; i < cl; i++)     cats.add(in.readBytes());
  }
}
