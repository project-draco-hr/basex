{
  final XQuery query=new XQuery("\n" + "         let $var := xs:integer(\"123\") \n" + "         return fn:trace($var,\"The Value of $var is: \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"123"));
}
