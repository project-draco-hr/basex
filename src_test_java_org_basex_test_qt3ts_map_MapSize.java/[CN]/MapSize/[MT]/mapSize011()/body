{
  final XQuery query=new XQuery("map:size(map:remove(map:remove(map{\"a\":=1,\"b\":=2},\"b\"),\"a\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") && assertType("xs:integer")));
}
