{
  final XQuery query=new XQuery("map:size(map:new((map:entry(\"a\", \"1\"), map:entry(\"b\", 2))))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("2") && assertType("xs:integer")));
}
