{
  final XQuery query=new XQuery("declare namespace prefix = \"http://example.com/NotRecognized\";\n" + "(::)1(::)eq(::)(#prefix:name ##cont## # # ( \"# ) #\n" + "\t\t)# )#ent #)(::){(::)1(::)}(::)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
