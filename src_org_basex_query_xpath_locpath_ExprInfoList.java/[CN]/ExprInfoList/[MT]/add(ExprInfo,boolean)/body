{
  if (ei instanceof PredSimple) {
    final PredSimple p=(PredSimple)ei;
    if (p.expr instanceof FTContains) {
      final FTContains ftc1=(FTContains)p.expr;
      for (int i=0; i < size; i++) {
        final Expr ftc=FTContains.sumUp(ftc1,(FTContains)((PredSimple)list[i]).expr,and);
        if (ftc != null) {
          ((PredSimple)list[i]).expr=ftc;
          return i;
        }
      }
      if (size == list.length)       list=Array.extend(list);
      list[size++]=ei;
      return size - 1;
    }
  }
 else   if (ei instanceof FTContains) {
    final FTContains ftc1=(FTContains)ei;
    for (int i=0; i < size; i++) {
      final Expr ftc=FTContains.sumUp(ftc1,(FTContains)list[i],and);
      if (ftc != null) {
        list[i]=ftc;
        return i;
      }
    }
    if (size == list.length)     list=Array.extend(list);
    list[size++]=ei;
    return size - 1;
  }
  return -1;
}
