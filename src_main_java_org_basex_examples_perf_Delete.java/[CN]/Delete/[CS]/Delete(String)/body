{
  if (!init(args))   return;
  update("delete node /*");
  update("delete node //node()");
  update("delete node //*");
  update("delete node //text()");
  final String qu=query("count(//text())");
  final int n=Math.min(500,Integer.parseInt(qu.trim()));
  update(n,"delete node (//text())[1]");
}
