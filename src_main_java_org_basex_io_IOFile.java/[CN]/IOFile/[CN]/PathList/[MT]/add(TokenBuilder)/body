{
  String s=tb.toString();
  if (s.length() > 1 && s.charAt(1) == ':' && size == 0) {
    s=Character.toUpperCase(s.charAt(0)) + s.substring(1);
  }
  if (s.equals("..") && size > 0) {
    if (list[size - 1].indexOf(':') == -1)     delete(size - 1);
  }
 else   if (!s.equals(".") && !s.isEmpty()) {
    add(s);
  }
  tb.reset();
}
