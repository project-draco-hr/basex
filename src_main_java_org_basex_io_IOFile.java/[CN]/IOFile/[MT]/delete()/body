{
  boolean ok=true;
  if (file.exists()) {
    if (isDir())     for (    final IOFile ch : children())     ok&=ch.delete();
    for (int i=0; i < 5; i++) {
      if (file.delete() && !file.exists())       return ok;
      Performance.sleep(i * 10);
      if (i != 0)       System.out.println("DELETE: " + i);
    }
  }
  return false;
}
