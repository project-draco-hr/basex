{
  boolean ok=true;
  if (file.exists()) {
    if (isDir())     for (    final IOFile ch : children())     ok&=ch.delete();
    for (int i=0; i < 3; i++) {
      if (file.delete() && !file.exists()) {
        if (i != 0)         System.out.println(i);
        return ok;
      }
      Performance.sleep(i * 10);
    }
  }
  return false;
}
