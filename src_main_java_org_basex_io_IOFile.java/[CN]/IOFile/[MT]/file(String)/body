{
  String file=url;
  if (file.indexOf("%") != -1) {
    try {
      file=URLDecoder.decode(file,Prop.ENCODING);
    }
 catch (    final Exception ex) {
    }
  }
  final String fn=file.startsWith(PREFILE) ? file.substring(PREFILE.length()) : file;
  return fn.length() > 2 && fn.charAt(0) == '/' && fn.charAt(2) == ':' ? fn.substring(1) : fn;
}
