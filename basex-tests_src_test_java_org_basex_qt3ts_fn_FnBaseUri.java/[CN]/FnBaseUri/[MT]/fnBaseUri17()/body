{
  final XQuery query=new XQuery("\n" + "         declare base-uri \"http://example.org\"; \n" + "         fn:base-uri(processing-instruction {\"PItarget\"} {\"PIcontent\"})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
