{
  final XQuery query=new XQuery("declare base-uri \"http://example.com/baseURI\"; empty(base-uri(<?target data?>))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
