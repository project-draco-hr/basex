{
  final XQuery query=new XQuery("declare base-uri \"http://example.org\"; \n" + "            fn:string(fn:base-uri(document {<aDocument>some content</aDocument>}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://example.org"));
}
