{
  final byte[] type=types.get(lvl - 1);
  if (eq(type,STRING)) {
    print('"');
    for (    final byte ch : value)     encode(ch);
    print('"');
  }
 else   if (eq(type,BOOLEAN)) {
    if (!eq(value,TRUE,FALSE))     throw error("Value of <%> is no boolean: \"%\"",elems.get(lvl - 1),value);
    print(value);
  }
 else   if (eq(type,NUMBER)) {
    if (Double.isNaN(toDouble(value)))     throw error("Value of <%> is no number: \"%\"",elems.get(lvl - 1),value);
    print(value);
  }
 else   if (trim(value).length != 0) {
    throw error("<%> is typed as \"%\" and cannot have a value",elems.get(lvl - 1),type);
  }
}
