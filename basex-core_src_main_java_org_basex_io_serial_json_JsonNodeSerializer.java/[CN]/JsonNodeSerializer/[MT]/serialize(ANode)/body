{
  final boolean doc=node.type == NodeType.DOC;
  final boolean elm=node.type == NodeType.ELM && eq(JSON,node.name());
  if (custom || doc || elm) {
    final boolean c=custom;
    if (!custom)     custom=elm;
    super.serialize(node);
    custom=c;
  }
 else {
    ser.serialize(node);
    ser.close();
    ser.reset();
    string(cache.toArray());
    cache.reset();
  }
}
