{
  if (docs == null || !data.meta.docindex) {
    docs=null;
    order=null;
    paths=null;
    final IntList il=new IntList();
    final int is=data.meta.size;
    for (int i=0; i < is; i+=data.size(i,Data.DOC))     il.add(i);
    docs=il.toArray();
    data.meta.docindex=true;
    data.meta.dirty=true;
  }
  return docs;
}
