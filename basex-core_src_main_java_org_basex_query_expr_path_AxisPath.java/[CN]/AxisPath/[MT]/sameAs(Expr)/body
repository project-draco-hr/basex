{
  if (!(cmp instanceof AxisPath))   return false;
  final AxisPath ap=(AxisPath)cmp;
  if ((root == null || ap.root == null) && root != ap.root || steps.length != ap.steps.length || root != null && !root.sameAs(ap.root))   return false;
  final int sl=steps.length;
  for (int s=0; s < sl; s++) {
    if (!steps[s].sameAs(ap.steps[s]))     return false;
  }
  return true;
}
