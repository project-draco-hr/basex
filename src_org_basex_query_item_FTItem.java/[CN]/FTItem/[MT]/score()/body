{
  if (score == -1) {
    if (ids) {
      Iterator<FTStringMatch> i=all.match[0].iterator();
      if (i.hasNext()) {
        score=i.next().e / 1000d;
      }
 else {
        score=Math.max((double)all.size / (double)is,Math.log(tl * all.size + 1) / Math.log(data.textLen(pre) + 1));
      }
    }
 else {
      score=Math.max((double)all.size / (double)is,Math.log(tl * all.size + 1) / Math.log(data.textLen(pre) + 1));
    }
  }
  return score;
}
