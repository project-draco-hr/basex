{
  if (cats != null && cats.size() == 0) {
    kind=Kind.NONE;
    leaf=false;
  }
  out.writeNum(kind.ordinal());
  if (kind == Kind.INT || kind == Kind.DBL) {
    out.writeDouble(min);
    out.writeDouble(max);
  }
 else   if (kind == Kind.CAT) {
    final int cl=cats.size();
    out.writeNum(cl);
    for (int i=1; i <= cl; ++i)     out.writeToken(cats.key(i));
  }
  out.writeNum(counter);
  out.writeBool(leaf);
  out.writeDouble(len);
}
