{
  final FTMatches all=new FTMatches(m.sTokenNum);
  if (i == m.size) {
    all.add(new FTMatch());
  }
 else {
    for (    final FTStringMatch s : m.match[i]) {
      s.ex^=true;
      for (      final FTMatch tmp : not(m,i + 1)) {
        all.add(new FTMatch().add(s).add(tmp));
      }
    }
  }
  return all;
}
