{
  final XQuery query=new XQuery("fn:tokenize(\"The cat sat on the mat\", \"\\s+\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"The cat sat on the mat"));
}
