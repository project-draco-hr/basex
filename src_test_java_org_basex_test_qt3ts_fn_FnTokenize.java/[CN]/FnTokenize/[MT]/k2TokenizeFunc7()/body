{
  final XQuery query=new XQuery("count(tokenize(\"a b\", \" \")), count(tokenize(\"a b\", \"\\s\")), string-join(tokenize(\"a b\", \" \"), '|'), string-join(tokenize(\"a b\", \"\\s\"), '|'), tokenize(\"a b\", \" \"), tokenize(\"a b\", \"\\s\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"2 2 a|b a|b a b a b"));
}
