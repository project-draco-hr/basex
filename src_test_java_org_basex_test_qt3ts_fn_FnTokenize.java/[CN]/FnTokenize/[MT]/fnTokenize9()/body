{
  final XQuery query=new XQuery("string-join(fn:tokenize(\"abracadabra\", \"(ab)|(a)\"), '#')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"#r#c#d#r#"));
}
