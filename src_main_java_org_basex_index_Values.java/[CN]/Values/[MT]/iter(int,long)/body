{
  return new IndexIterator(){
    /** 
 * Last index position. 
 */
    long p=ps;
    /** 
 * Current position. 
 */
    int c=-1;
    /** 
 * Last pre value. 
 */
    int v;
    @Override public boolean more(){
      return ++c < s;
    }
    @Override public int next(){
      v+=idxl.readNum(p);
      p=idxl.pos();
      return v;
    }
    @Override public double score(){
      return -1;
    }
  }
;
}
