{
  final XQuery query=new XQuery("text{ \"He said, \"\"I don't like it.\"\"\" }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"He said, \"I don't like it.\""));
}
