{
  final Type ip=it.type;
  final Item i=ip.isUntyped() ? Dbl.get(n) : it;
  if (n == d)   return i;
  if (ip instanceof AtomType) {
switch ((AtomType)ip) {
case DEC:
      return Dec.get(d);
case DBL:
    return Dbl.get(d);
case FLT:
  return Flt.get((float)d);
case ITR:
return Int.get((long)d);
default :
break;
}
}
return Dbl.get(d);
}
