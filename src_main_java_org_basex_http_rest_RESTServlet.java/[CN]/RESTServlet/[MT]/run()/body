{
  final RESTCode code;
switch (http.method) {
case DELETE:
    code=new RESTDelete();
  break;
case GET:
code=new RESTGet();
break;
case POST:
code=new RESTPost();
break;
case PUT:
code=new RESTPut();
break;
default :
throw new HTTPException(SC_NOT_IMPLEMENTED,http.method.toString());
}
code.run(http);
}
