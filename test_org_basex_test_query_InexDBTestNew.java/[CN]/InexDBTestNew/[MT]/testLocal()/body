{
  final Nodes[] roots=new Nodes[databases.size()];
  for (int d=0; d < databases.size(); d++) {
    final Data data=Open.open(ctx,databases.get(d));
    roots[d]=new Nodes(data.doc(),data);
  }
  for (int q=0; q < queries.size(); q++) {
    for (int d=0; d < databases.size(); d++) {
      ctx.current(roots[d]);
      query(d,q);
    }
  }
}
