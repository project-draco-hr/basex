{
  try {
    final DataAccess in=new DataAccess(new IOFile(fn + ".dat"));
    while (true) {
      final byte[] key=find(in,(byte)'\t');
      final byte[] val=find(in,(byte)'\n');
      if (key == null)       break;
      hash.put(key,val);
    }
  }
 catch (  final IOException ex) {
    Util.debug(ex);
  }
}
