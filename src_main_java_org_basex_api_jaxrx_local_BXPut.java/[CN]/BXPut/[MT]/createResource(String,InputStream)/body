{
  final Context ctx=new Context();
  final SAXSource source=new SAXSource(new InputSource(in));
  final Parser parser=new SAXWrapper(source,ctx.prop);
  try {
    CreateDB.xml(ctx,parser,resource);
  }
 catch (  final IOException ex) {
    BXUtil.error(ex);
  }
 finally {
    ctx.close();
  }
  return true;
}
