{
switch (def) {
case MAPNEW:
    return newMap(ctx,ii);
case ENTRY:
  return entry(ctx,ii);
case MAPCONT:
return contains(ctx,ii);
case MAPSIZE:
return getMap(0,ctx,ii).mapSize();
case MAPREM:
return remove(ctx,ii);
case MAPCOLL:
return getMap(0,ctx,ii).collation();
default :
return super.item(ctx,ii);
}
}
