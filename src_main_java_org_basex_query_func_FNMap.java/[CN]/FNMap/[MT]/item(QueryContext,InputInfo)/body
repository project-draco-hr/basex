{
switch (def) {
case MAPNEW:
    return newMap(ctx,ii);
case MAPENTRY:
  return entry(ctx,ii);
case MAPCONT:
return contains(ctx,ii);
case MAPSIZE:
return getMap(ctx).mapSize();
case MAPREM:
return remove(ctx,ii);
case MAPCOLL:
return getMap(ctx).collation();
default :
return super.item(ctx,ii);
}
}
