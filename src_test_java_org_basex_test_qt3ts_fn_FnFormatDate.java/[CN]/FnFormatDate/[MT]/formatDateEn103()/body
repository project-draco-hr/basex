{
  final XQuery query=new XQuery("\n" + "        for $i in 1 to 12 \n" + "        return let $d2 := $d + xs:yearMonthDuration('P1M')*$i \n"+ "        return format-date($d2, '[MNn]', 'en', (), ()) ",ctx);
  query.bind("d",new XQuery("xs:date('2003-12-07')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"\n            January February March April May June July August September October November December\n         "));
}
