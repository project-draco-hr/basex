{
  final XQuery query=new XQuery("for $i in 0 to 30 return let $d2 := $d + xs:dayTimeDuration('P1D')*$i return\n" + "         format-date($d2, '[D1o]', 'en', (), ()) ",ctx);
  query.bind("d",new XQuery("xs:date('2003-12-01')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"\n            1st 2nd 3rd 4th 5th 6th 7th 8th 9th 10th 11th 12th 13th 14th 15th\n            16th 17th 18th 19th 20th 21st 22nd 23rd 24th 25th 26th 27th 28th 29th 30th\n            31st\n         "));
}
