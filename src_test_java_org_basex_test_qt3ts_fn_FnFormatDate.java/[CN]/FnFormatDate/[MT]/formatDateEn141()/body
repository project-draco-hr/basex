{
  final XQuery query=new XQuery(" format-date($d1, '[Y][EN]', 'en', (), ()), format-date($d2, '[Y][EN]', 'en', (), ()) ",ctx);
  query.bind("d1",new XQuery("xs:date('1990-12-01')",ctx).value());
  query.bind("d2",new XQuery("xs:date('-0055-12-01')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test((assertDeepEq("\"1990AD\", \"55BC\"") || assertDeepEq("\"1990CE\", \"55BCE\"")));
}
