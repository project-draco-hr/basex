{
  final XQuery query=new XQuery("for $i in 0 to 30 return let $d2 := $d + xs:yearMonthDuration('P1Y')*$i return\n" + "         format-date($d2, '[Y1o]', 'en', (), ()) ",ctx);
  query.bind("d",new XQuery("xs:date('1990-12-01')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"\n            1990th 1991st 1992nd 1993rd 1994th 1995th 1996th 1997th 1998th 1999th\n            2000th 2001st 2002nd 2003rd 2004th 2005th 2006th 2007th 2008th 2009th 2010th 2011th\n            2012th 2013th 2014th 2015th 2016th 2017th 2018th 2019th 2020th\n         "));
}
