{
  final XQuery query=new XQuery("\n" + "        for $i in 1 to 12 \n" + "        return let $d2 := $d + xs:yearMonthDuration('P1M')*$i \n"+ "        return format-date($d2, '[Mn,3-3]', 'en', (), ()) \n"+ "      ",ctx);
  query.bind("d",new XQuery("xs:date('2003-12-07')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"jan feb mar apr may jun jul aug sep oct nov dec"));
}
