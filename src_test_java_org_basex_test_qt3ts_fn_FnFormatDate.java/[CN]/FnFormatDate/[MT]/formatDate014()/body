{
  final XQuery query=new XQuery("string-join( for $z in -28 to +28 return format-date(adjust-date-to-timezone($t,\n" + "         $z*xs:dayTimeDuration('PT30M')), '[Z]'), ' ')",ctx);
  query.bind("t",new XQuery("xs:date('1985-03-01Z')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"-14:00 -13:30 -13:00 -12:30 -12:00 -11:30 -11:00 -10:30 -10:00\n            -09:30 -09:00 -08:30 -08:00 -07:30 -07:00 -06:30 -06:00 -05:30 -05:00 -04:30\n            -04:00 -03:30 -03:00 -02:30 -02:00 -01:30 -01:00 -00:30 Z +00:30 +01:00\n            +01:30 +02:00 +02:30 +03:00 +03:30 +04:00 +04:30 +05:00 +05:30 +06:00 +06:30\n            +07:00 +07:30 +08:00 +08:30 +09:00 +09:30 +10:00 +10:30 +11:00 +11:30 +12:00\n            +12:30 +13:00 +13:30 +14:00"));
}
