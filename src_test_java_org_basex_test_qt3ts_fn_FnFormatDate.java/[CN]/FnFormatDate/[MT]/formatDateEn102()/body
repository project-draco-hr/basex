{
  final XQuery query=new XQuery("for $i in 1 to 12 return let $d2 := $d + xs:yearMonthDuration('P1M')*$i return\n" + "         format-date($d2, '[Mn]', 'en', (), ()) ",ctx);
  try {
    query.bind("d",new XQuery("xs:date('2003-12-07')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n            january february march april may june july august september october november december\n         "));
}
