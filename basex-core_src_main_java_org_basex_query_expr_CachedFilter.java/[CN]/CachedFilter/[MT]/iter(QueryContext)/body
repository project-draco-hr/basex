{
  Value val=root.value(qc);
  final Value cv=qc.value;
  final long cs=qc.size;
  final long cp=qc.pos;
  try {
    final ValueBuilder vb=new ValueBuilder();
    Expr pred=preds[0];
    long vs=val.size();
    qc.size=vs;
    qc.pos=1;
    final boolean scoring=qc.scoring;
    for (int s=0; s < vs; s++) {
      final Item item=val.itemAt(s);
      qc.value=item;
      final Item test=pred.test(qc,info);
      if (test != null) {
        if (scoring)         item.score(test.score());
        vb.add(item);
      }
      qc.pos++;
    }
    val=null;
    final int pl=preds.length;
    for (int i=1; i < pl; i++) {
      vs=vb.size();
      pred=preds[i];
      qc.size=vs;
      qc.pos=1;
      int c=0;
      for (int s=0; s < vs; ++s) {
        final Item item=vb.get(s);
        qc.value=item;
        final Item test=pred.test(qc,info);
        if (test != null) {
          if (scoring)           item.score(test.score());
          vb.set(c++,item);
        }
        qc.pos++;
      }
      vb.size(c);
    }
    return vb;
  }
  finally {
    qc.value=cv;
    qc.size=cs;
    qc.pos=cp;
  }
}
