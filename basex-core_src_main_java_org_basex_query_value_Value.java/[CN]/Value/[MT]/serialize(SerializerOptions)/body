{
  final ArrayOutput ao=new ArrayOutput();
  try {
    try (final Serializer ser=Serializer.get(ao,options)){
      for (      Item it : this)       ser.serialize(it);
    }
   }
 catch (  final QueryIOException ex) {
    throw ex;
  }
catch (  final IOException ex) {
    throw SER_X.getIO(ex);
  }
  return ao;
}
