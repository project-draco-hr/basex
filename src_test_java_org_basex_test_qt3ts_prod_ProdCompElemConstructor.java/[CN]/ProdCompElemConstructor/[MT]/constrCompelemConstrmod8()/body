{
  final XQuery query=new XQuery("declare construction preserve; (element elem {xs:decimal(//*:decimal[1]/@*:attr)}) cast as xs:integer",ctx);
  query.context(node(file("docs/atomicns.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertEq("12678967") || error("FORG0001") || error("XPTY0004")));
}
