{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http://example.com/NS\"; \n" + "        element {exactly-one((//*)[3])} {}",ctx);
  query.context(node(file("op/union/acme_corp.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<Boston xmlns=\"http://example.com/NS\"/>",false));
}
