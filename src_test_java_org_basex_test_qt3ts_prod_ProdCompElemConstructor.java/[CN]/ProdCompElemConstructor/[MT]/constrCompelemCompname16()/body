{
  final XQuery query=new XQuery("<elem xmlns:foo=\"http://www.example.com/foo\">{element {xs:untypedAtomic('foo:elem')} {'text'}}</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem xmlns:foo=\"http://www.example.com/foo\"><foo:elem>text</foo:elem></elem>",false));
}
