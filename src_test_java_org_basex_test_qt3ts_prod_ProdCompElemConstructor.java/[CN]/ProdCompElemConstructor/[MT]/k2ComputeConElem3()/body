{
  final XQuery query=new XQuery("<elem>{<e/>, <b></b>, \"\", \"\"}</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem><e/><b/> </elem>",false));
}
