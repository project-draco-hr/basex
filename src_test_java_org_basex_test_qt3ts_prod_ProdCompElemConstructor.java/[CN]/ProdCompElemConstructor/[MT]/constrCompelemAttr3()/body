{
  final XQuery query=new XQuery("element elem {//west/@mark, //west/@west-attr-1}",ctx);
  try {
    query.context(node(file("prod/AxisStep/TopMany.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem mark=\"w0\" west-attr-1=\"w1\"/>",false));
}
