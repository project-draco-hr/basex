{
  final XQuery query=new XQuery("declare namespace foo=\"http://www.example.com/foo\"; element foo:elem {'text'}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<foo:elem xmlns:foo=\"http://www.example.com/foo\">text</foo:elem>",false));
}
