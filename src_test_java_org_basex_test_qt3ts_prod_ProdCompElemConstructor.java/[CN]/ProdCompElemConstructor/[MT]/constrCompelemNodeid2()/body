{
  final XQuery query=new XQuery("for $x in <a b=\"b\"/>, $y in element elem {$x/@b} return $y/@b is $x/@b",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
