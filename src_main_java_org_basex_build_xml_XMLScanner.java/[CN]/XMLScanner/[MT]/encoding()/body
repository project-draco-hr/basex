{
  if (!consume(ENCOD))   return null;
  s();
  check('=');
  s();
  final TokenBuilder enc=new TokenBuilder();
  final int d=qu();
  int ch=nextChar();
  if (letter(ch) && ch != '_') {
    while (letterOrDigit(ch) || ch == '.' || ch == '-') {
      enc.add(ch);
      ch=nextChar();
    }
    prev(1);
  }
  check((char)d);
  if (enc.isEmpty())   error(DECLENCODE,enc);
  final String e=string(enc.finish());
  input.encoding(e);
  return e;
}
