{
  opened();
  if (tar == null)   return e;
  final Type t=((BXQItemType)tar).getType();
  if (e == t)   return e;
  boolean valid=false;
switch (e) {
case BYT:
case INT:
case LNG:
case SHR:
case DEC:
case ITR:
    valid=t.num && t != Type.DBL && t != Type.FLT;
  break;
case STR:
valid=t.str;
break;
case ATM:
valid=true;
break;
default :
break;
}
if (!valid) throw new BXQException(WRONG,tar,e);
return t;
}
