{
  final XQuery query=new XQuery("let $f := function($a, $b){($b, $a)} return fold-right($f, (), 1 to 5)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("5,4,3,2,1"));
}
