{
  final XQuery query=new XQuery("fold-right(fn:concat(\"$f(\", ?, \", \", ?, \")\"), \"$zero\", 1 to 5)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"$f(1, $f(2, $f(3, $f(4, $f(5, $zero)))))"));
}
