{
  final XQuery query=new XQuery("abs(xs:short(.)) instance of xs:short",ctx);
  try {
    query.context(node(file("fn/abs/e0.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || assertBoolean(true)));
}
