{
  final XQuery query=new XQuery("if (implicit-timezone() eq xs:dayTimeDuration('PT9M'))\n" + "            then xs:gDay(\"---31+09:01\") ne xs:gDay(\"---30\")\n" + "            else xs:gDay(\"---31+09:00\") ne xs:gDay(\"---30\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
