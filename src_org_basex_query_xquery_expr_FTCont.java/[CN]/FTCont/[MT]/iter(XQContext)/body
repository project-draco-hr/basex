{
  final Iter iter=ctx.iter(expr[0]);
  final FTTokenizer tmp=ctx.ftitem;
  double d=0;
  Item i;
  ctx.ftitem=ft;
  while ((i=iter.next()) != null) {
    ft.init(i.str());
    final Item it=ctx.iter(expr[1]).next();
    d=Scoring.and(d,it.dbl());
  }
  ctx.ftitem=tmp;
  return d == 0 ? Iter.EMPTY : new Bln(true,d).iter();
}
