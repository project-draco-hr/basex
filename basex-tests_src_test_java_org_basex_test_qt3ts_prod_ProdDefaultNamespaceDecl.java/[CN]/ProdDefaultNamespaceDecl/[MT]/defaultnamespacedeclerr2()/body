{
  final XQuery query=new XQuery("\n" + "        declare default function namespace \"http://example.org/names\"; \n" + "        declare default function namespace \"http://someexample.org/names\"; \n"+ "        \"abc\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0066"));
}
