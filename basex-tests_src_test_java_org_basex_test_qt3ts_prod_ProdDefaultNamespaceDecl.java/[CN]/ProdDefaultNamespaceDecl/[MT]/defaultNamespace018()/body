{
  final XQuery query=new XQuery("\n" + "        declare default function namespace \"http://example.org\"; \n" + "        declare default element namespace \"http://www.example.com/filesystem\"; \n"+ "        declare function udf1 () { <empty> {10*10} </empty> }; \n"+ "        udf1 ()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<empty xmlns=\"http://www.example.com/filesystem\">100</empty>",false));
}
