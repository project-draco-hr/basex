{
  if (isEmpty())   return true;
  final int ns=qnames.size();
  if (qname != null) {
    for (int n=0; n < ns; n+=2) {
      final byte[] ln=qnames.get(n), uri=qnames.get(n + 1);
      if (ln != null && !eq(qname[0],ln))       continue;
      if (uri != null && !eq(qname[1],uri))       continue;
      return true;
    }
  }
  return false;
}
