{
  checkCreate(ctx);
  final RepoManager rm=new RepoManager(ctx.context,ii);
  final String pkg=exprs.length == 0 ? null : Token.string(checkStr(exprs[0],ctx));
switch (func) {
case _REPO_INSTALL:
    rm.install(pkg);
  return null;
case _REPO_DELETE:
rm.delete(pkg);
return null;
default :
return super.item(ctx,ii);
}
}
