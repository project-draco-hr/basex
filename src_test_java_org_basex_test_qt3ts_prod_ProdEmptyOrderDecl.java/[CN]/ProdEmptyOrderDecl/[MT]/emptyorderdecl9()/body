{
  final XQuery query=new XQuery("declare default order empty least;  for $i in (1,4,0 div 0E0,7) return $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 4 NaN 7"));
}
