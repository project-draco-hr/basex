{
  final XQuery query=new XQuery("declare default order empty least;  for $i in (<a>1</a>,<a>4</a>,<a></a>,<a></a>,<a>7</a>) order by zero-or-one($i/text()) ascending return $i",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<a/><a/><a>1</a><a>4</a><a>7</a>",false));
}
