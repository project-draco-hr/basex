{
  final XQuery query=new XQuery("\n" + "        declare namespace p1 = \"http://example.org\"; \n" + "        fn:string(fn:local-name(element p1:anElement{\"Some content\"}))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"anElement"));
}
