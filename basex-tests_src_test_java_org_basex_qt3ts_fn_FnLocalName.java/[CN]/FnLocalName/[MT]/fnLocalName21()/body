{
  final XQuery query=new XQuery("fn:upper-case(fn:string(fn:local-name(attribute anAttribute {\"Some content\"})))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"ANATTRIBUTE"));
}
