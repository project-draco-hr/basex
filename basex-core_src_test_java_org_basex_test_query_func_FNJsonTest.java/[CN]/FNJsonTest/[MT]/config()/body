{
  query("json:parse('[\"foo\",{\"test\":\"asdf\"}]', {'format':'jsonml'})","<foo test=\"asdf\"/>");
  query("map:size(json:parse('[\"foo\",{\"test\":\"asdf\"}]', {'format':'map'}))","2");
  query("json:parse('\"\\t\\u000A\"'," + "  {'format':'map','unescape':false(),'spec':'liberal'})","\\t\\u000A");
  query("string-to-codepoints(json:parse('\"\\t\\u000A\"'," + "  {'format':'map','unescape':true(),'spec':'liberal'}))","9 10");
  error("json:parse('42', {'spec':'garbage'})",Err.BXJS_CONFIG);
}
