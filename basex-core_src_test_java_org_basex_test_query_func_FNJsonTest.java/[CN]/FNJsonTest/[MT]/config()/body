{
  query("json:parse('[\"foo\",{\"test\":\"asdf\"}]', map{'type':='jsonml'})","<foo test=\"asdf\"/>");
  query("map:size(json:parse('[\"foo\",{\"test\":\"asdf\"}]', map{'type':='maps'}))","2");
  query("json:parse('\"\\t\\u000A\"'," + "  map{'type':='maps','unescape':=false(),'spec':='liberal'})","\\t\\u000A");
  query("string-to-codepoints(json:parse('\"\\t\\u000A\"'," + "  map{'type':='maps','unescape':=true(),'spec':='liberal'}))","9 10");
  error("json:parse('42', map{'spec':='garbage'})",Err.BXJS_PARSE_CFG);
}
