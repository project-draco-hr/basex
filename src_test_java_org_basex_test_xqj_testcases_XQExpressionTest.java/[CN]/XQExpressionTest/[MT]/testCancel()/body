{
  XQExpression xqe;
  xqe=xqc.createExpression();
  try {
    xqe.cancel();
  }
 catch (  final XQException e) {
    fail("A-XQE-1.1: cancellation of expression failed with message: " + e.getMessage());
  }
  xqe.close();
  try {
    xqe.cancel();
    fail("A-XQE-1.2: closed expression supports cancel()");
  }
 catch (  final XQException e) {
  }
}
