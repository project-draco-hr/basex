{
  final XQuery query=new XQuery("fn:adjust-time-to-timezone(xs:time(\"10:00:00-05:00\"),())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"10:00:00"));
}
