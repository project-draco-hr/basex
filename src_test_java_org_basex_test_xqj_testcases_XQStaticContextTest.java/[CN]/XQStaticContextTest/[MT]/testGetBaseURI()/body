{
  String uri=null;
  final XQStaticContext xqsc=xqc.getStaticContext();
  try {
    uri=xqsc.getBaseURI();
  }
 catch (  final Exception e) {
    fail("A-XQSC-24.1: getBaseURI() failed with message: " + e.getMessage());
  }
  assertNotNull("A-XQSC-24.1: getBaseURI() returns a non-null URI.",uri);
}
