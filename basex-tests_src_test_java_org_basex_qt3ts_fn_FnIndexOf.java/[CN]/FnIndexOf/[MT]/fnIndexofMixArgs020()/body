{
  final XQuery query=new XQuery("fn:index-of((\"sport\", \"\", \"and\", \"\", \"\", \"pastime\"), \"\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2 4 5"));
}
