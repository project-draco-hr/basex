{
  final Iter iter=ctx.iter(expr[0]);
  Item it=iter.next();
  if (it == null)   return null;
  final TokenBuilder tb=new TokenBuilder();
  boolean more=false;
  do {
    if (more)     tb.add(' ');
    tb.add(it.str());
    more=true;
  }
 while ((it=iter.next()) != null);
  return new FTxt(tb.finish(),null);
}
