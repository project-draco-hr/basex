{
  for (int s=0; s < expr.length - 1; s++) {
    final Step curr=(Step)expr[s];
    if (curr.expr != null && curr.expr.length != 0 || curr.axis != Axis.ANC && curr.axis != Axis.ANCORSELF && curr.axis != Axis.DESC && curr.axis != Axis.SELF && curr.axis != Axis.DESCORSELF && curr.axis != Axis.CHILD && curr.axis != Axis.PARENT)     return false;
  }
  return true;
}
