{
  final Item it=ctx.iter(root).finish();
  if (cache && res != null && item == it && it.type == Type.DOC)   return res.iter();
  item=it;
  final Item c=ctx.item;
  final int cs=ctx.size;
  final int cp=ctx.pos;
  ctx.item=it;
  res=eval(ctx);
  ctx.item=c;
  ctx.size=cs;
  ctx.pos=cp;
  return res.iter();
}
