{
  if (size == 0) {
    ++size;
    add(k,pr);
  }
 else {
    int n=2;
    while (true) {
      int c=n >>> 1;
      final int d=Token.diff(k,t[c]);
      if (d == 0) {
        pre[c]=Num.add(pre[c],pr);
        ns[c]++;
        return;
      }
      c=z[n + (d > 0 ? 1 : 0)];
      if (c == 0) {
        ++size;
        c=size << 1;
        if (c == z.length)         z=Arrays.copyOf(z,c << 1);
        add(k,pr);
        z[n + (d > 0 ? 1 : 0)]=c;
        return;
      }
      n=c;
    }
  }
}
