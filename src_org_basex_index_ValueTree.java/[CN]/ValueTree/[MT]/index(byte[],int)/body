{
  if (root == -1) {
    root=n(tok,pre,-1);
    return;
  }
  int n=root;
  while (true) {
    final int c=Token.diff(tok,tokens.get(n));
    if (c == 0) {
      pres.set(Num.add(pres.get(n),pre),n);
      return;
    }
    int ch=c < 0 ? l(n) : r(n);
    if (ch != -1) {
      n=ch;
    }
 else {
      ch=n(tok,pre,n);
      if (c < 0) {
        l(n,ch);
        a(l(n));
      }
 else {
        r(n,ch);
        a(r(n));
      }
      return;
    }
  }
}
