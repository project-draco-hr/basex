{
  byte quote=next();
  if (quote != '\'' && quote != '"')   error(QUOTEERROR);
  byte c;
  final TokenBuilder tb=new TokenBuilder();
  while ((c=next()) != quote) {
    if (c == 0)     error();
    if (c == '&')     tb.add(getEntity());
 else     if (c != 0x0d)     tb.add(c);
  }
  return tb.finish();
}
