{
  if (consume(SBRACKETO)) {
    BaseX.debug("- Root Element Type: %",root);
    BaseX.debug("- Content:\n %",content);
    consumeContent();
  }
  String file=new File(xmlfile).getParent() + "\\" + string(extid);
  content=IOConstants.read(file);
  extern=true;
  pos=0;
  BaseX.debug("- Root Element Type: %",root);
  BaseX.debug("- Content:\n %",content);
  BaseX.debug("----------------");
  consumeContent();
  extern=false;
}
