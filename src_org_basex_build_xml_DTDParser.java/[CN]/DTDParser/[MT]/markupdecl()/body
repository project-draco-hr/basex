{
  consumeWS();
  if (consume(ELEM)) {
    elementDecl();
    markupdecl();
  }
 else   if (consume(ATTL)) {
    attlistDecl();
    consumeWS();
    if (!consume(GREAT)) {
      error(SIGNERROR);
    }
    markupdecl();
  }
 else   if (consume(ENT)) {
    entityDecl();
    consumeWS();
    if (!consume(GREAT))     error(SIGNERROR);
    markupdecl();
  }
 else   if (consume(NOTA)) {
    notationDecl();
    markupdecl();
  }
 else   if (consume(GQ)) {
    piDecl();
    markupdecl();
  }
 else   if (consume(COMS)) {
    commentDecl();
    markupdecl();
  }
 else {
    consumeWS();
    byte next=next();
    if (percentage(next)) {
      BaseX.debug(consumeSpecName());
      markupdecl();
    }
 else {
      if (XMLToken.isLetter(next)) {
        error(SIGNERROR);
      }
    }
  }
}
