{
  consumeWS();
  if (consume(BRACKETC)) {
    if (consume(STAR)) {
      consumeWS();
      if (!consume(GREAT))       error();
    }
 else {
      consumeWS();
      if (!consume(GREAT))       error();
    }
  }
 else   if (consume(DASH)) {
    consumeWS();
    BaseX.debug(consumeSpecName());
    mixedElement();
  }
 else {
    BaseX.debug(consumeSpecName());
    mixedElement();
  }
}
