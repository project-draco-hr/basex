{
  if (consume(CD)) {
    BaseX.debug(CD);
    dDecl();
  }
 else   if (consume(BRACKETO)) {
    enumeratedAttlist();
    dDecl();
  }
 else   if (consume(NOT)) {
    if (!consumeWS())     error();
    if (!consume(BRACKETO))     error();
    consumeWS();
    BaseX.debug(consumeSpecName());
    notationType();
    dDecl();
  }
 else   if (tokenizedType()) {
    BaseX.debug(tokenizedType);
    dDecl();
  }
 else   error();
}
