{
  if (consume(CD)) {
    BaseX.debug(CD);
    dDecl();
  }
 else   if (consume(BRACKETO)) {
    enumeratedAttlist();
    dDecl();
  }
 else   if (consume(NOT)) {
    if (!consumeWS())     error(WSERROR);
    if (!consume(BRACKETO))     error(SIGNERROR);
    consumeWS();
    BaseX.debug(consumeSpecName());
    enumeratedAttlist();
    dDecl();
  }
 else   if (tokenizedType()) {
    BaseX.debug(tokenizedType);
    dDecl();
  }
 else   error(SIGNERROR);
}
