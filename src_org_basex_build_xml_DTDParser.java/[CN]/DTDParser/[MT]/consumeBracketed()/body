{
  while (!consume(BRACKETC)) {
    consumeWS();
    if (consume(DASH) || consume(COLON)) {
      consumeWS();
      BaseX.debug(consumeName());
      consumeWS();
    }
 else {
      consumeWS();
      BaseX.debug(consumeName());
      consumeWS();
    }
  }
  if (checkQuantity())   BaseX.debug(consumeName());
  if (consumeWS()) {
    if (consume(DASH) || consume(COLON)) {
      consumeWS();
      if (consume(BRACKETO)) {
        consumeBracketed();
      }
    }
  }
}
