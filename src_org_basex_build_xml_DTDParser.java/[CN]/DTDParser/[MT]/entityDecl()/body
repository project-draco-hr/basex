{
  if (!consumeWS())   error(WSERROR);
  if (consume(PERCENT)) {
    if (!consumeWS())     error(WSERROR);
    byte[] name=consumeSpecName();
    BaseX.debug("----------------------");
    BaseX.debug("- Entity: '%'",name);
    final byte[] val=entDef();
    ents.add(name,val);
  }
 else {
    byte[] name=consumeSpecName();
    BaseX.debug("----------------------");
    BaseX.debug("- Entity: '%'",name);
    final byte[] val=entDef();
    ents.add(name,val);
  }
}
