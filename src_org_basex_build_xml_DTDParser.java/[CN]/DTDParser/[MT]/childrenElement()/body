{
  consumeWS();
  if (consume(BRACKETC)) {
    consumeWS();
    if (consume(PLUS) || consume(QUESTION) || consume(STAR)) {
      consumeWS();
      if (consume(DASH) || consume(COLON)) {
        childrenElement();
      }
 else       if (consume(BRACKETC)) {
        prev();
        childrenElement();
      }
    }
 else {
      if (consume(DASH) || consume(COLON)) {
        childrenElement();
      }
 else {
        if (consume(GREAT)) {
          prev();
        }
 else {
          childrenElement();
        }
      }
    }
  }
 else   if (consume(DASH) || consume(COLON)) {
    consumeWS();
    if (consume(BRACKETO)) {
      childrenElement();
    }
 else {
      BaseX.debug(consumeSpecName());
      childrenElement();
    }
  }
 else   if (consume(BRACKETO)) {
    childrenElement();
  }
 else {
    BaseX.debug(consumeSpecName());
    childrenElement();
  }
}
