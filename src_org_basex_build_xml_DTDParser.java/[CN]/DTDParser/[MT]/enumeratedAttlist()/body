{
  consumeWS();
  if (consume(BRACKETC)) {
    if (bopen) {
      enumeratedAttlist();
      bopen=false;
    }
  }
 else   if (consume(DASH)) {
    enumeratedAttlist();
  }
 else   if (consume(BRACKETO)) {
    bopen=true;
    enumeratedAttlist();
  }
 else {
    BaseX.debug(consumeSpecName());
    enumeratedAttlist();
  }
}
