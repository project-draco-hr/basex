{
  consumeWS();
  if (consume(SYSTEM) || consume(PUBLIC)) {
    consumeWS();
    BaseX.debug(consumeQuoted());
    if (!consumeWS())     error();
    if (consume(ND)) {
      BaseX.debug(ND);
      if (!consumeWS())       error();
      BaseX.debug(consumeName());
    }
 else     if (consume(GREAT)) {
    }
 else {
      BaseX.debug(consumeQuoted());
    }
  }
 else {
    consumeWS();
    BaseX.debug(consumeQuoted());
    if (consumeWS()) {
      if (consume(SYSTEM))       error();
    }
  }
}
