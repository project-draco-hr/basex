{
  final Data data=context.data;
switch (idx) {
case TAG:
    out.println(INFOTAGS);
  out.println(data.info(IndexType.TAG));
return true;
case ATTNAME:
out.println(INFOATTS);
out.println(data.info(IndexType.ATTNAME));
return true;
case TEXT:
out.println(INFOTEXTINDEX);
if (data.meta.txtindex) out.println(data.info(IndexType.TEXT));
return true;
case ATTRIBUTE:
out.println(INFOATTRINDEX);
if (data.meta.atvindex) out.println(data.info(IndexType.ATTRIBUTE));
return true;
case FULLTEXT:
out.println(INFOFTINDEX);
if (data.meta.ftxindex) out.println(data.info(IndexType.FULLTEXT));
return true;
case PATH:
out.println(INFOPATHINDEX);
if (data.meta.pthindex) out.println(data.info(IndexType.PATH));
return true;
default :
return false;
}
}
