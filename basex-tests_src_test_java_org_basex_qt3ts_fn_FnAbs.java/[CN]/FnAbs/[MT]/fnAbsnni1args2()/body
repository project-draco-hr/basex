{
  final XQuery query=new XQuery("fn:abs(xs:nonNegativeInteger(\"303884545991464527\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("303884545991464527") && assertType("xs:integer")));
}
