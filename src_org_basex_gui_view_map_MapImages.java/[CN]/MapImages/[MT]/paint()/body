{
  int imgSize=0;
  for (int j=(imgc == 0 ? MAXNR : imgc) - 1; j != imgc; ) {
    if (imgs[j] != null) {
      if (imgSize < MAXPIXELS) {
        imgSize+=imgs[j].getWidth(map) * imgs[j].getHeight(map);
      }
 else {
        imgid[j]=0;
        imgs[j]=null;
      }
    }
    j=(j == 0 ? MAXNR : j) - 1;
  }
  map.drawMap();
  map.repaint();
}
