{
  final Data data=GUI.context.data();
  tmpPres=new IntList();
  final int s=data.size;
  final int itmID=data.tagID(item);
  int p=1;
  while (p < s) {
    final int kind=data.kind(p);
    if (kind == Data.ELEM && data.tagID(p) == itmID) {
      if (data.tagID(p) == itmID) {
        tmpPres.add(p);
        p+=data.size(p,kind);
      }
 else {
        p+=data.attSize(p,kind);
      }
    }
 else {
      p++;
    }
  }
  pres=tmpPres.finish();
  size=pres.length;
}
