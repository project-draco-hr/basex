{
  final Item[] arg=new Item[expr.length];
  for (int a=0; a < expr.length; a++) {
    arg[a]=ctx.iter(expr[a]).atomic(this,false);
  }
  Object result=null;
  try {
    result=mth.equals("new") ? constructor(arg) : method(arg);
  }
 catch (  Exception ex) {
    Err.or(FUNJAVA,info());
  }
  if (result == null)   Err.or(FUNJAVA,info());
  return iter(result);
}
