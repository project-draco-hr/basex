{
  GUIProp.maxstate=getExtendedState() == MAXIMIZED_BOTH;
  if (!GUIProp.maxstate) {
    GUIProp.guiloc[0]=getX();
    GUIProp.guiloc[1]=getY();
    GUIProp.guisize[0]=getWidth();
    GUIProp.guisize[1]=getHeight();
  }
  boolean fs=context.data() != null && context.data().fs != null;
  query.quit();
  if (help != null)   help.close();
  if (!fs)   context.close();
  GUIProp.write();
  Prop.write();
  super.dispose();
  if (!fs)   System.exit(0);
}
