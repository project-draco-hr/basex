{
  super.compile(ctx);
  func.compile(ctx);
  if (func.expr.isValue() && allAreValues() && !func.uses(Use.NDT)) {
    for (int a=0; a < expr.length; ++a)     func.args[a].bind(expr[a],ctx);
    ctx.compInfo(OPTINLINE,func.name.string());
    return func.value(ctx);
  }
  type=func.type();
  return this;
}
