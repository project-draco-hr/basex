{
  final XQuery query=new XQuery("fn:number(xs:long(\"-92233720368547758\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-9.223372036854776E16"));
}
