{
  final XQuery query=new XQuery("fn:number(xs:negativeInteger(\"-297014075999096793\")) eq -297014075999096793",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
