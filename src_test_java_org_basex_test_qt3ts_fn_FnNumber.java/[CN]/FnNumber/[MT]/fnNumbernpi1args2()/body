{
  final XQuery query=new XQuery("fn:number(xs:nonPositiveInteger(\"-475688437271870490\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"-4.7568843727187049E17") || assertStringValue(false,"-4.756884372718705E17") || assertStringValue(false,"-4.7568843727187046E17")));
}
