{
  final XQuery query=new XQuery("-fn:month-from-dateTime(xs:dateTime(\"1970-02-01T10:00:00Z\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-2"));
}
