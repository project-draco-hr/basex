{
  final XQuery query=new XQuery("<e> { attribute name {<anElement/>}, attribute name2 {\"content\"} } </e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e name=\"\" name2=\"content\"/>",false));
}
