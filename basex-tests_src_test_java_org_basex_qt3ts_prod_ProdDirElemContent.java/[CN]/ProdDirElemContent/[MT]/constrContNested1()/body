{
  final XQuery query=new XQuery("<elem><a><b/></a><a/><c/></elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem><a><b/></a><a/><c/></elem>",false));
}
