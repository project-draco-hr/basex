{
  final XQuery query=new XQuery("<codepoints>{string-to-codepoints(<elem>1\n" + "2</elem>)}</codepoints>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<codepoints>49 10 50</codepoints>",false));
}
