{
  final XQuery query=new XQuery("\n" + "        for $x in <a xml:base=\"http://www.example.com/base1\"><b/></a>, \n" + "            $y in <a xml:base=\"http://www.example.com/base2\">{$x/b}</a> \n"+ "        return fn:base-uri(exactly-one($y/b))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/base2"));
}
