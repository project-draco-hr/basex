{
  final XQuery query=new XQuery("<elem>{//west/@west-attr-1}{//west/@west-attr-2}</elem>",ctx);
  try {
    query.context(node(file("prod/AxisStep/TopMany.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem west-attr-1=\"w1\" west-attr-2=\"w2\"/>",false));
}
