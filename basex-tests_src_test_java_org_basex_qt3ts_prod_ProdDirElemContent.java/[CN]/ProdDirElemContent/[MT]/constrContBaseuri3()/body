{
  final XQuery query=new XQuery("declare base-uri \"http://www.example.com\"; fn:base-uri(<elem/>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com"));
}
