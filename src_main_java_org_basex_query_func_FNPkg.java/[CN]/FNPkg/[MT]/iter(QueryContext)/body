{
  checkAdmin(ctx);
  final Iterator<byte[]> i=ctx.context.repo.pkgDict().iterator();
switch (def) {
case _PKG_LIST:
    return new Iter(){
      @Override public Item next() throws QueryException {
        final byte[] next=i.next();
        return next == null ? null : Str.get(next);
      }
    }
;
default :
  return super.iter(ctx);
}
}
