{
  final int s=op.size();
  nodes=new IntList(s);
  for (int i=1; i <= op.size(); i++)   nodes.add(op.key(i));
  nodes.sort();
  for (int i=0; i < s; ++i) {
    for (    final Primitive p : op.get(nodes.get(i))) {
      p.prepare();
      if (p.type() == PUT && parentDeleted(nodes.get(i))) {
        UPFOTYPE.thrw(p.input,p);
      }
    }
  }
}
