{
  final int s=op.size();
  nodes=new IntList(s);
  for (  final int i : op.keySet())   nodes.add(i);
  nodes.sort();
  for (int i=0; i < s; ++i) {
    for (    final UpdatePrimitive p : op.get(nodes.get(i))) {
      p.prepare();
      if (p.type() == PUT && parentDeleted(nodes.get(i))) {
        Err.or(p.input,UPFOEMPT,p);
      }
    }
  }
}
