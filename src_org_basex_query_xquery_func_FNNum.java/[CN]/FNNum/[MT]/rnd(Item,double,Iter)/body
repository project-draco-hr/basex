{
  final int pp=pr == null ? 0 : (int)checkItr(pr);
  if (it.type == Type.DEC && pp >= 0) {
    return Dec.iter(it.dec().setScale(pp,BigDecimal.ROUND_HALF_EVEN));
  }
  final double p=p(pp);
  double d=n;
  if (p == 1 && (d % 2 == .5 || d % 2 == -1.5))   d-=.5;
 else   d=Math.floor(d * p + .5) / p;
  return num(it,n,d);
}
