{
  final double d=it.dbl();
  final boolean s=d > 0d || 1 / d > 0;
switch (it.type) {
case DBL:
    return s ? it.iter() : Dbl.iter(Math.abs(it.dbl()));
case FLT:
  return s ? it.iter() : Flt.iter(Math.abs(it.flt()));
case DEC:
return s ? it.iter() : Dec.iter(it.dec().abs());
case ITR:
return s ? it.iter() : Itr.iter(Math.abs(it.itr()));
default :
return Itr.iter(Math.abs(it.itr()));
}
}
