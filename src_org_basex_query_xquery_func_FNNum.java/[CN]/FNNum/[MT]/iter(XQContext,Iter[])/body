{
  final Iter iter=arg[0];
  final Item it=iter.atomic(this,true);
  if (it == null)   return Iter.EMPTY;
  if (!it.u() && !it.n())   Err.num(info(),it);
  final double d=it.dbl();
switch (func) {
case ABS:
    return abs(it);
case CEIL:
  return num(it,d,Math.ceil(d));
case FLOOR:
return num(it,d,Math.floor(d));
case RND:
return num(it,d,Math.floor(d + .5));
case RNDHLF:
return rnd(it,d,arg.length == 2 ? arg[1] : null);
default :
throw new RuntimeException("Not defined: " + func);
}
}
