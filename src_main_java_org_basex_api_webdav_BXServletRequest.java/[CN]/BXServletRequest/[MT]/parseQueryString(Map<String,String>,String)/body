{
  if (qs == null)   return;
  for (  final String nv : qs.split("&")) {
    final String[] parts=nv.split("=");
    final String key=parts[0];
    String val=null;
    if (parts.length > 1) {
      val=parts[1];
      if (val != null) {
        try {
          val=URLDecoder.decode(val,"UTF-8");
        }
 catch (        final UnsupportedEncodingException ex) {
          throw new RuntimeException(ex);
        }
      }
    }
    map.put(key,val);
  }
}
