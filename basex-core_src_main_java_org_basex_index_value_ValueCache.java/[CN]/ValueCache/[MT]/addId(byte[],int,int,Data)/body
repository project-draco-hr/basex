{
  int i=keys.put(text) - 1;
  final boolean exists=i < ids.size();
  IntList list;
  if (exists) {
    list=ids.get(i);
  }
 else {
    list=new IntList(1);
    ids.add(list);
  }
  list.add(data.id(pre));
  if (pos != null) {
    if (exists) {
      list=pos.get(i);
    }
 else {
      list=new IntList(1);
      pos.add(list);
    }
    list.add(ps);
  }
}
