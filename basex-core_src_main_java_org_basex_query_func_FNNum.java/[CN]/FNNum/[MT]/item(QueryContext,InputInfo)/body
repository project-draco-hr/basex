{
  final Item it=exprs[0].item(ctx,info);
  if (it == null)   return null;
  final Type ip=it.type;
  if (!ip.isNumberOrUntyped())   throw numberError(this,it);
  final double d=it.dbl(info);
switch (func) {
case ABS:
    return abs(it,info);
case CEILING:
  return num(it,d,StrictMath.ceil(d));
case FLOOR:
return num(it,d,StrictMath.floor(d));
case ROUND:
return rnd(it,d,false,ctx);
case ROUND_HALF_TO_EVEN:
return rnd(it,d,true,ctx);
default :
return super.item(ctx,ii);
}
}
