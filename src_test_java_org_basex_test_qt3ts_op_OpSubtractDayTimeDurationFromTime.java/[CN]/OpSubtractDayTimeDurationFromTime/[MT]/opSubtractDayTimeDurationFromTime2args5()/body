{
  final XQuery query=new XQuery("xs:time(\"00:00:00Z\") - xs:dayTimeDuration(\"P31DT23H59M59S\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"00:00:01Z"));
}
