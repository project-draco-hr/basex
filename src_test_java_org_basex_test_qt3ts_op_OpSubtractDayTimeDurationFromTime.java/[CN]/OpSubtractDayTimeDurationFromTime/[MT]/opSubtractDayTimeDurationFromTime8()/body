{
  final XQuery query=new XQuery("(xs:time(\"01:01:01\") - xs:dayTimeDuration(\"-P11DT02H02M\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"03:03:01"));
}
