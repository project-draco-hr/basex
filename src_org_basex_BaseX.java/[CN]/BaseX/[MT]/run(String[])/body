{
  if (!parseArguments(args))   return;
  boolean user=false;
  if (file != null) {
    final String query=content();
    if (query != null)     process(new XQuery(query),true);
  }
 else   if (commands != null) {
    process(commands);
  }
 else {
    set(Prop.INFO,ON);
    user=console();
  }
  quit(user);
}
