{
  boolean user=false;
  if (parseArguments(args)) {
    if (console)     set(Prop.INFO,ON);
    if (file != null) {
      final String query=content();
      if (query != null)       process(new XQuery(query),true);
    }
 else     if (commands != null) {
      process(commands);
    }
 else {
      user=console();
    }
  }
  quit(user);
}
