{
  boolean user=false;
  if (file != null) {
    final String query=content();
    if (query != null)     process(new XQuery(query),true);
  }
 else   if (commands != null) {
    process(commands);
  }
 else {
    outln(CONSOLE,standalone ? LOCALMODE : CLIENTMODE,CONSOLE2);
    user=console();
  }
  quit(user);
}
