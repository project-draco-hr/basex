{
  boolean user=false;
  if (file != null) {
    Prop.xquery=IO.get(file);
    final String query=content();
    if (query != null)     process(new XQuery(query),true);
  }
 else   if (commands != null) {
    process(commands);
  }
 else {
    outln(CONSOLE,sa() ? LOCALMODE : CLIENTMODE,CONSOLE2);
    if (console) {
      if (!set(Prop.INFO,ON))       return;
      user=console();
    }
  }
  quit(user);
}
