{
  try {
    final ALauncher la=launcher();
    final boolean ok=la.execute(pr);
    if (ok && pr.printing()) {
      final PrintOutput out=output != null ? new PrintOutput(output) : new PrintOutput(System.out);
      la.output(out);
      out.close();
    }
    if (v || !ok) {
      final CachedOutput out=new CachedOutput();
      la.info(out);
      final String inf=out.toString();
      if (inf.length() != 0) {
        if (!ok) {
          error(null,inf);
        }
 else {
          out(inf);
        }
      }
    }
    if (v && console && !(pr instanceof IntPrompt))     outln();
    return ok;
  }
 catch (  final IOException ex) {
    error(ex,SERVERERR);
    return false;
  }
}
