{
  final XQuery query=new XQuery("fn:sum((xs:integer(\"-999999999999999999\"),xs:integer(\"830993497117024304\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("-169006502882975695"));
}
