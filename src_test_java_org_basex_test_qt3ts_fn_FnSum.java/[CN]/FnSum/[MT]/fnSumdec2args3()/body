{
  final XQuery query=new XQuery("fn:sum((xs:decimal(\"-999999999999999999\"),xs:decimal(\"617375191608514839\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("-382624808391485160"));
}
