{
  final XQuery query=new XQuery("fn:sum((xs:double(\"-1.7976931348623157E308\"),xs:double(\"-1.7976931348623157E308\"),xs:double(\"1.7976931348623157E308\")))",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"-INF") || assertEq("-1.7976931348623157E308") || error("FOAR0002")));
}
