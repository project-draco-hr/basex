{
  if (size == next.length)   rehash();
  final int h=i.hash();
  final int p=h & bucket.length - 1;
  for (int id=bucket[p]; id != 0; id=next[id]) {
    if (values[id].equiv(ii,i))     return false;
  }
  next[size]=bucket[p];
  keys[size]=h;
  values[size]=i;
  bucket[p]=size++;
  return true;
}
