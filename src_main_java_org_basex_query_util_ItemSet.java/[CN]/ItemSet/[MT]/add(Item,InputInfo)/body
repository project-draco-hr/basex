{
  if (size == next.length)   rehash();
  final int h=key.hash(ii);
  final int p=h & bucket.length - 1;
  for (int id=bucket[p]; id != 0; id=next[id]) {
    if (keys[id].equiv(ii,key))     return -id;
  }
  next[size]=bucket[p];
  hash[size]=h;
  keys[size]=key;
  bucket[p]=size;
  return size++;
}
