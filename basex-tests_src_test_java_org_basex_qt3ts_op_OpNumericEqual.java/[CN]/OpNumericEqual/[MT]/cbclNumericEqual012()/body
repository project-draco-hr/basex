{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:integer) as xs:integer { $arg * $arg }; \n" + "      \tlet $x := local:square(4) return 4 eq 1 + $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
