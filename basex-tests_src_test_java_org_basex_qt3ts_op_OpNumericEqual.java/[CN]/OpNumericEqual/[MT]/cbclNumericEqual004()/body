{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:integer) as xs:integer { $arg * $arg }; \n" + "      \tnot( local:square(1) eq local:square(2) )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
