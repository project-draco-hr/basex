{
  int e2=-1, f2=-1;
  final int sl=sb.length;
  for (int t=0; t < tl; t++) {
    final int e=Token.norm(tk[t]);
    int d=Integer.MAX_VALUE;
    for (int q=0; q < sl; q++) {
      final int f=Token.norm(sb[q]);
      int c=m(m[t][q + 1] + 1,m[t + 1][q] + 1,m[t][q] + (e == f ? 0 : 1));
      if (e == f2 && f == e2)       c=m[t][q];
      m[t + 1][q + 1]=c;
      d=Math.min(d,c);
      f2=f;
    }
    if (d > k)     return false;
    e2=e;
  }
  return m[tl][sl] <= k;
}
