{
  final XQuery query=new XQuery("fn:upper-case(xs:string(\"This is a characte\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"THIS IS A CHARACTE"));
}
