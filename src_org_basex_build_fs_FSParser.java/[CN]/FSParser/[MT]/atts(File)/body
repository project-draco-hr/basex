{
  final String name=f.getName();
  final int s=name.lastIndexOf('.');
  final long time=f.lastModified() / 60000;
  final byte[] suf=s != -1 ? lc(token(name.substring(s + 1))) : EMPTY;
  if (time == 0) {
    ATS2[1]=token(name);
    ATS2[3]=suf;
    ATS2[5]=token(f.length());
    return ATS2;
  }
  ATS1[1]=token(name);
  ATS1[3]=suf;
  ATS1[5]=token(f.length());
  ATS1[7]=token(time);
  return ATS1;
}
