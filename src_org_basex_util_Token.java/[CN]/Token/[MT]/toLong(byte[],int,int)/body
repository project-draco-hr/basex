{
  int t=ts;
  while (t < te && to[t] <= ' ')   t++;
  if (t == te)   return Long.MIN_VALUE;
  boolean m=false;
  if (to[t] == '-' || to[t] == '+')   m=to[t++] == '-';
  if (t == te)   return Long.MIN_VALUE;
  long v=0;
  for (; t < te; t++) {
    final byte c=to[t];
    if (c < '0' || c > '9')     break;
    long w=(v << 3) + (v << 1) + c - '0';
    if (w < v)     return Long.MIN_VALUE;
    v=w;
  }
  while (t < te && to[t] <= ' ')   t++;
  return t < te ? Long.MIN_VALUE : m ? -v : v;
}
