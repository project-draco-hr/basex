{
  if (enc == Token.UTF8 || Token.ascii(s))   return s;
  try {
    System.out.println(enc);
    System.out.println("-> " + new String(s,enc));
    return new String(s,enc).getBytes(UTF8);
  }
 catch (  final Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e.getMessage());
  }
}
