{
  final int v=t[p] & 0xFF;
  if (v < 192)   return v;
  final int c=v >> 4;
  int l=p;
  if (c == 12 || c == 13)   return (v & 0x1F) << 6 | (t[++l] & 0x3F);
  if (c == 14)   return (v & 0x0F) << 12 | (t[++l] & 0x3F) << 6 | (t[++l] & 0x3F);
  return (v & 0x07) << 18 | (t[++l] & 0x3F) << 12 | (t[++l] & 0x3F) << 6 | (t[++l] & 0x3F);
}
