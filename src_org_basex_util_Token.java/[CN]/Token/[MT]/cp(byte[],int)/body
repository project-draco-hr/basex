{
  int l=i;
  final int v=b[l] & 0xFF;
  final int c=v >> 4;
  if (c == 12 || c == 13)   return (v & 0x1F) << 6 | (b[++l] & 0x3F);
  if (c == 14)   return (v & 0x0F) << 12 | (b[++l] & 0x3F) << 6 | (b[++l] & 0x3F);
  if (c == 15)   return (v & 0x07) << 18 | (b[++l] & 0x3F) << 12 | (b[++l] & 0x3F) << 6 | (b[++l] & 0x3F);
  return v;
}
