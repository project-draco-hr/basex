{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}string-join#2(('abc', 'def'), '-')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abc-def"));
}
