{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}adjust-dateTime-to-timezone#2(xs:dateTime(\"1970-01-01T00:00:00Z\"),xs:dayTimeDuration(\"-PT10H\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1969-12-31T14:00:00-10:00"));
}
