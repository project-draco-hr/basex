{
  final XQuery query=new XQuery("Q{http://www.w3.org/2001/XMLSchema}normalizedString#1('normalized\n" + "string')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"normalized string"));
}
