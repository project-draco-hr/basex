{
  final XQuery query=new XQuery("exists(Q{http://www.w3.org/2005/xpath-functions}adjust-dateTime-to-timezone#1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
