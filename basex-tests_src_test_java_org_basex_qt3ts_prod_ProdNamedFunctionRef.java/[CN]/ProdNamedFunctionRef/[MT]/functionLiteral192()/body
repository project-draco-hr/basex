{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}seconds-from-duration#1(xs:dayTimeDuration(\"P3DT10H12.5S\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"12.5"));
}
