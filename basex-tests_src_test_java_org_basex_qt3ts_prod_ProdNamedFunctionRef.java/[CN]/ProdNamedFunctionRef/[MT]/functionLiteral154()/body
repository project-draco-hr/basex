{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}replace#4('string', 'I', 'o', 'i')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"strong"));
}
