{
  final XQuery query=new XQuery("Q{http://www.w3.org/2001/XMLSchema}dateTime#1('1970-01-02T04:05:06Z')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1970-01-02T04:05:06Z"));
}
