{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}map-pairs#3(concat#2, (\"a\", \"b\", \"c\"), (\"x\", \"y\", \"z\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("(\"ax\", \"by\", \"cz\")"));
}
