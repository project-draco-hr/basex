{
  check(r,XMLReader.class);
  try {
    final TokenBuilder tb=new TokenBuilder();
    final BufferedReader br=new BufferedReader(r);
    int i=0;
    while ((i=br.read()) != -1)     tb.add((char)i);
    br.close();
    return tb.finish();
  }
 catch (  final IOException ex) {
    throw new BXQException(ex);
  }
}
