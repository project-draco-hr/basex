{
  final XQuery query=new XQuery("*",ctx).context(expected);
  final TokenBuilder tb=new TokenBuilder();
  try {
    for (    final XQItem it : query) {
      final String msg=test(result,it);
      if (msg == null)       return null;
      tb.add(tb.size() != 0 ? ", " : "").add(msg);
    }
    return tb.toString();
  }
  finally {
    query.close();
  }
}
