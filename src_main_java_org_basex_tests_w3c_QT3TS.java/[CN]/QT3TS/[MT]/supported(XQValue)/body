{
  final XQuery q=new XQuery("*:environment/*:collation | *:dependency[" + "@type='feature' and " + "@value=('schemaImport','schemaValidation','namespace-axis') or "+ "@type='xml-version' and @value='1.1' or"+ "@type='xsd-version' and @value='1.1']",ctx).context(node);
  try {
    return q.next() == null;
  }
  finally {
    q.close();
  }
}
