{
  final Query qp=new Query("*",expected);
  final TokenBuilder tb=new TokenBuilder();
  try {
    for (Item it; (it=qp.next()) != null; ) {
      final String msg=test(result,(ANode)it);
      if (msg == null)       return null;
      tb.add(tb.size() != 0 ? ", " : "").add(msg);
    }
    return tb.toString();
  }
  finally {
    qp.close();
  }
}
