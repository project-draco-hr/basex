{
  final XQuery q=new XQuery("*:environment/*:collation |" + "*:dependency[" + "@type='feature' and"+ " @value=('schemaImport','schemaValidation','namespace-axis','staticTyping') or "+ "@type=('xml-version','xsd-version') and @value='1.1' or"+ "@type='spec' and not(contains(@value, 'XQ'))]",ctx).context(node);
  try {
    return q.next() == null;
  }
  finally {
    q.close();
  }
}
