{
  final XQuery query=new XQuery("*",ctx).context(exp);
  final TokenBuilder tb=new TokenBuilder();
  try {
    for (    final XdmItem it : query) {
      final String msg=test(res,it);
      if (msg == null)       return null;
      tb.add(tb.isEmpty() ? "" : ", ").add(msg);
    }
    return "any of { " + tb + " }";
  }
  finally {
    query.close();
  }
}
