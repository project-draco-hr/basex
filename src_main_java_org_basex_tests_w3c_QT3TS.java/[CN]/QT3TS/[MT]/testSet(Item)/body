{
  final String docts=path + '/' + set.toJava();
  final String quts="doc('" + docts + "')//*:test-case";
  final QueryProcessor qpts=new QueryProcessor(quts,context);
  final Iter irts=qpts.iter();
  for (Item itts; (itts=irts.next()) != null && total < MAX; ) {
    testCase(itts);
  }
  qpts.close();
}
