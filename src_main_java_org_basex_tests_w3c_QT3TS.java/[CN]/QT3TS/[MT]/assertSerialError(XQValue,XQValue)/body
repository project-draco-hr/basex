{
  final String exp=asString("@" + CODE,expect);
  try {
    value.toString();
    return exp;
  }
 catch (  final RuntimeException qe) {
    final String res=qe.getMessage().replaceAll("\\[|\\].*\r?\n?.*","");
    return !errors || exp.equals("*") || exp.equals(res) ? null : Util.info("% (found: %)",exp,res);
  }
}
