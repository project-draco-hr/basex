{
  final TokenBuilder tb=new TokenBuilder();
  tok=str;
  final int l=str.length;
  for (ts=0; ts < l; ts++) {
    if (str[ts] < 0 || str[ts] > ' ') {
      te=ts;
      while (++te < l && (str[te] < 0 || str[te] > ' '))       ;
      final int e=te;
      word();
      for (; ts < te; ts++)       tb.add(tok[ts]);
      ts=e - 1;
    }
 else {
      tb.add(str[ts]);
    }
  }
  return tb.finish();
}
