{
  if (first == null)   first=new Node(pre,-1);
 else {
    Node pointer=first;
    Node b4pointer=null;
    int addend=0;
    while (pointer != null && pointer.id < pre) {
      addend+=pointer.addend;
      b4pointer=pointer;
      pointer=pointer.next;
    }
    if (pointer != null && pointer.id == pre)     --pointer.addend;
 else {
      Node newNode=new Node(pre,-1);
      if (b4pointer == null)       first=newNode;
 else       b4pointer.next=newNode;
      newNode.next=pointer;
    }
  }
}
