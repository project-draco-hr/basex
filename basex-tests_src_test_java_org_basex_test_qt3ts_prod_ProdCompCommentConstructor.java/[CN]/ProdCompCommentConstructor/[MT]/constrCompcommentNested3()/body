{
  final XQuery query=new XQuery("document {comment {'one'}, <a/>, comment {'two'}, <b/>, comment {'three'}}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<!--one--><a/><!--two--><b/><!--three-->",false));
}
