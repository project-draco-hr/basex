{
  final FItem f=withArity(1,1,qc);
  final Iter xs=exprs[0].iter(qc);
  return new Iter(){
    @Override public Item next() throws QueryException {
      do {
        final Item it=xs.next();
        if (it == null)         return null;
        if (checkBln(f.invokeItem(qc,info,it)))         return it;
      }
 while (true);
    }
  }
;
}
