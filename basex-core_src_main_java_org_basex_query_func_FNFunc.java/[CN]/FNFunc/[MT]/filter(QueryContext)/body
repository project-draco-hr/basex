{
  final FItem fun=checkArity(1,1,qc);
  final Iter ir=exprs[0].iter(qc);
  return new Iter(){
    @Override public Item next() throws QueryException {
      do {
        final Item it=ir.next();
        if (it == null)         return null;
        if (checkBln(fun.invokeItem(qc,info,it)))         return it;
      }
 while (true);
    }
  }
;
}
