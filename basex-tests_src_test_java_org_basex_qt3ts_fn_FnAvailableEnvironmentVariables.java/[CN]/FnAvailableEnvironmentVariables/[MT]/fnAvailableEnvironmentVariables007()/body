{
  final XQuery query=new XQuery("\n" + "\t  let $all := fn:available-environment-variables(), \n" + "\t      $n := count(fn:available-environment-variables())\n"+ "\t  return\n"+ "\t      count(\n"+ "\t\t  for $e in $all return fn:environment-variable($e)\n"+ "\t      ) eq $n\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
