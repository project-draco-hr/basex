{
  final XQuery query=new XQuery("for $x in <!--comment-->, $y in document {$x} return exactly-one($y/comment()) is $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
