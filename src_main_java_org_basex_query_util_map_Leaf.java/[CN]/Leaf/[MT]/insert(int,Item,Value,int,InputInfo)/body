{
  if (h == hash)   return k.eq(ii,key) ? new Leaf(h,k,v) : new List(hash,key,value,k,v);
  final TrieNode[] ch=new TrieNode[KIDS];
  final int a=key(h,l), b=key(hash,l);
  final int used;
  if (a != b) {
    ch[a]=new Leaf(h,k,v);
    ch[b]=this;
    used=1 << a | 1 << b;
  }
 else {
    ch[a]=insert(h,k,v,l + 1,ii);
    used=1 << a;
  }
  return new Branch(ch,used,2);
}
