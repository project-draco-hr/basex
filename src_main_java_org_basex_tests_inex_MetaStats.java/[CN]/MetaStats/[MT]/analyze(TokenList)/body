{
  final Data data=ctx.data();
  tl.add(format(data.meta.filesize));
  tl.add(data.meta.size);
  tl.add("xxx");
  tl.add(data.tagindex.size());
  tl.add(data.atnindex.size());
  tl.add(data.ns.size());
  tl.add(ctx.data().meta.dbfile(DATATXT).length());
  tl.add(ctx.data().meta.dbfile(DATAATV).length());
  tl.add(format(data.meta.dbsize()));
  tl.add(data.docs().size());
}
