{
  final XQuery query=new XQuery("fn:map-pairs( function($arg1, $arg2) { ($arg1, $arg2) }, (1, fn:error()), 1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"1") || error("FOER0000")));
}
