{
  final XQuery query=new XQuery("fn:map-pairs( function($arg1 as xs:integer, $arg2 as xs:integer) { $arg1 + $arg2 }, (\"1\", \"2\"), (\"1\", \"2\") )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
