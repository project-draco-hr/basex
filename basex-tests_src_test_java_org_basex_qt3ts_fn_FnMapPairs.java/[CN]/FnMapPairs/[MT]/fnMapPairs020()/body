{
  final XQuery query=new XQuery("fn:map-pairs( function($arg, $arg2) { fn:error() }, (), ())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEmpty() || error("XPST0005")));
}
