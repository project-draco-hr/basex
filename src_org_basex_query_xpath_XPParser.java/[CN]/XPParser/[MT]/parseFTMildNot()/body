{
  Expr e=parseFTUnaryNot();
  consumeWS();
  if (!consume(NOT))   return e;
  consumeWS();
  if (!consume(IN))   error(WRONGTEXT,IN,curr());
  ExprList el=new ExprList();
  el.add(e);
  consumeWS();
  if (consume(FTNOT))   error(FTNOTEXC);
  el.add(parseFTUnaryNot());
  consumeWS();
  while (consume(NOT)) {
    consumeWS();
    if (!consume(IN))     error(WRONGTEXT,IN,curr());
    consumeWS();
    el.add(parseFTUnaryNot());
  }
  return new FTMildNot(el.get());
}
