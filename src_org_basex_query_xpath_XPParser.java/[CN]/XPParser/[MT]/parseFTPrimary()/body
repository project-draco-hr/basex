{
  Expr e=null;
  if (curr('"') || curr('\'')) {
    e=parseFTWords();
    consumeWS();
    return new FTPrimary(new Expr[]{e},parseFTTimes());
  }
 else   if (consume('(')) {
    consumeWS();
    e=parseFTSelection();
    consumeWS();
    if (!consume(')')) {
      error(") expected");
    }
  }
 else {
    error("\', \" or ( expected");
  }
  return e;
}
