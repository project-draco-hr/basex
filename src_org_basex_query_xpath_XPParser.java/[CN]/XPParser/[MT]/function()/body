{
  final String func=name();
  consumeWS();
  if (!consume('('))   error(UNEXPECTEDEND);
  Expr[] list={};
  while (!curr(0)) {
    consumeWS();
    if (consume(')')) {
      return XPathContext.get().getFunction(token(func),list);
    }
    if (list.length != 0 && !consume(','))     error(UNFINISHEDFUNC);
    list=Array.add(list,or());
  }
  error(UNFINISHEDFUNC);
  return null;
}
