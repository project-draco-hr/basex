{
  consumeWS();
  if (consume('$'))   return parseVarReference();
  if (consume('(')) {
    final Expr e=parseOr();
    return consume(')') ? e : error(NOPARENTHESIS);
  }
  if (curr('"') || curr('\''))   return parseLiteral();
  if (digit(curr()))   return parseNumber();
  return parseFunction();
}
