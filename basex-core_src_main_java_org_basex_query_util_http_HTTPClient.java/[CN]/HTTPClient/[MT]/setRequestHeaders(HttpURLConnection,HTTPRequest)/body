{
  for (  final byte[] headers : r.headers)   conn.addRequestProperty(string(headers),string(r.headers.get(headers)));
  final byte[] sendAuth=r.attrs.get(SEND_AUTHORIZATION);
  if (sendAuth != null && Bln.parse(sendAuth,info))   conn.setRequestProperty(AUTHORIZATION,encodeCredentials(string(r.attrs.get(USERNAME)),string(r.attrs.get(PASSWORD))));
}
