{
  final XQuery query=new XQuery("\n" + "        declare variable $i := false(); \n" + "        declare variable $t := false(); \n"+ "        deep-equal((let $i := true(), $t := true() return ($i, $t)), (true(), true()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
