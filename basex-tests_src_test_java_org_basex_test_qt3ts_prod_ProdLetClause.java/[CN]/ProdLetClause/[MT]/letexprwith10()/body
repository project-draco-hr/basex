{
  final XQuery query=new XQuery("let $var as xs:dateTime := xs:dateTime(\"1999-05-31T13:20:00Z\") return $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1999-05-31T13:20:00Z"));
}
