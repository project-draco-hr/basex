{
  if (data == null || size() != 0 || data instanceof MemData)   return;
  try {
    final File file=data.meta.dbfile(DATASWL);
    if (!file.exists())     return;
    final DataInput in=new DataInput(data.meta.dbfile(DATASWL));
    try {
      read(in);
    }
  finally {
      in.close();
    }
  }
 catch (  final Exception ex) {
    Util.debug(ex);
  }
}
