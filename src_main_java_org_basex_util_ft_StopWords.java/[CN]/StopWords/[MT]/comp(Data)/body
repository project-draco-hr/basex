{
  if (size() != 0)   return;
  try {
    final File file=data.meta.file(DATASWL);
    if (!file.exists())     return;
    final DataInput in=new DataInput(data.meta.file(DATASWL));
    try {
      read(in);
    }
  finally {
      in.close();
    }
  }
 catch (  final Exception ex) {
    Util.debug(ex);
  }
}
