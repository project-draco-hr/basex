{
  try {
    final byte[] content=norm(fl.read());
    final int s=Token.contains(content,' ') ? ' ' : '\n';
    for (    final byte[] sl : split(content,s)) {
      if (e)       delete(sl);
 else       if (!contains(sl))       add(sl);
    }
    return true;
  }
 catch (  final IOException ex) {
    return false;
  }
}
