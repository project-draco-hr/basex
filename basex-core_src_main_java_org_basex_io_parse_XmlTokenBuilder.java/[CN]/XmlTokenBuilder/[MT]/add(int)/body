{
  if (ch < ' ' && ch != '\n' && ch != '\t' || ch >= 0x7F && ch < 0xA0) {
    addHex(ch);
  }
 else   if (ch == '&') {
    cache.add(E_AMP);
  }
 else   if (ch == '>') {
    cache.add(E_GT);
  }
 else   if (ch == '<') {
    cache.add(E_LT);
  }
 else   if (ch == 0x2028) {
    cache.add(E_2028);
  }
 else {
    cache.add(ch);
  }
}
