{
  final String[] ex=!excl ? new String[0] : new String[]{AProp.toString(SerializerProp.S_METHOD),AProp.toString(SerializerProp.S_ENCODING)};
  final StringBuilder sb=new StringBuilder();
  for (  final String key : sp) {
    if (Token.eq(key,ex))     continue;
    final Object val=sp.get(key);
    if (!Serializer.PROPS.get(key).equals(val)) {
      if (sb.length() != 0)       sb.append(',');
      sb.append(key).append('=').append(val);
    }
  }
  return sb.toString();
}
