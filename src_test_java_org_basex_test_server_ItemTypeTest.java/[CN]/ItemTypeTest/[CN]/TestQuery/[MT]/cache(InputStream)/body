{
  cache=new TokenList();
  types=new IntList();
  final ByteList bl=new ByteList();
  while (true) {
    final int t=is.read();
    if (t <= 0)     break;
    types.add(t);
    final DecodingInput di=new DecodingInput(is);
    for (int b; (b=di.read()) != -1; )     bl.add(b);
    cache.add(bl.toArray());
    bl.reset();
  }
}
