{
  int lass=0;
  int lasp=0;
  int lw=0;
  int[] c=new int[4];
  init();
  while (more()) {
    if (lw + p - s + 1 < ww)     lw+=p - s + 1;
 else {
      c[3]++;
      lw=0;
    }
    c[0]++;
    if (sent != lass) {
      c[1]++;
      lass=sent;
    }
    if (para != lasp) {
      c[2]++;
      lasp=para;
      lw=0;
      c[3]++;
    }
  }
  c[3]++;
  return c;
}
