{
  final XQConnection conn=conn(drv);
  Object[] objects={new Boolean(true),new Byte((byte)2),new Float(3f),new Double(4),new Integer(5),new Long(6),new Short((short)7),new String("8"),new BigDecimal(9),new BigInteger("10"),new QName("elf")};
  for (  Object o : objects) {
    for (int t=1; t <= 51; t++) {
      try {
        conn.createItemFromObject(o,conn.createAtomicType(t));
      }
 catch (      final Exception ex) {
      }
    }
  }
}
