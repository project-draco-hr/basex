{
  final XQuery query=new XQuery("declare default element namespace \"http://www.example.com/\"; <a2 xmlns:p=\"http://ns.example.com/foo\"/>/element e { element {QName(\"http://example.com/2\", \"p:a\")} {} }",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns=\"http://www.example.com/\"><p:a xmlns:p=\"http://example.com/2\"/></e>",false));
}
