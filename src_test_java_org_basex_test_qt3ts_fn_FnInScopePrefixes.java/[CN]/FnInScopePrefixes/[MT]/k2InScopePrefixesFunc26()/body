{
  final XQuery query=new XQuery("let $i := <e> { attribute {QName(\"http://example.com/\", \"prefix:attributeName\")} {()} } </e> return ($i, for $ps in in-scope-prefixes($i) order by $ps return $ps)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns:prefix=\"http://example.com/\" prefix:attributeName=\"\"/>prefix xml",false));
}
