{
  final XQuery query=new XQuery("declare default element namespace \"http://example.com/\"; <e xmlns:p=\"http://example.com/\"> { count(in-scope-prefixes(<e/>)), count(in-scope-prefixes(element e {()})) } </e>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns=\"http://example.com/\" xmlns:p=\"http://example.com/\">3 3</e>",false));
}
