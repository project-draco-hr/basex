{
  final XQuery query=new XQuery("fn:substring-before(fn:in-scope-prefixes(<anElement>Some content</anElement>)[1],\"m\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"x"));
}
