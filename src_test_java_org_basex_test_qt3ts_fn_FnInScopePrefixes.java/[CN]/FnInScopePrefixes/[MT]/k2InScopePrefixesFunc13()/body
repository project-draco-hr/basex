{
  final XQuery query=new XQuery("declare default element namespace \"http://www.example.com/\"; let $i := element e { element b {()} } return (count(in-scope-prefixes($i/b)), count(in-scope-prefixes($i)))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"2 2"));
}
