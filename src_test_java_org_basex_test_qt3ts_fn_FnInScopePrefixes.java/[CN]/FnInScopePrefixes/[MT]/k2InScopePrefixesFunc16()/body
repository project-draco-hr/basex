{
  final XQuery query=new XQuery("\n" + "        let $i := <e xmlns=\"http://example.com/\"> \n" + "                    {element a {()}} \n"+ "                  </e> \n"+ "        return (count(in-scope-prefixes($i)), count(in-scope-prefixes(exactly-one($i/*))))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"2 2"));
}
