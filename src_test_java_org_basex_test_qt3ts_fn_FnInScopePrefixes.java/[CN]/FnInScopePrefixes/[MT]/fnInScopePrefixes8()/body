{
  final XQuery query=new XQuery("declare namespace p1 = \"http://www.example.com\"; fn:in-scope-prefixes(<anElement xmlns:p1=\"http://www.somenamespace.com\">Some content</anElement>)",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"xml p1") || assertStringValue(false,"p1 xml")));
}
