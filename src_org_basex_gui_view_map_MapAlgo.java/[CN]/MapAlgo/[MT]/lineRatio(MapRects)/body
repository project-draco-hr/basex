{
  if (r.size() == 0)   return Double.MAX_VALUE;
  double ar=0;
  double dev=0;
  for (  MapRect rect : r) {
    if (rect.w != 0 && rect.h != 0) {
      double d=rect.w * rect.h;
      dev+=d;
      if (rect.w > rect.h) {
        ar+=d * rect.w / rect.h;
      }
 else {
        ar+=d * rect.h / rect.w;
      }
    }
  }
  return ar / dev;
}
