{
  if (r.size() == 0)   return Double.MAX_VALUE;
  double worstar=0;
  double ar=0;
  for (  MapRect rect : r) {
    if (rect.w != 0 && rect.h != 0) {
      if (rect.w > rect.h) {
        ar=rect.w / rect.h;
      }
 else {
        ar=rect.h / rect.w;
      }
    }
    if (ar > worstar)     worstar=ar;
  }
  return worstar;
}
