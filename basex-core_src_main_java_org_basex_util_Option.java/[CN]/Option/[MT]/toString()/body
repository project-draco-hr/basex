{
  final StringBuilder sb=new StringBuilder(name);
switch (type) {
case COMMENT:
    break;
case STRING:
case BOOLEAN:
case NUMBER:
  sb.append('=').append(value);
break;
case NUMBERS:
sb.append('=').append(Arrays.asList((int[])value));
break;
case STRINGS:
sb.append('=').append(Arrays.asList((String[])value));
break;
}
return sb.toString();
}
