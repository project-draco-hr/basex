{
  if (value.length == 0) {
    return -1;
  }
  double percentage=0d;
  if (type != Kind.CAT) {
    final double d=toDouble(value);
    final double range=max - min;
    if (range == 0)     percentage=0.5d;
 else     percentage=1 / range * (d - min);
  }
 else {
    for (int i=0; i < nrCats; i++) {
      if (eq(value,cats[i])) {
        percentage=(1.0d / (nrCats - 1)) * i;
      }
    }
  }
  return percentage;
}
