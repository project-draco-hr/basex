{
  final String find="for $x in ." + " where $x[ends-with(document-uri(.), '" + FILES[1] + "')]"+ " return base-uri($x)";
  final QueryProcessor qp=new QueryProcessor(find,CTX);
  final Item it=qp.iter().next();
  final String expath='"' + CTX.data.meta.file.url().replace(DBNAME,"") + FILES[1]+ '"';
  assertEquals(expath,it.toString());
  qp.close();
}
