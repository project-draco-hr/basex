{
  processor=new QueryProcessor(s,ctx);
  try {
    iter=processor.iter();
    serializer=new XMLSerializer(out);
  }
 catch (  QueryException ex) {
    log.write(this,s,INFOERROR + ex.extended());
    send(false);
    out.print(ex.extended());
    send(true);
  }
  out.print(String.valueOf(id));
  send(true);
}
