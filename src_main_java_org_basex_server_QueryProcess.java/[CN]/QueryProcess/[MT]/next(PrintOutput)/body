{
  if (xml == null) {
    qp.parse();
    monitored=true;
    ctx.lock.before(qp.ctx.updating);
    iter=qp.iter();
    xml=qp.getSerializer(new ArrayOutput()).out(out);
  }
  if (stopped)   SERVERTIMEOUT.thrw(null);
  item=iter.next();
  if (item == null)   return false;
  xml.init();
  item.serialize(xml);
  return true;
}
