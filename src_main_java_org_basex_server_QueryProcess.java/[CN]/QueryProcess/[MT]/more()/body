{
  Item item;
  try {
    if (!running) {
      send(false);
      send(true);
      out.print(SERVERTIME);
      send(true);
      close();
    }
 else {
      if ((item=iter.next()) != null) {
        send(true);
        item.serialize(serializer);
        send(true);
      }
 else {
        send(false);
        send(false);
        close();
      }
    }
  }
 catch (  Exception ex) {
    send(false);
    send(false);
    close();
  }
}
