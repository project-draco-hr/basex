{
  Object rh=RenderingHints.VALUE_ANTIALIAS_ON;
  try {
    rh=RenderingHints.class.getField("VALUE_TEXT_ANTIALIAS_" + GUIConstants.FONTALIAS[pr.num(GUIProp.FONTALIAS)]).get(null);
  }
 catch (  final Exception ex) {
    BaseX.debug(ex);
  }
  ((Graphics2D)g).setRenderingHint(RenderingHints.KEY_TEXT_ANTIALIASING,rh);
}
