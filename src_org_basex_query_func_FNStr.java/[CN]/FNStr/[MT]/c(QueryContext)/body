{
switch (func) {
case CONTAINS:
    final byte[] i=expr[1].i() ? checkStr((Item)expr[1]) : null;
  if (expr[1].e() || i != null && i.length == 0)   return Bln.TRUE;
if (expr[0].e() && i != null && i.length != 0) return Bln.FALSE;
return this;
case CONCAT:
for (final Expr a : expr) if (!a.i()) return this;
return concat(ctx);
default :
return this;
}
}
