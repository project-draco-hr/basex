{
  final byte[] str=checkStr(args[0],ctx);
  Norm norm=null;
  if (args.length == 2) {
    final byte[] n=uc(trim(checkStr(args[1],ctx)));
    for (    final Norm f : Norm.values())     if (eq(f.name,n))     norm=f;
    if (norm == null)     Err.or(NORMUNI,n);
  }
 else {
    norm=Norm.C;
  }
  return Str.get(str);
}
