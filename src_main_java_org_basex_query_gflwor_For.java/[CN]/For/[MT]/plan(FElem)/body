{
  final FElem e=planElem();
  if (empty)   e.add(planAttr(Token.token(EMPTYORD),Token.TRUE));
  var.plan(e);
  if (pos != null) {
    final FElem e2=new FElem(Token.token(QueryText.AT));
    pos.plan(e2);
    e.add(e2);
  }
  if (score != null) {
    final FElem e2=new FElem(Token.token(QueryText.SCORE));
    score.plan(e2);
    e.add(e2);
  }
  expr.plan(e);
  plan.add(e);
}
