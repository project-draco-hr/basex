{
  boolean change=false;
  final int es=expr.length;
  for (int i=0; i < es; i++) {
    Expr nw;
    try {
      nw=expr[i].inline(ctx,scp,v,e);
    }
 catch (    final QueryException qe) {
      nw=FNInfo.error(qe,expr[i].type());
    }
    if (nw != null) {
      expr[i]=nw;
      change=true;
    }
  }
  return change ? optimize(ctx,scp) : null;
}
