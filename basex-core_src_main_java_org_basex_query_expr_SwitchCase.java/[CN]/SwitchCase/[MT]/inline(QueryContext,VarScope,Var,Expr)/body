{
  boolean change=false;
  final int es=exprs.length;
  for (int i=0; i < es; i++) {
    Expr nw;
    try {
      nw=exprs[i].inline(qc,scp,var,ex);
    }
 catch (    final QueryException qe) {
      nw=FnError.get(qe,exprs[i].seqType(),scp.sc);
    }
    if (nw != null) {
      exprs[i]=nw;
      change=true;
    }
  }
  return change ? optimize(qc,scp) : null;
}
