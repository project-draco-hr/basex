{
  if (ok || updStructural.size() < 2 && updValue.size() < 2)   return;
  int i=0;
  while (i + 1 < updStructural.size()) {
    final BasicUpdate current=updStructural.get(i);
    final BasicUpdate next=updStructural.get(++i);
    if (current.location < next.location)     Util.notexpected("Invalid order at location " + current.location);
    if (current.location == next.location) {
      if (current.destructive() && next.destructive())       Util.notexpected("Multiple deletes/replaces on node " + current.location);
    }
  }
  i=0;
  while (i + 1 < updValue.size()) {
    final BasicUpdate current=updValue.get(i++);
    final BasicUpdate next=updValue.get(i);
    if (current.location < next.location)     Util.notexpected("Invalid order at location " + current.location);
    if (current.location == next.location) {
      if (current instanceof Rename && next instanceof Rename)       Util.notexpected("Multiple renames on node " + current.location);
      if (current instanceof UpdateValue && next instanceof UpdateValue)       Util.notexpected("Multiple updates on node " + current.location);
    }
  }
  ok=true;
}
