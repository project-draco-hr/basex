{
  tmps=-1;
  while (++tmpe <= tmpl) {
    if (tmps == -1) {
      if (tmpe < tmpl && Token.ftChar(tmptok[tmpe]))       tmps=tmpe;
    }
 else     if (tmpe == tmpl) {
      return true;
    }
 else     if (!Token.ftChar(tmptok[tmpe]) && !(Token.digit(tmptok[tmpe - 1]) && tmptok[tmpe] == '.' && tmpe + 1 < tmpl && Token.digit(tmptok[tmpe + 1]))) {
      return true;
    }
  }
  tmptok=null;
  return false;
}
