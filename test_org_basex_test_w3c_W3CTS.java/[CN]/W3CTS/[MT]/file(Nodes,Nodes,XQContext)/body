{
  final TokenBuilder tb=new TokenBuilder();
  for (int c=0; c < nod.size; c++) {
    final byte[] nm=data.atom(nod.pre[c]);
    final String src=srcFiles.get(string(nm));
    if (tb.size != 0)     tb.add(", ");
    tb.add(nm);
    if (src == null) {
      final NodIter col=new NodIter();
      for (      final byte[] cl : colls.get(string(nm)))       col.add(ctx.doc(cl));
      ctx.addColl(col,nm);
      if (var != null) {
        final Var v=new Var(new QNm(data.atom(var.pre[c])));
        ctx.vars.addGlobal(v.item(Uri.uri(nm)));
      }
    }
 else {
      final Fun fun=FNIndex.get().get(token("doc"),Uri.FN,new Expr[]{Str.get(src)});
      final Var v=new Var(new QNm(data.atom(var.pre[c])));
      ctx.vars.addGlobal(v.expr(fun));
    }
  }
  return tb.finish();
}
