{
  return new NodeIter(){
    FTTokenizer tmp;
    Iter iter1;
    @Override public Nod next() throws XQException {
      while (true) {
        if (iter1 == null) {
          tmp=ctx.ftitem;
          ctx.ftitem=ft;
          iter1=ctx.iter(expr[1]);
        }
        final Item n=iter1.next();
        if (n != null && n.bool()) {
          return (Nod)ctx.item;
        }
 else {
          ctx.ftitem=tmp;
          return null;
        }
      }
    }
  }
;
}
