{
  return new Iter(){
    @Override public Item next() throws XQException {
      final FTTokenizer tmp=ctx.ftitem;
      ctx.ftitem=ft;
      final FTNodeItem it=ftexpr.iter(ctx).next();
      ctx.ftitem=tmp;
      final IntList[] pos=it.ftn.convertPos();
      if (ctx.ftpos != null)       ctx.ftpos.setPos(it.ftn.convertPos(),pos.length);
      return it.score() == 0 ? null : it;
    }
  }
;
}
