{
  return new Iter(){
    @Override public Item next() throws XQException {
      final FTTokenizer tmp=ctx.ftitem;
      ctx.ftitem=ft;
      final Item i1=ctx.iter(expr[1]).next();
      ctx.ftitem=tmp;
      if (i1.dbl() == 0)       return null;
      return ((FTNodeItem)i1).getDNode();
    }
  }
;
}
