{
  final XQuery query=new XQuery("declare default collation \"http://www.w3.org/2005/xpath-functions/collation/codepoint\"; declare default collation \"http://www.w3.org/2005/xpath-functions/collation/codepoint2\"; default-collation() eq \"http://www.w3.org/2005/xpath-functions/collation/codepoint\"",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XQST0038"));
}
