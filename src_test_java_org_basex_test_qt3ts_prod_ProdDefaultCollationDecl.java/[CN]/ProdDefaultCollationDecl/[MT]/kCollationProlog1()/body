{
  final XQuery query=new XQuery("declare base-uri \"http://www.w3.org/2005/xpath-functions/\"; declare default collation \"collation/codepoint\"; default-collation() eq \"http://www.w3.org/2005/xpath-functions/collation/codepoint\"",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
