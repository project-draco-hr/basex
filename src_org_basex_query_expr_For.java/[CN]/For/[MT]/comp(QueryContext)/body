{
  expr=expr.comp(ctx);
  if (expr.e())   return Seq.EMPTY;
  if (pos == null && score == null && expr.returned(ctx).single && expr.countVar(null) == 0) {
    ctx.compInfo(OPTBIND,var);
    var.bind(expr,ctx);
  }
  ctx.vars.add(var);
  if (pos != null)   ctx.vars.add(pos);
  if (score != null)   ctx.vars.add(score);
  return this;
}
