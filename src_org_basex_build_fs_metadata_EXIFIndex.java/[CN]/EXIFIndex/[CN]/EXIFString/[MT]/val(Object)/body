{
  final byte[] buffer=(byte[])obj[0];
  final int v=((Integer)obj[1]).intValue();
  final int n=((Integer)obj[2]).intValue();
  int s=v - 1;
  int e=v + n;
  while (++s < e)   if (buffer[s] > ' ')   break;
  while (--e > s)   if (buffer[e] > ' ')   break;
  if (s >= e)   return EMPTY;
  return Arrays.copyOfRange(buffer,s,e + 1);
}
