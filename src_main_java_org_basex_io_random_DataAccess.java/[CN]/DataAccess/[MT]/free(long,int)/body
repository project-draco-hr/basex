{
  int os=readNum(pos) + (int)(cursor() - pos);
  cursor(pos + os);
  for (; pos + os < len && os < size && read() == 0xFF; os++)   ;
  long o=pos;
  if (pos + os == len) {
    length(pos);
  }
 else {
    int t=size;
    if (os < size) {
      cursor(pos);
      t=0;
      o=len;
    }
 else {
      cursor(pos + size);
    }
    while (t++ < os)     write1(0xFF);
  }
  return o;
}
