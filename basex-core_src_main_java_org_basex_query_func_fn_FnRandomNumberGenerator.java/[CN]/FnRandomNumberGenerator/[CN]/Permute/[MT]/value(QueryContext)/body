{
  final Value value=qc.get(params[0]).value(qc);
  final int sz=(int)value.size();
  final Item[] items=new Item[sz];
  value.writeTo(items,0);
  for (int i=0; i < sz; i++) {
    final int r=i + random.nextInt(sz - i);
    final Item item=items[i];
    items[i]=items[r];
    items[r]=item;
  }
  return Seq.get(items);
}
