{
  final XQuery query=new XQuery("\n" + "        declare context item as xs:integer external := 17;\n" + "        . = 17\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
