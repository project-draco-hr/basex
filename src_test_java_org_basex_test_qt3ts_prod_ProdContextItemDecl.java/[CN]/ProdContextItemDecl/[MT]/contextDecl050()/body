{
  final XQuery query=new XQuery("\n" + "      \timport module namespace m=\"http://www.w3.org/TestModules/libmodule2\"; \n" + "        declare context item as xs:integer := 23;\n"+ "        . eq 23\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/libmodule2",file("prod/ContextItemDecl/libmodule-2.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
