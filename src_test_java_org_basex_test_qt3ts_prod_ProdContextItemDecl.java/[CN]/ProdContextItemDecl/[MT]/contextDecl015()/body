{
  final XQuery query=new XQuery("\n" + "        declare variable $y := /works/employee;\n" + "        declare context item := $y[9];\n"+ "        declare variable $x external := if (./*) then fn:position() else 0;\n"+ "        ($x, $y)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQDY0054"));
}
