{
  final XQuery query=new XQuery("string(if(boolean((1, current-time())[1] treat as xs:integer)) then true() else false()) eq \"true\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
