{
  final XQuery query=new XQuery("if(boolean((1, 2, 3, current-time())[1] treat as xs:integer)) then true() else 4",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
