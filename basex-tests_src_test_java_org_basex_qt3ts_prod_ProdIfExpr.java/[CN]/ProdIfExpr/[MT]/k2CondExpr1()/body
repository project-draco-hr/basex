{
  final XQuery query=new XQuery("empty(if(<e>{current-time()}</e>) then () else ())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPST0005")));
}
