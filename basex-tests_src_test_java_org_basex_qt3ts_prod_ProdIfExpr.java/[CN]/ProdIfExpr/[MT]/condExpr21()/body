{
  final XQuery query=new XQuery("if( true() ) then 10 cast as xs:double ? else /Folder[1] cast as xs:double ?",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("10"));
}
