{
  final XQuery query=new XQuery("fn:concat(fn:normalize-unicode(\"??\",\" NFC \"),fn:normalize-unicode(\"??\",\"NFC\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"????"));
}
