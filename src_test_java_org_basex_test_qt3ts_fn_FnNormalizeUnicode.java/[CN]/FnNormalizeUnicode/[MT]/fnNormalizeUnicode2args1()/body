{
  final XQuery query=new XQuery("normalize-unicode('Nothing to normalize.', 'NFC')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Nothing to normalize."));
}
