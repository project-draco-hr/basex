{
  query("insert node <f xmlns='F'/> into doc('d2')//*:e","");
  try {
    new Open("d2").execute(context);
    assertEquals("\n" + "  Pre[1] xmlns=\"A\" \n" + "    Pre[4] xmlns=\"D\" \n"+ "      Pre[5] xmlns=\"G\" \n"+ "    Pre[7] xmlns=\"F\" ",context.data.ns.toString());
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
 finally {
    try {
      new Close().execute(context);
    }
 catch (    BaseXException e) {
    }
  }
}
