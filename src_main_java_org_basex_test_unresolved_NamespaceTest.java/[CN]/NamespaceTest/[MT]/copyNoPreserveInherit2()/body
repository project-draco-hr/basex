{
  query("declare namespace my='ns'; " + "declare namespace my2='ns2'; " + "declare copy-namespaces no-preserve,inherit; "+ "let $og := <my:a xmlns='ns2'><n/></my:a> "+ "let $f:= (copy $c := $og/my2:n modify () return $c) "+ "return (namespace-uri-for-prefix('my',$f), "+ "namespace-uri-for-prefix('',$f))","ns2");
}
