{
  final XQuery query=new XQuery("map:entry(3, 5)",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:integer, xs:integer)")&& assertType("function(xs:anyAtomicType) as xs:integer?")&& assertQuery("$result(3) eq 5")&& assertQuery("map:size($result) eq 1")&& assertQuery("map:size(map:remove($result, 3)) eq 0")&& assertQuery("map:size(map:remove($result, 1)) eq 1")));
}
