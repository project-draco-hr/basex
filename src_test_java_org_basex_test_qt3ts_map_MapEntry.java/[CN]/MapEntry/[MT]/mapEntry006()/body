{
  final XQuery query=new XQuery("map:entry(xs:float('NaN'), 'NaN')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertQuery("map:size($result) eq 0")));
}
