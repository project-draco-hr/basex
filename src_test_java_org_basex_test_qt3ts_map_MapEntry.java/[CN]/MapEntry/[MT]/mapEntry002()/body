{
  final XQuery query=new XQuery("map:entry(3, ())",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:integer, empty-sequence())")&& assertQuery("empty($result(3))")&& assertQuery("map:size($result) eq 1")&& assertQuery("map:size(map:remove($result, 3)) eq 0")&& assertQuery("map:size(map:remove($result, 1)) eq 1")));
}
