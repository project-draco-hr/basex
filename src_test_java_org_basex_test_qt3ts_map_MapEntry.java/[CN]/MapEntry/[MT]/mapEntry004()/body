{
  final XQuery query=new XQuery("map:entry(xs:untypedAtomic(\"foo\"), map{})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:string, map(*))")&& assertQuery("empty($result(\"bar\"))")&& assertQuery("count($result(\"foo\")) eq 1")&& assertQuery("empty($result(\"foo\")(\"bar\"))")&& assertQuery("empty($result(xs:untypedAtomic(\"foo\"))(\"bar\"))")));
}
