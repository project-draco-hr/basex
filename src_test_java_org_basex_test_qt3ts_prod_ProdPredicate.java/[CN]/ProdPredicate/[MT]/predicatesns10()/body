{
  final XQuery query=new XQuery("(/root/decimal[(xs:decimal(.) = 12678967.543233)])",ctx);
  try {
    query.context(node(file("docs/atomicns.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<decimal attr=\"12678967.543233\">12678967.543233</decimal>",false));
}
