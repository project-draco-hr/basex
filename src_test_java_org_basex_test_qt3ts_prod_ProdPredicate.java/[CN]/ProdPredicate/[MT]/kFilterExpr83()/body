{
  final XQuery query=new XQuery("deep-equal((1, 2), (1, 2)[remove((true(), \"a string\"), 2)])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPTY0004")));
}
