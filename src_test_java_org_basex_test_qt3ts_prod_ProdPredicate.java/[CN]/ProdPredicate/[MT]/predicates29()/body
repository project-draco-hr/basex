{
  final XQuery query=new XQuery("let $foo := <element1><element2>some content</element2></element1> return $foo[(2 to 5)]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FORG0006"));
}
