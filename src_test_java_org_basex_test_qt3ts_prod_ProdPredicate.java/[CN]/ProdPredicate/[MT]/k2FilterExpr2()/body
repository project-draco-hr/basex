{
  final XQuery query=new XQuery("declare variable $var := (for $i in 1 to 100 return <e>{$i}</e>); $var[5]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e>5</e>",false));
}
