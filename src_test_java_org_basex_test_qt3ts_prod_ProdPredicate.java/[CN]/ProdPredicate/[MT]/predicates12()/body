{
  final XQuery query=new XQuery("(/root/double[xs:double(.) = 1267.43233E12])",ctx);
  try {
    query.context(node(file("docs/atomicns.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<double>1267.43233E12</double>",false));
}
