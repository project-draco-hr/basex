{
  final XQuery query=new XQuery("deep-equal((0, 1, 2), (0, 1, 2)[if(. eq 8) then \"str\" else position()])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
