{
  final XQuery query=new XQuery("(/works//hours[xs:integer(.) <= 12])",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<hours>12</hours>",false));
}
