{
  final byte[] str=checkEStr(expr[0],ctx);
  String nr=null;
  if (expr.length == 2) {
    final String n=string(uc(trim(checkStr(expr[1],ctx))));
    for (    final String nrm : NORMS)     if (nrm.equals(n))     nr=nrm;
    if (nr == null)     Err.or(input,NORMUNI,n);
  }
  return Str.get(str);
}
