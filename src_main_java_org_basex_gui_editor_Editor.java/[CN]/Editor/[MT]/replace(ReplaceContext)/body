{
  try {
    rend.replace(rc);
    if (rc.text != null) {
      setText(rc.text);
      release(Action.CHECK);
    }
    gui.status.setText(Util.info(STRINGS_REPLACED_X,rc.search.nr));
  }
 catch (  final Exception ex) {
    gui.status.setText(ERROR_C + ex.getMessage().replaceAll(Prop.NL + ".*",""));
  }
}
