{
  try {
    rend.replace(rc);
    if (rc.text != null) {
      setText(rc.text);
      release(Action.CHECK);
    }
    gui.status.setText(Util.info(STRINGS_REPLACED_X,rc.nr()));
  }
 catch (  final Exception ex) {
    final String msg=ex.getMessage().replaceAll(Prop.NL + ".*","");
    gui.status.setError(REGULAR_EXPR + COLS + msg);
  }
}
