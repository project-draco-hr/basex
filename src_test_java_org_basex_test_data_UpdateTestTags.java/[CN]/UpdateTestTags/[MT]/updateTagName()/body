{
  final Data data=CONTEXT.data();
  data.startUpdate();
  data.update(6,Data.ELEM,JUNIT,Token.EMPTY);
  data.finishUpdate();
  assertEquals(Data.ELEM,data.kind(6));
  assertArraysEquals(JUNIT,data.name(6,Data.ELEM));
  reload();
  assertEquals(Data.ELEM,data.kind(6));
  assertArraysEquals(JUNIT,data.name(6,Data.ELEM));
}
