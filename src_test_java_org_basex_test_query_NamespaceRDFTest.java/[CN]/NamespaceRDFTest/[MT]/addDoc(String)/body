{
  final StringBuilder qs=new StringBuilder();
  qs.append("<rdf:RDF xmlns:rdf=\"http://www.w3.org/1999/02/22-rdf-syntax-" + "ns#\" xmlns:core=\"http://rdfs.cyim.com/CoreContent#\">\n");
  qs.append(String.format("  <core:Container rdf:about=\"%s\"/>\n",uri));
  qs.append("</rdf:RDF>");
  final InputStream in=new ByteArrayInputStream(qs.toString().getBytes("UTF-8"));
  try {
    session.add(uri,in);
  }
  finally {
    in.close();
  }
}
