{
  final Serializer ser=new BuilderSerializer(build){
    @Override protected void start(    final byte[] t) throws IOException {
      super.start(t);
      pre++;
    }
    @Override protected void openDoc(    final byte[] name) throws IOException {
      super.openDoc(name);
      pre++;
    }
  }
;
  final IntList il=old.doc();
  for (int i=0, is=il.size(); i < is; i++)   ser.node(old,il.get(i));
}
