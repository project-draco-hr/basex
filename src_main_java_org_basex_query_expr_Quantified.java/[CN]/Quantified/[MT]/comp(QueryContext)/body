{
  final int vs=ctx.vars.size();
  for (  final For f : fl)   f.comp(ctx);
  sat=checkUp(sat,ctx).comp(ctx);
  ctx.vars.reset(vs);
  Expr e=null;
  for (  final For f : fl) {
    if (f.expr.empty()) {
      e=f;
      break;
    }
  }
  if (e == null && sat.empty())   e=sat;
  if (e == null)   return this;
  ctx.compInfo(every ? OPTTRUE : OPTFALSE,e);
  return Bln.get(every);
}
