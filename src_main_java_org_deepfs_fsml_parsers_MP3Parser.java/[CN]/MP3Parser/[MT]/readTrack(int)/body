{
  final byte[] value=readText(s);
  final int size=value.length;
  int i=0;
  while (i < size && (value[i] < '1' || value[i] > '9'))   value[i++]=0;
  final int start=i;
  while (i < size && value[i] >= '0' && value[i] <= '9')   i++;
  final int track=toInt(value,start,i);
  if (track == Integer.MIN_VALUE)   throw new ParserException("Failed to parse track number");
  return track;
}
