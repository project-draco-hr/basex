{
  setTypeAndFormat();
  final int size=readID3v2Header();
  int s=size;
  while (s >= MINIMAL_FRAME_SIZE) {
    final int res=readID3v2Frame();
    if (res > 0) {
      s-=res;
    }
 else {
      s+=res;
      if (s < MINIMAL_FRAME_SIZE)       break;
      bfc.skip(-res);
    }
  }
  final long pos=bfc.position();
  s=size;
  if (checkID3v1())   s+=128;
  bfc.position(pos);
  techInfo(s);
}
