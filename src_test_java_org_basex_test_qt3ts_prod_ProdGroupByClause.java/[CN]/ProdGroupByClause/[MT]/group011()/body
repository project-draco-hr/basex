{
  final XQuery query=new XQuery("\n" + "        for $a at $p in 1 to 10\n" + "        let $g := $p mod 2\n"+ "        group by $g\n"+ "        return string-join($p!string(), ' ')  \n"+ "     ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertPermutation("\"1 3 5 7 9\", \"2 4 6 8 10\""));
}
