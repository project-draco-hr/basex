{
  final XQuery query=new XQuery(" \n" + "            for $x in //employee \n" + "            let $key := $x/hours[1] \n"+ "            group by $key \n"+ "            where count($x) gt 2 \n"+ "            return concat($key, ':', count($x)) \n"+ "      ",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertPermutation("\"40:3\", \"80:3\", \"20:5\""));
}
