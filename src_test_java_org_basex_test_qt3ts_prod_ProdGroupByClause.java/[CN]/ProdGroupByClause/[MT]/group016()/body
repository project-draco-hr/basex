{
  final XQuery query=new XQuery("\n" + "       count(\n" + "         for $y in 1 to 10\n"+ "         group by $y := $y, $y := $y mod 2\n"+ "         return $y\n"+ "       )\n"+ "     ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("2"));
}
