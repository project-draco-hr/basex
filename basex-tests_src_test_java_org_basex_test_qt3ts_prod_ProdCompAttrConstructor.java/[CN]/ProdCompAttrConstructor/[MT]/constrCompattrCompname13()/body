{
  final XQuery query=new XQuery("<elem xmlns:foo=\"http://www.example.com/foo\">{element elem {attribute {'foo:attr'} {}}}</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem xmlns:foo=\"http://www.example.com/foo\"><elem foo:attr=\"\"/></elem>",false));
}
