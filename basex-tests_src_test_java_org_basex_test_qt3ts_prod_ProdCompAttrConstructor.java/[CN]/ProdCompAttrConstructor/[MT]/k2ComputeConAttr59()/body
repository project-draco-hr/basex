{
  final XQuery query=new XQuery("<a> { attribute xml:id {\"1\"} } </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<a xml:id=\"1\"/>",false) || error("XQDY0091")));
}
