{
  final XQuery query=new XQuery("<a>{(<?target content?>, attribute name{\"content\"})[2]} </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a name=\"content\"/>",false));
}
