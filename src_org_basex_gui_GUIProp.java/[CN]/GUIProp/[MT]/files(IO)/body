{
  final StringList sl=new StringList();
  String path=null;
  if (file != null) {
    path=file.path();
    xqpath=file.getDir();
    Prop.xquery=file;
    sl.add(path);
  }
  for (int q=0; q < queries.length && q < 9; q++) {
    final String f=queries[q];
    if (!f.equals(path) && IO.get(f).exists())     sl.add(f);
  }
  queries=sl.finish();
}
