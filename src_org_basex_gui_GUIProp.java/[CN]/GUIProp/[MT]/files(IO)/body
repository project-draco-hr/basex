{
  final StringList sl=new StringList();
  String path=null;
  if (file != null) {
    path=file.path();
    set(XQPATH,file.getDir());
    sl.add(path);
  }
  final String[] qu=strings(QUERIES);
  for (int q=0; q < qu.length && q < 9; q++) {
    final String f=qu[q];
    if (!f.equalsIgnoreCase(path) && IO.get(f).exists())     sl.add(f);
  }
  set(QUERIES,sl.finish());
}
