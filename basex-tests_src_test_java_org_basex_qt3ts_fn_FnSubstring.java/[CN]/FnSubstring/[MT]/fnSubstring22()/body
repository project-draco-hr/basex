{
  final XQuery query=new XQuery("concat('#', fn:substring(./concepts/@id, string-length(./concepts/@id) - 18, 1), '#')",ctx);
  try {
    query.context(node(file("fn/substring/concepts.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"##"));
}
