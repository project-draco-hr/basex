{
  final XQuery query=new XQuery("-1.7976931348623157E+308 castable as xs:decimal",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || assertBoolean(true)));
}
