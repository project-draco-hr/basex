{
  final XQuery query=new XQuery("xs:string(\"http://www.example.com/~b%C3%A9b%C3%A9\") castable as xs:anyURI",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
