{
  final XQuery query=new XQuery("\n" + "        declare variable $s := \"http://saxon.sf.net/\"; \n" + "        declare variable $xsl := \"http://www.w3.org/1999/XSL/Transform\"; \n"+ "        element {QName(\"http://saxon.sf.net/\", \"saxon:extension\")} { namespace saxon {$s}, attribute a {23}, namespace xsl {$xsl}, element f {42} }\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<saxon:extension xmlns:saxon=\"http://saxon.sf.net/\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\" \n            a=\"23\"><f>42</f></saxon:extension>",false));
}
