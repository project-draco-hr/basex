{
  final XQuery query=new XQuery("\n" + "        import module namespace mod1=\"http://www.w3.org/TestModules/cnc-module\";\n" + "        let $ns := mod1:one()\n"+ "        return ($ns is $ns, $ns is mod1:one())\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/cnc-module",file("prod/CompNamespaceConstructor/cnc-module.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"true false"));
}
