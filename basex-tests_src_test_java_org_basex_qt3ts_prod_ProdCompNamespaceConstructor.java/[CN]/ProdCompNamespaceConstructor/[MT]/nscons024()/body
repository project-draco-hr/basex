{
  final XQuery query=new XQuery("\n" + "        <e>{ namespace z { \"http://www.zorba-xquery.com/\" }, element z:e {} }</e>\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0081"));
}
