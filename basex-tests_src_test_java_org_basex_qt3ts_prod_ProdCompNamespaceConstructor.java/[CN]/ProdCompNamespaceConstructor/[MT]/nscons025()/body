{
  final XQuery query=new XQuery("\n" + "        element e { attribute z:a {},  namespace z { \"http://www.zorba-xquery.com/\" } }\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0081"));
}
