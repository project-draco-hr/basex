{
  final XQuery query=new XQuery("\n" + "      let $d := document { <root xml:base=\"http://www.w3.org/\"> <implicit-base><child /></implicit-base> <explicit-base xml:base=\"http://www.w3.org/TR/xquery\"><child /></explicit-base> </root> } \n" + "      return let $y := <copy xml:base=\"http://www.example.org\"> { $d/root/explicit-base } </copy> \n"+ "      return fn:base-uri(($y/explicit-base/child)[1])\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.w3.org/TR/xquery"));
}
