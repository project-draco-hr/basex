{
  final XQuery query=new XQuery("fn:count((document {<aDocument>some content</aDocument>})/base-uri())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") || assertEq("1")));
}
