{
  final XQuery query=new XQuery("declare base-uri \"http://example.com/BASEURI\"; base-uri(document {()})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://example.com/BASEURI"));
}
