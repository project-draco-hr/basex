{
  super.close();
  final String in1=input1.getText();
  final String in2=Token.string(input2.getText());
  for (int i=1; i < EDITKIND.length; i++)   if (radio[i].isSelected())   kind=i;
  result=new StringList();
switch (kind) {
case Data.ATTR:
case Data.PI:
    result.add(in1);
  result.add(in2);
break;
case Data.ELEM:
result.add(in1);
break;
case Data.TEXT:
case Data.COMM:
result.add(in2);
break;
}
lkind=kind;
}
