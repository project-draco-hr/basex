{
  super.close();
  final String in1=input.getText().replaceAll("\"","\\\"");
  final String in2=input2.getText().replaceAll("\"","\\\"");
  query=null;
  for (int i=1; i < KINDS.length; i++) {
    if (radio[i].isSelected()) {
      query=KINDS[i];
      if (i != 2 && i != 4)       query+=" \"" + in1 + "\"";
      if (i != 1)       query+=" \"" + in2 + "\"";
    }
  }
}
