{
  try {
    store.sync();
    texts.flush();
    values.flush();
    final DataOutput out=new DataOutput(meta.dbname,DATAINFO);
    meta.finish(out);
    tags.finish(out);
    atts.finish(out);
    skel.finish(out);
    ns.finish(out);
    out.close();
  }
 catch (  DatabaseException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
}
