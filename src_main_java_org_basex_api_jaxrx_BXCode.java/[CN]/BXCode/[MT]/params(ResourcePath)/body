{
  String ser=path.getValue(QueryParameter.OUTPUT).replaceAll("\1",",");
  if (ser == null)   ser="";
  final String wrap=path.getValue(QueryParameter.WRAP);
  if (wrap == null || wrap.equals(DataText.YES)) {
    ser+="," + SerializerProp.S_WRAP_PRE[0] + "="+ JAXRX+ ","+ SerializerProp.S_WRAP_URI[0]+ "="+ URL;
  }
 else   if (!wrap.equals(DataText.NO)) {
    throw new JaxRxException(400,SerializerProp.error(QueryParameter.WRAP.toString(),DataText.YES,DataText.NO));
  }
  return ser;
}
