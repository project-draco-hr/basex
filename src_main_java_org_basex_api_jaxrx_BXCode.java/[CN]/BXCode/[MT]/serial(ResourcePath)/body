{
  String ser=path.getValue(QueryParameter.OUTPUT);
  if (ser == null)   ser="";
  final String wrap=path.getValue(QueryParameter.WRAP);
  if (wrap == null || wrap.equals(DataText.YES)) {
    ser+="," + SerializerProp.S_WRAP_PREFIX[0] + "="+ JAXRX+ ","+ SerializerProp.S_WRAP_URI[0]+ "="+ URL;
  }
 else   if (!wrap.equals(DataText.NO)) {
    throw new JaxRxException(400,SerializerProp.error(QueryParameter.WRAP,wrap,DataText.YES,DataText.NO).getMessage());
  }
  return ser.replaceAll("^,","");
}
