{
  final XQuery query=new XQuery("fn:concat(xs:positiveInteger(\"52704602390610033\"),xs:positiveInteger(\"1\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"527046023906100331"));
}
