{
  final XQuery query=new XQuery("fn:concat(xs:short(\"-32768\"),xs:short(\"-32768\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-32768-32768"));
}
