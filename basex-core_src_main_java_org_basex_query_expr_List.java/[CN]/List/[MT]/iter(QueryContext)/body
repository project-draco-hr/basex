{
  return new Iter(){
    Iter ir;
    int e;
    @Override public Item next() throws QueryException {
      while (true) {
        if (ir == null) {
          if (e == exprs.length)           return null;
          ir=qc.iter(exprs[e++]);
        }
        final Item it=ir.next();
        if (it != null)         return it;
        ir=null;
      }
    }
    @Override public boolean reset(){
      ir=null;
      e=0;
      return true;
    }
  }
;
}
