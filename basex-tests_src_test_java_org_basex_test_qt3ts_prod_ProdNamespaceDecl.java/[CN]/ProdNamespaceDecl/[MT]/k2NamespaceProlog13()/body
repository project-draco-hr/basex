{
  final XQuery query=new XQuery("<e/>/<e a=\"{p:asd}\" xmlns:p=\"http://example.com/asd\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e xmlns:p=\"http://example.com/asd\" a=\"\"/>",false));
}
