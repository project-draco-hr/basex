{
  final XQuery query=new XQuery("declare namespace foo = \"http://example.org\"; <foo:bar> Lentils </foo:bar>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<foo:bar xmlns:foo=\"http://example.org\"> Lentils </foo:bar>",false));
}
