{
  final Args a=new Args(args,this,WEBDAVINFO);
  while (a.more()) {
    final char c=a.next();
switch (c) {
case 'h':
      set(DBHOST,a.string());
    break;
case 'r':
  set(DBPORT,a.string());
break;
case 'u':
set(DBUSER,a.string());
break;
case 'p':
set(DBPASS,a.string());
break;
case 'w':
set(WEBDAVPORT,a.string());
break;
case 's':
standalone=true;
break;
default :
break;
}
}
return true;
}
