{
  final Args arg=new Args(args,this,Text.WEBDAVINFO,Util.info(CONSOLE,WEBDAV));
  while (arg.more()) {
    if (arg.dash()) {
      final char c=arg.next();
switch (c) {
case 'n':
        set(DBHOST,arg.string());
      break;
case 'P':
    set(DBPASS,arg.string());
  break;
case 'r':
set(DBPORT,arg.string());
break;
case 's':
standalone=true;
break;
case 'U':
set(DBUSER,arg.string());
break;
case 'w':
set(WEBDAVPORT,arg.string());
break;
default :
arg.usage();
}
}
 else {
arg.usage();
}
}
}
