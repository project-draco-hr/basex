{
  final Data data=CONTEXT.data;
  data.rename(8,Data.ATTR,token("id"));
  data.replace(8,Data.ATTR,token("junit"));
  assertEquals(size,data.meta.size);
  assertByteArraysEqual(token("junit"),data.text(8,false));
  reload();
  assertEquals(size,data.meta.size);
  assertByteArraysEqual(token("junit"),data.text(8,false));
}
