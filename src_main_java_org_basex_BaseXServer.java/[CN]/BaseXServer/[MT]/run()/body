{
  running=true;
  while (running) {
    try {
      final ServerProcess s=new ServerProcess(server.accept(),this);
      if (stop.exists()) {
        if (!stop.delete())         log.write(Util.info(DBNOTDELETED,stop));
        quit(false);
      }
 else       if (s.init()) {
        context.add(s);
      }
    }
 catch (    final IOException ex) {
      break;
    }
  }
}
