{
  final XQuery query=new XQuery("\n" + "        declare variable $i := <root> <child/> <child/> <child> <child2> <child3> <leaf/> </child3> </child2> </child> </root>; \n" + "        root($i)//count(following-sibling::node())\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"0 2 1 0 0 0 0"));
}
