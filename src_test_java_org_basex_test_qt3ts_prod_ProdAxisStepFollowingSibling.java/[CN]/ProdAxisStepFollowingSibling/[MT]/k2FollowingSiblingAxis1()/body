{
  final XQuery query=new XQuery("<root> <child/> <child/> <child/> </root>/child[1]/following-sibling::node()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<child/><child/>",false));
}
