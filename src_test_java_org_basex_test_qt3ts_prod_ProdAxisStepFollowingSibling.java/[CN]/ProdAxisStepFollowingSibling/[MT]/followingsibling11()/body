{
  final XQuery query=new XQuery("(/works/employee[12]/*/day[1]/following-sibling::day) | (/works/employee[12]/*/day[1]/following-sibling::day)",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<day>Tuesday</day>",false));
}
