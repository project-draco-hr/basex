{
  int d=twoDigits();
  if (d == -1)   return false;
  BigDecimal bd=BigDecimal.valueOf(d);
  if (consume('.')) {
    if (!digit(curr()))     throw error("digit");
    BigDecimal f=BigDecimal.TEN;
    while (digit(curr())) {
      bd=bd.add(BigDecimal.valueOf(number()).divide(f));
      f=f.multiply(BigDecimal.TEN);
    }
  }
  seconds=bd;
  return true;
}
