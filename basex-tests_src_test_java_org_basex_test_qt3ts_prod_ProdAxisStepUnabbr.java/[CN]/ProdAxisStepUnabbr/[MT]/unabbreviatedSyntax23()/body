{
  final XQuery query=new XQuery("for $h in (/works/employee) return $h/child::hours[fn:position() > 1]",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<hours>20</hours><hours>40</hours><hours>30</hours>",false));
}
