{
  flush();
  super.close();
  final DataOutput info=new DataOutput(name,file + 'i');
  info.writeNum(bcount);
  info.writeNum(bcount);
  info.writeNum(fpre);
  info.writeNum(bcount);
  for (int i=0; i < bcount; i++)   info.writeNum(firstPres.list[i]);
  info.writeNum(bcount);
  for (int i=0; i < bcount; i++)   info.writeNum(blocks.list[i]);
  info.close();
}
