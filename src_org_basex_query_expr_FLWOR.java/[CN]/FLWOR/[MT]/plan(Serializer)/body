{
  ser.openElement(this,EVAL,ITER);
  for (  final ForLet f : fl)   f.plan(ser);
  if (where != null) {
    ser.openElement(WHR);
    where.plan(ser);
    ser.closeElement();
  }
  if (order != null)   order.plan(ser);
  ser.openElement(RET);
  ret.plan(ser);
  ser.closeElement();
  ser.closeElement();
}
