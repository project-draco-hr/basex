{
  final boolean more=p + 1 != fl.length;
  while (it[p].next().bool()) {
    if (more) {
      iter(ctx,seq,it,p + 1);
    }
 else {
      if (where == null || where.ebv(ctx).bool()) {
        if (group != null)         group.add(ctx);
        if (order != null)         order.add(ctx);
        seq.add(ctx.iter(ret).finish());
      }
    }
  }
}
