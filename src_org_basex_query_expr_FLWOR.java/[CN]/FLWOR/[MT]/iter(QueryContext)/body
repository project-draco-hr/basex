{
  final SeqIter seq=new SeqIter();
  final Iter[] iter=new Iter[fl.length];
  for (int f=0; f < fl.length; f++)   iter[f]=ctx.iter(fl[f]);
  iter(ctx,seq,iter,0);
  if (group != null) {
    System.out.println("Iterating group");
    group.sq=seq;
    return group.iter(ctx);
  }
 else {
    order.sq=seq;
  }
  return order.iter(ctx);
}
