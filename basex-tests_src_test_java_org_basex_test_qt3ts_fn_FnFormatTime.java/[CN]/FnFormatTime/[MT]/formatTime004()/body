{
  final XQuery query=new XQuery("\n" + "        for $i in 1 to 24 return\n" + "        format-time($t + xs:dayTimeDuration('PT1H')*$i, '[h].[m]')",ctx);
  try {
    query.bind("t",new XQuery("xs:time('09:15:06.456')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n         10.15 11.15 12.15 1.15 2.15 3.15 4.15 5.15 6.15 7.15 8.15 9.15 10.15 11.15 \n         12.15 1.15 2.15 3.15 4.15 5.15 6.15 7.15 8.15 9.15\n         "));
}
