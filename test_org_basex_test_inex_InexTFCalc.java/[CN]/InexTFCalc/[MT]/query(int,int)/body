{
  if (session.execute(new XQuery("distinct-values((for $i in //*[text() ftcontains \"" + words.get(qu) + "\"] return base-uri($i)))"))) {
    session.output(new CachedOutput());
    final String str=session.info();
    final String items=find(str,"Results   : ([0-9]+) Item");
    Main.outln("Query % on %: % items",qu + 1,databases.get(db),items);
    final int n=Integer.parseInt(items);
    if (n > 0)     freq[qu]+=n;
  }
 else   Main.outln(session.info());
}
