{
  final XQuery query=new XQuery("<a> <b c=\"\"> <c/> </b> <d/> </a>/b/c/ancestor::*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a><b c=\"\"><c/></b><d/></a><b c=\"\"><c/></b>",false));
}
