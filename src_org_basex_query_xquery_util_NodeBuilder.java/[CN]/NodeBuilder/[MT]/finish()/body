{
  if (size == 0)   return Seq.EMPTY;
  if (size == 1)   return list[0];
  if (ordered)   new Seq(list,size);
  if (sort)   sort(0,size - 1);
  final SeqBuilder sq=new SeqBuilder();
  sq.a(list[0]);
  for (int s=1; s < size; s++) {
    if (!list[s].is((Node)sq.item[sq.size - 1]))     sq.a(list[s]);
  }
  return sq.finish();
}
