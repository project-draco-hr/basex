{
  out.print(DOCTYPE);
  out.print(' ');
  out.print(t);
  if (te != null) {
    out.print(' ');
    out.print(SYSTEM);
    out.print(' ');
    out.print(te);
  }
  if (ti != null) {
    out.print(' ');
    out.print('"');
    out.print(ti);
    out.print('"');
  }
  out.print(ELEM2);
  out.println();
}
