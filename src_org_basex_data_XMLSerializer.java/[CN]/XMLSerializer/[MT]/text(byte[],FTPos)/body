{
  finishElement();
  int c=-1, pp=0, wl=0;
  final Tokenizer ftt=new Tokenizer(b,null);
  while (ftt.more()) {
    c++;
    for (int i=wl; i < ftt.p; ) {
      if (ftChar(cp(b,i)) && pp < ftp.pos.length && c == ftp.pos[pp]) {
        out.write(0x10 + (ftp.poi[pp++] & 0x0F));
      }
      int cl=cl(b[i]);
      while (cl-- != 0)       ch(b[i++]);
    }
    wl=ftt.p;
  }
  while (wl < b.length)   ch(b[wl++]);
  indent=false;
}
