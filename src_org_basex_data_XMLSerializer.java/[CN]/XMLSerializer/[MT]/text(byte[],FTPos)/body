{
  finishElement();
  int c=-1, wl=0;
  final Tokenizer ftt=new Tokenizer(b,null);
  while (ftt.more()) {
    c++;
    for (int i=wl; i < ftt.p; ) {
      if (ftChar(cp(b,i)) && ftp.contains(c))       out.write(0x10);
      int cl=cl(b[i]);
      while (cl-- != 0)       ch(b[i++]);
    }
    wl=ftt.p;
  }
  while (wl < b.length)   ch(b[wl++]);
  indent=false;
}
