{
  finishElement();
  int c=-1, pp=0, wl=0;
  FTTokenizer ftt=new FTTokenizer(b);
  while (ftt.more()) {
    c++;
    for (int i=wl; i < ftt.p; i++) {
      if (Token.letterOrDigit(b[i]) && pp < pos.length && c == pos[pp]) {
        ft.addTextPos(out.size(),poi[pp++]);
      }
      ch(b[i]);
    }
    wl=ftt.p;
  }
  while (wl < b.length)   ch(b[wl++]);
  indent=false;
}
