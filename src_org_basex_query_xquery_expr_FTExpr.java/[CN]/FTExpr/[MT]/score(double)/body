{
  return new FTNodeIter(){
    private boolean more;
    @Override public FTNodeItem next(){
      more^=true;
      if (!more)       return null;
      final FTNodeItem ftn=new FTNodeItem();
      ftn.score(s);
      return ftn;
    }
    @Override public long size(){
      return 1;
    }
  }
;
}
