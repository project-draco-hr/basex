{
  final XQuery query=new XQuery("fn:filter( function($a as item()) as xs:boolean? { () }, () )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPST0005") || assertEmpty()));
}
