{
  final XQuery query=new XQuery("let $data := (/employees)\n" + "              return filter(function($x as element(emp)){xs:int($x/@salary) lt 300}, $data/emp)",ctx);
  query.context(node(file("fn/filter/filter003.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertCount(3) && assertType("element(emp)*") && assertQuery("$result/@name = 'john'")&& assertQuery("$result/@name = 'anne'")&& assertQuery("$result/@name = 'kumar'")));
}
