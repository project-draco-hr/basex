{
  final String doc="<a><b><c/></b></a>";
  final Data d=data(doc);
  compare(new UpdatePrimitive[]{new InsertAfter(2,d,null,null),new InsertInto(2,d,null,null,false)});
  query(transform(doc,"insert node <smallerpre/> into $input/b," + "insert node <largerpre/> after $input/b"),"<a><b><c/><smallerpre/></b><largerpre/></a>");
}
