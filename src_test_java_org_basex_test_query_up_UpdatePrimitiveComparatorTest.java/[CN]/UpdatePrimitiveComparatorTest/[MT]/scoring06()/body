{
  final String doc="<a><b/></a>";
  final Data d=data(doc);
  compare(new UpdatePrimitive[]{new InsertInto(1,d,null,null,false),new InsertAfter(2,d,null,null)});
  query(transform(doc,"insert node <smallerpre/> after $input/b, insert node <largerpre/> into $input"),"<a><b/><smallerpre/><largerpre/></a>");
}
