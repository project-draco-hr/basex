{
  final int k=data.kind(p);
  final TokenList sl=new TokenList();
  sl.add(p);
  sl.add(p - data.parent(p,k));
  sl.add(data.size(p,k));
  sl.add(data.attSize(p,k));
  final int u=data.uri(p,k);
  if (data.nsFlag(p))   sl.add("+" + u);
 else   sl.add(u);
  sl.add(TABLEKINDS[k]);
  sl.add(replace(chop(k == Data.ELEM ? data.name(p,k) : k != Data.ATTR ? data.text(p,true) : concat(data.name(p,k),ATT1,data.text(p,false),ATT2),64),'\n',' '));
  t.contents.add(sl);
}
