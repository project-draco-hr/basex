{
  final int k=data.kind(p);
  final StringList sl=new StringList();
  sl.add(p);
  sl.add(p - data.parent(p,k));
  sl.add(data.size(p,k));
  sl.add(data.attSize(p,k));
  sl.add(data.uri(p,k));
  sl.add(TABLEKINDS[k]);
  sl.add(string(chop(k == Data.ELEM ? data.name(p,k) : k != Data.ATTR ? data.text(p,true) : concat(data.name(p,k),ATT1,data.text(p,false),ATT2),64)).replaceAll("\n","\\\\n"));
  t.contents.add(sl);
}
