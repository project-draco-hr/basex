{
  final int k=data.kind(p);
  final StringList sl=new StringList();
  sl.add(p);
  sl.add(p - data.parent(p,k));
  sl.add(data.size(p,k));
  sl.add(data.attSize(p,k));
  sl.add(data.tagNS(p));
  sl.add(TABLEKINDS[k]);
  sl.add(string(chop(k == Data.ELEM ? data.tag(p) : k != Data.ATTR ? data.text(p) : concat(data.attName(p),ATT1,data.attValue(p),ATT2),64)).replaceAll("\n","\\\\n"));
  t.contents.add(sl);
}
