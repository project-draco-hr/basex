{
  final XQuery query=new XQuery("\n" + "        <Q1> { for $n in distinct-values( for $i in (//* | //@*) return namespace-uri($i) ) return <ns>{$n}</ns> } </Q1>",ctx);
  query.context(node(file("docs/auction.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<Q1><ns>http://www.example.com/AuctionWatch</ns><ns>http://www.example.com/auctioneers#anyzone</ns><ns>http://www.w3.org/1999/xlink</ns><ns>http://www.w3.org/2001/XMLSchema</ns><ns>http://www.example.com/auctioneers#eachbay</ns><ns>http://www.example.org/music/records</ns><ns>http://www.example.com/auctioneers#yabadoo</ns><ns>http://www.w3.org/XML/1998/namespace</ns></Q1>",false));
}
