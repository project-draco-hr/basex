{
  final XQuery query=new XQuery("codepoints-to-string(one-or-more(string-to-codepoints(\"foo\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"foo"));
}
