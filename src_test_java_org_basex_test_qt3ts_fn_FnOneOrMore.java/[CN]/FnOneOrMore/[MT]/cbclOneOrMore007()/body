{
  final XQuery query=new XQuery("\n" + "      boolean(one-or-more(for $x in (1 to 10)[. div 2 = 1] return true()))\n" + "   ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
