{
  Data data=context.data();
  int[] del=FSUtils.getSpecificFilesOrDirs(data,curDirPre,path);
  long sizeOfNode=0;
  for (  int toDel : del) {
    if (toDel == -1) {
      out.print("rm: '" + path + "' No such file or directory");
      return;
    }
 else {
      toDel-=sizeOfNode;
      if ((FSUtils.isDir(data,toDel) && fRecursive) || (FSUtils.isFile(data,toDel))) {
        try {
          sizeOfNode+=data.size(toDel,Data.ELEM);
          data.delete(toDel);
          data.flush();
        }
 catch (        Exception e) {
          e.printStackTrace();
        }
      }
 else {
        out.print("rm: cannot remove '" + FSUtils.getFileName(data,toDel) + "': Is a directory");
      }
    }
  }
}
