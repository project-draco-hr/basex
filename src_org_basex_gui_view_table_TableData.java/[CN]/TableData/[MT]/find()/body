{
  final Data data=GUI.context.data();
  final boolean root=rows == rootRows;
  final String xpath=Find.findTable(filter,colNames,data.tags.key(rootTag),data,GUIProp.filterrt || root);
  if (xpath.equals(last))   return;
  last=xpath;
  GUI.get().execute(new XPath(xpath.length() != 0 ? xpath : "/"));
}
