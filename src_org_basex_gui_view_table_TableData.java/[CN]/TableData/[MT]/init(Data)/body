{
  roots=new TokenList();
  for (  final byte[] k : data.path.desc(Token.EMPTY,true,true)) {
    int c=0;
    for (    final byte[] kk : data.path.desc(k,true,false)) {
      final Names index=!startsWith(kk,'@') ? data.tags : data.atts;
      if (index.stat(index.id(delete(kk,'@'))).leaf)       c++;
    }
    if (c > 1)     roots.add(k);
  }
  init(data,-1);
}
