{
  final Step s=CmpG.indexStep(expr);
  if (s == null)   return false;
  final boolean text=ic.data.meta.txtindex && s.test.type == Type.TXT;
  final boolean attr=!text && ic.data.meta.atvindex && s.simple(Axis.ATTR,true);
  if (!text && !attr || !mni || !mxi || min == Double.NEGATIVE_INFINITY || max == Double.POSITIVE_INFINITY)   return false;
  final StatsKey key=getKey(ic,text);
  if (key == null)   return false;
  rt=new RangeToken(text,Math.max(min,key.min),Math.min(max,key.max));
  ic.is=rt.min > rt.max || rt.max < key.min || rt.min > key.max ? 0 : ic.data.meta.size / 5;
  return true;
}
