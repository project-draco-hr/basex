{
  final Ann ann=func.ann;
  final int as=ann.size();
  boolean found=false;
  for (int a=0; a < as; a++) {
    final QNm nm=ann.names[a];
    if (eq(nm.uri(),XQUNITURI) && eq(nm.local(),name)) {
      if (found)       BXUN_TWICE.thrw(info,'%',nm.local());
      found=true;
    }
  }
  return found;
}
