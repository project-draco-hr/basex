{
  final Code code=new Code(){
    Object run() throws XMLDBException {
      return coll.getServices();
    }
  }
;
  boolean xpath=false;
  for (  final Service s : (Service[])code.run()) {
    xpath|=s instanceof XPathQueryService;
  }
  assertTrue("XPath Implementation expected.",xpath);
  checkClosed(code);
}
