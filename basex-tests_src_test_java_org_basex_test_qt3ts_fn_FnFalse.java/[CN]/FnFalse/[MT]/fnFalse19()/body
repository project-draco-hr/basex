{
  final XQuery query=new XQuery("fn:concat(xs:string(fn:false()),xs:string(fn:false()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"falsefalse"));
}
