{
  final XQuery query=new XQuery("\n" + "        declare construction preserve; \n" + "        let $anElement := element someElement{\"some content\"} \n"+ "        return ($anElement instance of element(*,xs:anyType)) and fn:true()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
