{
  final XQuery query=new XQuery("let $tz := xs:dayTimeDuration(\"-PT10H\") \n" + "         return fn:adjust-time-to-timezone(xs:time(\"10:00:00-07:00\"), $tz)\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"07:00:00-10:00"));
}
