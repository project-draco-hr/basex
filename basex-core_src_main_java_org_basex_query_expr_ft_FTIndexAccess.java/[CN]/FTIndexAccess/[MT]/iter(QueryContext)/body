{
  final FTIter ir=ftexpr.iter(qc);
  final boolean scoring=qc.scoring;
  return new NodeIter(){
    @Override public ANode next() throws QueryException {
      final FTNode it=ir.next();
      if (it != null) {
        if (qc.ftPosData != null)         qc.ftPosData.add(it.data,it.pre,it.all);
        if (scoring)         it.score();
        it.all=null;
      }
      return it;
    }
  }
;
}
