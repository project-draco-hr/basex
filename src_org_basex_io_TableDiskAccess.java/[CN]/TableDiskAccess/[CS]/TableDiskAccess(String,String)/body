{
  db=nm;
  pref=f;
  final DataInput in=new DataInput(nm,f + 'i');
  nrBlocks=in.readNum();
  indexSize=in.readNum();
  count=in.readNum();
  firstPres=in.readNums();
  blocks=in.readNums();
  in.close();
  file=new RandomAccessFile(IO.dbfile(nm,f),"rw");
  readBlock(0,0,indexSize > 1 ? firstPres[1] : count);
}
