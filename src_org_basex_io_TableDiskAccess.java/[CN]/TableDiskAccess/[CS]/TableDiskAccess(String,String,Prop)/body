{
  super(nm,fn,pr);
  final DataInput in=new DataInput(pr.dbfile(nm,fn + 'i'));
  allBlocks=in.readNum();
  blocks=in.readNum();
  size=in.readNum();
  firstPres=in.readNums();
  blockIndex=in.readNums();
  in.close();
  file=new RandomAccessFile(pr.dbfile(nm,fn),"rw");
  readBlock(0,0,blocks > 1 ? firstPres[1] : size);
}
