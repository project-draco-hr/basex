{
  super.comp(ctx);
  if (expr[0] instanceof LocPathAbs)   return this;
  final Item i1=expr[0] instanceof Item ? (Item)expr[0] : null;
  final Item i2=expr[1] instanceof Item ? (Item)expr[1] : null;
