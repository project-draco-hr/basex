{
  if (!(ctx.local.data.meta.ftxindex && expr1 instanceof LocPathRel))   return Integer.MAX_VALUE;
  final LocPath path=(LocPathRel)expr1;
  final Step step=path.steps.last();
  final boolean text=step.test == TestNode.TEXT && step.preds.size() == 0;
  if (!text || !path.checkAxes())   return Integer.MAX_VALUE;
  if (expr1 instanceof FTUnaryNot || expr2 instanceof FTUnaryNot)   return Integer.MAX_VALUE;
  return 1;
}
