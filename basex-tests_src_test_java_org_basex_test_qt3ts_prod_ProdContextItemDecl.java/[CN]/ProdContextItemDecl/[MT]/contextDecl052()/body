{
  final XQuery query=new XQuery("\n" + "        import module namespace m=\"http://www.w3.org/TestModules/libmodule3\"; \n" + "        . eq 23\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/libmodule3",file("prod/ContextItemDecl/libmodule-3.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0113"));
}
