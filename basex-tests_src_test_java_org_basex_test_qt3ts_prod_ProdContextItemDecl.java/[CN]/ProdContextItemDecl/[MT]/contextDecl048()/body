{
  final XQuery query=new XQuery("\n" + "      \timport module namespace m=\"http://www.w3.org/TestModules/libmodule1\"; \n" + "      \t. = 17\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/libmodule1",file("prod/ContextItemDecl/libmodule-1.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0113"));
}
