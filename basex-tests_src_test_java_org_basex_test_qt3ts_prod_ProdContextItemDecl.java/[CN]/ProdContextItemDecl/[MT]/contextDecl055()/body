{
  final XQuery query=new XQuery("\n" + "      declare variable $f := function-lookup(xs:QName(\"fn:\"||$p), 0);\n" + "      declare context item := <e/>;\n"+ "      declare variable $p := \"local-name\";\n"+ "      $f()\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("\"e\""));
}
