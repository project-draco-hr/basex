{
  final TokenBuilder tb=new TokenBuilder(name());
  tb.add("(" + ind.type() + ", ");
  if (ind instanceof RangeToken) {
    final RangeToken rt=(RangeToken)ind;
    tb.add(rt.min + "-" + rt.max);
  }
 else {
    tb.add('"');
    tb.add(ind.get());
    tb.add('"');
  }
  return tb.add(")").toString();
}
