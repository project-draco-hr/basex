{
  return new Iter(){
    final IndexIterator it=data.ids(ind);
    final int s=it.size();
    int p=-1;
    @Override public Item next(){
      ctx.item=++p < s ? new DBNode(data,it.next()) : null;
      return ctx.item;
    }
    @Override public boolean ordered(){
      return true;
    }
  }
;
}
