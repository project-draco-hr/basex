{
  final XQuery query=new XQuery("if(not(count(remove((1, 2, 3, current-time()), 1)))) then 3 else true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
