{
  final XQuery query=new XQuery("if (doc-available('nodocument.xml')) then doc('nodocument.xml') else 10 cast as xs:double",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("10"));
}
