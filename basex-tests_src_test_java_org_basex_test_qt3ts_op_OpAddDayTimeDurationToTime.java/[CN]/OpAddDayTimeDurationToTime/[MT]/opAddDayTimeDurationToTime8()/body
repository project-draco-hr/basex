{
  final XQuery query=new XQuery("(xs:time(\"08:02:06\") + xs:dayTimeDuration(\"-P11DT02H02M\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"06:00:06"));
}
