{
  while (running) {
    try {
      final ServerSession s=new ServerSession(server.accept(),this,info);
      Prop tmp=new Prop();
      if (tmp.is(Prop.STOP)) {
        context.prop.set(Prop.STOP,false);
        context.prop.write();
        quit(false);
      }
      if (s.init())       context.add(s);
    }
 catch (    final IOException ex) {
      break;
    }
  }
}
