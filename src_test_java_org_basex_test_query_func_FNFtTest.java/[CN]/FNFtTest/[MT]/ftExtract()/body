{
  final String fun=check(Function.FTEXTRACT);
  query(fun + "(//*[text() contains text '1'])","<li>Exercise <mark>1</mark></li>");
  query(fun + "(//*[text() contains text '2'], 'b', 20)","<li>Exercise <b>2</b></li>");
  query(fun + "(//*[text() contains text '2'], '_o_', 1)","<li>...<_o_>2</_o_></li>");
  contains(fun + "(//*[text() contains text 'Exercise'], 'b', 1)","<li><b>Exercise</b>...</li>");
}
