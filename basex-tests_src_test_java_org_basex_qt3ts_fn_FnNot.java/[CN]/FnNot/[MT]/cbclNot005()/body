{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) as xs:integer* { 1 to $x }; \n" + "      \tnot(exists(local:f(0)))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
