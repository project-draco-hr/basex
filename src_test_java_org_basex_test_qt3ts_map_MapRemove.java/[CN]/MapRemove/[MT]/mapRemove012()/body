{
  final XQuery query=new XQuery("map:remove(map:new(for $n in 1 to 500000 return map:entry($n, $n+1)), 123456)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("map:contains($result, 1)") && assertQuery("map:contains($result, 500000)") && assertQuery("map:size($result) = 499999")&& assertQuery("not(map:contains($result, 123456))")));
}
