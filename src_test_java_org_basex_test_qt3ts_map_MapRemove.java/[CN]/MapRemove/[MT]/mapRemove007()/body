{
  final XQuery query=new XQuery("map:remove(map:new((map:entry(\"a\", \"1\"), map:entry(\"b\", 2))), \"c\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertQuery("map:keys($result) = \"a\"") && assertQuery("map:keys($result) = \"b\"") && assertQuery("map:size($result) = 2")));
}
