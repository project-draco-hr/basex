{
  final XQuery query=new XQuery("map:remove(map:new((map:entry(12, 1), map:entry(13, 2))), 12e0)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("map:size($result) = 1") && assertType("map(xs:integer, xs:integer)") && assertQuery("map:contains($result, 13)")));
}
