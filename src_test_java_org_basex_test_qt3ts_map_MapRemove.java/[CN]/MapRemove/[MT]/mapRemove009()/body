{
  final XQuery query=new XQuery("map:remove(map:new((map:entry(\"a\",1), map:entry(\"b\",2))), xs:untypedAtomic(\"b\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("map:size($result) = 1") && assertType("map(xs:string, xs:integer)") && assertQuery("map:contains($result, \"a\")")));
}
