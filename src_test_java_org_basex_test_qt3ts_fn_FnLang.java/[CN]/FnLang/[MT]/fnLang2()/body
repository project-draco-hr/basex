{
  final XQuery query=new XQuery("fn:count(fn:lang((), ./langs[1]/para[1]))",ctx);
  try {
    query.context(node(file("fn/lang/lang.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
