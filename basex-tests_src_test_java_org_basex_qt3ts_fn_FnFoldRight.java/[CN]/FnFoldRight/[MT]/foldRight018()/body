{
  final XQuery query=new XQuery("fold-right(function($a, $b){ string-length($a) + $b }, 0, (\"Hello\", \"World\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"10"));
}
