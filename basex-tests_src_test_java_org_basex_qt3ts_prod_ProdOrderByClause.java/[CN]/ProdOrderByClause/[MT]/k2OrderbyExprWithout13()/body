{
  final XQuery query=new XQuery("for $a in (3, 2, 1), $b in (6, 5, 4) stable order by $b return $a",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"3 2 1 3 2 1 3 2 1"));
}
