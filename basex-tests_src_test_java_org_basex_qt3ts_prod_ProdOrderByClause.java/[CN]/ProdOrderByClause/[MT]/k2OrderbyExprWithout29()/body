{
  final XQuery query=new XQuery("<r> { for $i in attribute name {()} order by () return () } </r>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<r/>",false) || error("XPST0005")));
}
