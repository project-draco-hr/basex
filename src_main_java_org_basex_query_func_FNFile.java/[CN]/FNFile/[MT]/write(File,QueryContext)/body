{
  final Iter ir=expr[1].iter(ctx);
  try {
    final BufferedOutputStream out=new BufferedOutputStream(new FileOutputStream(file,true));
    try {
      final XMLSerializer xml=new XMLSerializer(out,FNGen.serialPar(this,2,ctx));
      Item it;
      while ((it=ir.next()) != null)       it.serialize(xml);
      xml.close();
    }
  finally {
      out.close();
    }
  }
 catch (  final IOException e) {
    FILEWRITE.thrw(input,file.getName());
  }
  return null;
}
