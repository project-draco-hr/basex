{
  final TokenBuilder tb=new TokenBuilder();
  if (expr.length == 3) {
    final Iter ir=expr[2].iter(ctx);
    Item n;
    while ((n=ir.next()) != null) {
      final Nod p=checkNode(n);
      if (tb.size() != 0)       tb.add(',');
      tb.add(p.nname()).add('=').add(p.atom());
    }
  }
  return tb;
}
