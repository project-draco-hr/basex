{
  final B64 b64=(B64)checkType(expr[1].item(ctx,input),Type.B6B);
  FileOutputStream out=null;
  try {
    out=new FileOutputStream(file);
    try {
      out.write(b64.getVal());
    }
  finally {
      out.close();
    }
  }
 catch (  IOException ex) {
    Err.or(input,QueryText.FILEWRITE,file.getName());
  }
  return null;
}
