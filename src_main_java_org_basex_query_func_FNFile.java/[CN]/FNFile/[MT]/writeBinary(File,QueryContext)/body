{
  final B64 b64=(B64)checkType(expr[1].item(ctx,input),Type.B6B);
  try {
    final FileOutputStream out=new FileOutputStream(file);
    try {
      out.write(b64.getVal());
    }
  finally {
      out.close();
    }
  }
 catch (  IOException ex) {
    FILEWRITE.thrw(input,file.getName());
  }
  return null;
}
