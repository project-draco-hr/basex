{
  if (file.exists())   if (!file.isDirectory())   Err.or(input,QueryText.FILEEXISTS,file.getName());
  if (includeParents) {
    final File parent=getExistingParent(file);
    if (!parent.canWrite())     Err.or(input,QueryText.MKDIR,file.getPath(),parent.getPath());
    if (!file.mkdirs())     Err.or(input,QueryText.CANNOTMKDIR,file.getPath());
  }
 else {
    if (!file.mkdir())     Err.or(input,QueryText.CANNOTMKDIR,file.getPath());
  }
  return null;
}
