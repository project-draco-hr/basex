{
  if (!same)   return true;
  final IntList il=pos[0];
  int p=-1, q=0;
  for (int i=0; i < il.size && p != q; i++) {
    p=calcPosition(il.list[i],sdunit);
    q=p;
    for (int j=1; j < size && p == q; j++) {
      for (int k=0; k < pos[j].size; k++) {
        q=calcPosition(pos[j].list[k],sdunit);
        if (p == q)         break;
      }
    }
  }
  return p == q;
}
