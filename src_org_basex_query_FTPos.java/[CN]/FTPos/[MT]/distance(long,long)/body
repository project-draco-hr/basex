{
  if (dunit == null)   return true;
  final IntList[] il=sortPositions();
  IntList p=il[0];
  IntList pp=il[1];
  boolean b=false;
  for (int z=0; z < pp.size; z++) {
    BoolList bl=new BoolList(size);
    if (checkDist(z,p,pp,mn,mx,bl,true)) {
      b=true;
      break;
    }
  }
  return b;
}
