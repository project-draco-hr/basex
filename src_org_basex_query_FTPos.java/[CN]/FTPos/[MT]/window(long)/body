{
  if (wunit == null)   return true;
  int l=-1;
  for (int i=0; i < size; i++) {
    boolean o=false;
    for (int j=0; j < pos[i].size; j++) {
      final int p=calcPosition(pos[i].get(j),wunit);
      if (i == 0 || (Math.abs(p - l) - 1 < win)) {
        o=true;
        l=p;
        break;
      }
    }
    if (!o)     return false;
  }
  return true;
}
