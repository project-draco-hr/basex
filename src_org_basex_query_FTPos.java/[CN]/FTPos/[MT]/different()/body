{
  if (!different)   return true;
  int l=-1;
  for (int i=0; i < size; i++) {
    boolean o=false;
    for (int j=0; j < pos[i].size; j++) {
      final int p=calcPosition(pos[i].get(j),sdunit);
      if (i != 0 && p != l) {
        o=true;
        break;
      }
      l=p;
    }
    if (i != 0 && !o)     return false;
  }
  return true;
}
