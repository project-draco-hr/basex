{
  final XQuery query=new XQuery("( fn:unparsed-text-lines( \n" + "                if (current-date() eq xs:date('1900-01-01'))\n" + "                then \"http://www.w3.org/fots/unparsed-text/text-plain-utf-8.txt\"\n"+ "                else 1 ),\n"+ "              fn:unparsed-text-lines( \n"+ "                if (current-date() eq xs:date('1900-01-01'))\n"+ "                then 1\n"+ "                else \"http://www.w3.org/fots/unparsed-text/text-plain-utf-8.txt\") )",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XPTY0004"));
}
