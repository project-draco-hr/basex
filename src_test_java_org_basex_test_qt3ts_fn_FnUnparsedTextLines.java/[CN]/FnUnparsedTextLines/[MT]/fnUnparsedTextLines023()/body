{
  final XQuery query=new XQuery("fn:unparsed-text-lines(\"unparsed-text/text-plain-utf-8.txt\")",ctx);
  try {
    query.baseURI("#UNDEFINED");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0001"));
}
