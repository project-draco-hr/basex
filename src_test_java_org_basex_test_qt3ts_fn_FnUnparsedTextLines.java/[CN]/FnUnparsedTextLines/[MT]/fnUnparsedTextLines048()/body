{
  final XQuery query=new XQuery("fn:unparsed-text(\"http://www.w3.org/fots/unparsed-text/text-xml-iso-8859-1.xml\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"<?xml version=\"1.0\" encoding=\"iso-8859-1\"?><text>hello??world</text>") || error("FOUT1190")));
}
