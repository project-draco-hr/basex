{
  final XQuery query=new XQuery("\n" + "        declare variable $n as xs:integer external := 0; \n" + "        <out>{ string-join(for $x allowing empty in 1 to $n return concat('[',$x,']'), '|') }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out>[]</out>",false));
}
