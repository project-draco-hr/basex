{
  final XQuery query=new XQuery("\n" + "        declare variable $n as xs:integer external := 0; \n" + "        <out>{ for $x as xs:integer? allowing empty at $p in 1 to $n \n"+ "               for $y as xs:integer? allowing empty at $q in (if (empty($x)) then () else (1 to $x)) \n"+ "               where deep-equal($x,$y) return <a p=\"{$p}\" q=\"{$q}\">{$x},{$y}</a> }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><a p=\"0\" q=\"0\">,</a></out>",false));
}
