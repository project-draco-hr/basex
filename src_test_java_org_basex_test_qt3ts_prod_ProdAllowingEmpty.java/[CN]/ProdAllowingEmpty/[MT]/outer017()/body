{
  final XQuery query=new XQuery("\n" + "        declare variable $n as xs:integer external := 5; \n" + "        <out>{ for $x as xs:integer? allowing empty at $p in 1 to $n \n"+ "               for $y as xs:integer? allowing empty at $q in (if (empty($x)) then () else (1 to $x)) \n"+ "               where deep-equal($x,$y) \n"+ "               return <a p=\"{$p}\" q=\"{$q}\">{$x},{$y}</a> }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><a p=\"1\" q=\"1\">1,1</a><a p=\"2\" q=\"2\">2,2</a><a p=\"3\" q=\"3\">3,3</a><a p=\"4\" q=\"4\">4,4</a><a p=\"5\" q=\"5\">5,5</a></out>",false));
}
