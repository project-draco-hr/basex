{
  final XQuery query=new XQuery("\n" + "        declare variable $m as xs:integer external := 0; \n" + "        declare variable $n as xs:integer external := 0; \n"+ "        <out>{ for $x allowing empty at $p in 1 to $m, \n"+ "                   $y allowing empty at $q in 1 to $n return <a p=\"{$p}\" q=\"{$q}\">{$x},{$y}</a> }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><a p=\"0\" q=\"0\">,</a></out>",false));
}
