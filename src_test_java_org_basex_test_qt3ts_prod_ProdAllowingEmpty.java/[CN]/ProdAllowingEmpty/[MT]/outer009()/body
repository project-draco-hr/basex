{
  final XQuery query=new XQuery("\n" + "        declare variable $m as xs:integer external := 0; \n" + "        declare variable $n as xs:integer external := 5; \n"+ "        <out>{ for $x allowing empty at $p in 1 to $m, $y at $q in 1 to $n return <a p=\"{$p}\" q=\"{$q}\">{$x},{$y}</a> }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><a p=\"0\" q=\"1\">,1</a><a p=\"0\" q=\"2\">,2</a><a p=\"0\" q=\"3\">,3</a><a p=\"0\" q=\"4\">,4</a><a p=\"0\" q=\"5\">,5</a></out>",false));
}
