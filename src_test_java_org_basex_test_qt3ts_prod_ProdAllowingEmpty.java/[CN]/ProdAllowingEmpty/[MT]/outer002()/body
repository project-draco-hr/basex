{
  final XQuery query=new XQuery("\n" + "        declare variable $n as xs:integer external := 0; \n" + "        <out>{ for $x allowing empty in 1 to $n return <a>{$x}</a> }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><a/></out>",false));
}
