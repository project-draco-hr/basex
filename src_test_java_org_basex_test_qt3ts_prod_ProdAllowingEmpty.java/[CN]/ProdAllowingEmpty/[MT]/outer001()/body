{
  final XQuery query=new XQuery("\n" + "        declare variable $n as xs:integer external := 10; \n" + "        <out>{ for $x allowing empty in 1 to $n return <a>{$x}</a> }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><a>1</a><a>2</a><a>3</a><a>4</a><a>5</a><a>6</a><a>7</a><a>8</a><a>9</a><a>10</a></out>",false));
}
