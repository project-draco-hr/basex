{
  final XQuery query=new XQuery("\n" + "        declare variable $n as xs:integer external := 5; \n" + "        <out>{ for $x as xs:integer allowing empty at $p in 1 to $n, \n"+ "                   $y as xs:integer allowing empty at $q in ($x+1) to $n \n"+ "               return <a p=\"{$p}\" q=\"{$q}\">{$x},{$y}</a> }</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XPTY0004"));
}
