{
  final XQuery query=new XQuery("fn:string-to-codepoints(xs:string(\"A String\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("65, 32, 83, 116, 114, 105, 110, 103"));
}
