{
  parser=parserRegistry;
  bfc=bufferedFileChannel;
  fsmeta=meta;
  metaFinished=!fsmeta;
  fsxml=xmlContent;
  fscont=textContent;
  fstextmax=textMax;
  offset=position;
  size=contentSize;
  fileFragments=new ArrayList<DeepFile>();
