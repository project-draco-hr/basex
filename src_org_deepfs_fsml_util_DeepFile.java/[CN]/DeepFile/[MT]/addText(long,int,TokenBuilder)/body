{
  if (!fscont)   return;
  text.chop();
  if (text.size() == 0)   return;
  final byte[] data;
  if (text.size() > fstextmax) {
    data=new byte[fstextmax];
    System.arraycopy(text.finish(),0,data,0,fstextmax);
  }
 else   data=text.finish();
  textContents.add(new TextContent(position,byteCount,data));
}
