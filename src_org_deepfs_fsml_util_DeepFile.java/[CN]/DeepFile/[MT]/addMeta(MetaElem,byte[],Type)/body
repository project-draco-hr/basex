{
  if (!fsmeta)   return;
  if (e.equals(MetaElem.TYPE) | e.equals(MetaElem.FORMAT)) {
    Main.bug("The metadata attributes " + MetaElem.TYPE + " and "+ MetaElem.FORMAT+ " must not be set by an addMetaElem() method."+ " Use setMetaType() and setFormat() instead.");
  }
  if (dataType != null)   e.refineDataType(dataType);
 else   e.reset();
  metaElements.put(e,value);
}
