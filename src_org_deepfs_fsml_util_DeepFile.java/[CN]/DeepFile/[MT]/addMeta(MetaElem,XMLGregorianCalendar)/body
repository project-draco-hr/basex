{
  Type t=elem.getType();
  QName st=xgc.getXMLSchemaType();
  if ((t.instance(Type.DAT) && !st.equals(DatatypeConstants.DATE)) || (t.instance(Type.YEA) && !st.equals(DatatypeConstants.GYEAR))) {
    Main.debug("Invalid data type for metadata element " + elem + " (expected "+ t+ " got "+ st.getLocalPart()+ ").");
  }
  addMeta(elem,token(xgc.toXMLFormat()),null);
}
