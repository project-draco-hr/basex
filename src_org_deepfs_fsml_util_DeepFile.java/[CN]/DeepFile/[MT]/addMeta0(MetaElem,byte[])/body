{
  final ArrayList<byte[]> vals;
  if (metaElements.containsKey(e)) {
    if (!e.isMultiVal()) {
      Main.debug("Failed to add metadata value. Multiple values are forbidden for " + "attribute % (%).",e,bfc.getFileName());
      return;
    }
    vals=metaElements.get(e);
  }
 else {
    vals=new ArrayList<byte[]>();
    metaElements.put(e,vals);
  }
  vals.add(value);
}
