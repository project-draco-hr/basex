{
  xmlSer=new XMLSerializer(out);
  System.out.println("\n=== II Evaluating queries.");
  System.out.println("===== XQuery Proc: direct output.");
  directOutputExample();
  System.out.println("\n===== XQuery Node Iteration and XML Serializing.");
  iterateExample();
  xmlSer.close();
  out.close();
}
