{
  final XQuery query=new XQuery("fn:concat(xs:positiveInteger(\"1\"),xs:positiveInteger(\"999999999999999999\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1999999999999999999"));
}
