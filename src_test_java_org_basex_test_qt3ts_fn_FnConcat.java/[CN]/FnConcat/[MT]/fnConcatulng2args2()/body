{
  final XQuery query=new XQuery("fn:concat(xs:unsignedLong(\"130747108607674654\"),xs:unsignedLong(\"0\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1307471086076746540"));
}
