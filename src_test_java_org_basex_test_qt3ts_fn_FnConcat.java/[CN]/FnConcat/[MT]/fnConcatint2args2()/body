{
  final XQuery query=new XQuery("fn:concat(xs:int(\"-1873914410\"),xs:int(\"-2147483648\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-1873914410-2147483648"));
}
