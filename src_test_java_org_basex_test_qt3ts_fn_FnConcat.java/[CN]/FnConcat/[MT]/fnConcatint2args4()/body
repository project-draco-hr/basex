{
  final XQuery query=new XQuery("fn:concat(xs:int(\"-2147483648\"),xs:int(\"-1873914410\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-2147483648-1873914410"));
}
