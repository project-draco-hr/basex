{
  final XQuery query=new XQuery("\n" + "        declare function local:foo ($m as xs:integer) { $m };\n" + "        declare function local:foo ($n as xs:integer) { $n };\n"+ "        local:foo(4)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0034"));
}
