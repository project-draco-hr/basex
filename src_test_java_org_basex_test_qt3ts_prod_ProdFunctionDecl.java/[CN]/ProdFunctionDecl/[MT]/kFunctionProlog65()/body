{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction() as empty-sequence() { fn:error() };\n" + "        QName(\"http://example.com/ANamespace\", local:myFunction())",ctx);
  final QT3Result res=result(query);
  result=res;
