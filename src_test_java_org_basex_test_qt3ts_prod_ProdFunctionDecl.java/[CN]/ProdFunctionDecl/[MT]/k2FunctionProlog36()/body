{
  final XQuery query=new XQuery("\n" + "        declare function local:foo($arg) as xs:boolean { $arg };\n" + "        local:foo(xs:untypedAtomic(\"false\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
