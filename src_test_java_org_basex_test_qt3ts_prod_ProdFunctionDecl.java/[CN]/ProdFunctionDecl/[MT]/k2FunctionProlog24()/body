{
  final XQuery query=new XQuery("       declare function local:distinct-nodes-stable ($arg as node()*) as xs:boolean* { for $a in $arg return $a };\n" + "        local:distinct-nodes-stable((<element1/>,<element2/>))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("FORG0001"));
}
