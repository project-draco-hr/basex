{
  final XQuery query=new XQuery("\n" + "        declare function local:mysum($i as xs:integer, $j as xs:integer) { let $j := $i + $j return $j };\n" + "        declare function local:invoke_mysum($st as xs:integer) { for $d in (1,2,3,4,5) let $st := local:mysum($d, $st) return $st };\n"+ "        local:invoke_mysum(0)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1 2 3 4 5"));
}
