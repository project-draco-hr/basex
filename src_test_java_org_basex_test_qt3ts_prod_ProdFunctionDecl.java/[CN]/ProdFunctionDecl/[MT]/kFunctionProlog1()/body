{
  final XQuery query=new XQuery("declare namespace test = \"http://www.w3.org/2005/xpath-datatypes\";\n" + "        declare function test:myFunction() { 1};\n" + "        1 eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
