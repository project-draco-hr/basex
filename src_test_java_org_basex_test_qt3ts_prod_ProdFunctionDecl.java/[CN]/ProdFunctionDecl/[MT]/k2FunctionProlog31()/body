{
  final XQuery query=new XQuery("       declare function local:foo($arg) as xs:boolean* { $arg };\n" + "        local:foo((<e>true</e>, true(), xs:untypedAtomic(\"false\"), false(), <e> true </e>))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"true true false false true"));
}
