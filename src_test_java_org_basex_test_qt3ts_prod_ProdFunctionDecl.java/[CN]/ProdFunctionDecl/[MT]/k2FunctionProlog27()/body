{
  final XQuery query=new XQuery("       declare function local:myFunc() as attribute(foo) { <foo/> };\n" + "        local:myFunc()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
