{
  final XQuery query=new XQuery("\n" + "        declare variable $global := node();\n" + "        declare function local:function() { exists($global) };\n"+ "        local:function()",ctx);
  query.context(node(file("op/union/bib2.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
