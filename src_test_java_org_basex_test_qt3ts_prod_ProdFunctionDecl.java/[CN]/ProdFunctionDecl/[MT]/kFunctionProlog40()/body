{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction($local:myVar as xs:integer) { for $local:myVar in ($local:myVar, 3) return $local:myVar };\n" + "        deep-equal(local:myFunction(1), (1, 3))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
