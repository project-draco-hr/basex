{
  final XQuery query=new XQuery("declare default function namespace \"http://example.com\";\n" + "        declare function _() { 1 };\n" + "        _()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
