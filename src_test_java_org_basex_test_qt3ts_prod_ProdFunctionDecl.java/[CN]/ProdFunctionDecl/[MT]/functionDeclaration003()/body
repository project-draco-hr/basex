{
  final XQuery query=new XQuery("\n" + "        declare function local:price ($i as element()) as element()? { $i/price };\n" + "        for $j in /bib/book return local:price($j)",ctx);
  query.context(node(file("docs/bib.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<price>65.95</price><price>65.95</price><price>39.95</price><price>129.95</price>",false));
}
