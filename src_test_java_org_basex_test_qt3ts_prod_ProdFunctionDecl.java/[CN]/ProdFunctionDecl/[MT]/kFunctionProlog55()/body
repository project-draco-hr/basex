{
  final XQuery query=new XQuery("declare namespace my = \"http://example.com/MyNamespace/\";\n" + "        declare variable $my:error-qname := QName(\"http://example.com/MyErrorNS\", \"my:qName\");\n" + "        declare function my:error($choice, $msg as xs:string) as empty-sequence() { if($choice) then error($my:error-qname, concat('No luck: ', $msg)) else () };\n"+ "        my:error(false(), \"msg\"), my:error(true(), \"The message\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("*"));
}
