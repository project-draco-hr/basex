{
  final XQuery query=new XQuery("declare default function namespace \"http://www.w3.org/2005/xquery-local-functions\";\n" + "        declare function local:is() as xs:integer { 1 };\n" + "        is() eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
