{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction() { fn:error() };\n" + "        QName(\"http://example.com/ANamespace\", local:myFunction())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOER0000") || error("XPTY0004")));
}
