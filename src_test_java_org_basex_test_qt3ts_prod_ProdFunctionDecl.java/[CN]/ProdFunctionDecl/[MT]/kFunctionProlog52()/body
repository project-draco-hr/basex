{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction() as empty-sequence() { fn:error() };\n" + "        local:myFunction()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOER0000"));
}
