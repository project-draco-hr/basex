{
  final XQuery query=new XQuery("\n" + "        declare function local:title($a_book as element()) { for $i in $a_book return $i/title };\n" + "        /bib/book/(local:title(.))",ctx);
  query.context(node(file("op/union/bib2.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<title>TCP/IP Illustrated</title><title>Advanced Programming in the Unix environment</title><title>Data on the Web</title><title>The Economics of Technology and Content for Digital TV</title>",false));
}
