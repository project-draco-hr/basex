{
  final XQuery query=new XQuery("        \n" + "        declare function local:foo($n as xs:integer) { <tr> {$n} </tr> };\n" + "        local:foo(4)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<tr>4</tr>",false));
}
