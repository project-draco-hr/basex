{
  final XQuery query=new XQuery("declare variable $my := 3;\n" + "        declare function local:myFunction($my, $arg2, $arg4) { 1 };\n" + "        $my eq 3",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
