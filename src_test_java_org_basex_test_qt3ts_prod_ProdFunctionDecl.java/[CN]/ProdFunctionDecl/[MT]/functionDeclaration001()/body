{
  final XQuery query=new XQuery("\n" + "        declare namespace foo = \"http://www..oracle.com/xquery/test\";\n" + "        declare function foo:price ($b as element()) as element()* { $b/price };\n"+ "        1\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("1"));
}
