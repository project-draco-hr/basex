{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction as xs:integer { 1 };\n" + "        true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
