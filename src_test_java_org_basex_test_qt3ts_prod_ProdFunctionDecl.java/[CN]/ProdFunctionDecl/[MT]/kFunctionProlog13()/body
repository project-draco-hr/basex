{
  final XQuery query=new XQuery("\n" + "        declare function local:computeSum() { $prefix:myVariable };\n" + "        declare namespaces prefix = \"example.com/Anamespace\";\n"+ "        1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPST0081") || error("XPST0003")));
}
