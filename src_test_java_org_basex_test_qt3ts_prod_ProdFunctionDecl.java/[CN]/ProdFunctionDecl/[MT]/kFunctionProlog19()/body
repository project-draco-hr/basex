{
  final XQuery query=new XQuery("\n" + "        declare function local:func1($a1, $a2, $a3) { 1 };\n" + "        local:func1(1, 2, 3)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
