{
  cmd=null;
  ns=null;
  final int mh=gui.context.options.get(MainOptions.MAXHITS);
  boolean parse=false;
  if (mh >= 0 && r != null && r.size() >= mh) {
    parse=true;
  }
 else   if (out.finished()) {
    if (r instanceof Nodes)     ns=(Nodes)r;
 else     parse=true;
  }
  if (parse)   cmd=new CommandParser(c.toString(),gui.context).parseSingle();
}
