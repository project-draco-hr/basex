{
  int[] pres=nodesPerLevel.get(lv);
  TreeRect rect=null;
  int l=0;
  int r=pres.length - 1;
  int m=-1;
  while (r >= l && rect == null) {
    m=l + (r - l) / 2;
    if (pres[m] < pre) {
      l=m + 1;
    }
 else     if (pres[m] > pre) {
      r=m - 1;
    }
 else {
      rect=getTreeRect(lv,m);
    }
  }
  return rect;
}
