{
  final int kind=data.kind(pre);
  TokenBuilder tb=new TokenBuilder();
  if (data.meta.deepfs) {
    if (data.fs.isFile(pre))     tb.add(data.fs.name(pre));
 else     tb.add(data.text(pre + 1,false));
  }
 else {
    if (kind == Data.ELEM) {
      tb.add(data.name(pre,true));
    }
 else {
      tb.add(data.text(pre,true));
    }
  }
  return tb.toString();
}
