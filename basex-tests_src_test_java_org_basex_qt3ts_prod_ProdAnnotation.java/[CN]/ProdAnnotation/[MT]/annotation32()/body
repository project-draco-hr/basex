{
  final XQuery query=new XQuery("\n" + "         declare namespace eg = \"http://example.com\";\n" + "         %eg:sequential(\"abc\", 3) %eg:memo-function function () { \"bar\" } ()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("bar",false));
}
