{
  final XQuery query=new XQuery("fn:normalize-space(\"This\n" + "text\n" + "should\n"+ "contains\n"+ "no\n"+ "newline\n"+ "characters.\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"This text should contains no newline characters."));
}
