{
  final XQuery query=new XQuery("fn:normalize-space(\"This text\tshould contains no tabs or\n" + "newline characters.\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"This text should contains no tabs or newline characters."));
}
