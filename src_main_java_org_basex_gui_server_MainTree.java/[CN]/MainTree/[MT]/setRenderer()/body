{
  DefaultTreeCellRenderer renderer=new DefaultTreeCellRenderer(){
    @Override public Component getTreeCellRendererComponent(    final JTree tree,    final Object value,    final boolean sel,    final boolean expanded,    final boolean leaf,    final int row,    final boolean hf){
      TreeNode node=(TreeNode)value;
      if (node.getType() == 0) {
        setLeafIcon(new ImageIcon(BaseXLayout.image("server_database")));
      }
      return super.getTreeCellRendererComponent(tree,value,sel,expanded,leaf,row,hf);
    }
  }
;
  mtree.setCellRenderer(renderer);
}
