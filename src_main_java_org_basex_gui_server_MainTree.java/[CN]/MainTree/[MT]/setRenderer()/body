{
  DefaultTreeCellRenderer renderer=new DefaultTreeCellRenderer(){
    @Override public Component getTreeCellRendererComponent(    final JTree tree,    final Object value,    final boolean sel,    final boolean expanded,    final boolean leaf,    final int row,    final boolean hf){
      super.getTreeCellRendererComponent(tree,value,sel,expanded,leaf,row,hf);
      TreeNode node=(TreeNode)value;
      if (node.getType() == 1) {
        setIcon(BaseXLayout.icon("server_database"));
      }
 else       if (node.getType() == 2) {
        setIcon(BaseXLayout.icon("database"));
      }
 else       if (node.getType() == 3) {
        setIcon(BaseXLayout.icon("cmd-showtext"));
      }
 else       if (node.getType() == 4) {
        setIcon(BaseXLayout.icon("user"));
      }
 else       if (node.getType() == 5) {
        setIcon(BaseXLayout.icon("cmd-showinfo"));
      }
      return this;
    }
  }
;
  mtree.setCellRenderer(renderer);
}
