{
  if (!extractXML() || xml.isEmpty())   return;
  if (xml.length() > context.prop.num(Prop.FSTEXTMAX))   addText(pos,byteCount,xml);
 else {
    for (int i=0; i < xml.length(); ++i)     if (!XMLToken.valid(xml.charAt(i))) {
      unknown();
      return;
    }
    xmlContents.add(new Content(pos,byteCount,xml));
  }
}
