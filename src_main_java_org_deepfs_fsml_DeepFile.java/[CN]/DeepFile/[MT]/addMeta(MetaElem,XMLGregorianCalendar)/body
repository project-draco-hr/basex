{
  if (xgc == null)   return;
  final Type t=elem.getType();
  final QName st=xgc.getXMLSchemaType();
  if (SimpleType.DAT.instance(t) && !st.equals(DatatypeConstants.DATE) || SimpleType.YEA.instance(t) && !st.equals(DatatypeConstants.GYEAR))   metaDebug(elem,st.getLocalPart());
 else {
    try {
      addMeta(elem,xgc.toXMLFormat(),null);
    }
 catch (    final IllegalStateException ex) {
      Util.debug("DeepFile: Invalid date (file: %, error message: %)",bfc.getFileName(),ex.getMessage());
    }
  }
}
