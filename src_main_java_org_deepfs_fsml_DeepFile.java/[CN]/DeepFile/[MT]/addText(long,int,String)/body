{
  if (!extractText())   return;
  final String s=text.trim();
  if (s.isEmpty())   return;
  for (int i=0; i < s.length(); ++i)   if (!XMLToken.valid(s.charAt(i))) {
    unknown();
    return;
  }
  final int max=context.prop.num(Prop.FSTEXTMAX);
  final Content c=new Content(position,byteCount,s.length() > max ? s.substring(0,max) : s);
  textContents.add(c);
}
