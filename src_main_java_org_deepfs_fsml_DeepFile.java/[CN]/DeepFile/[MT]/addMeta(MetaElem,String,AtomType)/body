{
  if (metaFinished || value.isEmpty())   return;
  if (e.equals(MetaElem.TYPE) || e.equals(MetaElem.FORMAT)) {
    Util.debug("The metadata attributes " + MetaElem.TYPE + " and "+ MetaElem.FORMAT+ " must not be set by an addMetaElem() method."+ " Use setMetaType() and setFormat() instead.");
    return;
  }
  if (type != null)   e.refineDataType(type);
 else   e.reset();
  addMeta0(e,value);
}
