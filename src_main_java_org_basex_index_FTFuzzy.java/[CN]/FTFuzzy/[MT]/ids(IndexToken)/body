{
  final FTLexer ft=(FTLexer)ind;
  final byte[] tok=ft.get();
  if (ft.ftOpt().is(FZ)) {
    int k=data.meta.prop.num(Prop.LSERROR);
    if (k == 0)     k=tok.length >> 2;
    return fuzzy(tok,k,false);
  }
  final int id=cache.id(tok);
  if (id == 0) {
    final int p=token(tok);
    return p > -1 ? iter(pointer(p,tok.length),size(p,tok.length),dat,false) : FTIndexIterator.EMP;
  }
  return iter(cache.getPointer(id),cache.getSize(id),dat,false);
}
