{
  if (tp[tp.length - 1] == ptok)   return EMPTY;
  if (tp[ntl] == ptok || ntl == 0) {
    ctl++;
    while (tp[ctl] == -1)     ctl++;
    ntl=ctl + 1;
    while (tp[ntl] == -1)     ntl++;
  }
  if (ctl == tp.length)   return EMPTY;
  final byte[] tok=ti.readBytes(ptok,ptok + ctl);
  fts=ti.read4(ti.pos() + 5);
  ptok=(int)ti.pos();
  return tok;
}
