{
  final Tokenizer fto=(Tokenizer)ind;
  if (fto.fz || fto.wc)   return 1;
  final byte[] tok=fto.get();
  final int id=cache.id(tok);
  if (id > 0)   return cache.getSize(id);
  int s=0;
  long poi=0;
  final long p=token(tok);
  if (p > -1) {
    s=size(p,tok.length);
    poi=pointer(p,tok.length);
  }
  cache.add(tok,s,poi);
  return s;
}
