{
  return new Iter(){
    final Data data=((DNode)ctx.item).data;
    final IndexIterator it=data.ids(ind);
    @Override public Item next(){
      if (it.more()) {
        final int id=it.next();
        ctx.item=new DNode(data,id);
      }
 else {
        ctx.item=null;
      }
      return ctx.item;
    }
  }
;
}
