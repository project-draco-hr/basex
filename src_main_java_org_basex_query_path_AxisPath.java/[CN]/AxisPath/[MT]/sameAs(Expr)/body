{
  if (!(cmp instanceof AxisPath))   return false;
  final AxisPath ap=(AxisPath)cmp;
  if ((root == null || ap.root == null) && root != ap.root || step.length != ap.step.length || root != null && !root.sameAs(ap.root))   return false;
  for (int s=0; s < step.length; ++s) {
    if (!step[s].sameAs(ap.step[s]))     return false;
  }
  return true;
}
