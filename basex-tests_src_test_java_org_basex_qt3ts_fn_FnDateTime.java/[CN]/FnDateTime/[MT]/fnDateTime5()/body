{
  final XQuery query=new XQuery("fn:dateTime(xs:date(\"1999-12-31Z\"), xs:time(\"23:00:00Z\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1999-12-31T23:00:00Z"));
}
