{
  final XQuery query=new XQuery("empty(timezone-from-dateTime(xs:dateTime(\"2004-12-10T23:43:41.965\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
