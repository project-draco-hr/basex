{
  final XQuery query=new XQuery("fn:timezone-from-dateTime(xs:dateTime(\"2000-06-12T13:20:00Z\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"PT0S"));
}
