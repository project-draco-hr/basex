{
  int is=0;
  Expr[] exprs={};
  boolean ia=true;
  for (  final Expr e : expr) {
    if (e.indexAccessible(ic) && !ic.seq) {
      if (ic.costs == 0)       continue;
      is+=ic.costs;
    }
 else {
      ia=false;
    }
    exprs=Array.add(exprs,e);
  }
  ic.costs=is;
  expr=exprs;
  return ia;
}
