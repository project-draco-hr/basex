{
  final FTSelect tmp=ctx.ftselect;
  ctx.ftselect=this;
  init(ctx.ftitem);
  final Item it=expr[0].iter(ctx).next();
  ctx.ftselect=tmp;
  final double s=it.score();
  if (s == 0 || !filter(ctx))   return score(0);
  if (tmp.expr[0] != null)   tmp.addPos(pos,size,term);
  ctx.ftd=pos;
  return score(s);
}
