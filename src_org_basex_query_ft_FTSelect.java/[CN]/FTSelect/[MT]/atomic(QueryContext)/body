{
  final FTSelect tmp=ctx.ftselect;
  ctx.ftselect=this;
  init(ctx.fttoken);
  final FTNode it=expr[0].atomic(ctx);
  ctx.ftselect=tmp;
  double s=it.score();
  if (s != 0 && !filter(ctx))   s=0;
  if (s != 0) {
    if (tmp.expr[0] != null)     tmp.addPos(pos,size,term);
    ctx.ftd=pos;
  }
  it.score(s);
  return it;
}
