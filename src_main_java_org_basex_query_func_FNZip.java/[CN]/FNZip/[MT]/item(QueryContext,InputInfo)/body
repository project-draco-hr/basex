{
  checkAdmin(ctx);
switch (def) {
case BENTRY:
    return binaryEntry(ctx);
case TEXTENTRY:
  return textEntry(ctx);
case HTMLENTRY:
return xmlEntry(ctx,true);
case XMLENTRY:
return xmlEntry(ctx,false);
case ENTRIES:
return entries(ctx);
case ZIPFILE:
return zipFile(ctx);
case UPDATE:
return updateEntries(ctx);
default :
return super.item(ctx,ii);
}
}
