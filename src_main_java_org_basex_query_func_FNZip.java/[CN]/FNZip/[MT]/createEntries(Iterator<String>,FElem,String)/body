{
  String path=null;
  boolean curr=false;
  while (curr || it.hasNext()) {
    if (!curr) {
      path=it.next();
      curr=true;
    }
    if (path == null)     break;
    if (!path.startsWith(pref))     return path;
    int i=path.lastIndexOf('/');
    final String dir=i == -1 ? path : path.substring(0,i);
    final String name=path.substring(i + 1);
    if (name.isEmpty()) {
      path=createEntries(it,createDir(par,dir),dir);
    }
 else     if (i != -1 && pref.isEmpty()) {
      final FElem e=createDir(par,dir);
      createFile(e,name);
      path=createEntries(it,e,dir);
    }
 else {
      createFile(par,name);
      curr=false;
    }
  }
  return null;
}
