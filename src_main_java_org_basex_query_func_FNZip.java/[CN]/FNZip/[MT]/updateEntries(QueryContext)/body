{
  final ANode elm=(ANode)checkType(expr[0].item(ctx,info),NodeType.ELM);
  if (!elm.qname().eq(Q_FILE))   ZIP_UNKNOWN.thrw(info,elm.qname());
  final String in=attribute(elm,Q_HREF,true);
  final IOFile target=new IOFile(string(checkStr(expr[1],ctx)));
  IOFile out;
  do {
    out=new IOFile(target.path() + new Random().nextInt(0x7FFFFFFF));
  }
 while (out.exists());
  if (!new IOFile(in).exists())   ZIP_NOTFOUND.thrw(info,in);
  ZipFile zf=null;
  boolean ok=true;
  try {
    zf=new ZipFile(in);
    FileOutputStream fos=null;
    try {
      fos=new FileOutputStream(out.path());
      final ZipOutputStream zos=new ZipOutputStream(new BufferedOutputStream(fos));
      create(zos,elm.children(),"",zf,ctx);
      zos.close();
    }
 catch (    final IOException ex) {
      ok=false;
      ZIP_FAIL.thrw(info,ex);
    }
 finally {
      if (fos != null)       try {
        fos.close();
      }
 catch (      final IOException ignored) {
      }
    }
  }
 catch (  final IOException ex) {
    throw ZIP_FAIL.thrw(info,ex);
  }
 finally {
    if (zf != null)     try {
      zf.close();
    }
 catch (    final IOException ignored) {
    }
    if (ok) {
      target.delete();
      out.rename(target);
    }
 else {
      out.delete();
    }
  }
  return null;
}
