{
  String name=null;
  boolean dir=false;
  while (dir || it.hasNext()) {
    if (!dir)     name=it.next();
    if (name == null)     break;
    if (!name.startsWith(pref))     return name;
    final FElem e=map.get(name);
    e.parent(par);
    par.children.add(e);
    dir=name.endsWith("/");
    if (dir)     name=createNode(map,it,e,name);
  }
  return null;
}
