{
  checkCreate(qc);
switch (func) {
case _ZIP_BINARY_ENTRY:
    return binaryEntry(qc);
case _ZIP_TEXT_ENTRY:
  return textEntry(qc);
case _ZIP_HTML_ENTRY:
return xmlEntry(qc,true);
case _ZIP_XML_ENTRY:
return xmlEntry(qc,false);
case _ZIP_ENTRIES:
return entries(qc);
case _ZIP_ZIP_FILE:
return zipFile(qc);
case _ZIP_UPDATE_ENTRIES:
return updateEntries(qc);
default :
return super.item(qc,ii);
}
}
