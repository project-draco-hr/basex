{
  final Step step=super.step();
  filter(true);
  if (step == null) {
    ctx=skel.child(ctx,0,false);
    return null;
  }
  if (step.axis == Axis.CHILD) {
    ctx=skel.child(ctx,0,false);
  }
 else   if (step.axis == Axis.DESC || step.axis == Axis.DESCORSELF) {
    ctx=skel.child(ctx,0,true);
  }
 else {
    ctx=new ArrayList<Node>();
  }
  last=step;
  return step;
}
