{
  filter(true);
  if (axis == null) {
    stack.push(skel.child(stack.pop(),0,false));
    return;
  }
  if (axis == Axis.CHILD || axis == Axis.ATTR) {
    stack.push(skel.child(stack.pop(),0,false));
  }
 else   if (axis == Axis.DESC || axis == Axis.DESCORSELF) {
    stack.push(skel.child(stack.pop(),0,true));
  }
 else {
    stack.peek().clear();
  }
  laxis=axis;
  ltest=test;
}
