{
  final XQuery query=new XQuery("local-name-from-QName( QName(\"example.com/\", \"pre:lname\")) eq \"lname\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
