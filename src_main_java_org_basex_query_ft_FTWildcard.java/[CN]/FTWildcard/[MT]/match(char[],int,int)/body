{
  int ql=qp;
  int tl=tp;
  while (ql < chars.length) {
    if (chars[ql] == DOT) {
      int n=min[ql];
      final int m=max[ql++];
      while (!match(t,tl + n,ql))       if (tl + ++n > t.length)       return false;
      if (n > m)       return false;
      tl+=n;
    }
 else {
      if (tl >= t.length || t[tl++] != chars[ql++])       return false;
    }
  }
  return tl == t.length;
}
