{
  if (cn != b) {
    int[][] newData=dt;
    int[] ne=getNodeEntry(cn);
    long tdid=did;
    int[][] tmp=getDataFromDataArray(ne[ne.length - 1],tdid);
    if (tmp != null) {
      newData=calculateFTOr(dt,tmp);
    }
    if (hasNextNodes(ne)) {
      for (int i=ne[0] + 1; i < ne.length - 1; i+=2) {
        newData=getAllNodes(ne[i],b,newData);
      }
    }
    return newData;
  }
  return dt;
}
