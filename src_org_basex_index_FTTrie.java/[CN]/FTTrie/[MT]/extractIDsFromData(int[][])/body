{
  if (data == null || data.length == 0 || data[0] == null || data[0].length == 0)   return Array.NOINTS;
  final int l=data[0].length;
  final int[] ids=new int[l];
  ids[0]=data[0][0];
  int c=1;
  for (int i=1; i < l; i++) {
    final int j=data[0][i];
    if (ids[c - 1] != j)     ids[c++]=j;
  }
  return Array.finish(ids,c);
}
