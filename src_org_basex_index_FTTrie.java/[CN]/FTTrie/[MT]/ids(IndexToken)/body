{
  final Tokenizer ft=(Tokenizer)ind;
  final byte[] tok=ft.get();
  if (ft.fz) {
    int k=Prop.lserr;
    if (k == 0)     k=tok.length >> 2;
    return fuzzy(0,null,-1,tok,0,0,0,k);
  }
  if (ft.wc) {
    final int pw=indexOf(tok,'.');
    if (pw != -1)     return wc(tok,pw);
  }
  final int id=cache.id(tok);
  return id == 0 ? get(0,tok) : iter(cache.getPointer(id),cache.getSize(id),inD);
}
