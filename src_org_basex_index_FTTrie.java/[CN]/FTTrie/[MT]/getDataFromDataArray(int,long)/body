{
  if (s == 0 || ldid < 0)   return null;
  final int[][] dt=new int[2][s];
  dt[0][0]=inD.readNum(ldid);
  if (data.meta.ftittr) {
    dt[1][0]=inD.readNum();
    for (int i=1; i < s; i++) {
      dt[0][i]=inD.readNum();
      dt[1][i]=inD.readNum();
    }
  }
 else {
    for (int i=1; i < s; i++) {
      dt[0][i]=inD.readNum();
    }
    for (int i=0; i < s; i++) {
      dt[1][i]=inD.readNum();
    }
    System.out.println();
  }
  return dt;
}
