{
  final XQuery query=new XQuery("xs:date(\"2000-10-30\") - xs:dayTimeDuration(\"P3DT1H15M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2000-10-26"));
}
