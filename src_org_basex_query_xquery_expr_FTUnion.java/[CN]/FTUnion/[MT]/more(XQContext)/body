{
  boolean b=false;
  if (cp.size > 0) {
    for (int i=0; i < cp.size; i++) {
      mp[pex[cp.list[i]]]=(FTNodeItem)ctx.iter(expr[pex[cp.list[i]]]).next();
      if (!b)       b=mp[i].ftn.size > 0;
    }
    cp.reset();
  }
  if (!b) {
    for (    FTNodeItem c : mp)     if (c.ftn.size > 0)     return true;
  }
  return b;
}
