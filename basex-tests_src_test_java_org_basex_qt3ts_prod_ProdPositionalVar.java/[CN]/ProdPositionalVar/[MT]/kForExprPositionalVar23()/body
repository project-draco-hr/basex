{
  final XQuery query=new XQuery("1 eq (for $i at $p in subsequence((1, 2, 3, current-time()), 1, 1) return $p)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
