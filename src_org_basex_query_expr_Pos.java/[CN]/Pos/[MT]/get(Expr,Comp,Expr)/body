{
  if (!arg.i())   return expr;
  Item it=(Item)arg;
  if (it.n()) {
    final long p=it.itr();
    final boolean ex=p == it.dbl();
switch (cmp) {
case EQ:
      return ex ? Pos.get(p,p) : Bln.FALSE;
case GE:
    return get(ex ? p : p + 1,Long.MAX_VALUE);
case GT:
  return get(p + 1,Long.MAX_VALUE);
case LE:
return get(1,p);
case LT:
return get(1,ex ? p - 1 : p);
default :
}
}
return expr;
}
