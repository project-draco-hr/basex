{
  final XQuery query=new XQuery("<new>{//*:child1}</new>",ctx);
  query.context(node(file("prod/DirElemContent.namespace/inscope.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<new><child1 xmlns:foo=\"http://www.example.com/parent1\" attr=\"child\"/></new>",false));
}
