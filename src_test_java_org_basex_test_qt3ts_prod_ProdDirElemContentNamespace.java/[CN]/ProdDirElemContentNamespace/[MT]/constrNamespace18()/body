{
  final XQuery query=new XQuery("<elem xmlns=\"http://www.example.com/parent\"><child xmlns=\"http://www.example.com/child\"/></elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem xmlns=\"http://www.example.com/parent\"><child xmlns=\"http://www.example.com/child\"/></elem>",false));
}
