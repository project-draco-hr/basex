{
  final XQuery query=new XQuery("declare default element namespace \"http://example.com/\"; declare namespace p = \"http://example.com/\"; <r> { <e p:att=\"\"/>/(@att) } </r>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<r xmlns=\"http://example.com/\"/>",false));
}
