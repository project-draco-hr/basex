{
  final XQuery query=new XQuery("declare default element namespace \"http://example.com/\"; <a> <e xmlns=\"\"/> </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a xmlns=\"http://example.com/\"><e xmlns=\"\"/></a>",false));
}
