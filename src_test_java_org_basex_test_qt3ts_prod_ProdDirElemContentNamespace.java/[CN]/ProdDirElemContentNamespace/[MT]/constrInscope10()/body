{
  final XQuery query=new XQuery("<new xmlns=\"http://www.example.com\">{//*:child2}</new>",ctx);
  query.context(node(file("prod/DirElemContent.namespace/inscope.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<new xmlns=\"http://www.example.com\"><child2 xmlns=\"\" xmlns:foo=\"http://www.example.com/parent2\" attr=\"child\"/></new>",false));
}
