{
  final XQuery query=new XQuery("for $x in <parent3 xmlns:foo=\"http://www.example.com/parent3\"><foo:child3/></parent3> return <new>{$x//*:child3}</new>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<new><foo:child3 xmlns:foo=\"http://www.example.com/parent3\"/></new>",false));
}
