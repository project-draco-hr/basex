{
  final XQuery query=new XQuery("(<elem xmlns=\"http://www.example.com/parent\"><child xmlns=\"http://www.example.com/child\"><grand-child/></child></elem>)//*:grand-child",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<grand-child xmlns=\"http://www.example.com/child\"/>",false));
}
