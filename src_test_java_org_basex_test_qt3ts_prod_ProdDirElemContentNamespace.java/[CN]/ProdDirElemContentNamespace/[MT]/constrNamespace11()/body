{
  final XQuery query=new XQuery("(<elem xmlns:foo=\"http://www.example.com/parent\"><foo:child xmlns:foo=\"http://www.example.com/child\"><foo:grand-child/></foo:child></elem>)//*:grand-child",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<foo:grand-child xmlns:foo=\"http://www.example.com/child\"/>",false));
}
