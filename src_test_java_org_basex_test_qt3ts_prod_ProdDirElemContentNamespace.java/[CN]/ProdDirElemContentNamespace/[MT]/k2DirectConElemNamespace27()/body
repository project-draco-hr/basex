{
  final XQuery query=new XQuery("<e xmlns:p=\"http://www.w3.org/2001/XMLSchema\"> <b xmlns:p=\"http://www.w3.org/2001/XMLSchema\"/> {p:integer(1)} </e>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns:p=\"http://www.w3.org/2001/XMLSchema\"><b/>1</e>",false));
}
