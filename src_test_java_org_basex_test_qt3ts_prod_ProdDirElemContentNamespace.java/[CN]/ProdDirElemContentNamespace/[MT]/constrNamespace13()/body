{
  final XQuery query=new XQuery("<foo:elem xmlns:foo=\"http://www.example.com/parent\"><child xmlns:foo=\"\"/></foo:elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertSerialization("<foo:elem xmlns:foo=\"http://www.example.com/parent\"><child/></foo:elem>",false) || error("XQST0085")));
}
