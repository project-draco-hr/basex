{
  final XQuery query=new XQuery("declare namespace foo=\"http://www.example.com/prolog\"; (<elem xmlns:foo=\"http://www.example.com/parent\"><foo:child/></elem>)//*:child",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<foo:child xmlns:foo=\"http://www.example.com/parent\"/>",false));
}
