{
  final XQuery query=new XQuery("for $x in <parent4 xmlns=\"http://www.example.com/parent4\"><child4/></parent4> return <new>{$x//*:child4}</new>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<new><child4 xmlns=\"http://www.example.com/parent4\"/></new>",false));
}
