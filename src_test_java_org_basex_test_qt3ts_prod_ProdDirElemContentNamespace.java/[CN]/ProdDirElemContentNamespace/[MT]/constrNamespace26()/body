{
  final XQuery query=new XQuery("<elem xmlns:cm=\"http://www.example.com\">{count(//cm:b)}</elem>",ctx);
  query.context(node(file("prod/DirElemContent.namespace/MixNS.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem xmlns:cm=\"http://www.example.com\">1</elem>",false));
}
