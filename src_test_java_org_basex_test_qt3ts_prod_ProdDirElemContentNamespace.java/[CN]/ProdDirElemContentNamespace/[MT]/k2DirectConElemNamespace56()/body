{
  final XQuery query=new XQuery("declare default element namespace \"http://example.com/\"; <r xmlns:p=\"http://example.com/\"> { <e p:att=\"\"/>/(@att, attribute::att) } </r>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<r xmlns=\"http://example.com/\" xmlns:p=\"http://example.com/\"/>",false));
}
