{
  final XQuery query=new XQuery("declare namespace foo=\"http://www.example.com/prolog\"; <foo:elem xmlns:foo=\"http://www.example.com/element\"/>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<foo:elem xmlns:foo=\"http://www.example.com/element\"/>",false));
}
