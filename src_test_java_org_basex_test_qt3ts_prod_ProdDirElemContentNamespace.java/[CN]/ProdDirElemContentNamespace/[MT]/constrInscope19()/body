{
  final XQuery query=new XQuery("<new xmlns=\"http://www.example.com\">{//*:child4}</new>",ctx);
  query.context(node(file("prod/DirElemContent.namespace/inscope.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<new xmlns=\"http://www.example.com\"><child4 xmlns=\"http://www.example.com/parent4\"/></new>",false));
}
