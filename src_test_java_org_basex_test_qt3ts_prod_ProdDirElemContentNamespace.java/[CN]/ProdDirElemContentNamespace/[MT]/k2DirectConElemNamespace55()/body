{
  final XQuery query=new XQuery("<e xmlns=\"http://example.com/\"> <b xmlns=\"\"> { attribute {QName(\"http://example.com/2\", \"p:attr\")} {()} } </b> </e>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns=\"http://example.com/\"><b xmlns=\"\" xmlns:p=\"http://example.com/2\" p:attr=\"\"/></e>",false));
}
