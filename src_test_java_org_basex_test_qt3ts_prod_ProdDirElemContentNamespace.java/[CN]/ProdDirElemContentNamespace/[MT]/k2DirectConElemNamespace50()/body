{
  final XQuery query=new XQuery("<e xmlns=\"http://www.example.com/A\" xmlns:A=\"http://www.example.com/C\"> <b xmlns:B=\"http://www.example.com/C\" xmlns=\"\"/> </e>/b",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertSerialization("<b xmlns:B=\"http://www.example.com/C\" xmlns:A=\"http://www.example.com/C\"/>",false) || assertSerialization("<b xmlns:A=\"http://www.example.com/C\" xmlns:B=\"http://www.example.com/C\"/>",false)));
}
