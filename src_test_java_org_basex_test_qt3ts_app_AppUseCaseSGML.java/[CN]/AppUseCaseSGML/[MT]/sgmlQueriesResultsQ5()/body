{
  final XQuery query=new XQuery("<result> { //para[@security = \"c\"] } </result>",ctx);
  query.context(node(file("docs/sgml.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result><para security=\"c\">Exiled members of the former Soviet Union's secret\n police, the KGB, have infiltrated the upper ranks of the GCA and are\n planning the Final Revolution as soon as DSSSL is completed.</para></result>",false));
}
