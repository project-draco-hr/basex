{
  final XQuery query=new XQuery("QName(\"example.com/\", \"p:ncname\") lt QName(\"example.com/\", \"p:ncname\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
