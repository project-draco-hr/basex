{
  BDBNode node=new BDBNode();
  node.setPre(meta.size);
  node.setId(meta.size++);
  node.setKind(Data.ELEM);
  node.setNameId(t);
  node.setNumAtts(as);
  node.setDist(dis);
  node.setSize(as);
  try {
    nodeByPre.put(node);
  }
 catch (  DatabaseException e) {
    throw new IOException(e);
  }
  if (meta.size < 0)   throw new IOException(LIMITRANGE);
}
