{
  try {
    if (store != null)     store.close();
    if (dbEnv != null)     dbEnv.close();
  }
 catch (  DatabaseException e) {
    throw new IOException(e);
  }
  store=null;
  dbEnv=null;
  nodeByPre=null;
  if (xout != null)   xout.close();
  xout=null;
  if (vout != null)   vout.close();
  vout=null;
}
