{
  if (pos == 0) {
    nodes[0]=this;
    return 1;
  }
  final NodeLike<Node<N,E>,E> left=nodes[pos - 1];
  if (left instanceof Node) {
    nodes[pos]=this;
    return pos + 1;
  }
  final NodeLike<Node<N,E>,E>[] joined=left.concat(this);
  nodes[pos - 1]=joined[0];
  if (joined[1] == null)   return pos;
  nodes[pos]=joined[1];
  return pos + 1;
}
