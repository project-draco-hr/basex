{
  Session s=null;
  try {
    s=session.login();
    final String dbname=dbname(newName);
    s.create(dbname,inputStream);
    s.execute(new Close());
    return new BXDatabase(dbname,session);
  }
 catch (  final Exception ex) {
    handle(ex);
    throw new BadRequestException(this,ex.getMessage());
  }
 finally {
    try {
      if (s != null)       s.close();
    }
 catch (    final IOException e) {
      handle(e);
    }
  }
}
