{
  if (supported(contentType)) {
    try {
      final ClientSession cs=login(user,pass);
      try {
        final String dbname=dbname(newName);
        cs.create(dbname,inputStream);
        return new BXDatabase(dbname,user,pass);
      }
  finally {
        cs.close();
      }
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  return null;
}
