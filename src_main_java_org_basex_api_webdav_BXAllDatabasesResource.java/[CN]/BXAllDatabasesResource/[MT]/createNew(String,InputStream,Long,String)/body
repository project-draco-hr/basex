{
  if (supported(contentType)) {
    try {
      final Session s=factory.login(user,pass);
      try {
        final String dbname=dbname(newName);
        s.create(dbname,inputStream);
        return new BXDatabase(dbname,factory,user,pass);
      }
  finally {
        s.execute(new Close());
        s.close();
      }
    }
 catch (    Exception ex) {
      handle(ex);
    }
  }
  return null;
}
