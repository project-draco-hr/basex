{
  try {
    final List<BXResource> dbs=new ArrayList<BXResource>();
    final Session s=factory.login(user,pass);
    try {
      for (      final String d : listDbs(s))       dbs.add(new BXDatabase(d,factory,user,pass));
      return dbs;
    }
  finally {
      s.close();
    }
  }
 catch (  final Exception ex) {
    handle(ex);
  }
  return null;
}
