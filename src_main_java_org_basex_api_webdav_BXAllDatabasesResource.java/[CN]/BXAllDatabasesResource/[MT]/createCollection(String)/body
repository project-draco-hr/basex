{
  try {
    final Session s=factory.login(user,pass);
    try {
      s.execute(new CreateDB(dbname(newName)));
      return new BXDatabase(newName,factory,user,pass);
    }
  finally {
      s.execute(new Close());
      s.close();
    }
  }
 catch (  Exception ex) {
    handle(ex);
  }
  return null;
}
