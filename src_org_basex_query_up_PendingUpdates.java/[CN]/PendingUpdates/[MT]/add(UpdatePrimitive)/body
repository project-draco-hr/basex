{
  if (p.node instanceof FNode) {
    fragPrimitives.add(p);
  }
 else   if (p.node instanceof DBNode) {
    final Data d=((DBNode)p.node).data;
    DBPrimitives dp=dbPrimitives.get(d);
    if (dp == null) {
      dp=new DBPrimitives(false);
      dbPrimitives.put(d,dp);
    }
    dp.add(p);
  }
}
