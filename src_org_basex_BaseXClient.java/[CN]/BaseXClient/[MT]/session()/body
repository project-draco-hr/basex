{
  if (session == null) {
    while (user == null) {
      Main.out(SERVERUSER + COLS);
      user=System.console().readLine();
    }
    while (pass == null) {
      Main.out(SERVERPW + COLS);
      pass=new String(System.console().readPassword());
    }
    session=new ClientSession(context,user,pass);
  }
  return session;
}
