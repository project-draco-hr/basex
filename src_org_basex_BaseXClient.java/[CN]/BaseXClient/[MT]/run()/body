{
  if (allInfo || info) {
    execute(Commands.SET,Set.INFO + " " + (allInfo ? Set.ALL : ON),false);
  }
  try {
    if (query != null) {
      query=content();
      if (query == null)       return;
    }
    if (query != null) {
      execute(xpath ? Commands.XPATH : Commands.XQUERY,query,info);
    }
 else     if (commands != null) {
      parse(commands);
      quit(true);
    }
 else {
      console();
    }
  }
 catch (  final Exception ex) {
    error(ex,ex.getMessage());
  }
}
