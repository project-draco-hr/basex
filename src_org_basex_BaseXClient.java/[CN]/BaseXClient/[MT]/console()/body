{
  BaseX.outln(CONSOLE,standalone ? LOCALMODE : CLIENTMODE);
  String input=null;
  do {
    execute(Commands.PROMPT,null,false);
    input=input(INPUT);
    if ("xquery".equals(input)) {
      String in="";
      while ((in=input(INPUTCONT)).length() != 0)       input+=" " + in;
    }
  }
 while (input != null && process(input));
  quit(input == null);
}
