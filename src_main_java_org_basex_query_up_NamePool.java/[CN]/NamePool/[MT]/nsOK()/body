{
  final Atts at=new Atts();
  for (int i=0; i < size; i++) {
    if (occ[i] <= 0)     continue;
    final QNm nm=names[i];
    final byte[] pref=nm.pref();
    final byte[] uri=nm.uri.atom();
    final int ai=at.get(pref);
    if (ai == -1)     at.add(pref,uri);
 else     if (!eq(uri,at.val[ai]))     return false;
  }
  return true;
}
