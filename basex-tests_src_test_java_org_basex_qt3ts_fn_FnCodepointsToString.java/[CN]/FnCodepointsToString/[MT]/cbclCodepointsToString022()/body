{
  final XQuery query=new XQuery("let $y := 65536*65536 return for $x in $y to $y+10 return codepoints-to-string($x to $x+10)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOCH0001"));
}
