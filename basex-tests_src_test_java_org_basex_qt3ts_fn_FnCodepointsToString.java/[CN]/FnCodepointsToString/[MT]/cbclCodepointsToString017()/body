{
  final XQuery query=new XQuery("for $x in 32 to 64 return boolean(codepoints-to-string($x to $x + 10))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"true true true true true true true true true true true true true true true true true true true true true true true true true true true true true true true true true"));
}
