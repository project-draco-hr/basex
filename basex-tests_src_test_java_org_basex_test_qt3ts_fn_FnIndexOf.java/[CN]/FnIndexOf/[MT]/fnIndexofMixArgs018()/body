{
  final XQuery query=new XQuery("fn:index-of((/bib/book/publisher), \"Addison-Wesley\")",ctx);
  try {
    query.context(node(file("docs/bib.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2"));
}
