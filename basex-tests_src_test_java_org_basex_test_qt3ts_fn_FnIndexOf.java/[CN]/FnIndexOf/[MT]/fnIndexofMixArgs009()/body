{
  final XQuery query=new XQuery("fn:index-of((xs:double('NaN')), fn:exactly-one(xs:double('NaN')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,""));
}
