{
  final XQuery query=new XQuery("\n" + "        for $string in (\"&#xD; foo &#x9; bar &#xA;\" cast as xs:token) \n" + "        return not(contains($string, '&#x9;') or \n"+ "                   contains($string, '&#xA;') or \n"+ "                   contains($string, '&#xD;') or \n"+ "                   string-length($string) ne 7)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
