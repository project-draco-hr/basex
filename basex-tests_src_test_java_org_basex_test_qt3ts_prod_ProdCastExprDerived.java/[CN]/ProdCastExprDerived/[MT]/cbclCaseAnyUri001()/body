{
  final XQuery query=new XQuery("\"http://example.com?query=\" cast as xs:anyURI",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://example.com?query="));
}
