{
  final XQuery query=new XQuery("\n" + "      \t<element> { processing-instruction { 'pi' } { <element>?&gt;</element> } } </element>\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQDY0026"));
}
