{
  check(_VALIDATE_XSD);
  query(_VALIDATE_XSD.args(FILE,XSD),"");
  query(_VALIDATE_XSD.args(DOC.args(FILE),DOC.args(XSD)),"");
  query(_VALIDATE_XSD.args(_FILE_READ_TEXT.args(FILE),_FILE_READ_TEXT.args(XSD)),"");
  query("let $doc := <root/> " + "let $schema := <xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'> " + "<xs:element name='root'/> "+ "</xs:schema> "+ "return validate:xsd($doc, $schema)","");
  error(_VALIDATE_XSD.args("unknown"),Err.WHICHRES);
  error(_VALIDATE_XSD.args(FILE,"unknown.xsd"),Err.WHICHRES);
  error(_VALIDATE_XSD.args(FILE),Err.BXVA_FAIL);
  error("let $doc := <root/> " + "let $schema := <xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'> " + "<xs:element name='unknown'/> "+ "</xs:schema> "+ "return validate:xsd($doc, $schema)",Err.BXVA_FAIL);
}
