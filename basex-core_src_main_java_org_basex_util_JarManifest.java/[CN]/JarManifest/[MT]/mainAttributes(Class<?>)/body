{
  final Manifest manifest=getManifest(cls);
  if (manifest == null)   return emptyMap();
  final Map<String,String> result=new LinkedHashMap<String,String>();
  for (  Map.Entry<Object,Object> attribute : manifest.getMainAttributes().entrySet())   result.put(attribute.getKey().toString(),attribute.getValue().toString());
  return result;
}
