{
  final XQuery query=new XQuery("resolve-QName(\"blah\",<foo:a xmlns:foo=\"http://test/\"/>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"blah"));
}
