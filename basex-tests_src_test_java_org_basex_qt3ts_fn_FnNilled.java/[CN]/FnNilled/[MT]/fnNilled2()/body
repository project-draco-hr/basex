{
  final XQuery query=new XQuery(" fn:count(fn:nilled(/works[1]/employee[2]/child::text()[last()]))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0"));
}
