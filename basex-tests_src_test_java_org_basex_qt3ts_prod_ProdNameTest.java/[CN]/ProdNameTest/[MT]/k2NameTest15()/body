{
  final XQuery query=new XQuery("declare variable $e := <a b =\"content\"><?b asd?><b/><c b=\"content\"/></a>; $e/b",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<b/>",false));
}
