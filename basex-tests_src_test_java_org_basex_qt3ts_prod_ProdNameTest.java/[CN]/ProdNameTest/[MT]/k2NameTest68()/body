{
  final XQuery query=new XQuery("<e>{<e foo=\"1\"/>/attribute(foo, xs:integer)}</e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<e/>",false) || error("XPST0005")));
}
