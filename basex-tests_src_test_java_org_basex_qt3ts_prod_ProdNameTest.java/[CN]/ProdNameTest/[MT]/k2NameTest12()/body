{
  final XQuery query=new XQuery("declare variable $var := <elem xml:space=\"default\"/>; $var/@xml:space eq \"default\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
