{
  final XQuery query=new XQuery("let $var := /works[1]/child::employee[12]/overtime return $var/child::* intersect $var/child::day",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<day>Monday</day><day>Tuesday</day>",false));
}
