{
  final XQuery query=new XQuery("declare namespace a = \"http://example.com/1\"; declare namespace b = \"http://example.com/2\"; let $e := <e a:n1=\"content\" b:n1=\"content\"> <a:n1/> <b:n1/> <?n1 ?> <n1/> </e> return $e/a:*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a:n1 xmlns:b=\"http://example.com/2\" xmlns:a=\"http://example.com/1\"/>",false));
}
