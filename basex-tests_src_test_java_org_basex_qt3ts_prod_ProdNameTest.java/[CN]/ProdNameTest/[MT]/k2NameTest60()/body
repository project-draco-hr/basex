{
  final XQuery query=new XQuery("\n" + "        declare variable $input := <e/>; \n" + "        empty(for $PI as processing-instruction() in $input//processing-instruction() return $PI)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPST0005")));
}
