{
  final byte[] name=pkg.uniqueName();
  for (  final Component comp : pkg.comps) {
    if (comp.uri != null) {
      TokenSet dict=nsDict.get(comp.uri);
      if (dict == null) {
        dict=new TokenSet();
        nsDict.put(comp.uri,dict);
      }
      dict.add(name);
    }
  }
  pkgDict.put(name,token(dir));
}
