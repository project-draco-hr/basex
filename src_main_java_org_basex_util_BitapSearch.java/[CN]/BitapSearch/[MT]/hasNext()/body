{
  if (mask == null)   return false;
  if (calledHasNext)   return currentPosition >= 0;
  calledHasNext=true;
  while (haystack.hasNext()) {
    final T current=haystack.next();
    currentPosition++;
    for (int k=needle.length; k >= 1; k--)     mask.set(k,mask.get(k - 1) && cmp.compare(current,needle[k - 1]) == 0);
    if (mask.get(needle.length))     return true;
  }
  currentPosition=-1;
  return false;
}
