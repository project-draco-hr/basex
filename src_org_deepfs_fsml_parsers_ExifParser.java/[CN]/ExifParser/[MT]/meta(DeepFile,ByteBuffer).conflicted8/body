{
  final String s;
switch (b.getShort()) {
case 1:
    s="not defined";
  break;
case 2:
s="one-chip color area sensor";
break;
case 3:
s="two-chip color area sensor";
break;
case 4:
s="three-chip color area sensor";
break;
case 5:
s="color sequential area sensor";
break;
case 6:
s=null;
break;
case 7:
s="trilinear sensor";
break;
case 8:
s="color sequential linear sensor";
break;
default :
s=null;
}
if (s != null) d.addMeta(elem,s);
}
