{
  final DeepFile d=o.deepFile;
switch (f) {
case BYTE:
    d.addMeta(elem,(short)(buf.get() & 0xFF));
  break;
case ASCII:
try {
  d.addMeta(elem,readAscii(o.bfc,buf));
}
 catch (final IOException e) {
  err(o,e);
}
break;
case SHORT:
d.addMeta(elem,buf.getShort() & 0xFFFF);
break;
case LONG:
d.addMeta(elem,buf.getInt() & 0xFFFFFFFFL);
break;
case RATIONAL:
try {
d.addMeta(elem,readRational(o.bfc,buf));
}
 catch (final IOException e) {
err(o,e);
}
break;
case UNDEFINED:
break;
case SLONG:
break;
case SRATIONAL:
break;
default :
Main.debug("ExifParser: Unknown field type (%)",f);
}
}
