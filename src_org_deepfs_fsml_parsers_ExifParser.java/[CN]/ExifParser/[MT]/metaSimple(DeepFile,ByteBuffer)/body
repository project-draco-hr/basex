{
switch (format) {
case BYTE:
    d.addMeta(elem,(short)(buf.get() & 0xFF));
  break;
case ASCII:
d.addMeta(elem,buf.get(new byte[count]).array());
break;
case SHORT:
d.addMeta(elem,buf.getShort() & 0xFFFF);
break;
case LONG:
d.addMeta(elem,buf.getInt() & 0xFFFFFFFFL);
break;
default :
Main.debug("ExifParser: Unknown or unsupported field type for " + "inlined data (%)",format);
}
}
