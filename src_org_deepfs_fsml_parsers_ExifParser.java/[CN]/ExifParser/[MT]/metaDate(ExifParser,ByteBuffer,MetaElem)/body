{
  try {
    o.bfc.position(buf.getInt());
    o.bfc.buffer(20);
    final byte[] data=new byte[20];
    o.bfc.get(data);
    final Date d=SDF.parse(Token.string(data));
    o.deepFile.addMeta(elem,ParserUtil.convertDateTime(d));
    return;
  }
 catch (  final ParseException ex) {
    err(o,ex);
  }
catch (  final IOException e) {
    err(o,e);
  }
}
