{
  try {
switch (format) {
case ASCII:
      d.addMeta(elem,b.get(new byte[count]));
    break;
case RATIONAL:
  d.addMeta(elem,readRational(b));
break;
default :
Main.debug("ExifParser: Unknown or unsupported field type for " + "non-inlined data (%)",format);
}
}
 catch (final IOException e) {
err(d,e);
}
}
