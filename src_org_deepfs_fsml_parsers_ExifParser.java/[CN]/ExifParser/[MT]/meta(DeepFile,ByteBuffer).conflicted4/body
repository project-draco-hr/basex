{
  final String s;
switch (b.getShort()) {
case 1:
    s="top left";
  break;
case 2:
s="top right";
break;
case 3:
s="bottom right";
break;
case 4:
s="bottom left";
break;
case 5:
s="left top";
break;
case 6:
s="right top";
break;
case 7:
s="right bottom";
break;
case 8:
s="left bottom";
break;
default :
s=null;
}
if (s != null) d.addMeta(elem,s);
}
