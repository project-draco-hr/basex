{
  final String s;
  final int v=b.getShort();
switch (v) {
case 0:
    s="unknown";
  break;
case 1:
s="average";
break;
case 2:
s="center weighted average";
break;
case 3:
s="spot";
break;
case 4:
s="multi spot";
break;
case 5:
s="pattern";
break;
case 6:
s="partial";
break;
default :
s=v == 255 ? "other" : null;
}
if (s != null) d.addMeta(elem,s);
}
