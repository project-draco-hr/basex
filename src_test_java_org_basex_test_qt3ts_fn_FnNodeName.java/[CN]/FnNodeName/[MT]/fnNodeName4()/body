{
  final XQuery query=new XQuery("fn:local-name-from-QName(fn:node-name(<shoe size = \"5\"/>))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"shoe"));
}
