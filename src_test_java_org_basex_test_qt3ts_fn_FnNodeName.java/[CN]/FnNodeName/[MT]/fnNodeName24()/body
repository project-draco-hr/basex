{
  final XQuery query=new XQuery("fn:node-name(processing-instruction piName {\"Processing Instruction content\"},\"A Second Argument\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0017"));
}
