{
  final XQuery query=new XQuery("fn:local-name-from-QName(fn:node-name(processing-instruction piName {\"Processing Instruction content\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"piName"));
}
