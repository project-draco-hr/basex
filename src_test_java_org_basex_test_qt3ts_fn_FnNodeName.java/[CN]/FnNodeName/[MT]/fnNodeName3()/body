{
  final XQuery query=new XQuery("fn:local-name-from-QName(fn:node-name(/works[1]/employee[2]))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"employee"));
}
