{
  final XQuery query=new XQuery("fn:count(fn:local-name-from-QName(fn:node-name(document {\"<element1> text </element1>\"})))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0"));
}
