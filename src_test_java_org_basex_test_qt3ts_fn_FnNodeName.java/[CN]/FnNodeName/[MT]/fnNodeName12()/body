{
  final XQuery query=new XQuery("fn:local-name-from-QName(fn:node-name(attribute attributeName {\"an attribute value\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"attributeName"));
}
