{
  final XQuery query=new XQuery("namespace{\"foo\"}{\"http://example.com/foo\"}/node-name()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("QName(\"\", \"foo\")"));
}
