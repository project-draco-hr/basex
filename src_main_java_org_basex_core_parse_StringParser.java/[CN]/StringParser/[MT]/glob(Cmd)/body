{
  consumeWS();
  final StringBuilder sb=new StringBuilder();
  while (true) {
    final char c=parser.curr();
    if (!letterOrDigit(c) && c != '-' && c != '*' && c != '?' && c != ',') {
      return finish(cmd,!parser.more() || parser.curr(';') || ws(parser.curr()) ? sb : null);
    }
    sb.append(parser.consume());
  }
}
