{
  for (  final String line : lines) {
    if (line.isEmpty() || line.startsWith("#"))     continue;
    parser=new InputParser(line);
    while (true) {
      final Cmd cmd=consume(Cmd.class,null);
      list.add(parse(cmd));
      if (!parser.more())       break;
      if (!eoc())       throw help(null,cmd);
      parser.consume();
    }
  }
}
