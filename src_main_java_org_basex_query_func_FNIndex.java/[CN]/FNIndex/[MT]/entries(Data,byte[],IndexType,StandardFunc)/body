{
  final Index index;
  final boolean avl;
  if (it == IndexType.TEXT) {
    index=data.txtindex;
    avl=data.meta.textindex;
  }
 else   if (it == IndexType.ATTRIBUTE) {
    index=data.atvindex;
    avl=data.meta.attrindex;
  }
 else {
    index=data.ftxindex;
    avl=data.meta.ftxtindex;
  }
  if (!avl)   NOINDEX.thrw(call.input,data.meta.name,it);
  return entries(index,prefix);
}
