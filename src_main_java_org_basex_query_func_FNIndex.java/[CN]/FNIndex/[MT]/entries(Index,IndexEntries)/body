{
  return new Iter(){
    final EntryIterator ei=index.entries(entries);
    @Override public ANode next(){
      final byte[] token=ei.next();
      if (token == null)       return null;
      final FElem elem=new FElem(Q_ENTRY);
      elem.add(new FAttr(Q_COUNT,token(ei.count())));
      elem.add(token);
      return elem;
    }
  }
;
}
