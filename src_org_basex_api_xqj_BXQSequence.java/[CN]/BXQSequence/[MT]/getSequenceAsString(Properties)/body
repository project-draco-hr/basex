{
  opened();
  if (it != null && !next)   throw new BXQException(TWICE);
  if (!next && !next())   return "";
  final CachedOutput out=new CachedOutput();
  final XMLSerializer ser=new XMLSerializer(out);
  do {
    final BXQItem item=item();
    item.serialize(item.it,ctx,ser);
  }
 while (next());
  return out.toString();
}
