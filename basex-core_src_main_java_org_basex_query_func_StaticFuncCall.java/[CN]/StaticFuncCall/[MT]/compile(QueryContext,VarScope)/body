{
  super.compile(ctx,scp);
  if (func.ann.contains(Ann.Q_PRIVATE) && !Token.eq(func.sc.baseURI().string(),sc.baseURI().string()))   throw FUNCPRIV.get(info,name.string());
  func.compile(ctx);
  final Expr inl=func.inlineExpr(expr,ctx,scp,info);
  if (inl != null) {
    ctx.compInfo(OPTINLINEFN,func.name);
    return inl;
  }
  type=func.type();
  return this;
}
