{
  super.compile(qc,scp);
  if (func.ann.contains(Ann.Q_PRIVATE) && !Token.eq(func.sc.baseURI().string(),sc.baseURI().string()))   throw FUNCPRIV.get(info,name.string());
  func.compile(qc);
  final Expr inl=func.inlineExpr(exprs,qc,scp,info);
  if (inl != null)   return inl;
  seqType=func.seqType();
  return this;
}
