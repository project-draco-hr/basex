import BaseXClient
try:
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    session.create('database', '<x>Hello World!</x>')
    print session.info()
    print ('\n' + session.execute("xquery doc('database')"))
    session.execute('drop db database')
    session.close()
except IOError as e:
    print e
