{
  super(t,d,EXAMPLES[type(t)]);
  final int i=type(t);
  final Matcher mt=PATTERNS[i].matcher(Token.string(d).trim());
  if (!mt.matches())   dateErr(d,type,EXAMPLES[i]);
  zone(mt,ZONES[i],d);
  if (t == Type.MDA) {
    final int m=xc.getMonth() - 1;
    if (xc.getDay() > DAYS[m] + (m == 1 ? 1 : 0))     Err.or(DATERANGE,type,d);
  }
}
