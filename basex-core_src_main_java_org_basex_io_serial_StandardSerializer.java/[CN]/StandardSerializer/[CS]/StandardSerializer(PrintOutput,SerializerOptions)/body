{
  super(out,sopts);
  indent=sopts.yes(INDENT);
  itemsep(sopts,null);
  final String norm=sopts.get(NORMALIZATION_FORM);
  if (norm.equals(Text.NONE)) {
    form=null;
  }
 else {
    try {
      form=Form.valueOf(norm);
    }
 catch (    final IllegalArgumentException ex) {
      throw SERNORM_X.getIO(norm);
    }
  }
}
