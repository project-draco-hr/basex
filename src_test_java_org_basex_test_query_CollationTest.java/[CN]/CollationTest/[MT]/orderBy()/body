{
  query(PROLOG + "for $a in ('b', 'b') order by $a return $a","b b");
  query("for $a in ('b', 'b') order by $a collation '" + COLLATION + "'return $a","b b");
  query(PROLOG + "for $a in ('\u00c4', 'b') order by $a return $a","\u00c4 b");
