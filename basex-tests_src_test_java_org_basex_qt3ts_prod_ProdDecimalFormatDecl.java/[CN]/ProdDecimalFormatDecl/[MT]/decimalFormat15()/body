{
  final XQuery query=new XQuery("\n" + "        declare decimal-format local:df2 NaN=\"non-numeric\";\n" + "        format-number(number('none'), '#############', 'local:df2')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"non-numeric"));
}
