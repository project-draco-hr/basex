{
  final XQuery query=new XQuery("\n" + "        declare namespace a=\"http://a.com/\";\n" + "        declare namespace b=\"http://a.com/\";\n"+ "        declare decimal-format a:one digit='$' zero-digit=\"0\" minus-sign=\"_\" digit=\"#\";\n"+ "        format-number(931.4857,'000.$$0', 'a:one')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0114"));
}
