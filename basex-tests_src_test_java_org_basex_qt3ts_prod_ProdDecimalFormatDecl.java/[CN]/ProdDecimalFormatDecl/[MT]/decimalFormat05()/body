{
  final XQuery query=new XQuery("\n" + "        declare decimal-format local:df minus-sign=\"-\" percent=\"%\" decimal-separator=\".\";\n" + "      \tformat-number(0.4857,'###.###%', 'local:df')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"48.57%"));
}
