{
  final XQuery query=new XQuery("\n" + "        declare default decimal-format digit='$';\n" + "        format-number(931.4857,'000.$$0')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FODF1310"));
}
