{
  final XQuery query=new XQuery("\n" + "        declare decimal-format q decimal-separator=\".\" grouping-separator=\",\";\n" + "        format-number(931.4857,'fred.ginger', 'q')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FODF1310"));
}
