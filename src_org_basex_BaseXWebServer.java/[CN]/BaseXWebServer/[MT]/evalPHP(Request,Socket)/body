{
  final String phphelp="<?$f=$argv[1];$s=preg_split('/ /',$argv[2]);" + "for($i=0;$i<sizeof($s);$i+=2){$_GET[$s[$i]]=$s[$i+1];}include($f);?>";
  final IO file=IO.get(context.prop.get(Prop.WEBPATH) + "/php.tmp");
  file.write(Token.token(phphelp));
  final StringBuilder args=new StringBuilder();
  for (  final String[] arg : req.args)   args.append(arg[0] + " " + arg[1]+ " ");
  eval(s,context.prop.get(Prop.PHPPATH),file.path(),req.file.path(),args.toString());
  file.delete();
}
