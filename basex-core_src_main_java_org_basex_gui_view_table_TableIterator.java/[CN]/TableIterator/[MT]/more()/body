{
  while (++pre < last) {
    final int k=data.kind(pre);
    text=k == Data.TEXT;
    if (text || k == Data.ATTR) {
      final int id=text ? elem : data.name(pre);
      for (col=0; col < tdata.cols.length; ++col) {
        if (tdata.cols[col].id == id && tdata.cols[col].elem == text) {
          return true;
        }
      }
    }
 else     if (k == Data.ELEM) {
      elem=data.name(pre);
      if (elem == rootElem)       return false;
    }
  }
  return false;
}
