  query(_WEB_RESPONSE_HEADER.args(" map { 'media-type': 'X' }") + "/*:serialization-parameters/*:media-type/@value/string()","X");
  query(_WEB_RESPONSE_HEADER.args(" map { 'media-type': '' }") + "/*:serialization-parameters/*:media-type","");
