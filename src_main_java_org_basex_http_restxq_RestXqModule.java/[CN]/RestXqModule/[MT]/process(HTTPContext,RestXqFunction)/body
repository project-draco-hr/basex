{
  final QueryContext qc=parse(http);
  try {
    for (    final UserFunc uf : qc.funcs.funcs()) {
      if (func.function.info.equals(uf.info)) {
        final RestXqFunction rxf=new RestXqFunction(uf,qc,this);
        rxf.analyze();
        new RestXqResponse(rxf,qc,http).create();
        break;
      }
    }
  }
  finally {
    qc.close();
  }
}
