{
  final FTIter ir=ftexpr.iter(ctx);
  return new NodeIter(){
    @Override public ANode next() throws QueryException {
      final FTNode it=ir.next();
      if (it != null) {
        if (ctx.ftPosData != null)         ctx.ftPosData.add(it.data,it.pre,it.all);
        it.score();
        it.all=null;
      }
      return it;
    }
  }
;
}
