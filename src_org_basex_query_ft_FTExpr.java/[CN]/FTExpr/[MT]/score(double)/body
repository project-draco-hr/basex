{
  return new FTNodeIter(){
    private boolean more;
    @Override public FTNodeItem next(){
      return (more^=true) ? item() : null;
    }
    @Override public int size(){
      return 1;
    }
    @Override public Item get(    final long i){
      return i == 0 ? item() : null;
    }
    private FTNodeItem item(){
      final FTNodeItem ftn=new FTNodeItem();
      ftn.score(s);
      return ftn;
    }
  }
;
}
