{
  final FTNode it=atomic(ctx);
  return new FTNodeIter(){
    private boolean more;
    @Override public FTNode next(){
      return (more^=true) ? it : null;
    }
    @Override public int size(){
      return 1;
    }
    @Override public Item get(    final long i){
      return i == 0 ? it : null;
    }
  }
;
}
