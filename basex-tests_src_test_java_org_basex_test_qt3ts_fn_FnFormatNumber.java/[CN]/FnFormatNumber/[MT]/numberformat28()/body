{
  final XQuery query=new XQuery("format-number(2392.14*(-36.58),'000,000.000###;-###,###.000###')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-87,504.4812"));
}
