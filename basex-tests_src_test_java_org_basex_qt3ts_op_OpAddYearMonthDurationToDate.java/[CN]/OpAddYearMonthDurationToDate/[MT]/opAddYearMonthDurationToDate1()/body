{
  final XQuery query=new XQuery("xs:date(\"2000-10-30\") + xs:yearMonthDuration(\"P1Y2M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2001-12-30"));
}
