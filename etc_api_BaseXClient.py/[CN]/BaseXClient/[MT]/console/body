def console(self):
    if (self.session() == True):
        session.sendCommand('SET INFO ON')
        session.s.recv(1024)
        while True:
            com = self.readCommand()
            if (com == 'exit'):
                session.sendCommand('exit')
                break
            print session.execute(com)
        try:
            session.sendCommand('exit')
        except:
            session.close()
        print 'See you.'
    else:
        print 'Access denied.'
        session.close()
