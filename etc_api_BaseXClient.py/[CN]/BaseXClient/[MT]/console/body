def console(self):
    if (self.session() == True):
        session.sendCommand('SET INFO ON')
        session.s.recv(1024)
        while (self.readCommand() != 'exit'):
            session.sendCommand(self.com)
            data = session.receive()
            print data
        try:
            session.sendCommand('exit')
        except:
            session.close()
        print 'See you.'
    else:
        print 'Access denied.'
        session.close()
