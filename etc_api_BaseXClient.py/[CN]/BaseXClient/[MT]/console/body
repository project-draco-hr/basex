def console(self):
    if self.session():
        session.send('SET INFO ON')
        session.receive()
        while True:
            com = raw_input('> ').strip()
            if (com == 'exit'):
                break
            if com:
                print session.execute(com)
        try:
            session.send('exit')
        except:
            session.close()
        print 'See you.'
    else:
        print 'Access denied.'
        session.close()
