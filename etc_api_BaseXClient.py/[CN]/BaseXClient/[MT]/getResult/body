def getResult(self, com):
    fcom = self.formatCommand(((com + ' ') + self.sesid))
    if self.verb:
        print ('c=>' + repr(fcom))
    self.connect()
    self.s.send(fcom)
    self.data = self.s.recv(10000000)
    if self.verb:
        print ('R=>' + repr(self.data))
    try:
        self.sesid = str(((ord(self.data[2]) * 256) + ord(self.data[3])))
    except:
        self.data = (self.data + '')
    self.close()
