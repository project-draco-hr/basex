{
  tb.add(IDXENTRIES + size + NL);
  int max=0;
  int c=0;
  for (int o=0; o < nr; o++) {
    int tl=txtMin[o].length;
    if (tl == 0)     c++;
 else     if (max < tl)     max=tl;
    tl=txtMax[o].length;
    if (tl == 0)     c++;
 else     if (max < tl)     max=tl;
  }
  print(tb,txtMax,occMax,max + 2);
  if (c == 0)   tb.add("  " + DOTS + NL);
  print(tb,txtMin,occMin,max + 2);
}
