{
  final boolean text=ind == IndexType.TEXT;
  final byte kind=text ? Data.TEXT : Data.ATTR;
  return new NodeIter(){
    int pre=-1;
    @Override public Nod next(){
      while (++pre != data.meta.size) {
        if (data.kind(pre) == kind && eq(data.text(pre,text),val))         return new DBNode(data,pre,kind);
      }
      return null;
    }
  }
;
}
