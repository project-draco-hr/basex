{
  Iter[] iter={};
  final Iter ir=expr.iter(ctx);
  Item it;
  while ((it=ir.next()) != null) {
    final int s=iter.length;
    final Iter[] t=new Iter[s + 1];
    System.arraycopy(iter,0,t,0,s);
    iter=t;
    iter[s]=index(it.atom());
  }
  return iter.length == 0 ? Iter.EMPTY : iter.length == 1 ? iter[0] : new Union(new Expr[]{expr}).eval(iter);
}
