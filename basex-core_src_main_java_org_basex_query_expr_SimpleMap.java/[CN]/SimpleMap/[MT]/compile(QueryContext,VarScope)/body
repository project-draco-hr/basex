{
  final Value cv=qc.value;
  try {
    final int el=exprs.length;
    for (int e=0; e < el; e++) {
      final Expr ex=exprs[e].compile(qc,scp);
      if (ex.isEmpty())       return optPre(qc);
      exprs[e]=ex;
      qc.value=null;
    }
  }
  finally {
    qc.value=cv;
  }
  return optimize(qc,scp);
}
