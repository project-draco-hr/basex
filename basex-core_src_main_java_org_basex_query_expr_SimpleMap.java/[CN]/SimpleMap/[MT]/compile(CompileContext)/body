{
  final Value cv=cc.qc.value;
  try {
    final int el=exprs.length;
    for (int e=0; e < el; e++) {
      try {
        exprs[e]=exprs[e].compile(cc);
      }
 catch (      final QueryException ex) {
        exprs[e]=FnError.get(ex,seqType,cc.sc());
      }
      cc.qc.value=null;
    }
  }
  finally {
    cc.qc.value=cv;
  }
  return optimize(cc);
}
