{
  final int[] contentDir=FSUtils.getChildren(data,pre);
  final int[] allDir=print(contentDir,out);
  for (  final int dir : allDir) {
    if (!fListDot) {
      if (Token.startsWith(FSUtils.getName(data,dir),'.'))       continue;
    }
    out.print(NL);
    out.print(FSUtils.getPath(data,dir));
    out.print(NL);
    lsRecursive(dir,out);
  }
}
