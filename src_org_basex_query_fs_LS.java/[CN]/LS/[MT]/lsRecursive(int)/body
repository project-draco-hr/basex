{
  int[] dir=FSUtils.getAllOfDir(data,pre);
  int[] allDir=new int[dir.length];
  int j=0;
  for (  int i : dir) {
    if (FSUtils.isDir(data,i))     allDir[j++]=i;
  }
  print(dir);
  for (int i=0; allDir[i] != 0 && i < allDir.length; i++) {
    out.print(NL);
    printPath(allDir[i]);
    out.print(NL);
    lsRecursive(allDir[i]);
  }
}
