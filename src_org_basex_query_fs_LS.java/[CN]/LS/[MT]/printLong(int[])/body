{
  for (  int j : result) {
    byte[] name=FSUtils.getName(data,j);
    long size=FSUtils.getSize(data,j);
    byte[] time=FSUtils.getMtime(data,j);
    char file='d';
    if (FSUtils.isFile(data,j))     file='f';
    if (!fListDot) {
      if (Token.startsWith(name,'.'))       continue;
    }
    out.print(file);
    out.print("\t");
    out.print(name);
    out.print("\t");
    out.print("" + size);
    out.print("\t");
    out.print(time);
    out.print(NL);
  }
  out.print(NL);
}
