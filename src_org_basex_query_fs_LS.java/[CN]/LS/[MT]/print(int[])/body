{
  if (fPrintLong) {
    return printLong(result);
  }
 else {
    IntList allDir=new IntList();
    for (    int j : result) {
      if (FSUtils.isDir(data,j))       allDir.add(j);
      byte[] name=FSUtils.getName(data,j);
      if (!fListDot) {
        if (Token.startsWith(name,'.'))         continue;
      }
      out.print(name);
      out.print("\t");
    }
    out.print(NL);
    return allDir.finish();
  }
}
