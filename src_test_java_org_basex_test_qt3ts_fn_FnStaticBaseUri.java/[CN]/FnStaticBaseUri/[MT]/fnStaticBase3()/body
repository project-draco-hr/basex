{
  final XQuery query=new XQuery("declare base-uri \"mailto:John.Doe@example.com\"; fn:string(fn:static-base-uri())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"mailto:John.Doe@example.com"));
}
