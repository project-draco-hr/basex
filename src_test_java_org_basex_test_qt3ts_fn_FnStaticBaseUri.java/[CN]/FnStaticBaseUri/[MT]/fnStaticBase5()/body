{
  final XQuery query=new XQuery("declare base-uri \"telnet://192.0.2.16:80/\"; fn:string(fn:static-base-uri())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"telnet://192.0.2.16:80/"));
}
