{
  opened();
  try {
    for (    final Command c : new CommandParser(cmd,sc.context).parse()) {
      try {
        c.execute(sc.context);
      }
 catch (      final BaseXException ex) {
        throw new BXQException(ex.getMessage());
      }
    }
  }
 catch (  final QueryException ex) {
    throw new BXQException(ex);
  }
}
