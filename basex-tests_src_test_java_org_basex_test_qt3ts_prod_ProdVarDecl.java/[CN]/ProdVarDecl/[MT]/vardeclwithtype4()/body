{
  final XQuery query=new XQuery("declare variable $var as xs:boolean := fn:true(); declare variable $input-context1 external; $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
