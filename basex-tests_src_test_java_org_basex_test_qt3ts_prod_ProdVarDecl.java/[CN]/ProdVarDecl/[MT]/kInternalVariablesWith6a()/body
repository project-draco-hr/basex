{
  xquery10();
  final XQuery query=new XQuery("declare variable $var1 := $var2; declare variable $var2 := 2; true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
