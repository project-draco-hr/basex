{
  final XQuery query=new XQuery("declare variable $x := xs:decimal(\"999999999999999999\") ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("999999999999999999"));
}
