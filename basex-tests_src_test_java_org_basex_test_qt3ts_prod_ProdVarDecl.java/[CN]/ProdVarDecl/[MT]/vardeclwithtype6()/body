{
  final XQuery query=new XQuery("declare variable $var as xs:float := xs:float(12.5E10); declare variable $input-context1 external; $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1.25E11"));
}
