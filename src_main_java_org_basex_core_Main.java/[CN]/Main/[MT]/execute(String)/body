{
  try {
    for (    final Command cmd : new CommandParser(in,context).parse()) {
      if (cmd instanceof Exit)       return false;
      final int i=cmd instanceof Password && cmd.args[0] == null ? 0 : (cmd instanceof CreateUser || cmd instanceof AlterUser) && cmd.args[1] == null ? 1 : -1;
      if (i != -1) {
        Util.out(SERVERPW + COLS);
        cmd.args[i]=md5(Util.password());
      }
      execute(cmd,verbose);
    }
  }
 catch (  final QueryException ex) {
    Util.debug(ex);
    throw new BaseXException(ex);
  }
  return true;
}
