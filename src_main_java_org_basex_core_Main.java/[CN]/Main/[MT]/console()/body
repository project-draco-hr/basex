{
  final Scanner cr=new Scanner(System.in);
  while (console) {
    Util.out("> ");
    if (!cr.hasNextLine())     break;
    final String in=cr.nextLine();
    if (in.isEmpty())     continue;
    try {
      if (!execute(in)) {
        Util.outln(BYE[new Random().nextInt(4)]);
        return;
      }
    }
 catch (    final IOException ex) {
      Util.errln(ex);
    }
  }
}
