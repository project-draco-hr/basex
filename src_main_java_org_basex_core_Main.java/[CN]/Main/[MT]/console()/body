{
  while (console) {
    Util.out("> ");
    for (    final String in : inputs()) {
      if (in.isEmpty())       continue;
      try {
        if (!execute(in)) {
          Util.outln(CLIENTBYE[new Random().nextInt(4)]);
          return;
        }
      }
 catch (      final IOException ex) {
        Util.errln(ex);
      }
    }
  }
}
