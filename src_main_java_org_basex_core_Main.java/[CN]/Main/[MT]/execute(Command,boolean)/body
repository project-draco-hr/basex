{
  final Session ss=session();
  if (ss == null)   return false;
  try {
    ss.execute(cmd,out);
    if (cmd instanceof Exit)     return true;
    if (v) {
      final String inf=ss.info();
      if (!inf.isEmpty())       out(inf);
    }
    return true;
  }
 catch (  final BaseXException ex) {
    error(null,ex.getMessage());
    return false;
  }
}
