{
  out.writeDiffs(docs());
  if (dirty && pathIndex) {
    try (final DataOutput doc=new DataOutput(data.meta.dbfile(DATADOC))){
      final TokenList paths=paths();
      doc.writeNum(paths.size());
      for (      final byte[] path : paths)       doc.writeToken(path);
    }
     dirty=false;
  }
}
