{
  if (buf.getShort() == IFD_TYPE_ASCII && buf.getInt() == 20) {
    obj.bfc.position(ifdOff + buf.getInt());
    obj.bfc.buffer(20);
    byte[] data=new byte[20];
    obj.bfc.get(data);
    obj.fsparser.metaEvent(Element.DATE,DataType.DATETIME,Definition.CREATE_TIME,null,ParserUtil.convertDateTime(data));
  }
 else   error(obj,"DateTime (0x0132)");
}
