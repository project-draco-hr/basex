{
  if (bfc.get() == 8) {
    bfc.buffer(8);
    int width=bfc.getShort();
    int height=bfc.getShort();
    fsparser.metaEvent(Element.WIDTH,DataType.INTEGER,Definition.PIXEL,null,Token.token(width));
    fsparser.metaEvent(Element.HEIGHT,DataType.INTEGER,Definition.PIXEL,null,Token.token(height));
  }
 else {
    if (NewFSParser.VERBOSE)     BaseX.debug("Wrong data precision field (%).",bfc.getFileName());
  }
}
