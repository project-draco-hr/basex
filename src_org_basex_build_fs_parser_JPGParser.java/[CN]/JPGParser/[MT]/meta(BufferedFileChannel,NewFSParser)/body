{
  if (!check(f))   return;
  meta.setType(MetaType.PICTURE);
  meta.setFormat(MimeType.JPG);
  bfc=f;
  fsparser=parser;
  bfc.skip(-2);
  while (bfc.get() == 0xFF) {
    final int b=bfc.get();
    final int size=readSize();
switch (b) {
case 0xE0:
      readJFIF(size);
    break;
case 0xE1:
  readExif(size);
break;
case 0xFE:
readComment(size);
break;
default :
if (b >= 0xC0 && b <= 0xC3) {
readDimensions();
return;
}
bfc.skip(size);
}
bfc.buffer(4);
}
}
