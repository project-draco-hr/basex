{
  final int len=HEADER_EXIF.length;
  bfc.buffer(len);
  if (!checkNextBytes(HEADER_EXIF))   bfc.skip(size - len);
  final BufferedFileChannel sub=bfc.subChannel(size - len);
  try {
    exifParser.parse(sub,fsparser);
  }
  finally {
    try {
      sub.finish();
    }
 catch (    Exception ex) {
      Main.debug(ex);
    }
  }
}
