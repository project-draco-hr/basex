{
switch (sig) {
case _CONVERT_INTEGER_FROM_BASE:
    return fromBase(ctx,ii);
case _CONVERT_INTEGER_TO_BASE:
  return toBase(ctx,ii);
case _CONVERT_TO_STRING:
return toString(ctx);
case _CONVERT_TO_BASE64BINARY:
return toBase64(ctx);
case _CONVERT_TO_HEXBINARY:
return toHex(ctx);
default :
return super.item(ctx,ii);
}
}
