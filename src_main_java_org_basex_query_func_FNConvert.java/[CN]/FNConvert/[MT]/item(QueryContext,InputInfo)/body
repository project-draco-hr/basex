{
switch (sig) {
case _CONVERT_INTEGER_FROM_BASE:
    return integerFromBase(ctx,ii);
case _CONVERT_INTEGER_TO_BASE:
  return integerToBase(ctx,ii);
case _CONVERT_BINARY_TO_STRING:
return toString(ctx);
case _CONVERT_STRING_TO_BASE64:
return new B64(stringToBinary(ctx));
case _CONVERT_BYTES_TO_BASE64:
return new B64(bytesToBinary(ctx));
case _CONVERT_STRING_TO_HEX:
return new Hex(stringToBinary(ctx));
case _CONVERT_BYTES_TO_HEX:
return new Hex(bytesToBinary(ctx));
default :
return super.item(ctx,ii);
}
}
