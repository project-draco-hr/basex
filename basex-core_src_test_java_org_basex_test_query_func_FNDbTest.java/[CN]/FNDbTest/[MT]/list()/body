{
  new Add("test/docs",FLDR).execute(context);
  contains(_DB_LIST.args(NAME),"test/");
  new CreateDB(NAME + 1).execute(context);
  new CreateDB(NAME + 2).execute(context);
  contains(_DB_LIST.args(),NAME + 1 + ' '+ NAME+ 2);
