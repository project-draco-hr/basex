{
  if (pre == -1)   return null;
  final int kind=data.kind(pre);
switch (kind) {
case Data.DOC:
    return new DocImpl(data);
case Data.ELEM:
  return new ElementImpl(data,pre);
case Data.TEXT:
return new TextImpl(data,pre);
case Data.ATTR:
return new AttrImpl(data,pre);
case Data.COMM:
return new CommentImpl(data,pre);
case Data.PI:
return new PIImpl(data,pre);
}
return null;
}
