{
  if (store()) {
    try {
      final XMLBuilder xml=new XMLBuilder().indent().open(USERS);
      for (      final User user : list)       user.write(xml);
      output.write(xml.finish());
    }
 catch (    final IOException ex) {
      Util.errln(ex);
    }
  }
 else {
    output.delete();
  }
}
