{
  if (!w) {
    if (inDoc) {
      if (level == 1)       error(AFTERROOT,parser.det(),cut(t,20));
    }
 else {
      error(BEFOREROOT,parser.det(),cut(t,20));
    }
  }
 else   if (t.length == 0 || (!inDoc || level == 1)) {
    return;
  }
  final byte[] tok=meta.chop ? trim(t) : t;
  if (tok.length != 0)   addText(tok,Data.TEXT);
}
