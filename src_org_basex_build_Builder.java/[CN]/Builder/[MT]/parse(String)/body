{
  final Performance perf=Prop.debug ? new Performance() : null;
  Main.debug("Database: ");
  parser.parse(this);
  if (lvl != 0)   error(DOCOPEN,parser.det(),tags.key(tagStack[lvl]));
  meta.lastid=meta.size;
  if (meta.size == 0) {
    startDoc(token(db));
    endDoc();
    setSize(0,meta.size);
  }
  Main.gc(perf);
}
