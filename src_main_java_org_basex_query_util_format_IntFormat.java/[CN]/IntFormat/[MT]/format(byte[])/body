{
  final TokenParser tp=new TokenParser(format);
  if (tp.consume('c') || tp.consume('o')) {
    final TokenBuilder ord=new TokenBuilder();
    if (tp.consume('(')) {
      while (!tp.consume(')')) {
        if (!tp.more())         INVORDINAL.thrw(info,format);
        ord.add(tp.next());
      }
      ;
    }
    ordinal=ord.finish();
  }
  if (!tp.consume('a'))   tp.consume('t');
  if (tp.more())   INVORDINAL.thrw(info,format);
}
