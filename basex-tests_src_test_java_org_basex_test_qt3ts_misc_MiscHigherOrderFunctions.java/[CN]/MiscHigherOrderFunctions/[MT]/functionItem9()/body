{
  final XQuery query=new XQuery("function-name(let $a := 92, $b := true() return function($c) { $a, $b, $c })",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertCount(0));
}
