{
  final XQuery query=new XQuery("function($a as item()) as xs:integer { $a } instance of function(item()) as item()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
