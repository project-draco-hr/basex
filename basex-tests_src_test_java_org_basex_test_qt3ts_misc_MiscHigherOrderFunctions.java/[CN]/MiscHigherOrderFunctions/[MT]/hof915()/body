{
  final XQuery query=new XQuery("let $ops := substring-before('abc', ' ', (), ?) return $ops('Michael Kay')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0017"));
}
