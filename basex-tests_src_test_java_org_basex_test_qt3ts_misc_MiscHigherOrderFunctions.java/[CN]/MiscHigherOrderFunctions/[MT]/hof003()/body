{
  final XQuery query=new XQuery("\n" + "        import module namespace m=\"http://example.com/hof-003\"; \n" + "        let $f := m:f#1 return $f(17)",ctx);
  try {
    query.addModule("http://example.com/hof-003",file("misc/HigherOrderFunctions/module-hof-003.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("18"));
}
