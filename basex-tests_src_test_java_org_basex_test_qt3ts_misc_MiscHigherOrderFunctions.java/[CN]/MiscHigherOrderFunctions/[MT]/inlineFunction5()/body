{
  final XQuery query=new XQuery("function($a as xs:integer, $b as xs:double) as xs:double { $a * $b + 5 }(3, 2)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("11"));
}
