{
  final XQuery query=new XQuery("let $a := \"monkey\" return function($a) { \"lala\", $a }(\"gibbon\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"lala\", \"gibbon\""));
}
