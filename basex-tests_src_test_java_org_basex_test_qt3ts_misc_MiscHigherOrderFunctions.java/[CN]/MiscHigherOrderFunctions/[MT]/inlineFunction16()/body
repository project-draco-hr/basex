{
  final XQuery query=new XQuery("function($Q{http://local/}foo, $Q{http://local/}bar, $fn:foo) { \n" + "               \"lala\", $Q{http://local/}foo, $Q{http://local/}bar }(\"gibbon\", \"monkey\", \"ape\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"lala\", \"gibbon\", \"monkey\""));
}
