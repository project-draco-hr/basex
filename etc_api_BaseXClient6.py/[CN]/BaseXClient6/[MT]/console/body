def console(self):
    if (self.connect() == True):
        self.sendCommand('SET INFO ON')
        self.s.recv(1024)
        while (self.readCommand() != 'exit'):
            self.sendCommand(self.com)
            data = self.receive()
            print data
        try:
            self.sendCommand('exit')
        except:
            self.close()
        print 'See you.'
    else:
        print 'Access denied.'
        self.close()
