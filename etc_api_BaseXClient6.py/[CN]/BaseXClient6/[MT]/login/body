def login(self):
    ts = self.receive()
    user = raw_input('Username: ')
    pw = raw_input('Password: ')
    pwmd5 = hashlib.md5(pw).hexdigest()
    m = hashlib.md5()
    m.update(pwmd5)
    m.update(ts)
    complete = m.hexdigest()
    self.sendCommand(user)
    self.sendCommand(complete)
    data = self.s.recv(1)
    return ('\x00' == data)
