{
  final NodIter s=new NodIter();
  Nod i=n.next();
  while (i != null) {
    if (i.type == Type.TXT) {
      final TokenBuilder tb=new TokenBuilder();
      while (i != null && i.type == Type.TXT) {
        tb.add(i.str());
        i=n.next();
      }
      s.add(new FTxt(tb.finish(),null));
    }
 else {
      s.add(i);
      i=n.next();
    }
  }
  return s;
}
