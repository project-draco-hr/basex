{
  table.header.add("Document");
  for (  final String h : header)   table.header.add(h);
  if (db == null) {
    for (    final String d : List.list(ctx))     analyze(d);
  }
 else {
    analyze(db);
  }
  if (tab) {
    Util.out(table.toString());
  }
 else {
    Util.out(Token.string(table.finish()));
  }
}
