{
  while (++pre < last) {
    final int k=data.kind(pre);
    elem=k == Data.TEXT;
    if (elem || k == Data.ATTR) {
      final int id=elem ? tag : data.attNameID(pre);
      for (col=0; col < tdata.cols.length; col++) {
        if (tdata.cols[col].id == id && tdata.cols[col].elem == elem) {
          return true;
        }
      }
    }
 else     if (k == Data.ELEM) {
      tag=data.tagID(pre);
      if (tag == rootTag)       return false;
    }
  }
  return false;
}
