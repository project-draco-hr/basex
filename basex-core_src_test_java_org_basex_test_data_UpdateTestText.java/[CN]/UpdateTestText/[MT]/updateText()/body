{
  final Data data=context.data();
  data.startUpdate();
  data.update(10,Data.TEXT,T_JUNIT);
  data.finishUpdate();
  assertEquals(Data.TEXT,data.kind(10));
  assertArraysEquals(T_JUNIT,data.text(10,true));
  reload();
  assertEquals(Data.TEXT,data.kind(10));
  assertArraysEquals(T_JUNIT,data.text(10,true));
}
