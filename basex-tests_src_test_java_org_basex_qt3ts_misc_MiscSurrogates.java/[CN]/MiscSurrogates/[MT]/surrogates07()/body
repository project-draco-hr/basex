{
  final XQuery query=new XQuery("string-to-codepoints(\"abc&#x1D156;def\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"97 98 99 119126 100 101 102"));
}
