{
  final XQuery query=new XQuery("translate(\"abc&#x1D156;def\", \"&#x1D156;de\", \"#DE\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abc#DEf"));
}
