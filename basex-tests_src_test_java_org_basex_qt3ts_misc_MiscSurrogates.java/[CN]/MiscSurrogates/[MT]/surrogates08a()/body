{
  final XQuery query=new XQuery("codepoints-to-string((97, 98, 99, 119126, 100, 101, 102))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abc????def"));
}
