{
  final XQuery query=new XQuery("if (implicit-timezone() eq xs:dayTimeDuration('-PT9M'))\n" + "            then xs:gMonth(\"--06-09:01\") ne xs:gMonth(\"--06\")\n" + "            else xs:gMonth(\"--06-09:00\") ne xs:gMonth(\"--06\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
