{
  final XQuery query=new XQuery("declare default element namespace \"%gg\"; true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQST0046") || assertBoolean(true)));
}
