{
  final XQuery query=new XQuery("declare default collation \"%gg\"; fn:true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQST0046") || error("XQST0038") || assertBoolean(true)));
}
