{
  final XQuery query=new XQuery("declare namespace prefix = \"http://www.w3.org/\"; (# prefix:pragma #) { }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0079"));
}
