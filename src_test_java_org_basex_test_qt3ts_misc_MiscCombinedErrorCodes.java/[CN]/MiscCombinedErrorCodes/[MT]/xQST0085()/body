{
  final XQuery query=new XQuery("<element xmlns:foo=\"http://www.w3.org/\"> <element xmlns:foo=\"\" /> </element>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0085"));
}
