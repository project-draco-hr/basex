{
  final XQuery query=new XQuery("<a xml:space=\"space\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQDY0092") || assertSerialization("<a xml:space=\"space\"/>",false)));
}
