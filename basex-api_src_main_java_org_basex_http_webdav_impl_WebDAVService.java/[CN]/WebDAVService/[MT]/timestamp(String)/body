{
  final String s=DATA.args(_DB_INFO.args("$p") + "/descendant::" + FNDb.toName(Text.TIMESTAMP)+ "[1]");
  final Query q=http.session().query(s);
  q.bind("p",db);
  try {
    return DateTime.parse(q.execute(),DateTime.DATETIME).getTime();
  }
 catch (  final Exception ex) {
    Util.errln(ex);
    return 0L;
  }
}
