{
  final WebDAVQuery query=new WebDAVQuery(DATA.args(_DB_INFO.args("$path") + "/descendant::" + FNDb.toName(Text.TIMESTAMP)+ "[1]")).bind("path",db);
  try {
    return DateTime.parse(execute(query).get(0),DateTime.DATETIME).getTime();
  }
 catch (  final ParseException ex) {
    Util.errln(ex);
    return 0L;
  }
}
