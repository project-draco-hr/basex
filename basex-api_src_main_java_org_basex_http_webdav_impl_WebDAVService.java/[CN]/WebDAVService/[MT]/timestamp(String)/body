{
  final Query q=session().query(ext("path") + DATA.args(_DB_INFO.args("$path") + "/descendant::" + FNDb.toName(Text.TIMESTAMP)+ "[1]"));
  q.bind("path",db);
  try {
    return DateTime.parse(q.execute(),DateTime.DATETIME).getTime();
  }
 catch (  final ParseException ex) {
    Util.errln(ex);
    return 0L;
  }
}
