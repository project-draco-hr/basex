{
  final Session session=session();
  session.setOutputStream(out);
  final Query q=session.query(ext("db") + ext("path") + "declare option output:"+ (raw ? "method 'raw'; " + _DB_RETRIEVE.args("$db","$path") : "use-character-maps 'webdav'; " + _DB_OPEN.args("$db","$path")));
  q.bind("db",db);
  q.bind("path",path);
  q.execute();
}
