{
  session.setOutputStream(out);
  final WebDAVQuery query=new WebDAVQuery("declare option output:" + (raw ? "method 'raw'; " + _DB_RETRIEVE.args("$db","$path") : "use-character-maps 'webdav'; " + _DB_OPEN.args("$db","$path")));
  query.bind("db",db);
  query.bind("path",path);
  execute(query);
}
