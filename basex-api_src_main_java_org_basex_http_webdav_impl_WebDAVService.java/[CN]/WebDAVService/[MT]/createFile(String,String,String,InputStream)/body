{
  final Session session=session();
  session.execute(new Open(db));
  final String dbp=path.isEmpty() ? name : path + SEP + name;
  if (pathExists(db,dbp)) {
    session.execute(new Open(db));
    session.execute(new Delete(dbp));
  }
 else {
    deleteDummy(db,path);
  }
  return addFile(db,dbp,in);
}
