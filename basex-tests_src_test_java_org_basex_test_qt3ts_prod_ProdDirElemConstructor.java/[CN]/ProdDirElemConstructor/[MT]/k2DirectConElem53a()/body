{
  final XQuery query=new XQuery("namespace {\"p\"} {\"abc\"}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("namespace-node()") && assertStringValue(false,"abc")));
}
