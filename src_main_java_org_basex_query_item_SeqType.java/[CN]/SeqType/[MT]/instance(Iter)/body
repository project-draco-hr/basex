{
  Item it=iter.next();
  if (it == null)   return type == Type.EMP || occ % 2 != 0;
  if (occ < 2)   return iter.next() == null && it.type.instance(type) && checkInstance(it);
  do {
    if (!it.type.instance(type) || !checkInstance(it))     return false;
  }
 while ((it=iter.next()) != null);
  return true;
}
