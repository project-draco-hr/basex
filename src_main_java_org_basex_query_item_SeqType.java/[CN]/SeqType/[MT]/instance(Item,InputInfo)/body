{
  final Type ip=it.type;
  final boolean ins=ip.instanceOf(type);
  if (!ins && !ip.isUntyped() && !ip.isFunction()&& (ip != AtomType.FLT || type != AtomType.DBL)&& (ip != AtomType.URI || type != AtomType.STR)&& (type != AtomType.FLT && type != AtomType.DBL || !ip.instanceOf(AtomType.DEC)))   Err.promote(ii,this,it);
  return ins;
}
