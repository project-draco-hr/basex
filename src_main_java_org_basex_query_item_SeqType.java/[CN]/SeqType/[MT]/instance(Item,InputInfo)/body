{
  final Type t=it.type;
  final boolean ins=it.type.instanceOf(type);
  if (!ins && !it.isUntyped() && !it.isFunction()&& (t != AtomType.FLT || type != AtomType.DBL)&& (t != AtomType.URI || type != AtomType.STR)&& (type != AtomType.FLT && type != AtomType.DBL || !t.instanceOf(AtomType.DEC)))   Err.promote(ii,type,it);
  return ins;
}
