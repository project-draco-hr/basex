{
  final Type t=it.type;
  final boolean ins=it.type.instance(type);
  if (!ins && !t.unt() && !t.func()&& (t != AtomType.FLT || type != AtomType.DBL)&& (t != AtomType.URI || type != AtomType.STR)&& (type != AtomType.FLT && type != AtomType.DBL || !t.instance(AtomType.DEC)))   Err.promote(ii,type,it);
  return ins;
}
