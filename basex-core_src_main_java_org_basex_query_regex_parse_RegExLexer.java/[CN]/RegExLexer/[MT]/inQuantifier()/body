{
  final int curr=next();
  if (curr == ',')   return COMMA;
  if (curr == '}') {
    state=0;
    return QUANT_CLOSE;
  }
  if (curr < '0' || curr > '9')   throw error("{",curr);
  while (pos < input.length) {
    final byte b=input[pos];
    if (b < '0' || b > '9')     break;
    tb.add(b);
    pos++;
  }
  payload=tb.toString();
  return NUMBER;
}
