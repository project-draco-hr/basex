{
  final Data data=context.data();
  data.startUpdate();
  data.update(6,Data.ELEM,T_JUNIT,Token.EMPTY);
  data.finishUpdate();
  assertEquals(Data.ELEM,data.kind(6));
  assertArraysEquals(T_JUNIT,data.name(6,Data.ELEM));
  reload();
  assertEquals(Data.ELEM,data.kind(6));
  assertArraysEquals(T_JUNIT,data.name(6,Data.ELEM));
}
