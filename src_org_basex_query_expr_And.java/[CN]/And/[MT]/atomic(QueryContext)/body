{
  double s=0;
  for (  final Expr e : expr) {
    final Item it=e.ebv(ctx);
    if (!it.bool()) {
      if (ctx.ftdata != null && ctx.item != null)       ctx.ftdata.remove(((DBNode)ctx.item).pre + 1);
      return Bln.FALSE;
    }
    s=Scoring.and(s,it.score());
  }
  return s == 0 ? Bln.TRUE : Bln.get(s);
}
