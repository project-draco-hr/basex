{
  double s=0;
  for (  final Expr e : expr) {
    final Item it=e.ebv(ctx);
    if (!it.bool()) {
      if (ctx.ftpos != null && ctx.item != null && ctx.item instanceof DBNode)       ctx.ftpos.remove(((DBNode)ctx.item).pre + 1);
      return Bln.FALSE;
    }
    s=ctx.score.and(s,it.score());
  }
  return s == 0 ? Bln.TRUE : Bln.get(s);
}
