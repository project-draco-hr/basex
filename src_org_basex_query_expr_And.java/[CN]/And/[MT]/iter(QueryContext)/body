{
  double s=0;
  for (  final Expr e : expr) {
    final Item it=ctx.iter(e).ebv();
    if (!it.bool()) {
      if (ctx.ftdata != null)       ctx.ftdata.remove(((DBNode)ctx.item).pre + 1);
      return Bln.FALSE.iter();
    }
    s=Scoring.and(s,it.score());
  }
  return (s == 0 ? Bln.TRUE : Bln.get(s)).iter();
}
