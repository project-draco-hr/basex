{
  final XQuery query=new XQuery("fn:unparsed-text(\"http://www.w3.org/fots/unparsed-text/text-xml-utf-8.xml\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"<?xml version=\"1.0\" encoding=\"utf-8\"?><text>hello??world</text>"));
}
