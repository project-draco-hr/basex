{
  final XQuery query=new XQuery("fn:unparsed-text( \"http://www.w3.org/fots/unparsed-text/does-not-exist.txt\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOUT1170"));
}
