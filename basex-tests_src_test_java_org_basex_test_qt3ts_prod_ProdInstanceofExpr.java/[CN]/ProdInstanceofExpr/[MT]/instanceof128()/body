{
  final XQuery query=new XQuery("\n" + "         name#1 instance of function(element(A)) as xs:NCName\n" + "       ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
