{
  QNm[] polygon={Q_GML_POLYGON};
  QNm[] other={Q_GML_POINT,Q_GML_LINEARRING,Q_GML_LINESTRING,Q_GML_MULTILINESTRING,Q_GML_MULTIPOINT,Q_GML_MULTIPOLYGON};
  if (checkNode(node,polygon)) {
    Geometry geom=gmlReader(node);
    return Int.get(((Polygon)geom).getNumInteriorRing());
  }
  if (checkNode(node,other))   throw GeoErrors.polygonNeeded(node.qname().local());
  return null;
}
