{
  QNm[] line={Q_GML_LINEARRING,Q_GML_LINESTRING};
  QNm[] other={Q_GML_POLYGON,Q_GML_MULTILINESTRING,Q_GML_MULTIPOINT,Q_GML_MULTIPOLYGON};
  if (checkNode(node,line)) {
    Geometry geom=gmlReader(node);
    if (geom instanceof LineString)     return Bln.get(((LineString)geom).isRing());
    if (geom instanceof LinearRing)     return Bln.get(((LinearRing)geom).isRing());
  }
  if (checkNode(node,other))   throw GeoErrors.lineNeeded(node.qname().local());
  return null;
}
