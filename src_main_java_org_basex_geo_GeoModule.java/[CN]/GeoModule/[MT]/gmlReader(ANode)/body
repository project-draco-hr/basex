{
  Geometry geom=null;
  try {
    String input=element.serialize().toString();
    GMLReader gmlReader=new GMLReader();
    GeometryFactory geoFactory=new GeometryFactory();
    geom=gmlReader.read(input,geoFactory);
  }
 catch (  Throwable e) {
    throw GeoErrors.gmlReaderErr(e);
  }
  return geom;
}
