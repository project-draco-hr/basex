{
  if (node.type != NodeType.ELM)   Err.FUNCMP.thrw(null,this,NodeType.ELM,node.type);
  QNm qname=node.qname();
  if (qname.eq(Q_GML_LINESTRING) || qname.eq(Q_GML_LINEARRING) || qname.eq(Q_GML_MULTILINESTRING)|| qname.eq(Q_GML_POINT)|| qname.eq(Q_GML_MULTIPOINT)|| qname.eq(Q_GML_POLYGON)|| qname.eq(Q_GML_MULTIPOLYGON)) {
    Geometry geom=gmlReader(node);
    return Int.get(geom.getNumPoints());
  }
  throw GeoErrors.unrecognizedGeo(qname.local());
}
