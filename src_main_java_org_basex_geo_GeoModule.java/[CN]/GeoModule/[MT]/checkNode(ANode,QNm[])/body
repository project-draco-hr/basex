{
  if (node.type != NodeType.ELM)   Err.FUNCMP.thrw(null,this,NodeType.ELM,node.type);
  QNm qname=node.qname();
  if (geoName.length == 0) {
    if (qname.eq(Q_GML_POINT) || qname.eq(Q_GML_LINESTRING) || qname.eq(Q_GML_POLYGON)|| qname.eq(Q_GML_MULTIPOINT)|| qname.eq(Q_GML_MULTILINESTRING)|| qname.eq(Q_GML_MULTIPOLYGON)|| qname.eq(Q_GML_LINEARRING))     return true;
    throw GeoErrors.unrecognizedGeo(qname.local());
  }
  for (  QNm geo : geoName)   if (qname.eq(geo))   return true;
  return false;
}
