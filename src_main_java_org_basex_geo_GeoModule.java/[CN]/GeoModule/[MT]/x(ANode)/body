{
  if (node.type != NodeType.ELM)   Err.FUNCMP.thrw(null,this,NodeType.ELM,node.type);
  QNm qname=node.qname();
  if (qname.eq(Q_GML_POINT)) {
    Geometry geom=gmlReader(node);
    return Dbl.get(geom.getCoordinate().x);
  }
  if (qname.eq(Q_GML_MULTIPOINT) || qname.eq(Q_GML_POLYGON) || qname.eq(Q_GML_LINESTRING)|| qname.eq(Q_GML_MULTIPOLYGON)|| qname.eq(Q_GML_MULTILINESTRING)|| qname.eq(Q_GML_LINEARRING))   throw GeoErrors.pointNeeded(qname.local());
  throw GeoErrors.unrecognizedGeo(qname.local());
}
