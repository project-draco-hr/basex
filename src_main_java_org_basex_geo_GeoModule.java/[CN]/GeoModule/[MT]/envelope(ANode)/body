{
  if (node.type != NodeType.ELM)   Err.FUNCMP.thrw(null,this,NodeType.ELM,node.type);
  QNm qname=node.qname();
  if (qname.eq(Q_GML_POINT) || qname.eq(Q_GML_LINESTRING) || qname.eq(Q_GML_POLYGON)|| qname.eq(Q_GML_MULTIPOINT)|| qname.eq(Q_GML_MULTILINESTRING)|| qname.eq(Q_GML_MULTIPOLYGON)|| qname.eq(Q_GML_LINEARRING)) {
    Geometry geom=gmlReader(node);
    Geometry env=geom.getEnvelope();
    return gmlWriter(env);
  }
  throw GeoErrors.unrecognizedGeo(node);
}
