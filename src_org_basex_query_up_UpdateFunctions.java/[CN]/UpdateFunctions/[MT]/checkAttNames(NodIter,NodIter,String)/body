{
  final Set<String> s=new HashSet<String>();
  Nod n=at.next();
  while (n != null) {
    s.add(string(n.nname()));
    n=at.next();
  }
  n=r.next();
  while (n != null) {
    final String t=string(n.nname());
    final boolean b=s.add(t);
    if (!b) {
      if (m == null)       return false;
      if (!t.equals(m))       return false;
    }
    n=r.next();
  }
  return true;
}
