{
  final SeqIter s=new SeqIter();
  Nod i=(Nod)n.next();
  while (i != null) {
    if (i.type == Type.TXT) {
      byte[] t=EMPTY;
      while (i != null && i.type == Type.TXT) {
        t=concat(t,i.str());
        i=(Nod)n.next();
      }
      s.add(new FTxt(t,null));
      continue;
    }
    s.add(i);
    i=(Nod)n.next();
  }
  return s;
}
