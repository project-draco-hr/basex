{
  final MemData m=new MemData(20,new Names(),new Names(),new Namespaces(),new PathSummary(),new Prop());
  int dis=1;
  if (node instanceof DBNode) {
    DBNode n=(DBNode)node;
    final Data d=n.data;
    final int k=Nod.kind(n.type);
    if (k == Data.ELEM)     m.addElem(m.tags.index(d.tag(n.pre),null,false),0,dis,n.attr().size() + 1,d.size(n.pre,k),false);
  }
 else {
  }
  return m;
}
