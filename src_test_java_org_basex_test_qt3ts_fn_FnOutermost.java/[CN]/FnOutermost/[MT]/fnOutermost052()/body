{
  final XQuery query=new XQuery("fn:deep-equal( fn:outermost( /root/node()/.. ), \n" + "                           let $nodes := /root/node()/..\n" + "                           return $nodes except $nodes/descendant::node() )",ctx);
  query.context(node(file("fn/innermost/innermost.xml")));
  query.bind("$doc1",node(file("fn/innermost/doc1.xml")));
  query.bind("$doc2",node(file("fn/innermost/doc2.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
