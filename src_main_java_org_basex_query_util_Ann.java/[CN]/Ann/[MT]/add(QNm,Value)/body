{
  final int i=indexOf(name);
  if (i != -1) {
    final ItemCache ic=new ItemCache();
    ic.add(values[i]);
    ic.add(value);
    values[i]=ic.value();
  }
 else {
    if (size == names.length) {
      final int s=newSize();
      names=Arrays.copyOf(names,s);
      values=Arrays.copyOf(values,s);
    }
    names[size]=name;
    values[size]=value;
    size++;
  }
  return true;
}
