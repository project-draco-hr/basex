{
  try {
    final DataOutput out=new DataOutput(file);
    final int s=users.size();
    out.writeNum(s);
    for (int u=0; u < s; u++) {
      final User user=users.get(u);
      out.writeString(user.name);
      out.writeBytes(user.pw);
      out.writeNum(user.perm);
    }
    out.close();
  }
 catch (  final Exception ex) {
    Main.debug(ex);
  }
}
