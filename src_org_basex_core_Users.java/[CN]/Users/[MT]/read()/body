{
  if (IO.get(file).exists()) {
    try {
      final DataInput in=new DataInput(file);
      final int s=in.readNum();
      for (int u=0; u < s; u++) {
        final User user=new User(in.readString(),in.readBytes(),in.readNum());
        users.add(user);
      }
    }
 catch (    final Exception ex) {
      Main.debug(ex);
    }
  }
 else {
    users.add(new User(ADMIN,crypt.encrypt(Token.token(ADMIN)),User.READ | User.WRITE | User.CREATE| User.ADMIN));
    write();
  }
}
