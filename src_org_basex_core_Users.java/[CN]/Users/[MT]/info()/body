{
  int size=users.size();
  final TokenBuilder tb=new TokenBuilder();
  tb.add("% Users",size);
  tb.add(size != 0 ? COL : DOT);
  if (size != 0)   tb.add(NL + "Username\tRead\tWrite\tAdmin\tCreate");
  for (int i=0; i < size; i++) {
    tb.add(NL + users.get(i)[0] + "\t\t");
    for (int j=1; j <= 4; j++) {
      if ((Boolean)users.get(i)[j])       tb.add("  X\t");
 else       tb.add("\t");
    }
  }
  return tb.toString();
}
