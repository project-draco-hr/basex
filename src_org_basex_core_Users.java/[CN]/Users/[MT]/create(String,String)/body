{
  for (  final User user : users) {
    if (user.name.equals(usern))     return false;
  }
  final User user=new User();
  user.name=usern;
  user.pw=crypt.encrypt(Token.token(pass));
  user.read=READ;
  user.write=WRITE;
  user.create=CREATE;
  user.admin=ADMIN;
  users.add(user);
  write();
  return true;
}
