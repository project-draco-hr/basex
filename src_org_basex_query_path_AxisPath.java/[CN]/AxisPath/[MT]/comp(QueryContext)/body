{
  super.comp(ctx);
  if (root instanceof AxisPath) {
    Step[] st=((AxisPath)root).step;
    root=((AxisPath)root).root;
    for (    final Step s : step)     st=Array.add(st,s);
    step=st;
  }
  checkEmpty();
  final Item ci=ctx.item;
  setRoot(ctx);
  final Expr e=c(ctx);
  ctx.item=ci;
  return e;
}
