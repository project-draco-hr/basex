{
  for (int i=0; i != step.length; i++) {
    final Expr e=step[i].comp(ctx);
    if (!(e instanceof Step))     return e;
    step[i]=(Step)e;
  }
  mergeDesc(ctx);
  boolean preds=false;
  for (  final Step s : step) {
    if (s.pred.length != 0) {
      preds=true;
      if (s.countVar(null) != 0) {
        cache=false;
        return this;
      }
    }
  }
  cache=root != null && root.countVar(null) == 0;
  final DBNode db=ctx.dbroot();
  if (db != null) {
    final Data data=db.data;
    Expr e=index(ctx,data);
    if (e != this)     return e;
    if (!preds) {
      e=children(ctx,data);
      if (e != this)       return e;
    }
  }
  return iterator(preds);
}
