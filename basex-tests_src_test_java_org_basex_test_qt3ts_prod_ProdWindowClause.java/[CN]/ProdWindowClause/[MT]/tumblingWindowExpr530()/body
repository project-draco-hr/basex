{
  final XQuery query=new XQuery("\n" + "          for tumbling window $w in ()\n" + "          start $s at $x previous $sp next $sn when true()\n"+ "          end $e at $y previous $ep next $en when false() \n"+ "          return ($w, $s, $x, $sp, $sn, $e, $y, $ep, $en)\n"+ "        ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
