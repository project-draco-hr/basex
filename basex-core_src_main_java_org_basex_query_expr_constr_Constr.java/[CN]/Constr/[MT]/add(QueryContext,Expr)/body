{
  final int s=sc.ns.size();
  try {
    for (    final Expr e : expr) {
      more=false;
      final Iter iter=qc.iter(e);
      for (Item ch; (ch=iter.next()) != null && add(qc,ch); )       ;
    }
    if (!text.isEmpty())     children.add(new FTxt(text.toArray()));
    return this;
  }
  finally {
    sc.ns.size(s);
  }
}
