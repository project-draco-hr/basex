{
  final int[] set=build.nodes;
  final int sl=build.size;
  if (sl == 0)   return;
  if (sl == 1) {
    add(set[0]);
  }
 else {
    final int s=size + sl;
    int t=nodes.length;
    while (t <= s)     t<<=1;
    if (t != nodes.length)     nodes=Array.resize(nodes,size,t);
    System.arraycopy(set,0,nodes,size,sl);
    size=s;
    sort=true;
  }
}
