{
  tda.insert(nodes,getTestEntries(nodes));
  assertEquals(size + nodes,tda.size());
  assertEquals(blocks + 1,tda.blocks());
  assertEntrysEqual(0,0,nodes);
  assertAreInserted(nodes,nodes);
  assertEntrysEqual(nodes,2 * nodes,size - nodes);
  closeAndReload();
  assertEquals(size + nodes,tda.size());
  assertEquals(blocks + 1,tda.blocks());
  assertEntrysEqual(0,0,nodes);
  assertAreInserted(nodes,nodes);
  assertEntrysEqual(nodes,2 * nodes,size - nodes);
}
