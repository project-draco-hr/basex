{
  dupl=false;
  if (size > 1) {
    if (force)     sort(0,size);
    int i=1;
    for (int j=1; j < size; j++) {
      while (item[i - 1].is(item[j])) {
        item[i - 1].score=Math.max(item[j++].score,item[i - 1].score);
        if (j == size)         break;
      }
      if (j == size)       break;
      item[i++]=item[j];
    }
    size=i;
  }
}
