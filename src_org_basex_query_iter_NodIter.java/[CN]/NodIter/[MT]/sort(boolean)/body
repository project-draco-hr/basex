{
  dupl=false;
  if (size > 1) {
    if (force)     sort(0,size);
    int i=1;
    for (int j=1; j < size; j++) {
      int c=1;
      while (item[i - 1].is(item[j])) {
        item[i - 1].score+=item[j++].score;
        c++;
        if (j == size)         break;
      }
      item[i - 1].score/=c;
      if (j == size)       break;
      item[i++]=item[j];
    }
    size=i;
  }
}
