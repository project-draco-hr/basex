{
  final XQuery query=new XQuery("($works/works/employee[1]/hours[1],$staff/staff/employee[6]/hours[1]) != 10000",ctx);
  try {
    query.bind("$works",node(file("docs/works.xml")));
    query.bind("$staff",node(file("docs/staff.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
