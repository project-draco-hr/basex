{
  if (BUTTONOPT.equals(cmd)) {
    opt=true;
    close();
  }
  if (ftedit) {
    final boolean ftx=indexes[3].isSelected();
    for (int f=0; f < ft.length; f++) {
      ft[f].setEnabled(ftx);
      fl[f].setEnabled(ftx);
    }
  }
  dia.action(cmd);
  final Data data=gui.context.data();
  enableOK(buttons,BUTTONOPT,!data.meta.uptodate);
}
