{
  if (BUTTONOPT.equals(cmd)) {
    opt=true;
    close();
  }
  if (ftedit) {
    final boolean ftx=indexes[2].isSelected();
    for (int f=0; f < ft.length; f++) {
      ft[f].setEnabled(ftx);
      fl[f].setEnabled(ftx);
    }
  }
  BaseXLayout.enableOK(buttons,BUTTONOPT,!gui.context.data().meta.uptodate);
}
