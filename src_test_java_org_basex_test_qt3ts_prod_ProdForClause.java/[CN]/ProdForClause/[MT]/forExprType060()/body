{
  final XQuery query=new XQuery("declare namespace fs=\"http://www.example.com/filesystem\"; for $test as processing-instruction() in (//fs:Folder)[1]//processing-instruction() return $test",ctx);
  query.context(node(file("prod/ForClause/fsx_NS.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<?filesystem compressionUtil='foozip.exe'?>",false));
}
