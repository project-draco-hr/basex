{
  final XQuery query=new XQuery("for $PI as processing-instruction() in //processing-instruction() return $PI",ctx);
  query.context(node(file("prod/ForClause/fsx.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<?FileSystem scale='0'?><?FileSystem maxDepth='64'?>",false));
}
