{
  final XQuery query=new XQuery("for $num as xs:decimal in (xs:integer(1), xs:decimal(2), xs:integer(3)) return $num",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2 3"));
}
