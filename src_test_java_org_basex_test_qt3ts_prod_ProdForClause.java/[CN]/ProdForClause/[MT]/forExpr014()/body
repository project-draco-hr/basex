{
  final XQuery query=new XQuery("for $fileName in //File/FileName for $folderName in //Folder/FolderName return $folderName return $fileName",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
