{
  final XQuery query=new XQuery("for $comment as comment() in //comment() return $comment",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<!-- This is an official fsx file -->",false));
}
