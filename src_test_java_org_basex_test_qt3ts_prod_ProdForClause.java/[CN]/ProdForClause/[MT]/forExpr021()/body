{
  final XQuery query=new XQuery("for $i in (1, 2), $j in (3, 4) return ($i, $j)",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 3 1 4 2 3 2 4"));
}
