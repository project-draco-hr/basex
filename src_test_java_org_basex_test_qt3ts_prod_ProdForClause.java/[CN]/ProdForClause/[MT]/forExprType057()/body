{
  final XQuery query=new XQuery("\n" + "        declare namespace fs=\"http://www.example.com/filesystem\"; \n" + "        for $test as text() in (//fs:Folder)[1]/fs:File/fs:FileName/text() return $test\n"+ "      ",ctx);
  query.context(node(file("prod/ForClause/fsx_NS.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("File00000000000File00000000001File00000000002",false));
}
