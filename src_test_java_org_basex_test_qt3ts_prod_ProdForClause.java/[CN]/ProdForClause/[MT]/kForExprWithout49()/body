{
  final XQuery query=new XQuery("deep-equal((for $fo in trace((1, 2, 3), \"msg\") return $fo), (1, 2, 3))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
