{
  final XQuery query=new XQuery("\n" + "        for $test as xs:normalizedString in ( xs:language(\"en-US\"), xs:NCName(\"foobar\"), \n" + "            xs:NMTOKEN(\"token1\"), xs:ENTITY(\"entity1\") ) \n"+ "        return $test\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"en-US foobar token1 entity1"));
}
