{
  final XQuery query=new XQuery("\n" + "        for $test as xs:decimal in ( xs:integer(\"100\"), xs:short(\"1\"), xs:int(\"10000\"), \n" + "                xs:nonPositiveInteger(\"-10\"), xs:unsignedShort(\"100\"), xs:positiveInteger(\"100\"), xs:nonNegativeInteger(\"100\") ) \n"+ "        return $test\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"100 1 10000 -10 100 100 100"));
}
