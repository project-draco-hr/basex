{
  final XQuery query=new XQuery("declare function local:func($arg as element()* ) as element()* { for $n in $arg/some return $n }; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
