{
  final XQuery query=new XQuery("\n" + "        for $a in (3, 2, 1) \n" + "        for $b in (6, 5, 4) \n"+ "        let $c := $a + $b \n"+ "        let $d := $a - $b \n"+ "        let $e := () stable order by $a ascending, $b descending, $d empty greatest, $e empty least, $c descending empty greatest, $d ascending empty greatest, $e descending empty least, $b ascending empty least \n"+ "        return $a",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"1 1 1 2 2 2 3 3 3") || error("XPST0005")));
}
