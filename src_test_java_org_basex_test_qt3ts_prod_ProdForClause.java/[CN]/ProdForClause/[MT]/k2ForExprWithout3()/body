{
  final XQuery query=new XQuery("declare function local:myFunc() { for $i in (1, 2, 3) return position() }; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("1") || error("XPDY0002")));
}
