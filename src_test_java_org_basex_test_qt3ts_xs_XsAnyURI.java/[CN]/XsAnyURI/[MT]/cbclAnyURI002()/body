{
  final XQuery query=new XQuery("xs:anyURI(\"odd-scheme+1.://www.example.org/\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"odd-scheme+1.://www.example.org/"));
}
