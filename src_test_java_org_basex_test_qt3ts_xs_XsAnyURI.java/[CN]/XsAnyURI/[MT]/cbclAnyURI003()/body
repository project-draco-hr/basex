{
  final XQuery query=new XQuery("xs:anyURI(\"http://%0Ad%E2%9C%90%F0%98%9A%A0/\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://%0Ad%E2%9C%90%F0%98%9A%A0/"));
}
