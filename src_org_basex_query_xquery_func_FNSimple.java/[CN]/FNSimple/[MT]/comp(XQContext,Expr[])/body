{
switch (func) {
case TRUE:
    return Bln.TRUE;
case FALSE:
  return Bln.FALSE;
case BOOL:
return arg[0] instanceof Bln ? arg[0] : this;
case NOT:
if (arg[0] instanceof Fun) {
Fun fs=(Fun)arg[0];
if (fs.func == FunDef.EMPTY) {
  Fun f=new FNSimple();
  f.args=fs.args;
  f.func=FunDef.EXISTS;
  return f;
}
}
return this;
default :
return this;
}
}
