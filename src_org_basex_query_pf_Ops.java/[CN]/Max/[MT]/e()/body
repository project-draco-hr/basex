{
  final Col co=c(XPITEM,arg[0].tbl);
  final Col cn=tbl.s(co);
  final int s=co.sz;
  if (cn.tp == INT) {
    int n=0;
    for (int r=0; r < s; r++)     n=Math.max(co.r(r).i(),n);
    cn.a(new I(n));
  }
 else {
    double n=0;
    for (int r=0; r < s; r++)     n=Math.max(co.r(r).d(),n);
    cn.a(new D(n));
  }
}
