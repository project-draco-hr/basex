{
  if (closed)   return;
  closed=true;
  try {
    write();
    table.close();
    texts.close();
    values.close();
    close(IndexType.TEXT);
    close(IndexType.ATTRIBUTE);
    close(IndexType.TOKEN);
    close(IndexType.FULLTEXT);
  }
 catch (  final IOException ex) {
    Util.stack(ex);
  }
}
