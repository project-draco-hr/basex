{
  try {
    table.flush(all);
    if (all) {
      write();
      texts.flush();
      values.flush();
      if (textIndex != null)       ((DiskValues)textIndex).flush();
      if (attrIndex != null)       ((DiskValues)attrIndex).flush();
    }
  }
 catch (  final IOException ex) {
    Util.stack(ex);
  }
}
