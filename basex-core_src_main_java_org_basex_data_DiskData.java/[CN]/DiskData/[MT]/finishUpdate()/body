{
  if (!meta.options.get(MainOptions.AUTOFLUSH) || closed)   return;
  try {
    write();
    table.flush();
    texts.flush();
    values.flush();
    if (txtindex != null)     ((DiskValues)txtindex).flush();
    if (atvindex != null)     ((DiskValues)atvindex).flush();
  }
 catch (  final IOException ex) {
    Util.stack(ex);
  }
 finally {
    table.lock(false);
  }
}
