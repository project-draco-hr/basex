{
  int pos=0;
  int par=data.parent(p,data.kind(p));
  int pre=p;
  while (--pre > 0) {
    final int kind=data.kind(pre);
    if (kind == Data.ATTR)     continue;
    if (pre == par) {
      par=data.parent(pre,kind);
    }
 else     if (test.eval(data,pre,kind) && ++pos == posPred) {
      preds.posEval(ctx,pre,result);
      return;
    }
  }
  ;
}
