{
  final XQuery query=new XQuery("round-half-to-even(xs:float('4561.000005e0'), 0)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("4561") && assertType("xs:float")));
}
