{
  if (arg.length == 0)   return this;
  final Item it=arg[0].i() ? (Item)arg[0] : null;
switch (func) {
case STRING:
    return it != null && it.s() && !it.u() ? arg[0] : this;
case NUMBER:
  if (arg[0].e())   return Dbl.NAN;
return arg[0] instanceof Dbl ? arg[0] : this;
default :
return this;
}
}
