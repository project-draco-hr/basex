{
  final XQuery query=new XQuery("(/processing-instruction() except /bib/book[2]/title)/name()",ctx);
  try {
    query.context(node(file("op/union/bib2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"PI1 PI2"));
}
