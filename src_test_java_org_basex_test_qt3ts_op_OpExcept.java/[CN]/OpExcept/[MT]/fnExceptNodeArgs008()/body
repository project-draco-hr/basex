{
  final XQuery query=new XQuery("string-join((for $node in /bib/book/title/text() except /bib/book[3]/title/text() return $node/string()), \"|\")",ctx);
  query.context(node(file("op/union/bib2.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"TCP/IP Illustrated|Advanced Programming in the Unix environment|The Economics of Technology and Content for Digital TV"));
}
