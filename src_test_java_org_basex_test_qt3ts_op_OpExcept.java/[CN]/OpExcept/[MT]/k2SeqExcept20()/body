{
  final XQuery query=new XQuery("count(<e/>/(a except element {\"name\"} {()}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"0") || error("XPST0005")));
}
