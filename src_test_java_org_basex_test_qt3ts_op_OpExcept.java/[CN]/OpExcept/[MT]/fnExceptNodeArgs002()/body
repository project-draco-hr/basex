{
  final XQuery query=new XQuery("/bib/book/title except /bib/book[1]/title",ctx);
  try {
    query.context(node(file("op/union/bib2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<title>Advanced Programming in the Unix environment</title><title>Data on the Web</title><title>The Economics of Technology and Content for Digital TV</title>",false));
}
