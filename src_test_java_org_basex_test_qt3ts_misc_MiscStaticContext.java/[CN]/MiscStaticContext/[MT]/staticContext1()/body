{
  final XQuery query=new XQuery("declare namespace test = 'http://www.example.com'; \n" + "        <a/> instance of element(*, test:unknownType)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
