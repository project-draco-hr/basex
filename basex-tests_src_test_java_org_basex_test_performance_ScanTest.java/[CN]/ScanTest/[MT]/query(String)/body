{
  Util.outln("Query: " + query);
  new XQuery(query).execute(context);
  final Performance p=new Performance();
  final Performance pl=new Performance();
  for (int l=0; l < LOOPS; l++) {
    new XQuery(query).execute(context);
    Util.outln(pl);
  }
  Util.outln(p.getTime(LOOPS));
  Util.outln();
}
