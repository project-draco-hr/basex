{
  if (allAreValues() && exprs[0].size() < FnForEach.UNROLL_LIMIT) {
    qc.compInfo(QueryText.OPTUNROLL_X,this);
    final Value seq=(Value)exprs[0];
    Expr e=exprs[1];
    for (    final Item it : seq) {
      e=new DynFuncCall(info,sc,exprs[2],e,it).optimize(qc,scp);
    }
    return e;
  }
  return this;
}
