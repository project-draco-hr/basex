{
  final XQuery query=new XQuery("fn:map-pairs( fn:concat#3, (), () )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPTY0004") || error("XPST0005") || assertEmpty()));
}
