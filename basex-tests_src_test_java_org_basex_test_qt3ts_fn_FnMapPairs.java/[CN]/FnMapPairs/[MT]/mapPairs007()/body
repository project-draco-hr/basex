{
  final XQuery query=new XQuery(" \n" + "            let $millenium := year-from-date(current-date()) idiv 1000 \n" + "            return map-pairs(function($a, $b) as xs:integer* {1 to (string-length($a) + string-length($b))}, (\"a\", \"ab\", \"abc\", \"\"), (\"\", \"\", \"\", \"\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("1, 1, 2, 1, 2, 3"));
}
