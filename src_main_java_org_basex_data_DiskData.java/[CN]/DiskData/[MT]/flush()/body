{
  if (!meta.prop.is(Prop.AUTOFLUSH))   return;
  try {
    if (meta.dirty)     write();
    table.flush();
    texts.flush();
    values.flush();
    if (txtindex != null)     ((DiskValues)txtindex).flush();
    if (atvindex != null)     ((DiskValues)atvindex).flush();
    if (idmap != null)     idmap.write(meta.dbfile(DATAIDP));
    meta.dirty=false;
  }
 catch (  final IOException ex) {
    Util.stack(ex);
  }
}
