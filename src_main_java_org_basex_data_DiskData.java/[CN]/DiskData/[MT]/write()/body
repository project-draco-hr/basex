{
  final DataOutput out=new DataOutput(meta.dbfile(DATAINFO));
  meta.write(out);
  out.writeString(DBTAGS);
  tagindex.write(out);
  out.writeString(DBATTS);
  atnindex.write(out);
  out.writeString(DBPATH);
  pthindex.write(out);
  out.writeString(DBNS);
  ns.write(out);
  out.writeString(DBDOCS);
  docindex.write(out);
  out.write(0);
  out.close();
}
