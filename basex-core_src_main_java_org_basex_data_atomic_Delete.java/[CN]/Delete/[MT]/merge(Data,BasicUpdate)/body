{
  if (u != null && parent == u.parent && u instanceof Insert && location - shifts == u.location && data.kind(location) != Data.ATTR) {
    final Insert ins=(Insert)u;
    return new Replace(location,shifts + ins.shifts,ins.accumulatedShifts,preOfAffectedNode,ins.insseq,parent);
  }
  return null;
}
