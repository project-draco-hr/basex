{
  final XQuery query=new XQuery("\n" + "\t   (for $e in fn:available-environment-variables()\n" + "\t   return fn:environment-variable($e) instance of xs:string) = (false())\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
