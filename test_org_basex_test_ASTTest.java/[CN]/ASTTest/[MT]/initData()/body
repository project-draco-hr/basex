{
  Prop.read();
  Prop.chop=true;
  final Proc proc=Proc.get(CONTEXT,Commands.CREATEXML,TEST_FILE);
  if (!proc.execute()) {
    System.out.println(proc.info());
    return;
  }
  final Data data=CONTEXT.data();
  System.out.println("Read " + data.size + " nodes (tree height "+ data.meta.height+ ')');
}
