{
  execute(new DropIndex(CmdIndex.ATTRIBUTE));
  query(DATA.args(_DB_ATTRIBUTE.args(NAME,"0")),"0");
  execute(new CreateIndex(CmdIndex.ATTRIBUTE));
  query(DATA.args(_DB_ATTRIBUTE.args(NAME,"0")),"0");
