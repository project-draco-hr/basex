{
  if (!global)   return;
  file=new File(Prop.HOME,IO.BASEXSUFFIX + "perm");
  if (!file.exists()) {
    list.add(new User(ADMIN,token(md5(ADMIN)),User.ADMIN));
  }
 else {
    DataInput in=null;
    try {
      in=new DataInput(file);
      read(in);
    }
 catch (    final IOException ex) {
      Util.errln(ex);
    }
 finally {
      if (in != null)       try {
        in.close();
      }
 catch (      final IOException ex) {
      }
    }
  }
}
