{
  final XQuery query=new XQuery("map(function($x){$x[contains(., 'e')]}, (\"john\", \"mary\", \"jane\", \"anne\", \"peter\", \"ian\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"jane\", \"anne\", \"peter\""));
}
