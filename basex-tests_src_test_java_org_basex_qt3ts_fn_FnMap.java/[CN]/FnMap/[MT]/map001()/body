{
  final XQuery query=new XQuery("map(upper-case#1, (\"aa\", \"bb\", \"cc\", \"dd\", \"ee\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"AA\", \"BB\", \"CC\", \"DD\", \"EE\""));
}
