{
  final XQuery query=new XQuery("(/works/employee[2], /works/employee[1], /works/employee[2]) ! @name ! string()",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"John Doe 2\", \"Jane Doe 1\", \"John Doe 2\""));
}
