{
  final XQuery query=new XQuery("for $x in (1,2) return (if (($x eq 1)) then ($x) else ($x + 1))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 3"));
}
