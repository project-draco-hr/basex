{
  final XQuery query=new XQuery("<elem>{(//(north)/(/)//(@mark)[(1)]/(.)/(..))}</elem>",ctx);
  try {
    query.context(node(file("prod/AxisStep/Tree1Child.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem><west mark=\"w0\"/><center mark=\"c0\"><the1child/></center><east mark=\"e0\">Text in east</east></elem>",false));
}
