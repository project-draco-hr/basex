{
  final XQuery query=new XQuery("let $r := serialize-json(map{'abc':=23, 'xyz':=49}, map{\"spec\":=\"RFC4627\"})\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("starts-with($result, '{')") && assertQuery("contains($result, '\"abc\":23')") && assertQuery("contains($result, '\"xyz\":49')")&& assertQuery("ends-with($result, '}')")&& assertType("xs:string")&& assertCount(1)&& assertQuery("parse-json($result)(\"abc\") = 23")));
}
