{
  final XQuery query=new XQuery("let $r := serialize-json(map{\"a\":=xs:date('2011-04-06')}, map{\"fallback\":=function($v){string($v)}})\n" + "        return translate($r,' \t\n" + "\r', '')",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"{\"a\":\"2011-04-06\"}"));
}
