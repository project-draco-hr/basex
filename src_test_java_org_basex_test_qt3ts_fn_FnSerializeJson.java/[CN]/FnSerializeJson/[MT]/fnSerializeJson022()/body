{
  final XQuery query=new XQuery("let $r := serialize-json((number('NaN'), number('INF'), number('-INF')))\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[\"NaN\",\"INF\",\"-INF\"]"));
}
