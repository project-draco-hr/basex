{
  final XQuery query=new XQuery("let $r := serialize-json(map{1:=\"a\",2:=\"b\",4:=\"d\",10:=\"j\",7:=\"g\"})\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"[\"a\",\"b\",null,\"d\",null,null,\"g\",null,null,\"j\"]"));
}
