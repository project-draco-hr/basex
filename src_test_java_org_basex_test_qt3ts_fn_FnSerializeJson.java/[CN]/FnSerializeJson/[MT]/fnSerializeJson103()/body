{
  final XQuery query=new XQuery("let $r := serialize-json((0,0,map{true():=\"gogogo\"}), map{\"fallback\":=function($v){if ($v instance of map(xs:boolean, item()*)) then \"a-boolean-map\" else $v}})\n" + "        return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[0,0,\"a-boolean-map\"]"));
}
