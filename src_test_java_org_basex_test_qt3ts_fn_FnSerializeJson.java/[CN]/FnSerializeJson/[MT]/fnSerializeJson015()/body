{
  final XQuery query=new XQuery("let $r := serialize-json('\n" + "', map{\"spec\":=\"ECMA-262\"})\n" + "            return translate(normalize-space($r), 'abcdef', 'ABCDEF')",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"\"\\n\"") || assertStringValue(false,"\"\\u0010\"")));
}
