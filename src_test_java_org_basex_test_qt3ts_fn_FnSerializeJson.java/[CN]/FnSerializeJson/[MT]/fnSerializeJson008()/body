{
  final XQuery query=new XQuery("let $r := serialize-json(parse-json('[1, 2, 3, \"four\", true, false, null]'), map{\"spec\":=\"RFC4627\"})\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[1,2,3,\"four\",true,false,null]"));
}
