{
  final XQuery query=new XQuery("let $r := serialize-json((0,0,xs:untypedAtomic(\"abcd\")))\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[0,0,\"abcd\"]"));
}
