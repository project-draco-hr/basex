{
  final XQuery query=new XQuery("let $r := serialize-json(map{1:=\"a\",2:=\"b\",4:=\"d\",10:=\"j\",7:=\"g\",-1:=\"$$$\"})\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOJS0002"));
}
