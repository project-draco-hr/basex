{
  final XQuery query=new XQuery("serialize-json((map{\"abc\":=map{\"abc\":=1}},map{\"def\":=map{\"def\":=2}},map{\"ghi\":=map{\"ghi\":=3}}),\n" + "        map{\"indent\":=false()})",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"[{\"abc\":{\"abc\":1}},{\"def\":{\"def\":2}},{\"ghi\":{\"ghi\":3}}]"));
}
