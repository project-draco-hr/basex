{
  final XQuery query=new XQuery("let $r := serialize-json((map{\"abc\":=1},map{\"def\":=2},map{\"ghi\":=3}))\n" + "            return translate($r,' \t\n" + "\r', '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[{\"abc\":1},{\"def\":2},{\"ghi\":3}]"));
}
