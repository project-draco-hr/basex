{
  final XQuery query=new XQuery("(xs:long(\"-92233720368547758\") || xs:long(\"-47175562203048468\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-92233720368547758-47175562203048468"));
}
