{
  final XQuery query=new XQuery("(xs:positiveInteger(\"1\") || xs:positiveInteger(\"52704602390610033\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"152704602390610033"));
}
