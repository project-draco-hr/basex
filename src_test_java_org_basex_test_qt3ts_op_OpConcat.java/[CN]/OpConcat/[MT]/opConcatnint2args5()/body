{
  final XQuery query=new XQuery("(xs:negativeInteger(\"-999999999999999999\") || xs:negativeInteger(\"-1\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-999999999999999999-1"));
}
