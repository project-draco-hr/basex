{
  final XQuery query=new XQuery("declare ordering ordered; let $a := <a><b>1</b><c>2</c></a> return $a/b union $a/c",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<b>1</b><c>2</c>",false));
}
