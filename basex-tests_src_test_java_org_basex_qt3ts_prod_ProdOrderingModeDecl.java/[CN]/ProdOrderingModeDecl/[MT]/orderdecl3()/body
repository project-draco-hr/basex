{
  final XQuery query=new XQuery("declare ordering unordered; (<a>1</a>) union (<b>2</b>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<a>1</a><b>2</b>",false) || assertSerialization("<b>2</b><a>1</a>",false)));
}
