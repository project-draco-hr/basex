{
  final XQuery query=new XQuery("substring-before(\"a string\", \"a string\",\n" + "         \"http://www.example.com/COLLATION/NOT/SUPPORTED\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOCH0002"));
}
