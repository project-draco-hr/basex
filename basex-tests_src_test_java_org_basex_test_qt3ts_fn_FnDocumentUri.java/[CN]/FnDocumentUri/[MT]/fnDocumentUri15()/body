{
  final XQuery query=new XQuery("fn:string-length(fn:string(fn:contains(fn:document-uri(fn:doc($uri)),$uri)))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    query.bind("uri",new XQuery("'http://www.w3.org/fots/docs/works-mod.xml'",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("4") || assertEq("5")));
}
