{
  final XQuery query=new XQuery("fn:tokenize(\"A.BRA.CADA.BRA\", \"a.\", \"qi\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"\", \"BR\", \"CAD\", \"BRA\""));
}
