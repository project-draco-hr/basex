{
  final XQuery query=new XQuery("fn:tokenize(\"abracadabra+abracadabra\", \"\\+\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"abracadabra\", \"abracadabra\""));
}
