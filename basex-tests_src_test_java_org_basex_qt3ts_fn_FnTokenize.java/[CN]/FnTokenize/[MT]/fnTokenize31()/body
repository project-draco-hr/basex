{
  final XQuery query=new XQuery("fn:tokenize(\"abc.def.gh.ijk\", \".\", \"q\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"abc\", \"def\", \"gh\", \"ijk\""));
}
