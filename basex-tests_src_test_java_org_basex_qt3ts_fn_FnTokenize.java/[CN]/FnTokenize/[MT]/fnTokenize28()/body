{
  final XQuery query=new XQuery("fn:tokenize(\"abracadabraabracadabra\", \"aa{1}\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"abracadabr\", \"bracadabra\""));
}
