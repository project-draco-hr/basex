{
  final String fun=check(Function.ATTR);
  new DropIndex("attribute").execute(CONTEXT);
  query("data(" + fun + "('db', '0'))","0");
  new CreateIndex("attribute").execute(CONTEXT);
  query("data(" + fun + "('db', '0'))","0");
  query("data(" + fun + "('db', '0', 'id'))","0");
  query("data(" + fun + "('db', '0', 'XXX'))","");
  query(fun + "('db', 'XXX')","");
}
