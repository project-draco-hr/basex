{
  final XQuery query=new XQuery("deep-equal(string-to-codepoints('\r \r\n" + " \n" + "\r'), (10, 32, 10, 32, 10, 10))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
