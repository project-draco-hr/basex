{
  if (cats != null && cats.size() == 0) {
    type=StatsType.NONE;
    leaf=false;
  }
  out.writeNum(type.ordinal() | 0x10);
  if (type == StatsType.INTEGER || type == StatsType.DOUBLE) {
    out.writeDouble(min);
    out.writeDouble(max);
  }
 else   if (type == StatsType.CATEGORY) {
    cats.write(out);
  }
  out.writeNum(count);
  out.writeBool(leaf);
  out.writeDouble(0);
}
