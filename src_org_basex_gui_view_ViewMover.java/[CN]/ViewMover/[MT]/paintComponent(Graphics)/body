{
  final Color color1=active ? GUIConstants.COLORS[4] : GUIConstants.color2;
  final Color color2=active ? GUIConstants.COLORS[20] : GUIConstants.COLORS[10];
  final int w=getWidth();
  final int h=getHeight();
  g.setColor(color1);
  g.fillRect(0,0,w,h);
  g.setColor(color2);
  for (int x=-4; x < w; x+=4)   g.drawLine(x + 4,1,x,h - 2);
  g.drawRect(0,0,w - 1,h - 1);
}
