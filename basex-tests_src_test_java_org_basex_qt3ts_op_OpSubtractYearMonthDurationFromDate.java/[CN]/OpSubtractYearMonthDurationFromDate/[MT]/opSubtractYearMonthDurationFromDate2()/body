{
  final XQuery query=new XQuery("xs:date(\"2000-02-29Z\") - xs:yearMonthDuration(\"P1Y\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1999-02-28Z"));
}
