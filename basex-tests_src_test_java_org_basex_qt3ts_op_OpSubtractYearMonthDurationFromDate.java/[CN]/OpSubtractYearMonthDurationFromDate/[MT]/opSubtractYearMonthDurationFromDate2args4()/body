{
  final XQuery query=new XQuery("xs:date(\"1970-01-01Z\") - xs:yearMonthDuration(\"P1000Y6M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0969-07-01Z"));
}
