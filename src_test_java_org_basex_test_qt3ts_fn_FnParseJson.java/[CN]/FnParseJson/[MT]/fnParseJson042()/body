{
  final XQuery query=new XQuery("parse-json('\"abcd\\n\"', map{'spec':='ECMA-262','unescape':=false()})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("'abcd\\n'") && assertType("xs:string")));
}
