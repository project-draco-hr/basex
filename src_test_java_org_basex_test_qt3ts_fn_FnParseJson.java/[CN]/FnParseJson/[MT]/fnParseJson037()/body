{
  final XQuery query=new XQuery("parse-json('[\"\\u0000\"]', map{'unescape':=false()})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertQuery("$result(1) = '\\u0000'"));
}
