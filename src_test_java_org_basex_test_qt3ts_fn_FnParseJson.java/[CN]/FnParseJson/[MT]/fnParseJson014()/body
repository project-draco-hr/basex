{
  final XQuery query=new XQuery("parse-json(\"[true]\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("map(*)") && assertCount(1) && assertQuery("map:contains($result, 1)")&& assertQuery("$result(1) eq true()")));
}
