{
  final XQuery query=new XQuery("parse-json('93.7', map{'spec':='ECMA-262'})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("93.7e0") && assertType("xs:double")));
}
