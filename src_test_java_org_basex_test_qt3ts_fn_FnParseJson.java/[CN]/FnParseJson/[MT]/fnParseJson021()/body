{
  final XQuery query=new XQuery("parse-json('[{\"x\":12,\"y\":5}, {\"x\":13,\"y\":6}]')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("map:size($result) = 2") && assertQuery("$result(2)(\"y\") = 6")));
}
