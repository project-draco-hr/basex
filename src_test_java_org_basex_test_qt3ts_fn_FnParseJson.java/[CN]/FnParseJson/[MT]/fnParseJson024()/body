{
  final XQuery query=new XQuery("parse-json('[-0.123]')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("$result(1) = -0.123e0") && assertQuery("$result(1) instance of xs:double")));
}
