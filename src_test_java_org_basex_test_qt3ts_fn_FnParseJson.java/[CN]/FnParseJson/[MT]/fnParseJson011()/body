{
  final XQuery query=new XQuery("parse-json(\"[]\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("map(*)") && assertCount(1) && assertQuery("map:size($result) eq 0")&& assertQuery("map:collation($result) eq \"http://saxon.sf.net/json/array-collation\"")));
}
