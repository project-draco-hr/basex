{
  final XQuery query=new XQuery("parse-json('[1,2,3, \"abc\", \"def\", true, false, null]')",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertType("map(*)") && assertCount(1) && assertQuery("every $i in 1 to 8 satisfies map:contains($result, $i)")&& assertQuery("every $i in (0, 9, 10) satisfies not(map:contains($result, $i))")&& assertQuery("$result(1) eq 1")&& assertQuery("$result(5) eq \"def\"")&& assertQuery("$result(7) eq false()")));
}
