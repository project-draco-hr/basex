{
  final XQuery query=new XQuery("parse-json('{\"x\":[12,3], \"y\":[14,9]}')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("map:size($result) = 2") && assertQuery("$result(\"y\")(2) = 9")));
}
