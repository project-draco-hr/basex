{
  final XQuery query=new XQuery("parse-json('[1, 2, [], [1], [1,2], [1,2,3]]')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("map:size($result) = 6") && assertQuery("$result(6)(3) = 3")));
}
