{
  final XQuery query=new XQuery("parse-json(\"[12345]\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("map(*)") && assertCount(1) && assertQuery("map:contains($result, 1)")&& assertQuery("map:get($result, 1) eq 12345")));
}
