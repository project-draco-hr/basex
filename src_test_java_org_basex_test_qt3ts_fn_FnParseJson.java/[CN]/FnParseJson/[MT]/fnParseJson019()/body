{
  final XQuery query=new XQuery("parse-json(\"[[[],[]]]\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("map(xs:integer, map(xs:integer, map(*)))") && assertCount(1) && assertQuery("map:size($result) eq 1")&& assertQuery("map:size($result(1)) eq 2")&& assertQuery("map:size($result(1)(2)) eq 0")));
}
