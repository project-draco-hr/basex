{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:double) as xs:double { $arg * $arg }; \n" + "      \t1e0 * local:square(7)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"49"));
}
