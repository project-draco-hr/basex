{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:float) as xs:float { $arg * $arg }; \n" + "      \tlocal:square(7) * xs:float(1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"49"));
}
