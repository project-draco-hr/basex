{
  final XQuery query=new XQuery("empty(subsequence((1, 2, 3, current-time(), 5, 6, 7), 4, 1)[last() - 10])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
