{
  final XQuery query=new XQuery("subsequence(for $x in (1 to 100) return -$x, 3, 5)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-3 -4 -5 -6 -7"));
}
