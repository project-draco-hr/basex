{
  for (int i=0; i < indent; i++)   sb.append("  ");
  sb.append("Node(").append(size()).append(")[\n");
  child0.toString(sb,indent + 1);
  sb.append("\n");
  child1.toString(sb,indent + 1);
  sb.append("\n");
  child2.toString(sb,indent + 1);
  sb.append("\n");
  for (int i=0; i < indent; i++)   sb.append("  ");
  sb.append("]");
}
