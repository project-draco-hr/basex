{
  final XQuery query=new XQuery("fn:function-name( math:pow#2 )",ctx);
  try {
    query.namespace("math","http://www.w3.org/2005/xpath-functions/math");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("fn:QName(\"http://www.w3.org/2005/xpath-functions/math\", \n                              \"math:pow\")"));
}
