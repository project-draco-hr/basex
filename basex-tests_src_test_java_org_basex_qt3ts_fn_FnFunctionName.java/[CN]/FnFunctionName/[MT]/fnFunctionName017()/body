{
  final XQuery query=new XQuery("fn:function-name( concat#99 )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("fn:QName(\"http://www.w3.org/2005/xpath-functions\", \n                              \"fn:concat\")"));
}
