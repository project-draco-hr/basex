{
  if (r.isEmpty())   return 99999999999d;
  double ar=0;
  for (int i=0; i < r.size(); i++) {
    if (r.get(i).w != 0 && r.get(i).h != 0) {
      if (r.get(i).w > r.get(i).h) {
        ar+=r.get(i).w / r.get(i).h;
      }
 else {
        ar+=r.get(i).h / r.get(i).w;
      }
    }
  }
  return ar / r.size();
}
