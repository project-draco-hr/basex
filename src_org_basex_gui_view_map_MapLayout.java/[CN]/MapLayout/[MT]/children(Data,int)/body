{
  final IntList list=new IntList();
  final int kind=data.kind(par);
  final int last=par + data.size(par,kind);
  int p=par + (GUIProp.mapatts ? 1 : data.attSize(par,kind));
  while (p != last) {
    list.add(p);
    p+=data.size(p,data.kind(p));
  }
  if (list.size != 0)   list.add(p);
  return list;
}
