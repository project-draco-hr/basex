{
  double aar=0;
  for (int i=0; i < r.size(); i++) {
    MapRect curr=r.get(i);
    if (ViewData.isLeaf(data,curr.pre) && curr.w != 0 && curr.h != 0) {
      if (curr.w > curr.h) {
        aar+=curr.w / curr.h;
      }
 else {
        aar+=curr.h / curr.w;
      }
    }
  }
  return aar / r.size();
}
