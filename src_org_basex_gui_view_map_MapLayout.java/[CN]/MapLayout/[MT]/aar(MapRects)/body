{
  double aar=0;
  int nrLeaves=1;
  for (int i=0; i < r.size; i++) {
    final MapRect curr=r.get(i);
    if (curr.w != 0 && curr.h != 0) {
      nrLeaves++;
      aar+=curr.w > curr.h ? (double)curr.w / (double)curr.h : (double)curr.h / (double)curr.w;
    }
  }
  return nrLeaves > 0 ? aar / nrLeaves : -1;
}
