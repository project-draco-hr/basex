{
  double aar=0;
  int nrLeaves=0;
  for (int i=0; i < r.size; i++) {
    final MapRect curr=r.get(i);
    if (curr.w != 0 && curr.h != 0) {
      nrLeaves++;
      if (curr.w > curr.h) {
        aar+=curr.w / curr.h;
      }
 else {
        aar+=curr.h / curr.w;
      }
    }
  }
  return nrLeaves > 0 ? aar / nrLeaves : -1;
}
