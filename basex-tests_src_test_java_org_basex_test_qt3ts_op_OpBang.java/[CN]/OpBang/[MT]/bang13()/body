{
  final XQuery query=new XQuery("/ works ! employee[4] ! preceding-sibling::*[1] ! string(@name) ",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("\"Jane Doe 3\""));
}
