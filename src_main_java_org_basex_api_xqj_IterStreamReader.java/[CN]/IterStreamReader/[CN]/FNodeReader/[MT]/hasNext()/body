{
  final ANode n=iter.get(l).next();
  if (n != null) {
    while (l >= nodes.size())     nodes.add(null);
    nodes.set(l,n);
    node=n;
    type();
    if (kind == START_ELEMENT) {
      while (l + 1 >= iter.size())       iter.add(null);
      iter.set(++l,n.children());
    }
  }
 else {
    if (--l < 0)     return false;
    node=nodes.get(l);
    kind=END_ELEMENT;
  }
  return true;
}
