{
  if (item.type instanceof NodeType) {
switch ((NodeType)item.type) {
case DOC:
      kind=START_DOCUMENT;
    return;
case ATT:
  kind=ATTRIBUTE;
return;
case ELM:
kind=START_ELEMENT;
return;
case COM:
kind=COMMENT;
return;
case PI:
kind=PROCESSING_INSTRUCTION;
return;
default :
break;
}
}
kind=item.type.func() ? -1 : CHARACTERS;
}
