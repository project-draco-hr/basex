{
  int i=0;
  InputInfo ii=info;
  try {
    for (    final Clause c : clauses) {
      ii=c.info;
      c.compile(ctx,scp);
      if (c instanceof Let)       ((Let)c).bindConst(ctx);
      i++;
    }
    ii=info;
    ret=ret.compile(ctx,scp);
  }
 catch (  final QueryException qe) {
    clauseError(qe,i,ii);
  }
  return optimize(ctx,scp);
}
