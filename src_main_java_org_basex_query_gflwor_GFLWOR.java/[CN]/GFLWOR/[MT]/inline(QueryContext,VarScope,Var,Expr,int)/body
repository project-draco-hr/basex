{
  boolean change=false;
  final ListIterator<Clause> iter=clauses.listIterator(p);
  while (iter.hasNext()) {
    final Clause c=iter.next().inline(ctx,scp,v,e);
    if (c != null) {
      change=true;
      iter.set(c);
    }
  }
  final Expr rt=ret.inline(ctx,scp,v,e);
  if (rt != null) {
    change=true;
    ret=rt;
  }
  return change;
}
