{
switch (sig) {
case _HOF_SORT_WITH:
    return sortWith(ctx).iter();
case _HOF_ID:
case _HOF_CONST:
  return ctx.iter(expr[0]);
case _HOF_FOLD_LEFT1:
return foldLeft1(ctx).iter();
case _HOF_UNTIL:
return until(ctx).iter();
case _HOF_TOP_K_BY:
return topKBy(ctx).iter();
case _HOF_TOP_K_WITH:
return topKWith(ctx).iter();
default :
return super.iter(ctx);
}
}
