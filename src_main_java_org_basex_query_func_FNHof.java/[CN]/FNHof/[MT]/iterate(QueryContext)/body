{
  final FItem f=withArity(0,1,ctx);
  return new Iter(){
    Value v=ctx.value(expr[1]);
    long i, len=v.size();
    @Override public Item next() throws QueryException {
      while (i >= len) {
        v=f.invValue(ctx,input,v);
        i=0;
        len=v.size();
      }
      return v.itemAt(i++);
    }
  }
;
}
