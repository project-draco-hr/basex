{
  final XQuery query=new XQuery("<a> <?target content?> {<b>{attribute name{\"content\"}}</b>} </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a><?target content?><b name=\"content\"/></a>",false));
}
