{
  final XQuery query=new XQuery("element elem {attribute attr {1,'string',3.14,xs:float('1.2345e-2'),xs:dateTime('2002-04-02T12:00:00-01:00')}}",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem attr=\"1 string 3.14 0.012345 2002-04-02T12:00:00-01:00\"/>",false));
}
