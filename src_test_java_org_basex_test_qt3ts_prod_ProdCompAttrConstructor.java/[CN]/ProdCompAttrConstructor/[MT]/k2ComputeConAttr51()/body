{
  final XQuery query=new XQuery("<e> { attribute {QName(\"http://example.com/\", \"attr\")} {()} } </e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e xmlns:ns0=\"http://example.com/\" ns0:attr=\"\"/>",false));
}
