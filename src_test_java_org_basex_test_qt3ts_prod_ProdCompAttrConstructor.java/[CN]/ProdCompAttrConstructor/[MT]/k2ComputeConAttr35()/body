{
  final XQuery query=new XQuery("<elem><![CDATA[]]>{attribute name {\"content\"}}<alem/> </elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem name=\"content\"><alem/></elem>",false));
}
