{
  final XQuery query=new XQuery("<a> { attribute xml:space {\"DEFAULT\"} } </a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<a xml:space=\"DEFAULT\"/>",false) || error("XQDY0092")));
}
