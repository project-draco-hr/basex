{
  final XQuery query=new XQuery("<e> { attribute name {\"content\"}, attribute name2 {\"content\"} } </e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e name=\"content\" name2=\"content\"/>",false));
}
