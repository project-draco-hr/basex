{
  final XQuery query=new XQuery("element elem {attribute {(), 'attr'} {'text'}}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem attr=\"text\"/>",false));
}
