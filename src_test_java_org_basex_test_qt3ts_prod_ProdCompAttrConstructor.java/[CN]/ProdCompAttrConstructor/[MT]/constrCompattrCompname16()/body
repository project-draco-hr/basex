{
  final XQuery query=new XQuery("<elem xmlns:foo=\"http://www.example.com/foo\">{attribute {xs:untypedAtomic('foo:attr')} {'text'}}</elem>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem xmlns:foo=\"http://www.example.com/foo\" foo:attr=\"text\"/>",false));
}
