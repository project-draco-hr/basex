{
  final XQuery query=new XQuery("<e xmlns:p=\"http://example.com/\" p:attr1=\"value\"> { attribute {QName(\"http://example.com/\", \"attr2\")} {()} } </e>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns:p=\"http://example.com/\" xmlns:ns0=\"http://example.com/\" p:attr1=\"value\" ns0:attr2=\"\"/>",true));
}
