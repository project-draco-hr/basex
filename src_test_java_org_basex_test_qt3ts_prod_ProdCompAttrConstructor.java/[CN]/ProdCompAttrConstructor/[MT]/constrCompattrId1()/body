{
  final XQuery query=new XQuery("element elem {attribute xml:id {\"ncname\"}}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem xml:id=\"ncname\"/>",false));
}
