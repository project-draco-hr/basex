{
  final FakeHttpConnection conn=new FakeHttpConnection(new URL("http://www.test.com"));
  conn.contentType="text/plain; charset=CP1251";
  conn.content=Charset.forName("CP1251").encode("????????").array();
  final Iter i=ResponseHandler.getResponse(conn,Bln.FALSE.atom(null),null,context.prop,null);
  assertTrue(eq(i.get(1).atom(null),token("????????")));
}
