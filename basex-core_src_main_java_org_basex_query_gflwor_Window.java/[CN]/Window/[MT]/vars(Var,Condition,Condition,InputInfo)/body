{
  final int stn=st.nVars();
  final Var[] vs=new Var[1 + stn + (nd == null ? 0 : nd.nVars())];
  st.writeVars(vs,0);
  if (nd != null)   nd.writeVars(vs,stn);
  vs[vs.length - 1]=vr;
  for (int i=0; i < vs.length; i++) {
    final Var v=vs[i];
    for (int j=i; --j >= 0; )     if (v.name.eq(vs[j].name))     throw WINDOWUNIQ.get(info,vs[j]);
  }
  return vs;
}
