{
  final Expr e=expr.inline(qc,scp,v,ex);
  final Condition st=start.inline(qc,scp,v,ex);
  final Condition en=end == null ? null : end.inline(qc,scp,v,ex);
  if (e != null)   expr=e;
  if (st != null)   start=st;
  if (en != null)   end=en;
  return e != null || st != null || en != null ? optimize(qc,scp) : null;
}
