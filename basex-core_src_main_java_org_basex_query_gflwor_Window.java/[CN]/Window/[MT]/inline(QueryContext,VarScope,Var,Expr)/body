{
  final Expr ex=expr.inline(ctx,scp,v,e);
  final Condition st=start.inline(ctx,scp,v,e), en=end == null ? null : end.inline(ctx,scp,v,e);
  if (ex != null)   expr=ex;
  if (st != null)   start=st;
  if (en != null)   end=en;
  return ex != null || st != null || en != null ? optimize(ctx,scp) : null;
}
