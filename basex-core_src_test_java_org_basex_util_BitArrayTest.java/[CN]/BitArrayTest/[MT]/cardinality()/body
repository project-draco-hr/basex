{
  final BitArray a=new BitArray();
  a.init(new long[]{0L},0);
  assertEquals(0,a.cardinality());
  a.set(1);
  assertEquals(1,a.cardinality());
  a.set(64);
  assertEquals(2,a.cardinality());
  a.set(3);
  assertEquals(3,a.cardinality());
  a.clear(64);
  assertEquals(2,a.cardinality());
  a.clear(3);
  assertEquals(1,a.cardinality());
  a.clear(1);
  assertEquals(0,a.cardinality());
}
