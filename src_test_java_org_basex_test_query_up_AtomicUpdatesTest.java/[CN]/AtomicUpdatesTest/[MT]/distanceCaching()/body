{
  final String doc="<n1>" + "<n2>T3</n2>T4<n5/>T6<n7/>" + "<n8><n9><n10><n11/><n12/></n10></n9></n8><n13/><n14/>"+ "</n1>";
  final AtomicUpdateList l=atomics(doc);
  l.addDelete(13);
  l.addInsert(13,10,insseq(l.data),false);
  l.addInsert(11,10,insseq(l.data),false);
  l.addReplace(5,insseq(l.data));
  l.addDelete(3);
  l.execute();
  checkDistances(l.data,new int[][]{new int[]{17,1},new int[]{16,15},new int[]{15,10},new int[]{14,10},new int[]{13,10},new int[]{12,11},new int[]{11,10},new int[]{10,9},new int[]{9,8},new int[]{8,1},new int[]{7,1},new int[]{6,1},new int[]{5,4},new int[]{4,1},new int[]{3,1},new int[]{2,1},new int[]{1,0}});
}
