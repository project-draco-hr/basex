{
  no(new Copy("//title","// li"));
  ok(new CreateDB(FILE));
  final Data data=CONTEXT.data();
  final int size=data != null ? data.meta.size : 0;
  ok(new Copy("//unknown","//li"));
  ok(data,size);
  ok(new Copy("//title","//li"));
  ok(data,size + 4);
  ok(new Copy("//  title","//li",1));
  no(new Copy("/","//text()"));
}
