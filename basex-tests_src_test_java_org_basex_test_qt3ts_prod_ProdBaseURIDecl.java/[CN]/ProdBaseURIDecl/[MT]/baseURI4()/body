{
  final XQuery query=new XQuery("declare base-uri \"http://www.example.com/abc&amp;\"; \"aaa\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"aaa"));
}
