{
  error(_FILE_APPEND_TEXT.args(PATH,"x"),FILE_IS_DIR_X);
  error(_FILE_APPEND_TEXT.args(PATH1," 123"),INVCAST_X_X_X);
  query(_FILE_APPEND_TEXT.args(PATH1,"x"));
  query(_FILE_SIZE.args(PATH1),"1");
