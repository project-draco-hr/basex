{
  final XQuery query=new XQuery("subsequence(for $x in 1 to 10 return $x[. mod 2 = 0],2)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"4 6 8 10"));
}
