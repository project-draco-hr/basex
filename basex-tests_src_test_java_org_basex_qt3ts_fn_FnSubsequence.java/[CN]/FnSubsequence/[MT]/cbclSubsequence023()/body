{
  final XQuery query=new XQuery("subsequence(for $x in 1 to 10 return $x * $x,2)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"4 9 16 25 36 49 64 81 100"));
}
