{
  final XQuery query=new XQuery("let $start := (current-time(), 2)[2] treat as xs:integer,\n" + "                $len := (current-time(), 1)[2] treat as xs:integer\n" + "                return subsequence((1, 2, 3), $start, $len)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("2"));
}
