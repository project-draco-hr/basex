{
  try {
    if ((len - (p * BUFFERSIZE)) < BUFFERSIZE) {
      mbytebuffer=fc.map(FileChannel.MapMode.READ_WRITE,p * BUFFERSIZE,len - (p * BUFFERSIZE));
    }
 else {
      mbytebuffer=fc.map(FileChannel.MapMode.READ_WRITE,p * BUFFERSIZE,BUFFERSIZE);
    }
    currentMapping=p;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
