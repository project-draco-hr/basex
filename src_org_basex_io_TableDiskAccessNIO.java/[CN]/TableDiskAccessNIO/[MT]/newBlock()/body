{
  try {
    fc.position(fc.size());
    fc.write(tmpblock);
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
  block=nrBlocks++;
  try {
    mbbuffer.add(fc.map(FileChannel.MapMode.READ_WRITE,block * IO.BLOCKSIZE,IO.BLOCKSIZE));
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
