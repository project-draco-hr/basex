{
  try {
    final int b=blocks[ind];
    if (mbbuffer.get(b) == null) {
      currentmbb=fc.map(FileChannel.MapMode.READ_WRITE,b * IO.BLOCKSIZE,IO.BLOCKSIZE);
      mbbuffer.set(currentmbb,b);
    }
 else {
      currentmbb=mbbuffer.get(b);
    }
    block=b;
    index=ind;
    firstPre=first;
    nextPre=next;
  }
 catch (  final IOException ex) {
    ex.printStackTrace();
  }
}
