{
  final XQuery query=new XQuery("typeswitch(<e/>, <e/>) case $b as element() return concat(\"\", $b treat as element()) default return 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
