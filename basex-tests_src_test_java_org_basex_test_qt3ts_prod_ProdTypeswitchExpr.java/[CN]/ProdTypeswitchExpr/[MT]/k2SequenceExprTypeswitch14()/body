{
  final XQuery query=new XQuery("<e/>/(typeswitch (self::node()) case $i as node() return . default return 1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e/>",false));
}
