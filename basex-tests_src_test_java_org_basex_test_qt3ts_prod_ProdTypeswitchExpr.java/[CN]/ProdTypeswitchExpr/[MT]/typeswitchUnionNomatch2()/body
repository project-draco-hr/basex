{
  final XQuery query=new XQuery("\n" + "\t typeswitch (<e/>) \n" + "\t case xs:integer | xs:string return 1\n"+ "\t default $v return 0\n"+ "\t ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
