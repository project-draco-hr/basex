{
  final Iter ir=expr[0].iter(ctx);
  final byte[] s=expr.length > 1 ? checkStr(expr[1],ctx) : null;
  for (Item it; (it=ir.next()) != null; ) {
    try {
      FNInfo.dump(it.serialize().toArray(),s,ctx);
    }
 catch (    final QueryIOException ex) {
      throw ex.getCause(info);
    }
  }
  return null;
}
