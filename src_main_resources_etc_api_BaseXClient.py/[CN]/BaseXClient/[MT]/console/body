def console(self):
    print 'Python BaseX client'
    print 'v0.1'
    self.sendCommand('SET INFO ON')
    self.getResult('INTOUTPUT')
    self.write()
    self.data = ' '
    while (self.data != ''):
        sys.stdout.write('>')
        com = sys.stdin.readline().rstrip()
        if (com != ''):
            self.sendCommand(com)
            self.getResult('INTOUTPUT')
            self.write()
            self.getResult('INTINFO')
    print 'Profitez de la vie !!!'
