{
  words=w;
  opt=o;
  lserr=pr.num(Prop.LSERROR);
  cmp=new TokenComparator(){
    @Override public boolean equal(    final byte[] in,    final byte[] qu) throws QueryException {
      return opt.sw != null && opt.sw.id(qu) != 0 || (opt.is(FZ) ? ls.similar(in,qu,lserr) : opt.is(WC) ? wc(words.input,in,qu,0,0) : eq(in,qu));
    }
  }
;
}
