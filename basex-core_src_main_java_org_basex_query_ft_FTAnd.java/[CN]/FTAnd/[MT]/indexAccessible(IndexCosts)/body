{
  final int es=exprs.length;
  neg=new boolean[es];
  int is=0;
  int n=0;
  for (int i=0; i < es; ++i) {
    if (!exprs[i].indexAccessible(ic))     return false;
    neg[i]=ic.not;
    if (ic.not)     ++n;
    ic.not=false;
    if (is == 0 || ic.costs() < is)     is=ic.costs();
    if (ic.costs() == 0)     break;
  }
  ic.costs(is);
  return !neg[0] && n < es;
}
