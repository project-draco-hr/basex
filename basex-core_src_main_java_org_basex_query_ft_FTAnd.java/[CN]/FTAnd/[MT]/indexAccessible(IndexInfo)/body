{
  final int es=exprs.length;
  final boolean[] ng=new boolean[es];
  int costs=0;
  for (int e=0; e < es; ++e) {
    if (!exprs[e].indexAccessible(ii))     return false;
    if (ii.not) {
      if (e == 0)       return false;
      ng[e]=true;
      ii.not=false;
    }
    if (costs < ii.costs)     costs=ii.costs;
  }
  ii.costs=costs;
  negated=ng;
  return true;
}
