{
  final XQuery query=new XQuery("element elem {1, element a {2, element b {element c {}, element d {3}}, 4}}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>1<a>2<b><c/><d>3</d></b>4</a></elem>",false));
}
