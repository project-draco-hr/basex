{
  final XQuery query=new XQuery("fn:avg((xs:integer(\"830993497117024304\"),xs:integer(\"-999999999999999999\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("-84503251441487847.5"));
}
