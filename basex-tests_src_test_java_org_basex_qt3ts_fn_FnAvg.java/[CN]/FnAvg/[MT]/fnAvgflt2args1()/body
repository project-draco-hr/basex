{
  final XQuery query=new XQuery("fn:avg((xs:float(\"-3.4028235E38\"),xs:float(\"-3.4028235E38\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"-INF") || assertEq("-3.4028235E38") || error("FOAR0002")));
}
