{
  final XQuery query=new XQuery("fn:avg((xs:decimal(\"79000000000000000000000000000\"),\n" + "                    xs:decimal(\"79000000000000000000000000000\")))\n" + "            eq 79000000000000000000000000000\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOAR0002")));
}
