{
  final XQuery query=new XQuery("/bib/book[3]/title/text() intersect /bib/book/title/text()",ctx);
  try {
    query.context(node(file("op/union/bib2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Data on the Web"));
}
