{
  final XQuery query=new XQuery("for $h in ( (//hours) intersect (//hours[xs:integer(.) gt 12])) order by number($h) return $h",ctx);
  query.context(node(file("docs/works.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<hours>20</hours><hours>20</hours><hours>20</hours><hours>20</hours><hours>20</hours><hours>20</hours><hours>40</hours><hours>40</hours><hours>40</hours><hours>80</hours><hours>80</hours><hours>80</hours>",false));
}
