{
  final XQuery query=new XQuery("(/bib/book[1]/title intersect /bib/book[1]/title)/string()",ctx);
  try {
    query.context(node(file("op/union/bib2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"TCP/IP Illustrated"));
}
