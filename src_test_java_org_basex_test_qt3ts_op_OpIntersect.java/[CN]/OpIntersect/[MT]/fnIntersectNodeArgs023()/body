{
  final XQuery query=new XQuery("for $node in ((//price/text()) , (//price/text())) intersect ((//price/text()) , (//price/text())) return <a> {$node} </a>",ctx);
  try {
    query.context(node(file("op/union/bib2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a> 65.95</a><a>65.95</a><a> 39.95</a><a>129.95</a>",false));
}
