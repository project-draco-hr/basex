{
  final XQuery query=new XQuery("//(employee[location = \"Denver\"] intersect //employee[last()])/./location",ctx);
  try {
    query.context(node(file("op/union/acme_corp.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
