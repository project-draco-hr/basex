{
  final XQuery query=new XQuery("\n" + "         declare function local:function ($c as node()) { $c intersect $c }; \n" + "         empty(local:function(document{()}))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
