{
  final QueryProcessor qp=new QueryProcessor("http:send-request(" + "<http:request method='post' status-only='true'>" + "<http:body media-type='text/xml'>"+ "<book id='4'>"+ "<name>The Celebrated Jumping Frog of Calaveras County</name>"+ "<author>Twain</author>"+ "</book>"+ "</http:body>"+ "</http:request>, '" + URL + "/doc.xml')",CONTEXT);
  checkResponse(qp.execute(),HttpURLConnection.HTTP_CREATED,1);
  qp.close();
}
