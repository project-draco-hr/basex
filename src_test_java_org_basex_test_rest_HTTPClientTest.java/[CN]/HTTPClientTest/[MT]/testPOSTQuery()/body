{
  final Command postQuery=new XQuery("http:send-request(" + "<http:request method='post'>" + "<http:body media-type='application/query+xml'>"+ "<query xmlns='" + Text.URL + "/rest'>"+ "<text>1</text>"+ "<parameter name='wrap' value='yes'/>"+ "</query>"+ "</http:body>"+ "</http:request>, "+ URL+ ")");
  postQuery.execute(context);
  checkResponse(postQuery,HttpURLConnection.HTTP_OK,2);
