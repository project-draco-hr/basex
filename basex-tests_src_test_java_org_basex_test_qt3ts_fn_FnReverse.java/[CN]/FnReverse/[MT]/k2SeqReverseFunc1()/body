{
  final XQuery query=new XQuery("reverse((<a> <b> <c/> <d/> </b> <e/> </a> , <f/>))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<f/><a><b><c/><d/></b><e/></a>",false));
}
