{
  plotImg=new BufferedImage(getWidth(),getHeight(),Transparency.BITMASK);
  final Graphics g=plotImg.getGraphics();
  smooth(g);
  drawAxis(g,true);
  drawAxis(g,false);
  g.setColor(color6);
  for (int i=0; i < plotData.size; ++i) {
    drawItem(g,plotData.xAxis.co[i],plotData.yAxis.co[i],false,false,false);
  }
}
