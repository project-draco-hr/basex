{
  for (int i=0; i < 25; i++) {
    run(new Add("a","<x c='c'/>"));
    run(new Add("a","<x a='a' b='b'/>"));
    run(new Replace("a","<x/>"));
  }
  query(Function._DB_ATTRIBUTE.args(NAME,"a"),"");
  query(Function._DB_ATTRIBUTE.args(NAME,"b"),"");
  query(Function._DB_ATTRIBUTE.args(NAME,"c"),"");
}
