{
  final XQuery query=new XQuery("round-half-to-even(xs:positiveInteger(\"1\")) instance of xs:positiveInteger",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || assertBoolean(true)));
}
