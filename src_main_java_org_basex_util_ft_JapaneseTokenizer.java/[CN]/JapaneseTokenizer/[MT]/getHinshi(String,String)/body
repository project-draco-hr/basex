{
  int hinshi=0;
  final byte[] s=token(srfc);
  if (s.length == 1 && !letter(s[0]) && !digit(s[0])) {
    hinshi=HINSHI_MARK;
  }
 else {
    final String[] parts=ftr.split(",");
    if (parts[0].equals("\u8A18\u53F7")) {
      hinshi=HINSHI_MARK;
    }
 else {
      hinshi=HINSHI_OTHERS;
    }
  }
  return hinshi;
}
