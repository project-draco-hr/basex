{
  GetOpts g=new GetOpts(cmd,"h",1);
  int ch=g.getopt();
  while (ch != -1) {
switch (ch) {
case 'h':
      printHelp();
    return;
case ':':
  out.print("cp: missing argument");
return;
case '?':
out.print("cp: illegal option");
return;
}
ch=g.getopt();
}
if (g.getPath() != null) {
cp(g.getFoundArgs());
}
}
