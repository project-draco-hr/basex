{
  final XQuery query=new XQuery("for $s in //section[section.title = \"Procedure\"] return ($s//instrument)[position()<=2]",ctx);
  query.context(node(file("docs/report1.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<instrument>using electrocautery.</instrument><instrument>electrocautery</instrument>",false));
}
