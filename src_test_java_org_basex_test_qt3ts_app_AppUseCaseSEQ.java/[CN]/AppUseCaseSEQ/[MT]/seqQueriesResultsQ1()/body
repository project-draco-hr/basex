{
  final XQuery query=new XQuery("for $s in //section[section.title = \"Procedure\"] return ($s//incision)[2]/instrument",ctx);
  query.context(node(file("docs/report1.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<instrument>electrocautery</instrument>",false));
}
