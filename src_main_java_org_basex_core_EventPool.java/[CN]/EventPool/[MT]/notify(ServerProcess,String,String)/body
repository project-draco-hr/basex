{
  Sessions sessions=events.get(name);
  if (sessions == null)   return;
  for (  ServerProcess srv : sessions) {
    if (!srv.equals(sp)) {
      try {
        srv.tout.print(name);
        srv.tout.write(0);
        srv.tout.print(msg);
        srv.tout.write(0);
        srv.tout.flush();
      }
 catch (      IOException e) {
        sessions.delete(srv);
      }
    }
  }
}
