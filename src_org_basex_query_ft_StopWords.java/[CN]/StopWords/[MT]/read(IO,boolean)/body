{
  try {
    final byte[] content=norm(fl.content());
    final int s=contains(content,' ') ? ' ' : '\n';
    for (    final byte[] sl : split(content,s)) {
      if (e)       delete(sl);
 else       if (id(sl) == 0)       add(sl);
    }
    return true;
  }
 catch (  final IOException ex) {
    return false;
  }
}
