{
  return new Iterator<E>(){
    private int c=-1;
    private boolean r;
    @Override public boolean hasNext(){
      if (!r)       r=more();
      return r;
    }
    @Override @SuppressWarnings("unchecked") public E next(){
      if (!r)       more();
      r=false;
      return (E)list[c];
    }
    private boolean more(){
      return ++c < size;
    }
    @Override public void remove(){
      Util.notexpected();
    }
  }
;
}
