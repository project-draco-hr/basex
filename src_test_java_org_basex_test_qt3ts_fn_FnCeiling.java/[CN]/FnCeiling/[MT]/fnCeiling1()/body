{
  final XQuery query=new XQuery("for $x in (1, xs:decimal(2), xs:float(3), xs:double(4)) return \n" + "           if ((ceiling($x)) instance of xs:integer) then \"integer\" \n" + "           else if ((ceiling($x)) instance of xs:decimal) then \"decimal\" \n"+ "           else if ((ceiling($x)) instance of xs:float) then \"float\"\n"+ "           else if ((ceiling($x)) instance of xs:double) then \"double\" \n"+ "           else error()\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertDeepEq("\"integer\", \"decimal\", \"float\", \"double\""));
}
