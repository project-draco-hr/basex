{
  final XQuery query=new XQuery("let $var := /works/employee[1] return $var/fn:namespace-uri()",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("\"\"") && assertType("xs:anyURI")));
}
