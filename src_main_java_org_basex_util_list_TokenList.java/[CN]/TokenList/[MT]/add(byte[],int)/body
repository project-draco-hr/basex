{
  if (size == list.length) {
    final int s=newSize();
    final byte[][] tmp=new byte[s][];
    System.arraycopy(list,0,tmp,0,i);
    System.arraycopy(list,i,tmp,i + 1,size - i);
    tmp[i]=e;
    list=tmp;
  }
 else {
    System.arraycopy(list,i,list,i + 1,size - i);
    list[i]=e;
  }
  ++size;
}
