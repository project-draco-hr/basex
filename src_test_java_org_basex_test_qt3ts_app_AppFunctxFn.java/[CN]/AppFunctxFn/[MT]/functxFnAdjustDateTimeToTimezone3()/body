{
  final XQuery query=new XQuery("(adjust-dateTime-to-timezone( xs:dateTime('2006-02-15T17:00:00'), ()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2006-02-15T17:00:00"));
}
