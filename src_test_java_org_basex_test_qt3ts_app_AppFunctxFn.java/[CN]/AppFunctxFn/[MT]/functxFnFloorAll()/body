{
  final XQuery query=new XQuery("(floor(5), floor(5.1), floor(5.7), floor(-5.1), floor(-5.7), floor( () ))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"5 5 5 -6 -6"));
}
