{
  final XQuery query=new XQuery("(upper-case('query'), upper-case('QUERY'), upper-case('Query'), upper-case('query-123'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"QUERY QUERY QUERY QUERY-123"));
}
