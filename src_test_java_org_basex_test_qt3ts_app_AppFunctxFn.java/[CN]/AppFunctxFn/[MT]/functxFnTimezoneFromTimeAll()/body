{
  final XQuery query=new XQuery("(timezone-from-time( xs:time('09:54:00-05:00')), timezone-from-time( xs:time('09:54:00+05:00')), timezone-from-time( xs:time('09:54:00Z')), timezone-from-time( xs:time('09:54:00')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"-PT5H PT5H PT0S"));
}
