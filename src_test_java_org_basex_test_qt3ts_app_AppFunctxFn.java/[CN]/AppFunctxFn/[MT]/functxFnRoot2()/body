{
  final XQuery query=new XQuery("let $in-xml := <a><x>123</x></a> return (root($in-xml/x))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a><x>123</x></a>",false));
}
