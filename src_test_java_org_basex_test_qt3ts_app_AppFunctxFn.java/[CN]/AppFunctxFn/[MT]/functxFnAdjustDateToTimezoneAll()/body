{
  final XQuery query=new XQuery("(adjust-date-to-timezone( xs:date('2006-02-15'), xs:dayTimeDuration('-PT8H')), adjust-date-to-timezone( xs:date('2006-02-15-03:00'), xs:dayTimeDuration('-PT8H')), adjust-date-to-timezone( xs:date('2006-02-15'), ()), adjust-date-to-timezone( xs:date('2006-02-15-03:00'), ()))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"2006-02-15-08:00 2006-02-14-08:00 2006-02-15 2006-02-15"));
}
