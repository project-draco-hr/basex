{
  final XQuery query=new XQuery("(string-join( ('a', 'b', 'c'), ''), string-join( ('a', 'b', 'c'), '/*'), string-join( ('a', '', 'c'), '/*'), string-join( 'a', '/*'), string-join((), '/*'))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"abc a/*b/*c a/*/*c a "));
}
