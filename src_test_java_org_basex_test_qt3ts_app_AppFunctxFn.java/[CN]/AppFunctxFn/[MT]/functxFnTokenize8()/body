{
  final XQuery query=new XQuery("(tokenize( '2006-12-25T12:15:00', '[\\-T:]'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2006 12 25 12 15 00"));
}
