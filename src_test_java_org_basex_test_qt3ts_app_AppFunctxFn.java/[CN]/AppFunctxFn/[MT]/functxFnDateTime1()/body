{
  final XQuery query=new XQuery("(dateTime(xs:date('2006-08-15'), xs:time('12:30:45-05:00')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2006-08-15T12:30:45-05:00"));
}
