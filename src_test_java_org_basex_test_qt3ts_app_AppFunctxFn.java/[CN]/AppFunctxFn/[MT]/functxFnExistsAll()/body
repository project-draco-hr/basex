{
  final XQuery query=new XQuery("(exists( ('a', 'b', 'c') ), exists( '' ), exists( () ), exists( false() ))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"true true false true"));
}
