{
  final XQuery query=new XQuery("(/ catalog/product[last()])",ctx);
  query.context(node(file("app/FunctxFn/functx_catalog.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<product dept=\"MEN\">\n  <number>784</number>\n  <name language=\"en\">Cotton Dress Shirt</name>\n  <colorChoices>white gray</colorChoices>\n  <desc>Our <i>favorite</i> shirt!</desc>\n </product>",false));
}
