{
  final XQuery query=new XQuery("let $ordDoc := (/) return (max( (2, 1, 5, 4, 3) ), max( ('a', 'b', 'c') ), max( 2 ), max($ordDoc//item/string(@dept)))",ctx);
  query.context(node(file("app/FunctxFn/functx_order.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"5 c 2 WMN"));
}
