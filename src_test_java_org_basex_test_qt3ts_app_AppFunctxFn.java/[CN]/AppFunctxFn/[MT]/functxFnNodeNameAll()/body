{
  final XQuery query=new XQuery("declare namespace pre2 = \"http://datypic.com/pre\";\n" + "         declare namespace unpre2 = \"http://datypic.com/unpre\";\n" + "         let $in-xml := <noNamespace> <pre:prefixed xmlns=\"http://datypic.com/unpre\" xmlns:pre=\"http://datypic.com/pre\"> <unprefixed pre:prefAttr=\"a\" noNSAttr=\"b\">123</unprefixed> </pre:prefixed> </noNamespace> return (node-name($in-xml), node-name($in-xml/pre2:prefixed), node-name($in-xml//unpre2:unprefixed), node-name($in-xml//@pre2:prefAttr), node-name($in-xml//@noNSAttr))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"noNamespace pre:prefixed unprefixed pre:prefAttr noNSAttr"));
}
