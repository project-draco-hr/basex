{
  final XQuery query=new XQuery("(index-of( ('a', 'b', 'c'), 'a'), index-of( ('a', 'b', 'c'), 'd'), index-of( (4, 5, 6, 4), 4), index-of( (4, 5, 6, 4), 04.0), index-of( ('a', 5, 6), 'a'), index-of( (), 'a'), index-of( (<a>1</a>, <b>1</b>), <c>1</c> ))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1 1 4 1 4 1 1 2"));
}
