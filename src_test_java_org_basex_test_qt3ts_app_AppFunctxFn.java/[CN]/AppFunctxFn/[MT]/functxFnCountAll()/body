{
  final XQuery query=new XQuery("let $ordDoc := (/) return (count( (1, 2, 3) ), count($ordDoc//item), count( distinct-values($ordDoc//item/@num)), count( (1, 2, 3, () ) ), count( () ))",ctx);
  query.context(node(file("app/FunctxFn/functx_order.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"3 6 4 3 0"));
}
