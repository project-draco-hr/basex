{
  final XQuery query=new XQuery("(translate('1999/01/02', '/', '-'), translate('xml query', 'qlmx', 'QLMX'), translate('xml query', 'qlmx ', 'Q'), translate('xml query', 'qlmx ', ''), translate('xml query', 'abcd', 'ABCD'), translate('', 'qlmx ', 'Q'), translate((), 'qlmx ', 'Q'))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1999-01-02 XML Query Query uery xml query  "));
}
