{
  final XQuery query=new XQuery("(round(5), round(5.1), round(5.5), round(-5.5), round(-5.51))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"5 5 6 -5 -6"));
}
