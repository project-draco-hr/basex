{
  final XQuery query=new XQuery("(string-length('query'), string-length(' \n" + "query \n" + " '), string-length(normalize-space('  \n"+ "query \n"+ " ')), string-length('xml query'), string-length(''), string-length(()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"5 10 5 9 0 0"));
}
