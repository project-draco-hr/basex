{
  final XQuery query=new XQuery("let $cats := (/) return (base-uri($cats//catalog[1]))",ctx);
  try {
    query.context(node(file("app/FunctxFn/functx_cats.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://example.org/ACC/"));
}
