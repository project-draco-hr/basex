{
  final XQuery query=new XQuery("(adjust-date-to-timezone( xs:date('2006-02-15'), ()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2006-02-15"));
}
