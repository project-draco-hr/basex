{
  final XQuery query=new XQuery("let $priceDoc := (/) return (number( $priceDoc//prod[1]/price))",ctx);
  try {
    query.context(node(file("app/FunctxFn/functx_prices.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"29.99"));
}
