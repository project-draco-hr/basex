{
  final XQuery query=new XQuery("declare namespace ord = \"http://datypic.com/ord\";\n" + "         declare namespace dty = \"http://datypic.com\";\n" + "         declare namespace dty2 = \"http://datypic.com/ns2\";\n"+ "         let $root := <root> <order xmlns:ord=\"http://datypic.com/ord\" xmlns=\"http://datypic.com\"> <!-- ... --> </order> </root> return (resolve-QName( 'ord:myName', $root/dty:order))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"ord:myName"));
}
