{
  final XQuery query=new XQuery("fn:path(text{\"fred\"})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"Q{http://www.w3.org/2005/xpath-functions}root()") && assertType("xs:string")));
}
