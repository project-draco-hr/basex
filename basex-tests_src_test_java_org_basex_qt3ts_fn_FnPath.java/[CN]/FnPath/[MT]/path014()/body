{
  final XQuery query=new XQuery("\n" + "        import module namespace copy=\"http://www.w3.org/QT3/copy\";\n" + "        fn:path(copy:copy((//employee)[1])/pnum)\n"+ "      ",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    query.addModule("http://www.w3.org/QT3/copy",file("fn/id/copy.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Q{http://www.w3.org/2005/xpath-functions}root()/Q{}pnum[1]"));
}
