{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:generate($arg as xs:integer?) as xs:integer* { if ($arg = 0) then (1, 2, 3) else $arg }; \n" + "      \t( local:generate(()), for $x in local:generate(0) return $x + 2 )[last()]\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"5"));
}
