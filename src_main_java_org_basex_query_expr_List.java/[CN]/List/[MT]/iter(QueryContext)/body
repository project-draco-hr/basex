{
  return new Iter(){
    Iter ir;
    int e;
    @Override public Item next() throws QueryException {
      while (true) {
        if (ir == null) {
          if (e == expr.length)           return null;
          ir=ctx.iter(expr[e++]);
        }
        final Item it=ir.next();
        if (it != null)         return it;
        ir=null;
      }
    }
  }
;
}
