{
  try {
    final File path=mprop.dbpath(db);
    final IOFile file=new IOFile(mprop.dbpath(db + "-" + IO.DATE.format(new Date())+ IO.ZIPSUFFIX));
    final Zip zip=progress(new Zip(file));
    zip.zip(path);
    return true;
  }
 catch (  final IOException ex) {
    Util.debug(ex);
    return false;
  }
}
