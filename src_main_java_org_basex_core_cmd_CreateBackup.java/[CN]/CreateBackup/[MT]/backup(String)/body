{
  try {
    final IOFile path=mprop.dbpath(db);
    final IOFile file=mprop.dbpath(db + '-' + IO.DATE.format(new Date())+ IO.ZIPSUFFIX);
    final Zip zip=progress(new Zip(file));
    zip.zip(path);
    context.getDatabases().add(db,true);
    return true;
  }
 catch (  final IOException ex) {
    Util.debug(ex);
    return false;
  }
}
