{
  Class<?> c=classes.get(name);
  try {
    if (c == null) {
      try {
        c=Class.forName(name);
        classes.put(name,c);
        return c;
      }
 catch (      final ClassNotFoundException ex) {
        if (jarLoader != null) {
          c=Class.forName(name,true,jarLoader);
          classes.put(name,c);
        }
        return c;
      }
    }
    return c;
  }
 catch (  final Exception ex) {
  }
  return null;
}
