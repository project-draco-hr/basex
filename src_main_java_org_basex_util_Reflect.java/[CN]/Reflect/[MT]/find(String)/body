{
  for (  final String n : names) {
    Class<?> c=classes.get(n);
    try {
      if (c == null) {
        try {
          c=Class.forName(n);
          classes.put(n,c);
          return c;
        }
 catch (        final ClassNotFoundException ex) {
          if (jarLoader != null) {
            c=Class.forName(n,true,jarLoader);
            classes.put(n,c);
          }
          return c;
        }
      }
      return c;
    }
 catch (    final Exception ex) {
    }
  }
  return null;
}
