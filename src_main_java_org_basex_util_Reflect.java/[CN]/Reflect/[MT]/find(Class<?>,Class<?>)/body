{
  try {
    try {
      return clazz != null ? clazz.getConstructor(types) : null;
    }
 catch (    final Exception ex) {
      final Constructor<?> m=clazz.getDeclaredConstructor(types);
      m.setAccessible(true);
      return m;
    }
  }
 catch (  final Exception ex) {
    Util.debug(ex);
    return null;
  }
}
