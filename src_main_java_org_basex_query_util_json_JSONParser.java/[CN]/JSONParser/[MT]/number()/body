{
  tb.reset();
  if (curr() == '-')   tb.add(consume());
  if (curr() == '0') {
    tb.add(consume());
  }
 else {
    digits();
  }
  if (curr() == '.') {
    tb.add(consume());
    digits();
  }
  if (curr() == 'e' || curr() == 'E') {
    tb.add(consume());
    if (curr() == '+' || curr() == '-')     tb.add(consume());
    digits();
  }
  return tb.finish();
}
