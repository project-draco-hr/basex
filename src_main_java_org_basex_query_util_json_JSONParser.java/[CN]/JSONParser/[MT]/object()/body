{
  if (!wsConsume('{'))   return null;
  final JObject o=new JObject();
  do {
    final byte[] key=str();
    if (key == null) {
      if (o.size() != 0)       error(INVALEXP,curr(),'"');
      break;
    }
    wsCheck(':');
    o.add(key,value(true));
  }
 while (wsConsume(','));
  wsCheck('}');
  return o;
}
