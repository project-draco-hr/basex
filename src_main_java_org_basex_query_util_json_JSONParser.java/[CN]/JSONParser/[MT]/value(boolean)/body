{
  skipWS();
  final char ch=curr();
  if (digit(ch) || ch == '-')   return new JNumber(number());
  if (ch == '"')   return new JString(str());
  if (ch == '{')   return object();
  if (ch == '[')   return array();
  if (ch == 't' || ch == 'f')   return new JBoolean(bool());
  if (ch == 'n') {
    for (    final byte b : NULL)     check((char)b);
    return new JNull();
  }
  if (mand)   error(INVALEXP,curr(),'"');
  return null;
}
