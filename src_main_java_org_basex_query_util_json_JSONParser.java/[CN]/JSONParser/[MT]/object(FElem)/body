{
  if (!wsConsume('{'))   return;
  do {
    final byte[] key=string();
    if (key == null) {
      if (root.hasChildren())       error(INVALEXP,curr(),'"');
      break;
    }
    final FElem pair=new FElem(E_PAIR);
    pair.add(new FAttr(A_NAME,key));
    wsCheck(':');
    value(pair,true);
    root.add(pair);
  }
 while (wsConsume(','));
  wsCheck('}');
}
