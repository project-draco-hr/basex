{
  final XQuery query=new XQuery("for $folder in //Folder where $folde/@name = \"ABC\" return <folder/>",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
