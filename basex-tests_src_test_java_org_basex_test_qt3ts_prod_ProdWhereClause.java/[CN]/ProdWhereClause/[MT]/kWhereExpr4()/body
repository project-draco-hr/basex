{
  final XQuery query=new XQuery("for $i in 1 where count(($i, 2, timezone-from-time(current-time()))) return true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
