{
  final XQuery query=new XQuery("\n" + "         for $file in //File\n" + "         count $c\n"+ "         where ($file//Allow/user = 'system\\Aladin') \n"+ "         return $c\n"+ "      ",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2"));
}
