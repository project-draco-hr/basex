{
  final XQuery query=new XQuery("let $var := \"String\" where fn:string($var) = \"String\" return $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"String"));
}
