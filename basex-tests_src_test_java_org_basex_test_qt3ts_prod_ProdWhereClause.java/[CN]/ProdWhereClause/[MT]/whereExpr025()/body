{
  final XQuery query=new XQuery("\n" + "         for $file in //File \n" + "         where current-date() gt xs:date('1900-01-01')\n"+ "         let $prefix := substring($file/FileName, 1, 4)\n"+ "         where ($prefix = 'File')\n"+ "         where ($file//Allow/user = 'system\\Hyungjeong')\n"+ "         return $file/FileName\n"+ "      ",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"File00000000000"));
}
