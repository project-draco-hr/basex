{
  final XQuery query=new XQuery("fn:round-half-to-even(xs:positiveInteger(\"52704602390610033\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("52704602390610033"));
}
