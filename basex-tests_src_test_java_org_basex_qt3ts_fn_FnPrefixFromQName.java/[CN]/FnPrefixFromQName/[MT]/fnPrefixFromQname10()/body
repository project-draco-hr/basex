{
  final XQuery query=new XQuery("fn:lower-case(fn:prefix-from-QName(xs:QName(\"FOO:bar\")))",ctx);
  try {
    query.namespace("FOO","http://example.org");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"foo"));
}
