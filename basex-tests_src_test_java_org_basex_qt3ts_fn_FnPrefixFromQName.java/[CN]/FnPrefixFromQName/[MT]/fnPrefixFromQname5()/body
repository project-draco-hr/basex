{
  final XQuery query=new XQuery("xs:string(fn:prefix-from-QName(xs:QName(\"foo:name\")))",ctx);
  try {
    query.namespace("foo","http://example.org");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"foo"));
}
