{
  GetOpts g=new GetOpts(cmd,"h");
  int ch=g.getopt();
  while (ch != -1) {
switch (ch) {
case 'h':
      printHelp();
    return;
case ':':
  out.print("mkdir: missing argument");
return;
case '?':
out.print("mkdir: illegal option");
return;
}
ch=g.getopt();
}
if (g.getPath() != null) {
mkdir(g.getPath());
}
}
