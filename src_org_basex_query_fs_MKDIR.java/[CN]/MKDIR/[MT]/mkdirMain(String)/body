{
  GetOpts g=new GetOpts(cmd,"h");
  int ch=g.getopt();
  while (ch != -1) {
switch (ch) {
case 'h':
      printHelp();
    return;
case ':':
  FSUtils.printError(out,"mkdir",g.getPath(),99);
return;
case '?':
FSUtils.printError(out,"mkdir",g.getPath(),22);
return;
}
ch=g.getopt();
}
if (g.getPath() != null) {
mkdir(g.getPath());
}
}
