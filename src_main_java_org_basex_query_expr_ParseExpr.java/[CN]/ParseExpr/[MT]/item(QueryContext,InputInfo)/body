{
  final Iter ir=iter(ctx);
  final Item it=ir.next();
  if (it == null || ir.size() == 1)   return it;
  Item n=ir.next();
  if (n != null) {
    final ItemCache ic=new ItemCache();
    ic.add(it);
    ic.add(n);
    n=ir.next();
    if (n != null)     ic.add(Str.get("..."));
    XPSEQ.thrw(ii,ic.value());
  }
  return it;
}
