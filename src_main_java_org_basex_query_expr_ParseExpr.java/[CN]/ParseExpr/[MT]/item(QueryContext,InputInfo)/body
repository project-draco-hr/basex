{
  final Iter ir=iter(ctx);
  final Item it=ir.next();
  if (it == null || ir.size() == 1)   return it;
  Item n=ir.next();
  if (n != null) {
    final ValueBuilder vb=new ValueBuilder().add(it).add(n);
    n=ir.next();
    if (n != null)     vb.add(Str.get("..."));
    SEQCAST.thrw(ii,vb.value());
  }
  return it;
}
