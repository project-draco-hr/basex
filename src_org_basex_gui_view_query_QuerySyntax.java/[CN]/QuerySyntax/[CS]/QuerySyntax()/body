{
  if (keys != null)   return;
  keys=new HashSet<String>();
  try {
    for (    final Field f : XQTokens.class.getFields()) {
      final String name=f.getName();
      if (name.equals("SKIP"))       break;
      keys.add(Token.string((byte[])f.get(null)));
    }
  }
 catch (  final Exception ex) {
    ex.printStackTrace();
  }
}
