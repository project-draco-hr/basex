{
  char ch=word.charAt(0);
  if (quote == 0 && (ch == '"' || ch == '\'')) {
    quote=ch;
    ch=0;
  }
  if (quote != 0) {
    if (ch == quote)     quote=0;
    return GUIConstants.COLORERROR;
  }
  if (ch == '$') {
    var=true;
    ch='.';
  }
  if (var) {
    var&=Character.isLetterOrDigit(ch) || ch == ':' || ch == '.' || ch == '-';
    return VAR;
  }
  if (keys.contains(word))   return GUIConstants.COLORQUOTE;
  if (!Token.letterOrDigit(ch))   return KEY;
  return Color.black;
}
