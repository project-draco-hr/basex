{
  check(_ZIP2_DELETE);
  query("let $a := " + _FILE_READ_BINARY.args(ZIP) + "let $b := "+ _ZIP2_DELETE.args("$a","infos/stopWords")+ "let $c := "+ _ZIP2_ENTRIES.args("$b")+ "return count($c)",4);
  query("let $a := " + _FILE_READ_BINARY.args(ZIP) + "let $b := "+ _ZIP2_ENTRIES.args("$a")+ "let $c := "+ _ZIP2_DELETE.args("$a","($b/text())[position() > 1]")+ "return count(zip2:entries($c))","1");
}
