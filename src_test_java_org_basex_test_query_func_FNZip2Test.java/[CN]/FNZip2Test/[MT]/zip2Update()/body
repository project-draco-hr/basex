{
  check(_ZIP2_UPDATE);
  query(_FILE_READ_BINARY.args(ZIP) + " ! " + _ZIP2_UPDATE.args(" .","<entry>X</entry>","X")+ " ! "+ COUNT.args(_ZIP2_ENTRIES.args(" .")),6);
  query(_ZIP2_CREATE.args("<entry>X</entry>","X") + " ! " + _ZIP2_UPDATE.args(" .","<entry>Y</entry>","Y")+ " ! "+ _ZIP2_EXTRACT_TEXT.args(" ."),"X Y");
  query(_ZIP2_CREATE.args("<entry>X</entry>","X") + " ! " + _ZIP2_UPDATE.args(" .","<entry>X</entry>","Y")+ " ! "+ _ZIP2_EXTRACT_TEXT.args(" ."),"Y");
}
