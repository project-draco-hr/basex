  query("let $a := " + _FILE_READ_BINARY.args(ZIP) + "let $b := "+ _ZIP2_EXTRACT_BINARIES.args("$a","test/input.xml")+ "let $c := "+ _CONVERT_TO_STRING.args("$b")+ "let $d := "+ PARSE_XML.args("$c")+ "return $d//title/text()","XML");
}
