  query("let $a := " + _FILE_READ_BINARY.args(ZIP) + "let $b := "+ _ZIP2_EXTRACT_TEXT.args("$a","test/input.xml")+ "let $c := "+ PARSE_XML.args("$b")+ "return $c//title/text()","XML");
}
