{
  final XQuery query=new XQuery("\n" + "\t  let $all := fn:available-environment-variables()\n" + "\t  return if (fn:empty($all)) then true()\n"+ "\t  else fn:empty(fn:environment-variable(fn:string-join($all)))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
