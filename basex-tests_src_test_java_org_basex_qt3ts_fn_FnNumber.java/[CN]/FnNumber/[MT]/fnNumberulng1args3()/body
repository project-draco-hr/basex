{
  final XQuery query=new XQuery("fn:number(xs:unsignedLong(\"184467440737095516\")) eq 1.8446744073709551E17",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
