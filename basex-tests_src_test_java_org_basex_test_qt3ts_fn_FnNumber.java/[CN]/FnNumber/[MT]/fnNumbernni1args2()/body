{
  final XQuery query=new XQuery("fn:number(xs:nonNegativeInteger(\"303884545991464527\")) eq 303884545991464527",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
