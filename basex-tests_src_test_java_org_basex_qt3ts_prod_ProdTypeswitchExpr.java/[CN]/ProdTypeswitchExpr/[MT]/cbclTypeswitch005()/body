{
  final XQuery query=new XQuery("\n" + "        subsequence((1 to 10)[. mod 2 = 0] instance of xs:integer+,1)\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
