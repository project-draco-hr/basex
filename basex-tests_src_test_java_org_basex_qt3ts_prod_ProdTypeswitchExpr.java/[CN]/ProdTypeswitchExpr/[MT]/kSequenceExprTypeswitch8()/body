{
  final XQuery query=new XQuery("(typeswitch(1, 2, current-time()) case element() return -1 case document-node() return -2 default return 1) eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
