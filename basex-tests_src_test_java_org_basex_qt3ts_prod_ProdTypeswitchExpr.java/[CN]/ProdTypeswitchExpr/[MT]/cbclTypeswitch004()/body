{
  final XQuery query=new XQuery("\n" + "        typeswitch ((1 to 10)[. mod 2 = 0]) case xs:integer+ return false() default return true()\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
