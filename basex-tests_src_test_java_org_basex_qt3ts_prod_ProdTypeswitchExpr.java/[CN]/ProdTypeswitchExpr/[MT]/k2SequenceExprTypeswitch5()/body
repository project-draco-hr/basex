{
  final XQuery query=new XQuery("typeswitch (1, 2, 3) case node() return $i case $i as xs:integer return 1 default return true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
