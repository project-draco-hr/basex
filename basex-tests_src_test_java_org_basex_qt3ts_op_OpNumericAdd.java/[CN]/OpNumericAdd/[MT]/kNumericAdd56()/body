{
  final XQuery query=new XQuery("2 eq (1 + remove((xs:untypedAtomic(\"1\"), \"two\"), 2))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPTY0004")));
}
