{
  final XQuery query=new XQuery("translate('newline\n" + "tab\tspace ','\n" + "\t ','123')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"newline1tab2space3"));
}
