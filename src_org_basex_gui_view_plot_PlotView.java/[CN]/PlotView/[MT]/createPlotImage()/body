{
  final BufferedImage img=new BufferedImage(getWidth(),getHeight(),Transparency.BITMASK);
  final Graphics g=img.getGraphics();
  ((Graphics2D)g).setRenderingHint(RenderingHints.KEY_ANTIALIASING,RenderingHints.VALUE_ANTIALIAS_ON);
  g.setColor(GUIConstants.color1);
  g.fillRect(MARGIN[1],MARGIN[0],plotWidth,plotHeight);
  drawAxis(g,true);
  drawAxis(g,false);
  g.setColor(GUIConstants.color6);
  for (int i=0; i < plotData.size; i++) {
    drawItem(g,plotData.xAxis.co[i],plotData.yAxis.co[i],false,false);
  }
  return img;
}
