{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:integer) as xs:integer { $arg * $arg }; \n" + "      \tlocal:square(2) * 0",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0"));
}
