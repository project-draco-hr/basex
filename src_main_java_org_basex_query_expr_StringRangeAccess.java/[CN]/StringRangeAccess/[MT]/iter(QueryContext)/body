{
  final boolean text=sr.type == IndexType.TEXT;
  final byte kind=text ? Data.TEXT : Data.ATTR;
  final Data data=ictx.data;
  final int ml=data.meta.maxlen;
  final IndexIterator ii=sr.min.length <= ml && sr.max.length <= ml && (text ? data.meta.textindex : data.meta.attrindex) ? data.iter(sr) : scan();
  return new AxisIter(){
    @Override public ANode next(){
      return ii.more() ? new DBNode(data,ii.next(),kind) : null;
    }
  }
;
}
