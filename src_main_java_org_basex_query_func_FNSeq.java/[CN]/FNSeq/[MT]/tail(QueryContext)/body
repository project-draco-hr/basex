{
  final Expr e=expr[0];
  if (e instanceof Seq)   return ((Seq)e).sub(1,e.size() - 1).iter();
  if (e.type().zeroOrOne())   return Empty.ITER;
  final Iter ir=e.iter(ctx);
  if (ir.next() == null)   return Empty.ITER;
  return new Iter(){
    @Override public Item next() throws QueryException {
      return ir.next();
    }
  }
;
}
