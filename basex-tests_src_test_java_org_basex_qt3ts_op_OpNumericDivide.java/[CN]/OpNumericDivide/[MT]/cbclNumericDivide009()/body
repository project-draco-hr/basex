{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:square($arg as xs:decimal) as xs:decimal { $arg * $arg }; \n" + "      \tlocal:square(2.0) div 0.0\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOAR0001"));
}
