{
  final XQuery query=new XQuery("xs:short(\"-32768\") div xs:short(\"32767\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertQuery("abs($result + 1.000030518509475997) lt 1e-12"));
}
