{
  mprop=mp;
  datas=new Datas();
  events=new Events();
  sessions=new Sessions();
  blocker=new ClientBlocker();
  databases=new Databases(this);
  locks=mp.is(MainProp.DBLOCKING) ? new DBLocking(mp) : new ProcessLocking(this);
  users=new Users(true);
  repo=new Repo(this);
  log=new Log(this);
  user=users.get(ADMIN);
  listener=null;
}
