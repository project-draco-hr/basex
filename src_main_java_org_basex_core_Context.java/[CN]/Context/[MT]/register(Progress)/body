{
  if (!user.has(Perm.ADMIN))   pr.startTimeout(mprop.num(MainProp.TIMEOUT));
  StringList sl=new StringList(1);
  if (!pr.databases(sl)) {
    sl=null;
  }
 else {
    for (int d=0; d < sl.size(); d++) {
      if (sl.get(d).isEmpty())       if (null == data)       sl.deleteAt(d);
 else       sl.set(d,data.meta.name);
    }
  }
  locks.acquire(pr,pr.updating ? new StringList(0) : sl,pr.updating ? sl : new StringList(0));
}
