{
  if (!user.has(Perm.ADMIN))   pr.startTimeout(mprop.num(MainProp.TIMEOUT));
  StringList sl=new StringList();
  if (!pr.databases(sl)) {
    sl=null;
  }
 else   if (data != null) {
    for (int d=0; d < sl.size(); d++) {
      if (sl.get(d).isEmpty())       sl.set(d,data.meta.name);
    }
  }
  locked=sl;
  locks.acquire(pr,sl);
}
