{
  globalopts=gopts;
  dbs=new Datas();
  events=new Events();
  sessions=new Sessions();
  blocker=new ClientBlocker();
  databases=new Databases(this);
  locks=gopts.is(GlobalOptions.GLOBALLOCK) || Prop.gui ? new ProcLocking(this) : new DBLocking(gopts);
  users=new Users(this);
  repo=new Repo(this);
  log=new Log(this);
  user=users.get(ADMIN);
  listener=null;
}
