{
  mprop=mp;
  dbs=new Datas();
  events=new Events();
  sessions=new Sessions();
  blocker=new ClientBlocker();
  databases=new Databases(this);
  locks=mp.is(MainProp.GLOBALLOCK) || Prop.gui ? new ProcLocking(this) : new DBLocking(mp);
  users=new Users(this);
  repo=new Repo(this);
  log=new Log(this);
  user=users.get(ADMIN);
  listener=null;
}
