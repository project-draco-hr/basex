{
  if (!ordered() || !content() || !same()|| !different())   return false;
  if (dunit != null) {
    final long mn=checkItr(ctx.iter(dist[0]));
    final long mx=checkItr(ctx.iter(dist[1]));
    if (!distance(mn,mx))     return false;
  }
  if (wunit != null) {
    final long c=checkItr(ctx.iter(window));
    if (!window(c))     return false;
  }
  return true;
}
