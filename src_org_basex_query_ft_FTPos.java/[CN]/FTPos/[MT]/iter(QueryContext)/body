{
  final FTPos tmp=ctx.ftpos;
  ctx.ftpos=this;
  init(ctx.ftitem);
  final Item it=expr[0].iter(ctx).next();
  ctx.ftpos=tmp;
  final double s=it.score();
  if (s == 0 || !filter(ctx)) {
    return score(0);
  }
  if (tmp != null)   tmp.addPos(pos,pos.length,term);
  ctx.ftd=pos;
  return score(s);
}
