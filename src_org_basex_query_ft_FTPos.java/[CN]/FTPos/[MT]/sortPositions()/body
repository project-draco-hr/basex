{
  final IntList[] il={new IntList(),new IntList()};
  final int[] k=new int[size];
  int min=0;
  while (true) {
    min=0;
    boolean q=true;
    for (int j=0; j < size; j++) {
      if (k[j] > -1) {
        if (k[min] == -1)         min=j;
        q=false;
        if (pos[min].list[k[min]] > pos[j].list[k[j]])         min=j;
      }
    }
    if (q)     break;
    il[0].add(pos[min].list[k[min]]);
    il[1].add(min);
    if (++k[min] == pos[min].size)     k[min]=-1;
  }
  return il;
}
