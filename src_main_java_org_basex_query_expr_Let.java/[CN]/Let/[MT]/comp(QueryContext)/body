{
  expr=checkUp(expr,ctx).comp(ctx);
  if (!score && !(expr.uses(Use.VAR,ctx) || expr.uses(Use.POS,ctx) || expr.uses(Use.CTX,ctx)|| expr.uses(Use.FRG,ctx))) {
    ctx.compInfo(OPTBIND,var);
    var.bind(expr,ctx);
  }
 else {
    var.ret=expr.returned(ctx);
  }
  ctx.vars.add(var);
  return this;
}
