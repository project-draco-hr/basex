{
  min=Integer.MAX_VALUE;
  max=Integer.MIN_VALUE;
  int i=0;
  boolean b=false;
  while (i < vals.length) {
    if (vals[i].length > 0) {
      b=true;
      final double d=toDouble(vals[i]);
      if (d < min)       min=d;
      if (d > max)       max=d;
    }
    i++;
  }
  if (!b) {
    min=0;
    max=0;
  }
  if (log) {
    logMin=ln(min);
    logMax=ln(max);
    return;
  }
}
