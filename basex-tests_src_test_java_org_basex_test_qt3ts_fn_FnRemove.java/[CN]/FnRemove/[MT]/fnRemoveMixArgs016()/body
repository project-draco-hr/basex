{
  final XQuery query=new XQuery("fn:remove( (xs:boolean(\"1\"), xs:double(\"-INF\"), \"s\"), 3)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("true(), xs:double(\"-INF\")"));
}
