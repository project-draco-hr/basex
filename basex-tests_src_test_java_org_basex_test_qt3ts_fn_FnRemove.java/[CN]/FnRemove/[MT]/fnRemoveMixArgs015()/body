{
  final XQuery query=new XQuery("fn:remove( (xs:double(\"NaN\"), \"a\", \"b\"), 3)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("xs:double(\"NaN\"), \"a\""));
}
