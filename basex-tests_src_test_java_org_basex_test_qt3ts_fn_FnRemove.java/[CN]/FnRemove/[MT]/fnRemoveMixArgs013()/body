{
  final XQuery query=new XQuery("fn:remove( (xs:double(\"1.34\"), xs:float(\"INF\"), true()), 1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("xs:float(\"INF\"), true()"));
}
