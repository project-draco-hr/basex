{
  funcs=new FunDef[CAP];
  for (  final FunDef def : FunDef.values()) {
    final String dsc=def.desc;
    final byte[] key=Token.token(dsc.substring(0,dsc.indexOf("(")));
    final int i=add(key);
    if (i < 0)     throw new RuntimeException("Function defined twice:" + def);
    funcs[i]=def;
  }
}
