{
  final XQuery query=new XQuery("<a attr=\"&#xD;&#xA;&#x9;&#x85;&#x2028;\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((serializationMatches("&#xD;&#xA;&#x9;&#x85;&#x2028;","i") || serializationMatches("&#13;&#10;&#9;&#133;&#8232;","")));
}
