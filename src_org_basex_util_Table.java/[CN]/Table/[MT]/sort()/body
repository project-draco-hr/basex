{
  for (int i=0; i < contents.size() - 2; i++) {
    final byte[] s=lc(contents.get(i).get(0));
    for (int j=i + 1; j < contents.size(); j++) {
      if (diff(s,lc(contents.get(j).get(0))) > 0) {
        final TokenList tmp=contents.get(i);
        contents.set(i,contents.get(j));
        contents.set(j,tmp);
      }
    }
  }
}
