{
  final IntList res=new IntList();
  final DirIterator it=new DirIterator(data,pre);
  String fileToFind=FSUtils.transformToRegex(path);
  while (it.more()) {
    final int n=it.next();
    if (Pattern.matches(fileToFind,Token.string(getName(data,n))))     res.add(n);
  }
  return res.finish();
}
