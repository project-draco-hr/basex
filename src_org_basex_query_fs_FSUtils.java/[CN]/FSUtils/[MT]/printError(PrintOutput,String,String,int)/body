break;
case 99:
out.print(programm + ": " + arg+ ": "+ EMISSARG);
break;
case 100:
out.print(programm + ": " + arg+ ": "+ EOMDIR);
break;
default :
out.print(programm + ": " + arg+ ": "+ EUND);
break;
}
out.print(NL);
}
