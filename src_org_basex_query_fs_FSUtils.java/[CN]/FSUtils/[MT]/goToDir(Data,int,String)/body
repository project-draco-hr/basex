{
  int n=pre;
  int kind=data.kind(pre);
  if (path.length() < 1) {
    return pre;
  }
  String[] paths=path.split("/");
  for (  String p : paths) {
    if (p.equalsIgnoreCase("..")) {
      n=data.parent(n,kind);
    }
 else     if (p.equalsIgnoreCase("")) {
      n=3;
    }
 else     if (!p.equalsIgnoreCase(".")) {
      n=getSpecificDir(data,n,p,kind);
    }
    if (n == -1) {
      return -1;
    }
  }
  return n;
}
