{
  String file=path;
  int curDirPre=0;
  int lastSlash=path.lastIndexOf('/');
  if (lastSlash != -1) {
    curDirPre=FSUtils.goToDir(data,pre,path.substring(0,lastSlash));
    if (curDirPre == -1) {
      return new int[]{-1};
    }
 else {
      file=path.substring(lastSlash + 1);
    }
  }
  final IntList res=new IntList();
  final DirIterator it=new DirIterator(data,curDirPre);
  String fileToFind=FSUtils.transformToRegex(file);
  while (it.more()) {
    final int n=it.next();
    if (Pattern.matches(fileToFind,Token.string(getName(data,n))))     res.add(n);
  }
  return res.finish();
}
