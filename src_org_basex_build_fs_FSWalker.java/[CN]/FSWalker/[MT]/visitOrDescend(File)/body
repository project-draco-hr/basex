{
  final File[] files=d.listFiles();
  if (files != null)   for (  final File f : files) {
    try {
      curr=f;
      if (isSymlink(f))       for (      final FSVisitor v : visitors)       v.symlinkEvent(f);
 else       if (f.isDirectory())       descend(f);
 else       for (      final FSVisitor v : visitors)       v.regfileEvent(f);
    }
 catch (    IOException e) {
      BaseX.debug(f + FSText.IOEXSKIP + e.getMessage());
    }
  }
}
