{
  final XQuery query=new XQuery("(: Purpose: List the reserves of those open auctions where a certain person issued a bid before another person. :) \n" + "        <XMark-result-Q4> { \n" + "            let $auction := (/) return \n"+ "            for $b in $auction/site/open_auctions/open_auction \n"+ "            where some $pr1 in $b/bidder/personref[@person = \"person20\"], $pr2 in $b/bidder/personref[@person = \"person51\"] satisfies $pr1 << $pr2 \n"+ "            return <history>{$b/reserve/text()}</history> } \n"+ "        </XMark-result-Q4>",ctx);
  query.context(node(file("app/XMark/XMarkAuction.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<XMark-result-Q4/>",false));
}
