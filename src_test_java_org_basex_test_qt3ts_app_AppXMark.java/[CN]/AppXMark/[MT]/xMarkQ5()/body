{
  final XQuery query=new XQuery("\n" + "        <XMark-result-Q5> { \n" + "          let $auction := (/) return count(\n"+ "            for $i in $auction/site/closed_auctions/closed_auction \n"+ "            where $i/price/text() >= 40.0 return $i/price) } \n"+ "        </XMark-result-Q5>",ctx);
  query.context(node(file("app/XMark/XMarkAuction.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<XMark-result-Q5>200</XMark-result-Q5>",false));
}
