{
  final XQuery query=new XQuery("<XMark-result-Q15> { \n" + "            let $auction := (/) return \n" + "            for $a in $auction/site/closed_auctions/closed_auction/annotation/description/parlist/ \n"+ "               listitem/ parlist/ listitem/ text/ emph/ keyword/ text() \n"+ "            return <text>{$a}</text> } </XMark-result-Q15>",ctx);
  query.context(node(file("app/XMark/XMarkAuction.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<XMark-result-Q15><text> went bows </text><text> hercules pillars reversion angel songs defy hast </text><text> success </text></XMark-result-Q15>",false));
}
