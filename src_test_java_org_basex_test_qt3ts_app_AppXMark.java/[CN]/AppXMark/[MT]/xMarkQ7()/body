{
  final XQuery query=new XQuery("<XMark-result-Q7> { let $auction := (/) return for $p in $auction/site return count($p//description) + count($p//annotation) + count($p//emailaddress) } </XMark-result-Q7>",ctx);
  query.context(node(file("app/XMark/XMarkAuction.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<XMark-result-Q7>2734</XMark-result-Q7>",false));
}
