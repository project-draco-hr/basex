{
  final XQuery query=new XQuery("text {text {'one', text {'two'}}, 'three', text {'four'}}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"one two three four"));
}
