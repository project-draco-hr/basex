{
  final XQuery query=new XQuery("<elem>{text {'one'}}<a>{text {'two'}}</a>{text {'three'}}</elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>one<a>two</a>three</elem>",false));
}
