{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := <a>42</a>; \n" + "        <out>{ switch ($in) \n"+ "            case 42 return \"Moo\" \n"+ "            case \"42\" return \"Meow\" \n"+ "            case 42e0 return \"Quack\" \n"+ "            case \"42e0\" return \"Oink\" \n"+ "            default return \"Expletive deleted\" }</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>Meow</out>",false));
}
