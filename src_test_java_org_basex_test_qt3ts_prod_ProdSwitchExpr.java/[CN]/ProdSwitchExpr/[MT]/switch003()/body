{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $animal as xs:string := \"!?!?\"; \n" + "        <out>{ switch (upper-case($animal)) \n"+ "            case \"COW\" return \"Moo\"\n"+ "            case \"CAT\" return \"Meow\" \n"+ "            case \"DUCK\" return \"Quack\" \n"+ "            case lower-case($animal) return \"Oink\" \n"+ "            default return \"What's that odd noise?\" }</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>Oink</out>",false));
}
