{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := xs:double('NaN'); \n" + "        <out>{ switch ($in) \n"+ "            case 42 return \"Moo\" \n"+ "            case <a>42</a> return \"Meow\" \n"+ "            case 42e0 return \"Quack\" \n"+ "            case \"42e0\" return \"Oink\" \n"+ "            case xs:float('NaN') return \"Woof\" \n"+ "            default return \"Expletive deleted\" }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out>Woof</out>",false));
}
