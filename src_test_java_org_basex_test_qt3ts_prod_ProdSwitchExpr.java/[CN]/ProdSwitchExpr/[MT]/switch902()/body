{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := 2; \n" + "        <out>{ switch ($in) \n"+ "            case 1 return \"Moo\" \n"+ "            case 5 return \"Meow\" \n"+ "            case 3 return \"Quack\" \n"+ "            case ($in to 4) return \"Oink\" \n"+ "            default return \"Baa\" }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XPTY0004"));
}
