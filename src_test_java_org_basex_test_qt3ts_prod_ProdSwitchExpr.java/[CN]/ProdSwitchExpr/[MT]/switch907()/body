{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := 2; \n" + "        <out>{ switch $in \n"+ "            case 1 return \"Moo\", \"Boo\" \n"+ "            case 5 return \"Meow\" \n"+ "            case 7 return \"Quack\" \n"+ "            case 4 return \"Oink\" \n"+ "            default return \"Baa\" }</out>",ctx);
