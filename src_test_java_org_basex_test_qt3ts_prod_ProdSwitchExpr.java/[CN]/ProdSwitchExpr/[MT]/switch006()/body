{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $number as xs:decimal := 42; \n" + "        <out>{ switch ($number) case 21 return \"Moo\" \n"+ "            case current-time() return \"Meow\" \n"+ "            case 42 return \"Quack\" \n"+ "            default return 3.14159 }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out>Quack</out>",false));
}
