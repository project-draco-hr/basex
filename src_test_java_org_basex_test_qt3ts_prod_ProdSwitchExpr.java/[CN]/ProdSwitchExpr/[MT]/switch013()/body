{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := 25; \n" + "        declare variable $zero := 0; \n"+ "        <out>{ switch ($in) \n"+ "            case 42 return \"Quack\" \n"+ "            case 25 return \"Baa\" \n"+ "            case $in div $zero return \"Neigh\" \n"+ "            default return \"Woof\" }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out>Baa</out>",false));
}
