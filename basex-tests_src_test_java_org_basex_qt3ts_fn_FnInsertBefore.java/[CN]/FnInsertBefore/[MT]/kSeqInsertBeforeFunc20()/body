{
  final XQuery query=new XQuery("(insert-before((1, current-time(), 3), 10, ())[last()] treat as xs:integer) eq 3",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
