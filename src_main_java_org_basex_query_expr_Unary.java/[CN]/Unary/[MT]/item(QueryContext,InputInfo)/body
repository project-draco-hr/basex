{
  final Item it=expr.item(ctx,input);
  if (it == null)   return null;
  if (!it.unt() && !it.num())   Err.number(this,it);
  final double d=it.dbl(input);
  if (it.unt())   return Dbl.get(minus ? -d : d);
  if (!minus)   return it;
switch ((SimpleType)it.type) {
case DBL:
    return Dbl.get(-d);
case FLT:
  return Flt.get(-it.flt(input));
case DEC:
return Dec.get(it.dec(input).negate());
default :
return Itr.get(-it.itr(input));
}
}
