{
  final Item it=expr.item(ctx,input);
  if (it == null)   return null;
  final Type ip=it.type;
  if (!ip.isUntyped() && !ip.isNumber())   Err.number(this,it);
  final double d=it.dbl(input);
  if (ip.isUntyped())   return Dbl.get(minus ? -d : d);
  if (!minus)   return it;
switch ((AtomType)ip) {
case DBL:
    return Dbl.get(-d);
case FLT:
  return Flt.get(-it.flt(input));
case DEC:
return Dec.get(it.dec(input).negate());
default :
return Int.get(-it.itr(input));
}
}
