{
  checkUp(expr,ctx);
  super.comp(ctx);
  Expr e=this;
  if (expr.value()) {
    e=preEval(ctx);
  }
 else   if (type.type == SimpleType.BLN || type.type == SimpleType.FLT || type.type == SimpleType.DBL || type.type == SimpleType.QNM || type.type == SimpleType.URI) {
    final SeqType t=expr.type();
    if (t.eq(type))     e=expr;
 else     if (t.type == type.type && t.one() && type.zeroOrOne())     e=expr;
    if (e != this)     optPre(e,ctx);
  }
  return e;
}
