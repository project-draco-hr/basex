{
  final XQuery query=new XQuery("/processing-instruction() union /bib/book[2]/title",ctx);
  query.context(node(file("op/union/bib2.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<?PI1 Processing Instruction 1?><title>Advanced Programming in the Unix environment</title><?PI2 Processing Instruction 2?>",false));
}
