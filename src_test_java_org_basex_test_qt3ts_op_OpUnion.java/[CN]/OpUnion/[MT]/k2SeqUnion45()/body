{
  final XQuery query=new XQuery(" <r> { //(employee[location = \"Denver\"] union //employee[last()])/./location } </r>",ctx);
  query.context(node(file("op/union/acme_corp.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<r><location>Denver</location><location>Denver</location><location>Denver</location><location>Boston</location></r>",false));
}
