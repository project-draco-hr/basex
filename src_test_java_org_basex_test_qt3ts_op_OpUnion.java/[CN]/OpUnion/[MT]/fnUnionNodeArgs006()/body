{
  final XQuery query=new XQuery("/processing-instruction(PI1) union /bib/book[3]/title",ctx);
  query.context(node(file("op/union/bib2.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<?PI1 Processing Instruction 1?><title>Data on the Web</title>",false));
}
