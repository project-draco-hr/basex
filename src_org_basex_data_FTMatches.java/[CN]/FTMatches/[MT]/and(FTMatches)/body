{
  final FTMatches tmp=new FTMatches();
  for (int a=0; a < size; a++) {
    for (int b=0; b < all.size; b++) {
      final FTMatch m=new FTMatch(match[a]);
      m.add(all.match[b]);
      tmp.add(m);
    }
  }
  tmp.sTokenNum=sTokenNum < all.sTokenNum ? all.sTokenNum : sTokenNum;
  return tmp;
}
