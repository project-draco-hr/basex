{
  int a=-1, b=-1;
  while (++a < size && ++b < all.size) {
    final int e=all.match[b].match[0].start;
    final int d=e - match[a].match[0].end - 1;
    if (d == 0) {
      match[a].match[0].end=e;
    }
 else     if (d < 0) {
      Array.move(match,a + 1,-1,--size - a--);
    }
 else {
      b--;
    }
  }
  size=a;
  return size != 0;
}
