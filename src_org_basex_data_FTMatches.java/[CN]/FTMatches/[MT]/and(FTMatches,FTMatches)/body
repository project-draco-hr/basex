{
  final FTMatches tmp=new FTMatches();
  for (int a=0; a < m1.size; a++) {
    for (int b=0; b < m2.size; b++) {
      final FTMatch m=new FTMatch(m1.match[a]);
      m.add(m2.match[b]);
      tmp.add(m);
    }
  }
  tmp.sTokenNum=m1.sTokenNum > m2.sTokenNum ? m1.sTokenNum : m2.sTokenNum;
  return tmp;
}
