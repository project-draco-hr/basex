{
  final XQuery query=new XQuery("fn:root(element anElement {attribute anAttribute {\"Attribute Value\"}})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<anElement anAttribute=\"Attribute Value\"/>",false));
}
