{
  final XQuery query=new XQuery("let $var := document {<anElement><anInternalElement>element content</anInternalElement></anElement>} return fn:root($var)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<anElement><anInternalElement>element content</anInternalElement></anElement>",false));
}
