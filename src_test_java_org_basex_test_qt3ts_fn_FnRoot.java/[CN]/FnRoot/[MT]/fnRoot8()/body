{
  final XQuery query=new XQuery("let $var := <?format role=\"output\" ?> return fn:root($var)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<?format role=\"output\" ?>",false));
}
