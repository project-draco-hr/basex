{
  final XQuery query=new XQuery("let $var := element anElement {attribute anAttribute {\"Attribute Value\"}} return fn:root($var)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<anElement anAttribute=\"Attribute Value\"/>",false));
}
