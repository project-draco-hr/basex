{
  final XQuery query=new XQuery("<e>{fn:root(attribute name {\"value\"})}</e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e name=\"value\"/>",false));
}
