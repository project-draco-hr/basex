{
  try {
    final BufferedReader br=new BufferedReader(new FileReader("words.freq"));
    String l;
    freq=new IntMap();
    while ((l=br.readLine()) != null) {
      final int i=l.indexOf(';');
      freq.add(l.substring(0,i).getBytes(),Integer.valueOf(l.substring(i + 1)));
    }
    br.close();
  }
 catch (  Exception e) {
  }
}
