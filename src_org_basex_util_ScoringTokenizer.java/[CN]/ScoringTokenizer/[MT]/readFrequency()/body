{
  try {
    final BufferedReader br=new BufferedReader(new FileReader("words.freq"));
    String l;
    freq=new IntMap();
    while ((l=br.readLine()) != null) {
      final int i=l.indexOf(';');
      freq.add(Token.token(l.substring(0,i)),Token.toInt(l.substring(i + 1)));
    }
    br.close();
  }
 catch (  final Exception ex) {
    Main.errln(ex);
  }
}
