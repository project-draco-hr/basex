{
  final StringList sl=new StringList();
  for (  final IOFile f : ctx.mprop.dbpath().children()) {
    final String name=f.name();
    if (name.endsWith(IO.ZIPSUFFIX) && db.equals(dbname(name)))     sl.add(Pattern.compile(IO.ZIPSUFFIX + '$').split(name)[0]);
  }
  sl.sort(false,false);
  return sl;
}
