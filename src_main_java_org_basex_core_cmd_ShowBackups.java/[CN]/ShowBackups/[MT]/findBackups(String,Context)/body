{
  final StringList sl=new StringList();
  for (  final IOFile f : ctx.mprop.dbpath().children()) {
    final String name=f.name();
    if (name.endsWith(IO.ZIPSUFFIX) && db.equals(dbname(name)))     sl.add(name.substring(0,name.length() - IO.ZIPSUFFIX.length()));
  }
  sl.sort(false,true);
  return sl;
}
