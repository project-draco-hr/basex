{
  final StringList sl=new StringList();
  for (  final IOFile f : ctx.mprop.dbpath().children()) {
    final String name=f.name();
    if (name.matches(db + IO.DATEPATTERN + IO.ZIPSUFFIX)) {
      sl.add(full ? f.path() : name.replaceAll(IO.ZIPSUFFIX + '$',""));
    }
  }
  sl.sort(false,false);
  return sl;
}
