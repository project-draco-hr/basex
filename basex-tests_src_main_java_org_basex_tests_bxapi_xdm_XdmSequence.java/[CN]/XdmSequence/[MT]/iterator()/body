{
  return new Iterator<XdmItem>(){
    private int c;
    @Override public boolean hasNext(){
      return c < sequence.size();
    }
    @Override public XdmItem next(){
      return XdmItem.get(sequence.itemAt(c++));
    }
    @Override public void remove(){
      Util.notexpected();
    }
  }
;
}
