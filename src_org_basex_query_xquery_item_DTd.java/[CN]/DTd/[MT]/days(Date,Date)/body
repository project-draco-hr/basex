{
  int ys=s.mon / 12;
  int ms=s.mon % 12;
  int ds=(int)(s.sec / DAYSECONDS);
  final int ye=e.mon / 12;
  final int me=e.mon % 12;
  final int de=(int)(e.sec / DAYSECONDS);
  long days=0;
  while (ys < ye || ms < me || ds < de) {
    ds++;
    if (ds > dpm(ys,ms)) {
      ds=0;
      ms++;
    }
    if (ms == 12) {
      ms=0;
      ys++;
    }
    days++;
  }
  return days;
}
