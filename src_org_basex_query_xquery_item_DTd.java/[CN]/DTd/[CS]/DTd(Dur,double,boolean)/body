{
  this(it);
  if (f != f)   Err.or(DATECALC,info(),f);
  if (m ? f == 1 / 0d || f == -1 / 0d : f == 0)   Err.or(DATEZERO,info(),f);
  final double d=m ? (sec + mil) * f : (sec + mil) / f;
  sec=Math.abs((long)d);
  mil=Math.abs(Math.floor((Math.abs(d) - sec) * 1000000 + .5) / 1000000);
  minus=d < 0 && (sec != 0 || mil != 0);
}
