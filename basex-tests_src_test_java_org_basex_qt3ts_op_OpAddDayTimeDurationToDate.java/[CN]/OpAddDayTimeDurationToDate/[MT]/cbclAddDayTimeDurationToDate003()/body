{
  final XQuery query=new XQuery("\n" + "      declare function local:two-digit($number as xs:integer) { \n" + "      \tlet $string := string($number) return if (string-length($string) lt 2) then concat('0', $string) else $string \n"+ "      }; \n"+ "      declare function local:date($year as xs:integer, $month as xs:integer, $day as xs:integer) { \n"+ "      \tlet $m := local:two-digit($month), $d := local:two-digit($day) return xs:date(concat($year, '-', $m, '-', $d)) \n"+ "      }; \n"+ "      local:date(25252734927766555, 05, 12) + xs:dayTimeDuration(\"P4267296D\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FODT0001"));
}
