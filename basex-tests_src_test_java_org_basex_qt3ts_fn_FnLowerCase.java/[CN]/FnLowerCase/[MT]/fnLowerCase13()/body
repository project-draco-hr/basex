{
  final XQuery query=new XQuery("fn:concat(fn:lower-case(\"abcde\"), fn:lower-case(\"fghi\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abcdefghi"));
}
