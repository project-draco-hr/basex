{
  final XQuery query=new XQuery("document {/root}",ctx);
  query.context(node(file("prod/CompAttrConstructor/DupNode.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<root><child1><a>text</a><!--comment--><?pi content?></child1><child2><a>text</a><!--comment--><?pi content?></child2></root>",false));
}
