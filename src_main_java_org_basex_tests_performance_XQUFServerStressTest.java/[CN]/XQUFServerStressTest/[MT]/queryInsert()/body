{
  try {
    p("INSERT ... creating database.");
    final ClientSession s=newSession();
    s.execute(new CreateDB(DBNAME,"<doc/>"));
    s.close();
    runClients("insert node <node/> into doc('" + DBNAME + "')/doc");
    p("done.");
  }
 catch (  final Exception e) {
    e.printStackTrace();
  }
}
