{
switch (def) {
case JPARSE:
    return new JSONConverter(input).parse(checkStr(expr[0],ctx));
case JPARSEML:
  return new JsonMLConverter(input).parse(checkStr(expr[0],ctx));
case JSERIALIZE:
return serialize(false,ctx);
case JSERIALIZEML:
return serialize(true,ctx);
default :
return super.item(ctx,ii);
}
}
