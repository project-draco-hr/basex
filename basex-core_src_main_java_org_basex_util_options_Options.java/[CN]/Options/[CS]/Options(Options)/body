{
  for (  final Entry<String,Option<?>> e : opts.options.entrySet())   options.put(e.getKey(),e.getValue());
  for (  final Entry<String,Object> e : opts.values.entrySet())   values.put(e.getKey(),e.getValue());
  for (  final Entry<String,String> e : opts.free.entrySet())   free.put(e.getKey(),e.getValue());
  user.add(opts.user);
  file=opts.file;
}
