{
  final XQuery query=new XQuery("string-join((\n" + "                format-number(-26931.4,'-###,###.###'),\n" + "                format-number(-26931.4,'zzz-###,###.###','myminus'),\n"+ "                format-number(-26931.4,'_###,###.###','myminus')), ' ')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"--26,931.4 _zzz-26,931.4 __26,931.4"));
}
