{
  final XQuery query=new XQuery("concat(format-number(1234e0, '0000.####'), '|',\n" + "                    format-number(1234.00, '0000.####'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1234|1234"));
}
