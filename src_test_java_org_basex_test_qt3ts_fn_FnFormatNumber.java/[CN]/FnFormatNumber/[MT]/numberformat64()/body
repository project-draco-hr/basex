{
  final XQuery query=new XQuery("format-number(000123456789012345678901234567890123456789012345678900000,     '# #0.0####################################################')",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"12 34 56 78 90 12 34 56 78 90 12 34 56 78 90 12 34 56 78 90 12 34 56 78 90 00 00.0"));
}
