{
  final XQuery query=new XQuery("format-number(1234567890.123456,'\ud82b\uddb1000\ud82b\uddb0000')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1\ud82b\uddb1234\ud82b\uddb1567\ud82b\uddb1890\ud82b\uddb0123"));
}
