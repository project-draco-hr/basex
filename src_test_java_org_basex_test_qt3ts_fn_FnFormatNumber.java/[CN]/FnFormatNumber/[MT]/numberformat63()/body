{
  final XQuery query=new XQuery("format-number(000123456789012345678901234567890.123456789012345678900000,     '##0.0####################################################')",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"123456789012345678901234567890.1234567890123456789"));
}
