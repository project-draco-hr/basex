{
  final XQuery query=new XQuery("concat(format-number(-26931.4,'###,###.###','myminus'), '/',\n" + "            format-number(-42857.1,'###,###.###'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"_26,931.4/-42,857.1"));
}
