{
  final XQuery query=new XQuery("format-number(2.14*86.58,'PREFIX##00.000###SUFFIX')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"PREFIX185.2812SUFFIX"));
}
