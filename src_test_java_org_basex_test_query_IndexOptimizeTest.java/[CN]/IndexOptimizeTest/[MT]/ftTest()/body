{
  createDoc();
  new Open(NAME).execute(CONTEXT);
  check("//*[text() <- '1']","<a>1</a>");
  check("//*[text() <- '1 2' any word]","<a>1</a><a>2 3</a>");
  check("//*[text() <- {'2','4'} all]","");
  check("//*[text() <- {'2','3'} all words]","<a>2 3</a>");
  check("//*[text() <- {'2','4'} all words]","");
}
