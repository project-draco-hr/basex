{
  final XQuery query=new XQuery("<a><b name=\"C\"/><b name= \"D\"/></a>//b[@name=\"D\"][last() = 1]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<b name=\"D\"/>",false));
}
