{
  checkAdmin(ctx);
switch (def) {
case SQLINIT:
    return init(ctx);
case SQLCONNECT:
  return connect(ctx);
case SQLPREPARE:
return prepare(ctx);
case SQLCLOSE:
return close(ctx);
case SQLCOMMIT:
return commit(ctx);
case SQLROLLBACK:
return rollback(ctx);
default :
return super.item(ctx,ii);
}
}
