{
  final Performance p=new Performance();
  Util.outln(Util.name(this));
  databases=new StringList();
  if (!parseArguments(args))   return;
  final BufferedReader br=new BufferedReader(new FileReader(WORDS));
  words=new StringList();
  String l;
  while ((l=br.readLine()) != null)   words.add(l);
  br.close();
  freq=new int[words.size()];
  if (databases.size() == 0)   for (  final String s : List.list(ctx))   if (s.startsWith(DBPREFIX))   databases.add(s);
  Util.outln("=> % words on % databases: time in ms\n",words.size(),databases.size());
  test();
  for (int i=0; i < freq.length; ++i)   res.println(words.get(i) + ";" + freq[i]);
  res.close();
  Util.outln("Total Time: " + p);
}
