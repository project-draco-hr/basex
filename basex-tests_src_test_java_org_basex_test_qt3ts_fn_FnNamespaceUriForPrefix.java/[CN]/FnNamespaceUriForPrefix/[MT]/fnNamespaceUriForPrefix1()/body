{
  final XQuery query=new XQuery("let $var := <anElement xmlns:p1 = \"http://www.example.com/examples\">some context</anElement> \n" + "      return string(fn:namespace-uri-for-prefix(\"p1\",$var))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/examples"));
}
