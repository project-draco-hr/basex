{
  if (!parseArguments(args))   return;
  final TokenList list=new TokenList();
  list.add("Document");
  list.add("Length");
  list.add("Pre");
  list.add("Attsize");
  list.add("Content(t)");
  list.add("Content(a)");
  list.add("Name(e)");
  list.add("Name(a)");
  list.add("Namespaces");
  table.header=list;
  if (doc == null) {
    parse(new File("."));
  }
 else {
    analyze(doc);
  }
  System.out.print(Token.string(table.finish()));
}
