{
  return new NodeIter(){
    /** 
 * Temporary node. 
 */
    private final DBNode node=copy();
    /** 
 * Current pre value. 
 */
    private int p=pre + 1;
    /** 
 * Current size value. 
 */
    private final int s=pre + data.attSize(pre,data.kind(pre));
    @Override public Nod next(){
      if (p == s)       return null;
      node.set(p++,Data.ATTR);
      return node;
    }
  }
;
}
