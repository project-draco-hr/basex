  check("declare function local:pow($n, $k) {" + "  if($k = 0) then xs:decimal(1)" + "  else if($k mod 2 = 0) then local:pow($n * $n, $k idiv 2)"+ "  else $n * local:pow($n * $n, $k idiv 2)"+ "};"+ "local:pow(4, 5)","1024","exists(//" + Util.name(StaticFuncCall.class) + "[@tailCall eq 'true'])","count(//" + Util.name(StaticFuncCall.class) + "[@tailCall eq 'false']) eq 2");
