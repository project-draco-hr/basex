{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http&#x3a;//www.w3.org/2000/xmlns/\";\n" + "        <a/>\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0070"));
}
