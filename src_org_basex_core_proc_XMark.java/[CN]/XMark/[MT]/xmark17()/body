{
  final Nodes p=qu("/site/people/person[not(homepage/text())]");
  final XPathProcessor stepName=parse("name/text()");
  final int ps=p.size;
  for (int pi=0; pi != ps; pi++) {
    writeSep(pi);
    out.out.print("<person name=\"");
    out.out.printToken(token(p,pi,stepName),data.meta.entity);
    out.out.print("\"/>");
  }
  return p.size;
}
