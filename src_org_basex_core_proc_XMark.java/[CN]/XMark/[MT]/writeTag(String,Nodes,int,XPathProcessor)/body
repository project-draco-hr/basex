{
  final Nodes tmp=eval(steps,in,pre);
  out.out.print("<");
  out.out.print(tag);
  if (tmp.size == 0) {
    out.out.print("/>");
  }
 else {
    out.out.print(">");
    for (int t=0; t < tmp.size; t++) {
      out.out.printToken(data.atom(tmp.pre[0]),data.meta.entity);
    }
    out.out.print("</");
    out.out.print(tag);
    out.out.print(">");
  }
}
