{
  final Map<String,String> params=new HashMap<String,String>();
  params.put("query",doc.getElementsByTagName("text").item(0).getTextContent());
  final NodeList props=doc.getElementsByTagName("property");
  for (int i=0; i < props.getLength(); i++) {
    final Node prop=props.item(i);
    params.put(prop.getAttributes().getNamedItem("name").getTextContent(),prop.getAttributes().getNamedItem("value").getTextContent());
  }
  return params;
}
