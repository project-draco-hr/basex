{
  for (  final byte[] nextPkg : ctx.repo.pkgDict()) {
    if (nextPkg != null && !eq(nextPkg,pkgName)) {
      final File desc=new File(new File(ctx.prop.get(Prop.REPOPATH),string(ctx.repo.pkgDict().get(nextPkg))),DESCRIPTOR);
      final Package pkg=new PkgParser(ctx,ii).parse(new IOFile(desc));
      for (      final Dependency dep : pkg.dep)       if (eq(dep.pkg,Package.getName(pkgName)))       return Package.getName(nextPkg);
    }
  }
  return null;
}
