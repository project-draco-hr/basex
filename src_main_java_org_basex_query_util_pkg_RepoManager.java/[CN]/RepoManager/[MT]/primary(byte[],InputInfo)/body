{
  for (  final byte[] nextPkg : repo.pkgDict()) {
    if (nextPkg != null && !eq(nextPkg,pkgName)) {
      final IOFile desc=new IOFile(repo.path(string(repo.pkgDict().get(nextPkg))),DESCRIPTOR);
      final Package pkg=new PkgParser(repo,ii).parse(desc);
      final byte[] name=Package.name(pkgName);
      for (      final Dependency dep : pkg.dep)       if (dep.pkg != null && eq(dep.pkg,name))       return Package.name(nextPkg);
    }
  }
  return null;
}
