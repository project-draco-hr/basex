{
  final byte[] req=token("<http:request " + "xmlns:http='http://expath.org/ns/http-client' " + "method='GET' href='http://basex.org' "+ "send-authorization='true' auth-method='Basic' username='admin' password='admin'/>");
  final DBNode dbNode=new DBNode(new IOContent(req));
  final HttpRequestParser rp=new HttpRequestParser(null);
  rp.parse(dbNode.children().next(),null);
}
