{
  try (final QueryProcessor qp=new QueryProcessor(_HTTP_SEND_REQUEST.args("<http:request method='put'>" + "<http:body media-type='text/xml'><ToBeDeleted/></http:body>" + "</http:request>",RESTURL),ctx)){
