{
  try (final QueryProcessor qp=new QueryProcessor(_HTTP_SEND_REQUEST.args("<http:request xmlns:http='http://expath.org/ns/http-client' " + "method='GET' href='" + REST_ROOT + "' send-authorization='true' "+ "auth-method='Basic' username='admin' password='admin'/>"),ctx)){
    checkResponse(qp.execute(),2,HttpURLConnection.HTTP_OK);
  }
   try (final QueryProcessor qp=new QueryProcessor(_HTTP_SEND_REQUEST.args("<http:request xmlns:http='http://expath.org/ns/http-client' " + "method='GET' href='" + REST_ROOT + "' send-authorization='true' "+ "auth-method='Basic' username='unknown' password='wrong'/>"),ctx)){
    checkResponse(qp.execute(),1,HttpURLConnection.HTTP_UNAUTHORIZED);
  }
 }
