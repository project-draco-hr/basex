{
  final XQuery query=new XQuery("<e><?name data?></e>/processing-instruction()/..",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e><?name data?></e>",false));
}
