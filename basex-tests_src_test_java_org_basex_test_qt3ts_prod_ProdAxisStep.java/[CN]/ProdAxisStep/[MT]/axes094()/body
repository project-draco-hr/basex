{
  final XQuery query=new XQuery("let $attribute as attribute(foo) := attribute foo { } \n" + "            return count($attribute/self::*:bar)\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") || error("XPST0005")));
}
