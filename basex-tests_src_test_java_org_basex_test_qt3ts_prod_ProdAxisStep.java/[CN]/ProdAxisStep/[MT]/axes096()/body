{
  final XQuery query=new XQuery("let $element as element(*) := <foo/> return count($element/self::bar)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") || error("XPST0005")));
}
