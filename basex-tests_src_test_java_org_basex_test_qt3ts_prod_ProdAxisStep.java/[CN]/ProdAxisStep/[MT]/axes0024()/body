{
  final XQuery query=new XQuery("fn:count(//center/child::south-east)",ctx);
  try {
    query.context(node(file("prod/AxisStep/TreeRepeat.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("2"));
}
