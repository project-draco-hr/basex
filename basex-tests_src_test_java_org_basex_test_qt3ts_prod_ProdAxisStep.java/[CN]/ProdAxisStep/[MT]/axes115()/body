{
  final XQuery query=new XQuery("/*/attribute::namespace-node()",ctx);
  try {
    query.context(node(file("prod/AxisStep/TreeTrunc.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
