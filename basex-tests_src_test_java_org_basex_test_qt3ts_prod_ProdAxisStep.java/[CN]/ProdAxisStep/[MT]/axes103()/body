{
  final XQuery query=new XQuery("let $comment as comment() := <!--comment--> return count($comment/self::*)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") || error("XPST0005")));
}
