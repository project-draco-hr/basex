{
  for (int i=0; i < contents.size() - 1; i++) {
    for (int j=i + 1; j < contents.size(); j++) {
      if (diff(lc(contents.get(i).get(0)),lc(contents.get(j).get(0))) > 0) {
        final TokenList tmp=contents.get(i);
        contents.set(i,contents.get(j));
        contents.set(j,tmp);
      }
    }
  }
}
