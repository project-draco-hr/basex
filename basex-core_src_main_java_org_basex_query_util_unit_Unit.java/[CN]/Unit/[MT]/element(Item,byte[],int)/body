{
  final FElem exp=new FElem(name);
  if (it != null) {
    if (it instanceof ANode) {
      exp.add((ANode)it);
    }
 else {
      try {
        exp.add(it.string(null));
      }
 catch (      final QueryException ignored) {
      }
    }
    if (c != -1)     exp.add(ITEM,token(c)).add(TYPE,it.type.toString());
  }
  return exp;
}
