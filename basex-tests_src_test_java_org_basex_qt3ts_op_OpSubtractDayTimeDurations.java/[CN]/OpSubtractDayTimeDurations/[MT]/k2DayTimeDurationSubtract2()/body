{
  final XQuery query=new XQuery("(current-dateTime() - xs:dayTimeDuration(\"PT3H\")) lt current-dateTime()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
