{
  final XQuery query=new XQuery("ceiling(-1234567891234567.2e0)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("-1234567891234567.0e0") && assertType("xs:double")));
}
