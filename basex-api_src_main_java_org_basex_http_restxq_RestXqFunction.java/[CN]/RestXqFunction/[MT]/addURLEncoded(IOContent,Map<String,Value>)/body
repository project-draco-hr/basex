{
  for (  final String nv : body.toString().split("&")) {
    final String[] parts=nv.split("=",2);
    if (parts.length < 2)     continue;
    try {
      pars.put(parts[0],Str.get(URLDecoder.decode(parts[1],UTF8)));
    }
 catch (    final Exception ex) {
      throw Util.notExpected(ex);
    }
  }
}
