{
  final XQuery query=new XQuery("fn:path(//text()[.='2147483647'][1])",ctx);
  query.context(node(file("fn/path/pathdata.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"/Q{http://www.w3.org/2010/09/qt-fots-catalog}test-set[1]/Q{http://www.w3.org/2010/09/qt-fots-catalog}test-case[3]/Q{http://www.w3.org/2010/09/qt-fots-catalog}result[1]/Q{http://www.w3.org/2010/09/qt-fots-catalog}all-of[1]/Q{http://www.w3.org/2010/09/qt-fots-catalog}assert-eq[1]/text()[1]") && assertType("xs:string")));
}
