{
  final XQuery query=new XQuery("fn:path((//comment())[2])",ctx);
  query.context(node(file("fn/path/pathdata.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"/Q{http://www.w3.org/2010/09/qt-fots-catalog}test-set[1]/Q{http://www.w3.org/2010/09/qt-fots-catalog}test-case[2]/Q{http://www.w3.org/2010/09/qt-fots-catalog}description[1]/comment()[1]") && assertType("xs:string")));
}
