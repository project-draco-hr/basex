{
  bl.list=txt;
  bl.size=txt.length;
  c=Num.len(txt,0);
  o=0;
  final int l=Num.read(txt,0);
  final byte[] res=new byte[l];
  for (int r=0; r < l; r++) {
    int b=0;
    if (pull() != 0) {
      b=pull(3);
    }
 else     if (pull() != 0) {
      b=pull(3) | 0x08;
    }
 else     if (pull() != 0) {
      b=pull(4) | 0x10;
    }
 else     if (pull() != 0) {
      b=pull(5) | 0x20;
    }
 else {
      b=pull(8);
    }
    res[r]=(byte)(b >= 128 ? b : UNPACK[b]);
  }
  return res;
}
