{
  if (!ex.expr[1].isItem())   return ex;
  final Item it=(Item)ex.expr[1];
  if (!it.type.isString())   return ex;
  final Expr e=ex.expr[0];
  final byte[] d=it.string(ex.info);
switch (ex.op.op) {
case GE:
    return new CmpSR(e,d,true,null,true,ex.info);
case GT:
  return new CmpSR(e,d,false,null,true,ex.info);
case LE:
return new CmpSR(e,null,true,d,true,ex.info);
case LT:
return new CmpSR(e,null,true,d,false,ex.info);
default :
return ex;
}
}
