{
  final XQuery query=new XQuery("map(string-to-codepoints#1, (\"john\", \"jane\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("106, 111, 104, 110, 106, 97, 110, 101"));
}
