{
  final XQuery query=new XQuery("\n" + "            map(function($e as xs:string) as xs:string { lower-case($e) }, map(function($n as xs:string){upper-case($n)},(\"john\", \"mary\", \"jane\", \"anne\", \"peter\", \"ian\")))\n" + "        ",ctx);
  query.addDocument("fn/higherOrder/names.xml",file("fn/higherOrder/names.xml"));
  final QT3Result res=result(query);
  result=res;
  test(assertDeepEq("\"john\", \"mary\", \"jane\", \"anne\", \"peter\", \"ian\""));
}
