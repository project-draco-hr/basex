{
  final XQuery query=new XQuery("map(function($x){upper-case($x)} , (\"john\", \"mary\", \"jane\", \"anne\", \"peter\", \"ian\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"JOHN\", \"MARY\", \"JANE\", \"ANNE\", \"PETER\", \"IAN\""));
}
