{
  final XQuery query=new XQuery("let $f := function($x as xs:double*){map(round#1, $x)} return $f((1.2345, 6.789))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("1, 7"));
}
