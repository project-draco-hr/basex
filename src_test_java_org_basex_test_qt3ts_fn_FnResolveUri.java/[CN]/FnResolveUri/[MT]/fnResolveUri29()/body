{
  final XQuery query=new XQuery("string(resolve-uri(\"urn:isbn:01234567890X\", \"http://www.example.com/\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"urn:isbn:01234567890X"));
}
