{
  final XQuery query=new XQuery("\n" + "         resolve-uri(\"%C3%A0.html\", \"http://www.example.com/%C3%A7.html\")\n" + "         = \"http://www.example.com/%C3%A0.html\"\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
