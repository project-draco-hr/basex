{
  final XQuery query=new XQuery("fn:string(fn:resolve-uri(fn:lower-case(\"http://www.examples.com\"),\"\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.examples.com"));
}
