{
  final XQuery query=new XQuery("(: Description: Evaluation of resolve-uri function with an absolute URI for the first argument and a relative URI reference for second argument.:) string(resolve-uri(\"http://www.example.com/a.html\",\"b.html\"))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.com/a.html"));
}
