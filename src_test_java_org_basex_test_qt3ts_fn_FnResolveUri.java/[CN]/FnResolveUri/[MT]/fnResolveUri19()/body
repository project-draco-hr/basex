{
  final XQuery query=new XQuery("fn:string(fn:resolve-uri(fn:substring(\"123http://www.examples.com\",4),\"\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.examples.com"));
}
