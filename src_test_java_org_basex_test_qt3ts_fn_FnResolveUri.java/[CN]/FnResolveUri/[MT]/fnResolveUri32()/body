{
  final XQuery query=new XQuery("resolve-uri(\"this doc.html\", \"http://www.example.com/that doc.html\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"http://www.example.com/this doc.html") || assertStringValue(false,"http://www.example.com/this%20doc.html") || error("FORG0002")));
}
