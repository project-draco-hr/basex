{
  final Iter iter=ctx.iter(expr);
  final TokenBuilder tb=new TokenBuilder();
  boolean more=false;
  Item it;
  while ((it=iter.next()) != null) {
    if (more)     tb.add(' ');
    tb.add(it.str());
    more=true;
  }
  final byte[] atom=tb.finish();
  if (Token.contains(atom,DASHES) || Token.startsWith(atom,'-') || Token.endsWith(atom,'-'))   Err.or(COMINVALID,atom);
  return new FComm(atom,null).iter();
}
