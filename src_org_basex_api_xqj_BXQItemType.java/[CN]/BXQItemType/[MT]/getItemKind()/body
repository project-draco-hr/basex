{
switch (type) {
case ATT:
    return XQITEMKIND_ATTRIBUTE;
case COM:
  return XQITEMKIND_COMMENT;
case DOC:
return XQITEMKIND_DOCUMENT;
case ELM:
return XQITEMKIND_ELEMENT;
case NOD:
return XQITEMKIND_NODE;
case PI:
return XQITEMKIND_PI;
case TXT:
return XQITEMKIND_TEXT;
default :
return XQITEMKIND_ITEM;
}
}
