{
  final byte[] contentType=reqAttrs.get(OVERMEDIATYPE) == null ? token(conn.getContentType()) : reqAttrs.get(OVERMEDIATYPE);
  if (startsWith(contentType,TXT_XML) || startsWith(contentType,TXT_EXT_XML) || startsWith(contentType,APPL_XML)|| startsWith(contentType,APPL_EXT_XML)|| endsWith(contentType,MIME_XML_SUFFIX))   return processXML(conn,ctx);
 else   if (startsWith(contentType,TXT_HTML)) {
    if (!HTMLParser.available())     throw HTMLERR.thrw(info);
    return processHTML(conn,ctx);
  }
 else   if (startsWith(contentType,MIME_TEXT_PREFIX))   return Str.get(readHttpContent(conn));
 else   return null;
}
