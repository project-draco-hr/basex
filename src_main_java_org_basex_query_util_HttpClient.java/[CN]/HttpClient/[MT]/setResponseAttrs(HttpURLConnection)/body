{
  final NodIter responseAttrs=new NodIter();
  final FAttr attrStatus=new FAttr(new QNm(STATUS,QueryTokens.HTTPURI),token(conn.getResponseCode()),null);
  final FAttr attrStatusMsg=new FAttr(new QNm(MSG,QueryTokens.HTTPURI),token(conn.getResponseMessage()),null);
  responseAttrs.add(attrStatus);
  responseAttrs.add(attrStatusMsg);
  return responseAttrs;
}
