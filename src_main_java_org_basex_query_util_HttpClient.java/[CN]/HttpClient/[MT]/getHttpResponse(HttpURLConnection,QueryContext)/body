{
  final FElem responseElem=new FElem(new QNm(RESPONSE),setResponseChildren(conn),setResponseAttrs(conn),EMPTY,new Atts());
  final ItemIter iter=new ItemIter();
  iter.add(responseElem);
  final byte[] attrStatusOnly=reqAttrs.get(STATUSONLY);
  if (attrStatusOnly == null || !Bln.parse(attrStatusOnly,info))   iter.add(setResultContent(conn,ctx));
  return iter;
}
