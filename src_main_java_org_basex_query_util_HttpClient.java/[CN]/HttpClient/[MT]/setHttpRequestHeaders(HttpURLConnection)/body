{
  final byte[][] headerNames=headers.keys();
  for (int i=0; i < headerNames.length; i++)   conn.addRequestProperty(string(headerNames[i]),string(headers.get(headerNames[i])));
  final byte[] sendAuth=reqAttrs.get(SENDAUTH);
  if (sendAuth != null && Bln.parse(sendAuth,info))   conn.setRequestProperty(AUTH,encodeCredentials(string(reqAttrs.get(USRNAME)),string(reqAttrs.get(PASSWD))));
}
