{
  final byte[][] headerNames=req.getHeaders().keys();
  for (  final byte[] headerName : headerNames)   conn.addRequestProperty(string(headerName),string(req.getHeaders().get(headerName)));
  final byte[] sendAuth=req.getAttrs().get(SENDAUTH);
  if (sendAuth != null && Bln.parse(sendAuth,info))   conn.setRequestProperty(AUTH,encodeCredentials(string(req.getAttrs().get(USRNAME)),string(req.getAttrs().get(PASSWD))));
}
