{
  final byte[] sep=exprs.length == 2 ? toToken(exprs[1],qc) : EMPTY;
  final TokenBuilder tb=new TokenBuilder();
  final Iter iter=exprs[0].atomIter(qc,info);
  int c=0;
  for (Item it; (it=iter.next()) != null; ) {
    if (c++ != 0)     tb.add(sep);
    tb.add(toToken(it));
  }
  return Str.get(tb.finish());
}
