{
  if (!(node instanceof DBNode))   return;
  final DBNode n=(DBNode)node;
  final Data d=n.data;
  final int k=d.kind(n.pre);
  final byte[] nn=name.str();
  if (k == Data.ATTR) {
    d.update(n.pre,d.attName(n.pre),nn);
  }
 else   if (k == Data.PI) {
    final byte[] nm=n.nname();
    d.update(n.pre,concat(nm,SPACE,nn));
  }
 else {
    d.update(n.pre,nn);
  }
}
