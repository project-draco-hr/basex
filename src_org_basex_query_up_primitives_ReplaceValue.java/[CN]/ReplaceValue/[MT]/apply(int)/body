{
  final DBNode n=(DBNode)node;
  final Data d=n.data;
  final int k=d.kind(n.pre);
  final byte[] nn=name.str();
  if (k == Data.TEXT && nn.length == 0)   d.delete(n.pre);
 else   if (k == Data.ATTR) {
    d.update(n.pre,d.attName(n.pre),nn);
  }
 else {
    d.update(n.pre,k == Data.PI ? concat(n.nname(),SPACE,nn) : nn);
  }
}
