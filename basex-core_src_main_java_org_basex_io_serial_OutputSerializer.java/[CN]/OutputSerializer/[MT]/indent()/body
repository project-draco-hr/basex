{
  if (item) {
    item=false;
  }
 else   if (indent) {
    if (!suppress.isEmpty() && !tags.isEmpty()) {
      for (      final byte[] t : tags)       if (suppress.contains(t))       return;
    }
    print(nl);
    final int ls=level * indents;
    for (int l=0; l < ls; ++l)     print(tab);
  }
}
