{
  if (ct != 1)   return false;
  ct++;
  if (empty)   finishOpen();
  lvl++;
  startOpen(META);
  attribute(HTTPEQUIV,token(MimeTypes.CONTENT_TYPE));
  attribute(CONTENT,new TokenBuilder(media.isEmpty() ? MimeTypes.TEXT_HTML : media).add(CHARSET).addExt(encoding).finish());
  if (html) {
    print(ELEM_C);
  }
 else {
    print(' ');
    print(ELEM_SC);
  }
  lvl--;
  if (empty)   finishClose();
  return true;
}
