{
  print(' ');
  print(name);
  print(ATT1);
  final byte[] val=norm(value);
  final int vl=val.length;
  for (int k=0; k < vl; k+=cl(val,k)) {
    final int cp=cp(val,k);
    if (cp == '"') {
      print(E_QU);
    }
 else     if (cp == 0x9 || cp == 0xA) {
      hex(cp);
    }
 else {
      encode(cp);
    }
  }
  print(ATT2);
}
