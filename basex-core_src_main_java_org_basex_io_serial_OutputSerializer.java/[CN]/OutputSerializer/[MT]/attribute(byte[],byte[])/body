{
  print(' ');
  print(name);
  print(ATT1);
  final int vl=value.length;
  for (int k=0; k < vl; k+=cl(value,k)) {
    final int ch=cp(value,k);
    if (!format) {
      printChar(ch);
    }
 else     if (ch == '"') {
      print(E_QU);
    }
 else     if (ch == 0x9 || ch == 0xA) {
      hex(ch);
    }
 else {
      encode(ch);
    }
  }
  print(ATT2);
}
