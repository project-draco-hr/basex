{
  print(' ');
  print(n);
  print(ATT1);
  for (int k=0; k < v.length; k+=cl(v,k)) {
    final int ch=cp(v,k);
    if (!format) {
      printChar(ch);
    }
 else     if (ch == '"') {
      print(E_QU);
    }
 else     if (ch == 0x9 || ch == 0xA) {
      hex(ch);
    }
 else {
      encode(ch);
    }
  }
  print(ATT2);
}
