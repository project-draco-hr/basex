{
  if (lvl != 0 || docsys == null && docpub == null)   return false;
  if (sep)   indent();
  print(DOCTYPE);
  if (type == null)   print(HTML);
 else   print(type);
  if (docpub != null)   print(' ' + PUBLIC + " \""+ docpub+ '"');
 else   print(' ' + SYSTEM);
  if (docsys != null)   print(" \"" + docsys + '"');
  print(ELEM_C);
  sep=true;
  return true;
}
