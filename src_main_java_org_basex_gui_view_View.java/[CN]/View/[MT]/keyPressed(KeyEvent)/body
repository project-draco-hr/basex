{
  if (gui.updating)   return;
  if (ESCAPE.is(e)) {
    gui.fullscreen(false);
  }
 else   if (SPACE.is(e)) {
    gui.notify.mark(sc(e) ? 2 : e.isShiftDown() ? 1 : 0,null);
  }
 else   if (ENTER.is(e)) {
    GUICommands.FILTER.execute(gui);
  }
 else   if (GOBACK2.is(e)) {
    GUICommands.GOBACK.execute(gui);
  }
}
