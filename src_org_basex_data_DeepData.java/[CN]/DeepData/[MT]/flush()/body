{
  try {
    table.flush();
    texts.flush();
    values.flush();
    final DataOutput out=new DataOutput(meta.dbname,DATAINFO);
    meta.finish(out,size);
    tags.finish(out);
    atts.finish(out);
    skel.finish(out);
    ns.finish(out);
    out.close();
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
}
