{
  final XQuery query=new XQuery("iri-to-uri(\"http://www.example.com/00/Weather/CA/Los%20Angeles#ocean\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/00/Weather/CA/Los%20Angeles#ocean"));
}
