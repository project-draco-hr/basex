{
  if (it1 == null || it1.size() == 0)   return it2;
  final ItemIter tmp=new ItemIter();
  Item i1=it1.next(), i2=it2.next();
  while (i1 != null && i2 != null) {
    if (i1.score() < i2.score()) {
      tmp.add(i2);
      i2=it2.next();
    }
 else     if (i1.score() > i2.score()) {
      tmp.add(i1);
      i1=it1.next();
    }
 else {
      tmp.add(i2);
      i1=it1.next();
      i2=it2.next();
    }
  }
  while (i1 != null) {
    tmp.add(i1);
    i1=it1.next();
  }
  while (i2 != null) {
    tmp.add(i2);
    i2=it2.next();
  }
  return tmp;
}
