{
  check(MAPKEYS);
  query("for $i in " + MAPKEYS.args(MAPNEW.args(" for $i in 1 to 3 return " + MAPENTRY.args("$i","$i+1"))) + " order by $i return $i","1 2 3");
  query("let $map := " + MAPNEW.args(" for $i in 1 to 3 return " + MAPENTRY.args("$i","$i + 1")) + "for $k in "+ MAPKEYS.args("$map")+ " order by $k return "+ MAPGET.args("$map","$k"),"2 3 4");
}
