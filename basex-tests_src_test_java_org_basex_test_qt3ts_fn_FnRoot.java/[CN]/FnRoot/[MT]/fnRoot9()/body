{
  final XQuery query=new XQuery("fn:root(<?format role=\"output\" ?>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<?format role=\"output\" ?>",false));
}
