{
  final XQuery query=new XQuery("fn:root(<anElement>An Element Content</anElement>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<anElement>An Element Content</anElement>",false));
}
