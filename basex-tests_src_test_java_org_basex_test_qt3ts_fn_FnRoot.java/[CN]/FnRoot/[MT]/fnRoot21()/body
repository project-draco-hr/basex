{
  final XQuery query=new XQuery("let $var := <!-- An Element Node --> return fn:count(fn:namespace-uri(fn:root($var)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
