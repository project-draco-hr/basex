{
  final XQuery query=new XQuery("fn:string(fn:implicit-timezone() div xs:dayTimeDuration(\"P0DT60M00S\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("xs:string"));
}
