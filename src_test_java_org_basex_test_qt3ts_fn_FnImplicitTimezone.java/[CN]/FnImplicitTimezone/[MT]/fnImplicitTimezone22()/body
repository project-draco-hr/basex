{
  final XQuery query=new XQuery("(fn:adjust-time-to-timezone(xs:time(\"10:00:00\"),fn:implicit-timezone()))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("xs:time"));
}
