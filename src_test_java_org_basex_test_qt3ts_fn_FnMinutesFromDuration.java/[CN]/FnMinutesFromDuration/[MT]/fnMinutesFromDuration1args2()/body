{
  final XQuery query=new XQuery("fn:minutes-from-duration(xs:dayTimeDuration(\"P15DT11H59M59S\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"59"));
}
