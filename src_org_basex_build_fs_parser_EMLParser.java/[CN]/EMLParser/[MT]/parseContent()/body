{
  boolean multipart=false;
  if (mBoundary != null && !mBoundary.isEmpty()) {
    if (mContentType != null && mContentType.startsWith("multipart")) {
      multipart=true;
      while (mCurrLine != null && !mCurrLine.contains(mBoundary))       readLine();
    }
  }
  while (true)   if (!getBodyData(multipart))   break;
}
