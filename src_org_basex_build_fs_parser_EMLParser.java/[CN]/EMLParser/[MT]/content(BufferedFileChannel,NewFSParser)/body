{
  if (!check(f))   return;
  fsparser=parser;
  do {
    String type="";
    final Matcher m=KEYPAT.matcher(mCurrLine);
    if (m.matches()) {
      type=m.group(1);
      if (type.equals("Content-Type"))       getContentType();
 else       if (type.equals("Content-Transfer-Encoding"))       getEncoding();
    }
 else     if (mCurrLine.contains("boundary=")) {
      getBoundary();
    }
    if (!readLine())     return;
  }
 while (mCurrLine.length() != 0);
  parseContent();
}
