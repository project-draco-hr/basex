{
  final XQuery query=new XQuery("//news_item/title[contains(., \"Foobar Corporation\")]",ctx);
  try {
    query.context(node(file("docs/string.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<title>Foobar Corporation releases its new line of Foo products\r\n   today</title><title>Foobar Corporation is suing Gorilla Corporation for\r\n   patent infringement </title>",false));
}
