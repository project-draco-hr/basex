{
  final CachedOutput out=new CachedOutput();
  if (global) {
    if (!cs.execute(new Show("Users"),out)) {
      throw new BaseXException(cs.info());
    }
  }
 else {
    if (!cs.execute(new InfoUsers(),out)) {
      throw new BaseXException(cs.info());
    }
  }
  data=new Table(out.toString());
}
