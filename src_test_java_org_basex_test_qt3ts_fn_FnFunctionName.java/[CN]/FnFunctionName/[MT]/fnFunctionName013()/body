{
  final XQuery query=new XQuery("fn:function-name( fn:substring(?, 1) )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("empty-sequence()") || error("XPST0005")));
}
