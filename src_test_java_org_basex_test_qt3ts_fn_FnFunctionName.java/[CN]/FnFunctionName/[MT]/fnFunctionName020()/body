{
  final XQuery query=new XQuery("fn:function-name( function($arg1, $arg2)\n" + "                               { subsequence($arg1, $arg2, 1) } )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
