{
  final XQuery query=new XQuery("fn:function-name( concat#340282366920938463463374607431768211456 )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOAR0002") || assertEq("fn:QName(\"http://www.w3.org/2005/xpath-functions\", \n                              \"fn:concat\")")));
}
