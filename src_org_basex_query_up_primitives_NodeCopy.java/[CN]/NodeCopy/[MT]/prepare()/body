{
  if (c.size() == 0)   return;
  final NodIter seq=new NodIter();
  final Iterator<NodIter> it=c.iterator();
  while (it.hasNext()) {
    final NodIter ni=it.next();
    ni.reset();
    Nod i;
    while ((i=ni.next()) != null)     seq.add(i);
  }
  m=UpdateFunctions.buildDB(mergeText(seq),((DBNode)node).data);
}
