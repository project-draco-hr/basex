{
  il.sort();
  final int is=il.size();
  outR.write5(outL.size());
  outL.writeNum(is);
  for (int i=0, old=0; i < is; i++) {
    final long value=il.get(i);
    if (tokenize) {
      int v=(int)(value >> 32);
      outL.writeNum(v - old);
      old=v;
      outL.writeNum((int)value);
    }
 else {
      outL.writeNum((int)value - old);
      old=(int)value;
    }
  }
  il.reset();
}
