{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http://example.com/\"; \n" + "        declare namespace p = \"http://example.com/2\"; \n"+ "        <e xmlns=\"\" xmlns:p=\"http://example.com/3\">[{namespace-uri-from-QName(xs:QName(\"n1\"))}|{namespace-uri-from-QName(xs:QName(\"p:n2\"))}]</e>/text()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[|http://example.com/3]"));
}
