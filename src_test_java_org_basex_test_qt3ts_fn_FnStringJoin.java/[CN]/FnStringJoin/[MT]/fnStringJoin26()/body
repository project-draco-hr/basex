{
  final XQuery query=new XQuery("\n" + "         fn:string-join((1 to 9)!string())\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"123456789"));
}
