{
  final XQuery query=new XQuery("<result>{ (\"test1\", \"test2\") }</result>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result>test1 test2</result>",false));
}
