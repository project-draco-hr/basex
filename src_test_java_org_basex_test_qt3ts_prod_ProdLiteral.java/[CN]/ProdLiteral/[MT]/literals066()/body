{
  final XQuery query=new XQuery("<test>{ 'He said, \"I don''t like it.\"' }</test>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<test>He said, \"I don't like it.\"</test>",false));
}
