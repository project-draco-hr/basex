{
  final XQuery query=new XQuery(queryFile(file("prod/Literal/K2-Literals-39.xq")),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"a \n\n\n \nstring literal \n"));
}
