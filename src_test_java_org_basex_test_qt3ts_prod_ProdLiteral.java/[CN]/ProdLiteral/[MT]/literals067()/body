{
  final XQuery query=new XQuery("<test check='He said, \"I don''t like it.\"' />",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<test check=\"He said, &quot;I don't like it.&quot;\"/>",false));
}
