{
  final XQuery query=new XQuery("module namespace prefix = \"http://example.com\"; declare function prefix:myFunction() { 1 };",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("true()") || error("*")));
}
