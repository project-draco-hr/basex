{
  final XQuery query=new XQuery("xs:date(\"2000-10-30+05:00\") - xs:date(\"1999-11-28Z\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P336DT19H"));
}
