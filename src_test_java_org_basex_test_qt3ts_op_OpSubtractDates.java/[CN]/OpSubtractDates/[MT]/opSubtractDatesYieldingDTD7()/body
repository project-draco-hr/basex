{
  final XQuery query=new XQuery("fn:string(xs:date(\"1989-07-05Z\") - xs:date(\"1962-09-04Z\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P9801D"));
}
