{
  final XQuery query=new XQuery("xs:date(\"2000-10-15-05:00\") - xs:date(\"2000-10-10+02:00\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P5DT7H"));
}
