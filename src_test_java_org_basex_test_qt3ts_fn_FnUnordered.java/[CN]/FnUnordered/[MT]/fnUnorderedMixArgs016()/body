{
  final XQuery query=new XQuery("fn:unordered ( (\"a\", xs:double(\"INF\"), \"b\", \"c\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertPermutation("\"a\", xs:double(\"INF\"), \"b\", \"c\""));
}
