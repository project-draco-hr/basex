{
  if (preds.length == 0)   return true;
  final Value cv=ctx.value;
  try {
    for (    final Expr p : preds) {
      ctx.value=it;
      final Item i=p.test(ctx,info);
      if (i == null)       return false;
      it.score(i.score());
    }
    return true;
  }
  finally {
    ctx.value=cv;
  }
}
