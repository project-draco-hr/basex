{
  final int bs=basemap.size();
  for (int pre=0; pre < bs; pre++) {
    final int id=basemap.id(pre);
    final int p=testedmap.pre(id);
    if (pre != p)     fail("Wrong PRE for ID = " + id + ": expected "+ pre+ ", actual "+ p+ "\nInserted PREs: "+ insertedpres+ "\nDelete PREs: "+ deletedpres);
  }
}
