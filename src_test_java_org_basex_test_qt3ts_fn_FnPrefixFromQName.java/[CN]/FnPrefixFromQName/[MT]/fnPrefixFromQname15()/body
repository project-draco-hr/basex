{
  final XQuery query=new XQuery("fn:substring(fn:prefix-from-QName(xs:QName(\"foo:bar\")),2)",ctx);
  try {
    query.namespace("foo","http://example.org");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"oo"));
}
