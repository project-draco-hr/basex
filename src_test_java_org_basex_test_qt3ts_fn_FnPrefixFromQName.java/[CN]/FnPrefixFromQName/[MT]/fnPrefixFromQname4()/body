{
  final XQuery query=new XQuery("fn:string-length(xs:string(fn:prefix-from-QName(xs:QName(\"name\"))))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0"));
}
