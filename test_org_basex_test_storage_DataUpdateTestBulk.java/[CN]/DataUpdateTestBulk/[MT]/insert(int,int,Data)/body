{
  int pre=par;
  final Data data=ctx.data();
  int k=data.kind(pre);
  if (pos == 0) {
    pre+=data.size(pre,k);
  }
 else {
    pre+=data.attSize(pre,k);
    for (int p=1; p < pos && data.parent(pre,k) == par; pre+=data.size(pre,k), p++) {
      k=data.kind(pre);
    }
  }
  data.insert(pre,par,tmp);
}
