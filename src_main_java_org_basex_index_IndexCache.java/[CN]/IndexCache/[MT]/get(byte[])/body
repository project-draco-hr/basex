{
  final int hash=hash(key);
  readLock.lock();
  try {
    final int i=indexFor(hash,buckets.length);
    BucketEntry e=buckets[i];
    while (e != null) {
      final IndexEntry entry=e.get();
      if (entry != null && e.hash == hash && eq(entry.key,key))       return entry;
      e=e.next;
    }
  }
  finally {
    readLock.unlock();
  }
  return null;
}
