{
  purge();
  final int s=size << 1;
  final BucketEntry[] tmp=new BucketEntry[s];
  final int l=bucket.length;
  for (int i=0; i < l; ++i) {
    BucketEntry e=bucket[i];
    bucket[i]=null;
    while (e != null) {
      final BucketEntry next=e.next;
      final int p=indexFor(e.hash,tmp.length);
      e.next=tmp[p];
      tmp[p]=e;
      e=next;
    }
  }
  bucket=tmp;
}
