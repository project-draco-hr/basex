{
  for (Object x; (x=queue.poll()) != null; ) {
synchronized (queue) {
      BucketEntry e=(BucketEntry)x;
      int i=indexFor(e.hash,bucket.length);
      BucketEntry prev=bucket[i];
      BucketEntry p=prev;
      while (p != null) {
        BucketEntry next=p.next;
        if (p == e) {
          if (prev == e)           bucket[i]=next;
 else           prev.next=next;
          e.next=null;
          --size;
          break;
        }
        prev=p;
        p=next;
      }
    }
  }
}
