{
  purge();
  final int hash=hash(key);
  final int i=indexFor(hash,bucket.length);
  BucketEntry e=bucket[i];
  BucketEntry prev=e;
  while (e != null) {
    final BucketEntry next=e.next;
    final IndexEntry entry=e.get();
    if (entry == null) {
      delete(i,e,prev,next);
    }
 else     if (e.hash == hash && eq(entry.key,key)) {
      delete(i,e,prev,next);
      return;
    }
    prev=e;
    e=next;
  }
}
