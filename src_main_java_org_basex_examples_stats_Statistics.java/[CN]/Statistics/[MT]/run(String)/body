{
  final TokenList tl=new TokenList();
  tl.add("Document");
  for (  final String h : header)   tl.add(h);
  table.header=tl;
  if (db == null) {
    for (    final String d : List.list(ctx))     analyze(d);
  }
 else {
    analyze(db);
  }
  if (tab) {
    Main.out(table.toString());
  }
 else {
    Main.out(Token.string(table.finish()));
  }
}
