{
  final int toScan=i;
  final int[] subContentDir=fs.children(toScan);
  final IntList allDir=new IntList();
  for (  final int j : subContentDir) {
    if (!cFlag) {
      out.print(fs.path(j));
      out.print(NL);
    }
    if (fs.isDir(j)) {
      allDir.add(j);
    }
    ++filesfound;
  }
  while (allDir.size > 0) {
    printDir(allDir.remove(0),out);
  }
}
