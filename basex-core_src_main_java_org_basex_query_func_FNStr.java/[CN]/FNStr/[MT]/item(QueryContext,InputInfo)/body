{
switch (func) {
case CODEPOINTS_TO_STRING:
    return codepointsToString(qc);
case COMPARE:
  return compare(qc);
case CODEPOINT_EQUAL:
return codepointEqual(qc);
case STRING_JOIN:
return stringJoin(qc);
case SUBSTRING:
return substr(qc);
case NORMALIZE_UNICODE:
return normalizeUnicode(qc);
case UPPER_CASE:
return Str.get(uc(toToken(exprs[0],qc,true)));
case LOWER_CASE:
return Str.get(lc(toToken(exprs[0],qc,true)));
case TRANSLATE:
return translate(qc);
case ENCODE_FOR_URI:
return Str.get(uri(toToken(exprs[0],qc,true),false));
case IRI_TO_URI:
return Str.get(uri(toToken(exprs[0],qc,true),true));
case ESCAPE_HTML_URI:
return Str.get(escape(toToken(exprs[0],qc,true)));
case CONCAT:
return concat(qc);
case CONTAINS:
return contains(qc);
case CONTAINS_TOKEN:
return containsToken(qc);
case STARTS_WITH:
return startsWith(qc);
case ENDS_WITH:
return endsWith(qc);
case SUBSTRING_AFTER:
return substringAfter(qc);
case SUBSTRING_BEFORE:
return substringBefore(qc);
default :
return super.item(qc,ii);
}
}
