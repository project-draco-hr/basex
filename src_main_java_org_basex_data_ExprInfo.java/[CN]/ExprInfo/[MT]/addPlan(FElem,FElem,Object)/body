{
  plan.add(el);
  for (  final Object o : expr) {
    if (o instanceof ExprInfo) {
      ((ExprInfo)o).plan(el);
    }
 else     if (o instanceof ExprInfo[]) {
      for (      final ExprInfo e : (ExprInfo[])o) {
        if (e != null)         e.plan(el);
      }
    }
 else     if (o instanceof byte[]) {
      el.add(new FTxt((byte[])o));
    }
 else     if (o != null) {
      el.add(new FTxt(Token.token(o.toString())));
    }
  }
}
