{
  final String selAttr=xAxis ? (String)xCombo.getSelectedItem() : (String)yCombo.getSelectedItem();
  if (GUI.context.data().fs != null) {
    if (selAttr.equals("@mtime"))     return deepFsFormat(value,1);
    if (selAttr.equals("@size"))     return deepFsFormat(value,0);
  }
  return string(chopNumber(token(value)));
}
