{
  final BufferedImage img=new BufferedImage(getWidth(),getHeight(),Transparency.BITMASK);
  final Graphics g=img.getGraphics();
  Graphics2D g2d=(Graphics2D)g;
  g2d.setRenderingHint(RenderingHints.KEY_ANTIALIASING,RenderingHints.VALUE_ANTIALIAS_ON);
  if (scatterData.size == 0)   return img;
  g.setColor(GUIConstants.color6);
  for (int i=0; i < scatterData.size; i++) {
    drawItem(g,scatterData.xAxis.co[i],scatterData.yAxis.co[i],false,false);
  }
  return img;
}
