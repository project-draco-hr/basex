{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http://www.example.com/filesystem\";\n" + "        declare variable $v as xs:integer := 100;\n"+ "        declare function local:udf1 ($CUSTNO as xs:integer) { <empty> {$CUSTNO*$v} </empty> };\n"+ "        local:udf1(10)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<empty xmlns=\"http://www.example.com/filesystem\">1000</empty>",false));
}
