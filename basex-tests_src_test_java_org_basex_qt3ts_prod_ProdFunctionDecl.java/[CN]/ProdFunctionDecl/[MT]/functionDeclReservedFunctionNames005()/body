{
  final XQuery query=new XQuery("\n" + "\tdeclare default function namespace \"http://www.w3.org/2005/xquery-local-functions\";\n" + "\tdeclare function document-node() { fn:true() };\n"+ "\tlocal:document-node()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
