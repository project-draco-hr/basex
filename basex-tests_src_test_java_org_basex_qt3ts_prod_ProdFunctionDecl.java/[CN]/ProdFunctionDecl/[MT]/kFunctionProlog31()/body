{
  final XQuery query=new XQuery("\n" + "        declare function fn:count($var) { fn:count($var) };\n" + "        1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0045"));
}
