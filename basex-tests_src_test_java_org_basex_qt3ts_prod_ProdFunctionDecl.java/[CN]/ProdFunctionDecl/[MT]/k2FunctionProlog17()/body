{
  final XQuery query=new XQuery("       declare function local:func() as xs:integer { <e>1</e> };\n" + "        local:func()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
