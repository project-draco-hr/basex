{
  final XQuery query=new XQuery("\n" + "        declare function local:_($arg as attribute()?) { 1 };\n" + "        local:_(1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("1") || error("XPTY0004")));
}
