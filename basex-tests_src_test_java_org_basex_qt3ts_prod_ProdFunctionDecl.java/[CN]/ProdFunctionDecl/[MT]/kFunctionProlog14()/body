{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction() as xs:integer { subsequence((1, 2, \"a string\"), 3 ,1) };\n" + "        fn:boolean(local:myFunction())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
