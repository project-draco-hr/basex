{
  final XQuery query=new XQuery("declare variable $var1 := let $var1 := 1 return 1;\n" + "        $var1 eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
