{
  final XQuery query=new XQuery("\n" + "        declare function local:myName() as xs:integer { 1 };\n" + "        declare function local:myName() as xs:nonPositiveInteger { 1 };\n"+ "        1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0034"));
}
