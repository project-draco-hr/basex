{
  final XQuery query=new XQuery("declare namespace my = \"http://example.com/ANamespace\";\n" + "        my:function(1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0017"));
}
