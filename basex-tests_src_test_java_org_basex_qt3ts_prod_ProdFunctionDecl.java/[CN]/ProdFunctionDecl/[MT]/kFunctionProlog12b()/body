{
  final XQuery query=new XQuery("\n" + "        declare function local:computeSum() { $myVariable };\n" + "        declare variable $myVariable := 1;\n"+ "        1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
