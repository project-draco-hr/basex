{
  final XQuery query=new XQuery("\n" + "        declare function local:error() as none() { 1 };\n" + "        local:error()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPST0003") || error("XPST0051")));
}
