{
  final XQuery query=new XQuery("declare namespace my = \"http://www.w3.org/2001/XMLSchema-instance\";\n" + "        declare function my:wrongNS() { 1 };\n" + "        1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0045"));
}
