{
  final XQuery query=new XQuery("declare function local:count($x) { count($x) }; local:count((1 to 100000, 1 to 100000))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"200000"));
}
