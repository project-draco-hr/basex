{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction() as xs:double { \"This is not a double, it's an xs:string.\" };\n" + "        local:myFunction()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
