{
  TextInput nli=null;
  try {
    nli=new TextInput(input);
    nli.encoding(encoding).validate(validate);
    return nli;
  }
 catch (  final IOException ex) {
    if (nli != null)     try {
      nli.close();
    }
 catch (    final IOException ignored) {
    }
    throw error.get(ii,ex);
  }
}
