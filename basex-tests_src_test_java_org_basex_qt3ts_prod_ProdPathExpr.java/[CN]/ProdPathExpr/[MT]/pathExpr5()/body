{
  final XQuery query=new XQuery("fn:count(.[/ < 5])",ctx);
  try {
    query.context(node(file("prod/PathExpr/OneTopElement.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
