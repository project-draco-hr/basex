{
  final XQuery query=new XQuery("declare boundary-space preserve; <a> z {\"abc\"}</a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a> z abc</a>",false));
}
