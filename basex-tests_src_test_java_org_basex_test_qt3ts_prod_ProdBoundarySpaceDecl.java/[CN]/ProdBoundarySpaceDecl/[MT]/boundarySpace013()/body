{
  final XQuery query=new XQuery("\n" + "declare boundary-space strip;\n" + "<res>\n"+ "  a\n"+ "  b\n"+ "  c\n"+ "</res>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<res>\n  a\n  b\n  c\n</res>",false));
}
