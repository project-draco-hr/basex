{
  final XQuery query=new XQuery("declare variable $myvar := <elem> <a/> <b/> <c/></elem>; $myvar/*[last() - 1]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<b/>",false));
}
