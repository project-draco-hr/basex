{
  final XQuery query=new XQuery("(/root/date[xs:date(.) = xs:date(\"2000-01-01+05:00\")])",ctx);
  try {
    query.context(node(file("docs/atomicns.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<date>2000-01-01+05:00</date>",false));
}
