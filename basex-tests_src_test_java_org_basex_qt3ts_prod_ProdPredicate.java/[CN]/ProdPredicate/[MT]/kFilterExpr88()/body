{
  final XQuery query=new XQuery("empty(remove((1, 2, 3, current-time()), 4)[false()])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPST0005")));
}
