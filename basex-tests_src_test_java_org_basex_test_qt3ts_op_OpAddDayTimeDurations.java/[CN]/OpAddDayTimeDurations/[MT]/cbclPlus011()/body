{
  final XQuery query=new XQuery("\n" + "        declare function local:f($x) { if ($x) then xs:duration(\"P1D\") else xs:dayTimeDuration(\"P1D\") };\n" + "        xs:dayTimeDuration(\"PT1H\") + local:f(false())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P1DT1H"));
}
