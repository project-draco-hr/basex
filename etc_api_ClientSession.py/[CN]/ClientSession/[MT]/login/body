def login(self):
    ts = self.result()
    pwmd5 = hashlib.md5(self.pw).hexdigest()
    m = hashlib.md5()
    m.update(pwmd5)
    m.update(ts)
    complete = m.hexdigest()
    self.s.send(str.encode(self.user))
    self.s.send('\x00')
    self.s.send(str.encode(complete))
    self.s.send('\x00')
    data = self.read1byte()
    return ('\x00' == data)
