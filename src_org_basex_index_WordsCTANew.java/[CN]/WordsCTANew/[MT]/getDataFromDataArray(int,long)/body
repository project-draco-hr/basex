{
  if (s == 0 && ldid <= 0)   return null;
  int[][] dt=new int[2][s];
  if (Prop.ftcompress) {
    dt[0][0]=inD.readNum(ldid);
    for (int i=1; i < s; i++)     dt[0][i]=inD.readNum();
    for (int i=0; i < s; i++)     dt[1][i]=inD.readNum();
  }
 else {
    dt[0]=inD.readInts(ldid,s * 4L + ldid);
    dt[1]=inD.readInts(s * 4L + ldid,s * 8L + ldid);
  }
  return dt;
}
