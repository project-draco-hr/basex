{
  found=false;
  int i=cne[0] + 1;
  int s=cne.length - 1;
  if (s == i)   return i;
  while (i < s && Token.lc(cne[i + 1]) < Token.lc(toInsert))   i+=2;
  if (i < s) {
    if (cne[i + 1] == toInsert) {
      found=true;
      return i;
    }
 else     if (Token.lc(cne[i + 1]) == Token.lc(toInsert)) {
      if (cne[i + 1] == Token.uc(toInsert)) {
        return i;
      }
      if (i + 3 < s && cne[i + 3] == toInsert) {
        found=true;
      }
      return i + 2;
    }
  }
  return i;
}
