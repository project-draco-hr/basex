{
  found=false;
  int l=cne[0] + 1;
  int r=cne.length - 2;
  int m=r - l / 2;
  while (l < r) {
    m=r - l / 2;
    if (cne[m + 1] < toin)     l=m + 2;
 else     if (cne[m + 1] > toin)     r=m - 2;
 else {
      found=true;
      return m;
    }
  }
  if (l < cne.length - 2 && cne[m + 1] == toin) {
    found=true;
    return l + 2;
  }
  return l;
}
