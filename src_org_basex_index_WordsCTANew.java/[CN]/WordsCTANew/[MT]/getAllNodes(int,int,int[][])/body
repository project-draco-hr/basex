{
  if (cn != b) {
    int[][] newData=data;
    int[] ne=getNodeEntry(cn);
    int[][] tmp=getDataFromDataArray(ne[ne.length - 2],ne[ne.length - 1]);
    if (tmp != null) {
      newData=FTUnion.calculateFTOr(data,tmp);
    }
    if (hasNextNodes(ne)) {
      for (int i=ne[0] + 1; i < ne.length - 2; i+=2) {
        newData=getAllNodes(ne[i],b,newData);
      }
    }
    return newData;
  }
  return data;
}
