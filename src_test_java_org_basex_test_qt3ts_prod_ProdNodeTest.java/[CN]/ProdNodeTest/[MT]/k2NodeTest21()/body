{
  final XQuery query=new XQuery("\n" + "         declare namespace ex = \"http://www.example.com/\"; \n" + "         declare function local:userFunction() { document-node(element(local:ncname)) }; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("1") || error("XPDY0002")));
}
