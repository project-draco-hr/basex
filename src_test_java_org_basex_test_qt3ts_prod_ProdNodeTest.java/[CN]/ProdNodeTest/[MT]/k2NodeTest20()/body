{
  final XQuery query=new XQuery("\n" + "         declare namespace ex = \"http://www.example.com/\"; \n" + "         document-node(schema-element(ex:thisTypeIsNotRecognizedExample.Com))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
