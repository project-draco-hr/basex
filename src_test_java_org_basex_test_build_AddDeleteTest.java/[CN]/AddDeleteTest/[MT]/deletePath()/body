{
  new Add("foo/pub",FLDR).execute(CONTEXT);
  assertEquals(NFLDR,docs());
  new Delete("foo").execute(CONTEXT);
  assertEquals(0,docs());
  new Add("/foo///bar////",FILE).execute(CONTEXT);
  new Add("foobar",FLDR).execute(CONTEXT);
  new Delete("foo").execute(CONTEXT);
  assertEquals(NFLDR,docs());
}
