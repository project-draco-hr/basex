{
  new Add(FLDR,null,"foo/pub").execute(CONTEXT);
  assertEquals(NFLDR,docs());
  new Delete("foo").execute(CONTEXT);
  assertEquals(0,docs());
  new Add(FILE,null,"/foo///bar////").execute(CONTEXT);
  new Add(FLDR,null,"foobar").execute(CONTEXT);
  new Delete("foo").execute(CONTEXT);
  assertEquals(NFLDR,docs());
}
