{
  check(_CLIENT_QUERY);
  contains(_CLIENT_EXECUTE.args(connect(),new ShowUsers()),Text.USERHEAD[0]);
  query("let $a := " + connect() + ", $b := "+ connect()+ " return "+ _CLIENT_QUERY.args("$a","1")+ "+"+ _CLIENT_QUERY.args("$b","2"),"3");
  error(_CLIENT_QUERY.args(connect(),"x"),Err.XPNOCTX);
}
