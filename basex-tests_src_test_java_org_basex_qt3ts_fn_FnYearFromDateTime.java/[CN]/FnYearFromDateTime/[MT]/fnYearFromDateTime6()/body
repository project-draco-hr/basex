{
  final XQuery query=new XQuery("fn:year-from-dateTime(xs:dateTime(\"-1999-05-31T00:20:00-05:00\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-1999"));
}
