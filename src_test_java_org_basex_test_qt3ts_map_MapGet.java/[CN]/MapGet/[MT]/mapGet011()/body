{
  final XQuery query=new XQuery("map:get(map{1:=\"Sunday\",2:=\"Monday\",3:=\"Tuesday\",4.0e0:=\"Wednesday\",5:=\"Thursday\",6:=\"Friday\",7:=\"Saturday\"}, 4)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Wednesday"));
}
