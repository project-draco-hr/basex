{
  final XQuery query=new XQuery("map:get(map:new(map{\"su\":=\"Sunday\",\"mo\":=\"Monday\",\"tu\":=\"Tuesday\",\"we\":=\"Wednesday\",\"th\":=\"Thursday\",\"fr\":=\"Friday\",\"sa\":=\"Saturday\"}, \n" + "            \"http://www.w3.org/2005/xpath-functions/collation/codepoint\"), \"TH\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
