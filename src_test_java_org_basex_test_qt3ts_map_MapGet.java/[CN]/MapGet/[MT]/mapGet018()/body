{
  final XQuery query=new XQuery("map:get(map{1:=\"Sunday\",2:=\"Monday\",3:=\"Tuesday\",function-name(abs#1):=\"Wednesday\",5:=\"Thursday\",6:=\"Friday\",7:=\"Saturday\"}, xs:QName('fn:abs'))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"Wednesday"));
}
