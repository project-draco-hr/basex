{
  final XQuery query=new XQuery("fn:map(\n" + "            map{\"su\":=\"Sunday\",\"mo\":=\"Monday\",\"tu\":=\"Tuesday\",\"we\":=\"Wednesday\",\"th\":=\"Thursday\",\"fr\":=\"Friday\",\"sa\":=\"Saturday\"},\n" + "            (\"we\", \"th\"))",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertCount(2) && assertQuery("$result[1] eq \"Wednesday\"") && assertQuery("$result[2] eq \"Thursday\"")));
}
