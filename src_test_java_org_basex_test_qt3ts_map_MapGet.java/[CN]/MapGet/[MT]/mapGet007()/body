{
  final XQuery query=new XQuery("map:get(map:entry(xs:untypedAtomic(\"foo\"), \"bar\"), \"foo\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"bar"));
}
