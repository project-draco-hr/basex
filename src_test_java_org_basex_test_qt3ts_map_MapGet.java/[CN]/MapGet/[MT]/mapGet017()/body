{
  final XQuery query=new XQuery("map:get(map{1:=\"Sunday\",2:=\"Monday\",3:=\"Tuesday\",xs:duration('P1Y'):=\"Wednesday\",5:=\"Thursday\",6:=\"Friday\",7:=\"Saturday\"}, xs:yearMonthDuration('P12M'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Wednesday"));
}
