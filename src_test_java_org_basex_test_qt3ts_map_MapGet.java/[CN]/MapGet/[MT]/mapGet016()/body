{
  final XQuery query=new XQuery("map:get(\n" + "                map:new(((for $i in 1 to 1000 return map:entry($i, $i*$i)),\n" + "                         (for $i in 2000 to 3000 return map:entry($i, $i+30)),\n"+ "                         (for $i in 2500 to 3500 return map:entry($i, $i+30)))),\n"+ "                         3260)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("3290"));
}
