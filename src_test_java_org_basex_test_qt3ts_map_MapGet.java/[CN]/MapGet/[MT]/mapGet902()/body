{
  final XQuery query=new XQuery("map:get(map{1:=\"Sunday\",2:=\"Monday\",3:=\"Tuesday\",number('NaN'):=\"Wednesday\",5:=\"Thursday\",6:=\"Friday\",7:=\"Saturday\"}, (1 to 5)[. mod 2 = 0])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
