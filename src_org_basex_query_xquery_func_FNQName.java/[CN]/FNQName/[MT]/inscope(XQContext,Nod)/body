{
  final SeqIter seq=new SeqIter();
  seq.add(new NCN(Token.XML).iter());
  if (ctx.nsElem != Uri.EMPTY)   seq.add(Str.ZERO);
  Nod n=node;
  while (n != null) {
    final FAttr[] at=n.ns();
    if (at == null)     break;
    for (    final FAttr ns : at) {
      final QNm name=ns.qname();
      if (name.ns())       seq.add(Str.get(name.ln()).iter());
    }
    n=n.parent();
  }
  return seq;
}
