{
  if (markedImage != null) {
    final int h=markedImage.getHeight();
    final int w=markedImage.getWidth();
    if (y > h || y < 0 || x > w || x < 0)     return false;
    final int marc=markedImage.getRGB(x,y);
    return colormark1.equals(marc) || colormarkA.equals(marc);
  }
  return false;
}
