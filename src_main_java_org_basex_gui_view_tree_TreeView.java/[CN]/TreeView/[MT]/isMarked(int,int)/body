{
  if (markedImage != null) {
    final int h=markedImage.getHeight();
    final int w=markedImage.getWidth();
    if (y >= h || 0 > y || x >= w || 0 > x)     return false;
    final int marc=markedImage.getRGB(x,y);
    return colormark1.getRGB() == marc || colormarkA.getRGB() == marc;
  }
  return false;
}
