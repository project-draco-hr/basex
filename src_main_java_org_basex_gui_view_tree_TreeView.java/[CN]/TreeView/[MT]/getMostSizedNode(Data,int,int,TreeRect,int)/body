{
  final int size=getHitBigRectNodesNum(rn,lv,r);
  final int idx=sub.getPreIndex(rn,lv,p);
  if (idx < 0)   return -1;
  int dpre=-1;
  int si=0;
  for (int i=0; i < size; ++i) {
    final int pre=sub.getPrePerIndex(rn,lv,i + idx);
    final int k=d.kind(pre);
    final int s=d.size(pre,k);
    if (s > si) {
      si=s;
      dpre=pre;
    }
  }
  return dpre;
}
