{
  int xx=x;
  final int y=getYperLevel(lv);
  int nh=nodeHeight;
  g.setColor(GUIConstants.back);
  while (nh > 0) {
    nh=nh - ss;
    while (nh < 0)     nh++;
    g.drawLine(xx,y + nh,w,y + nh);
  }
  while (xx < w) {
    xx=xx + ss - 1 < w ? xx + ss : xx + ss - 1;
    g.drawLine(xx,y,xx,y + nodeHeight);
  }
}
