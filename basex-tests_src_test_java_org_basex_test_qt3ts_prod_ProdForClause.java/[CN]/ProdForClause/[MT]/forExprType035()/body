{
  final XQuery query=new XQuery("for $test as xs:anyAtomicType in ( 1, \"string\", 1e2, 0.3 ) return $test",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 string 100 0.3"));
}
