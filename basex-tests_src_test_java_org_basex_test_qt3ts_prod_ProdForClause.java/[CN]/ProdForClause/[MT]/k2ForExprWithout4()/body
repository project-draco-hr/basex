{
  final XQuery query=new XQuery("declare function local:myFunc() { for $i at $p in (1, 2, 3) return position() }; local:myFunc()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
