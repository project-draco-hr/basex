{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunc() { for $i in (1, 2, 3) return position() }; \n" + "        local:myFunc()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
