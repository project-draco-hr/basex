{
  try {
    for (    final Option opt : options(getClass())) {
      if (opt.value != null) {
        values.put(opt.name,opt.value);
        options.put(opt.name,opt);
      }
    }
  }
 catch (  final Exception ex) {
    ex.printStackTrace();
    Util.notexpected(ex);
  }
  if (suffix != null)   read(suffix);
  setSystem();
}
