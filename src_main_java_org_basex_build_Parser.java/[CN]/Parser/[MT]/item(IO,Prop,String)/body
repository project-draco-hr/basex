{
  final boolean ml=eq(type,APP_JSONML);
  if (ml || APP_JSON.equals(type))   return new DBNode(new JSONParser(in,prop,ParserProp.JSONML[0] + "=" + ml));
  if (TEXT_CSV.equals(type))   return new DBNode(new CSVParser(in,prop));
  if (TEXT_HTML.equals(type))   return new DBNode(new HTMLParser(in,prop));
  if (MimeTypes.isXML(type))   return new DBNode(in,prop);
  if (MimeTypes.isText(type))   return Str.get(new TextInput(in).content());
  return new B64(in.read());
}
