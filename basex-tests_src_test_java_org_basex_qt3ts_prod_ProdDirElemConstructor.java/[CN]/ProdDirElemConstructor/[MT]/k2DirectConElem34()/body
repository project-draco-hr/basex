{
  final XQuery query=new XQuery("<foo > {attribute name {\"content\"}} <doo/> </foo>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<foo name=\"content\"><doo/></foo>",false));
}
