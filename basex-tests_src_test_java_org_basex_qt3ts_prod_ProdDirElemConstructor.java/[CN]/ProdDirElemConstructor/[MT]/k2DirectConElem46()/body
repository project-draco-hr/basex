{
  final XQuery query=new XQuery("declare namespace p = \"http://www.example.com/\"; <p:e/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<p:e xmlns:p=\"http://www.example.com/\"/>",false));
}
