{
  if (item instanceof Array) {
    final Array o=(Array)item;
    if (size != o.size)     return false;
    for (int v=0; v < size; v++) {
      final Value v1=get(v), v2=o.get(v);
      if (v1.size() != v2.size() || !new DeepCompare(ii).collation(coll).equal(v1,v2))       return false;
    }
    return true;
  }
  return item instanceof FItem && !(item instanceof Map) && super.deep(item,ii,coll);
}
