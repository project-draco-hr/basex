{
  tb.add('[');
  int c=0;
  for (int i=0; i < size; i++) {
    if (c++ > 0)     tb.add(", ");
    final Value v=get(i);
    if (v.size() != 1)     tb.add('(');
    int cc=0;
    for (    final Item it : v) {
      if (cc++ > 0)       tb.add(", ");
      if (it instanceof Array)       ((Array)it).string(tb,ii);
 else       if (it instanceof Map)       ((Map)it).string(tb,0,ii);
 else       tb.add(it.toString());
    }
    if (v.size() != 1)     tb.add(')');
  }
  tb.add(']');
}
