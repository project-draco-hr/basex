{
  return new NodeIter(){
    boolean stop;
    @Override public ANode next() throws QueryException {
      if (stop)       return null;
      stop=true;
      final AxisIter iter=axis.iter(checkNode(qc));
      ANode litem=null;
      final Test tst=test;
      for (ANode item; (item=iter.next()) != null; ) {
        qc.checkStop();
        if (tst.eq(item))         litem=item.finish();
      }
      return litem == null ? null : litem;
    }
  }
;
}
