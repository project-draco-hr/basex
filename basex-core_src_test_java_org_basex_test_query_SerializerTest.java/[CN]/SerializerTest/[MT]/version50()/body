{
  final String option="declare option output:method 'html';" + "declare option output:version '5.0';";
  query(option + "<html/>","<!DOCTYPE html><html></html>");
  final String[] empties={"area","base","br","col","command","embed","hr","img","input","keygen","link","meta","param","source","track","wbr"};
  for (  final String e : empties) {
    query(option + "<" + e+ "/>","<!DOCTYPE html><" + e + ">");
  }
  query(option + "<a>&#x90;</a>","<!DOCTYPE html><a>&#x90;</a>");
}
