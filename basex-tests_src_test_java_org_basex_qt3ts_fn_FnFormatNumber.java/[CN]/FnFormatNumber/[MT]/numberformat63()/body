{
  final XQuery query=new XQuery("format-number(000123456789012345678901234567890.123456789012345678900000,     '##0.0####################################################')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"123456789012345678901234567890.1234567890123456789") || error("FOAR0002")));
}
