{
  final XQuery query=new XQuery("format-number(12.34, '0.000,00', 'Q{http://a.ns/}test ')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0.012,34"));
}
