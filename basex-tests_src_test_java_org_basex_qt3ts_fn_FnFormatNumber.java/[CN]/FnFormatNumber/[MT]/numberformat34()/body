{
  final XQuery query=new XQuery("format-number(4030201.0506,'#!!!,!!!,??????.????????????0')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"#??,??????,??????.????????????0"));
}
