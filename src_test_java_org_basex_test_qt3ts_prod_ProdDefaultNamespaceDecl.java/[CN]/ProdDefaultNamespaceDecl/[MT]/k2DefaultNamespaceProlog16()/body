{
  final XQuery query=new XQuery("declare option local:myOption \"foo\"; declare default element namespace \"http://example.com\"; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
