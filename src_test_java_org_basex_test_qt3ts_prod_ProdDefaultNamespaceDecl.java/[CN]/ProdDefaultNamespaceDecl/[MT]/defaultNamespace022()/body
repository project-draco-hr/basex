{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http://www.example.com/test\"; \n" + "        <test/>\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<test xmlns=\"http://www.example.com/test\"/>",false));
}
