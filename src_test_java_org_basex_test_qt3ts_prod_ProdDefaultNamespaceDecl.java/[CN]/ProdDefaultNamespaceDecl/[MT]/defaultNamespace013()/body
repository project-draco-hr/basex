{
  final XQuery query=new XQuery("\n" + "      \tdeclare default function namespace \"http://example.org\"; \n" + "      \tdeclare function foo2($i as xs:string) as xs:string {foo($i)}; \n"+ "      \tdeclare function foo($i as xs:string) as xs:string {$i}; \n"+ "      \tfoo2(\"abc\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"abc"));
}
