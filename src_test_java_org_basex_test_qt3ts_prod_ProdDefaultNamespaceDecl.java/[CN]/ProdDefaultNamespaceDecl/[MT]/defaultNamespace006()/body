{
  final XQuery query=new XQuery("\n" + "      \tdeclare default function namespace \"http://example.org\"; \n" + "      \tdeclare function mysum($i as xs:integer, $j as xs:integer) { let $j := $i + $j return $j }; \n"+ "      \tdeclare function invoke_mysum($st) { for $d in (1,2,3,4,5) let $st := mysum($d, $st) return $st }; \n"+ "      \tinvoke_mysum(0)\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1 2 3 4 5"));
}
