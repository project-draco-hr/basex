{
  final XQuery query=new XQuery("\n" + "        declare default function namespace \"http://www.w3.org/XML/1998/namespace\";\n" + "        declare function go() {3};\n"+ "        go()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0070"));
}
