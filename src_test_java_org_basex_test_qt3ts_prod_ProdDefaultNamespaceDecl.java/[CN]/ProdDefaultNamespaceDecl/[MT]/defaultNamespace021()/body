{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http:/www.oracle.com/xquery\"; \n" + "        declare variable $x := 7.5; \n"+ "        $x + 2",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"9.5"));
}
