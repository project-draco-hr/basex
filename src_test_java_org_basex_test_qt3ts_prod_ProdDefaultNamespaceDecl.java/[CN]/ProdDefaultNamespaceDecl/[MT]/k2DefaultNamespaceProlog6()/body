{
  final XQuery query=new XQuery("declare default element namespace \"http://example.com/\"; namespace-uri-from-QName(xs:QName(\"localName\")), <e xmlns=\"\"> { \" | \", namespace-uri-from-QName(xs:QName(\"localName\")) } </e>, namespace-uri-from-QName(xs:QName(\"localName\"))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("http://example.com/<e> |  </e>http://example.com/",false));
}
