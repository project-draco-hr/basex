{
  final XQuery query=new XQuery("\n" + "        declare default function namespace \"http://www..oracle.com/xquery/test\"; \n" + "        declare function foo($n as xs:integer) { <tr> {$n} </tr> }; \n"+ "        foo(4)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<tr>4</tr>",false));
}
