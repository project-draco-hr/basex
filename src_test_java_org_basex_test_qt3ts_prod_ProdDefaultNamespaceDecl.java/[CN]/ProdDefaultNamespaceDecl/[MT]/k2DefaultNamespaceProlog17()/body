{
  final XQuery query=new XQuery("\n" + "         declare default element namespace \"http://example.com/\"; \n" + "         for $test as attribute(integer, xs:anyAtomicType) in (<e integer=\"1\"/>/@integer) \n"+ "         return data($test)\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1"));
}
