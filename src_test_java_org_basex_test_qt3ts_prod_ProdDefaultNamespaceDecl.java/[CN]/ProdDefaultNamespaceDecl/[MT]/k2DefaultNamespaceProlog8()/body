{
  final XQuery query=new XQuery("<e xmlns=\"http://www.example.com/1\" xmlns=\"http://www.example.com/2\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0071"));
}
