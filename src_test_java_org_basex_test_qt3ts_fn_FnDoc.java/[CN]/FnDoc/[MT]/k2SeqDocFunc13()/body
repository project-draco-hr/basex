{
  final XQuery query=new XQuery("\n" + "        declare variable $doc1 := doc(\"http://example.com\"); \n" + "        let $doc2 := doc(\"ftp://example.com/\") \n"+ "        for $i in doc(\"localFile.xml\") \n"+ "        return (1, 2, 3)",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"1 2 3") || error("FODC0002")));
}
