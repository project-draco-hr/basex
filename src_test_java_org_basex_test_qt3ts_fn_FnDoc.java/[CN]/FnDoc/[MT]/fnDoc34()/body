{
  final XQuery query=new XQuery("doc($uri)",ctx);
  query.addDocument("id/0x010D.xml",file("fn/id/0x010D.xml"));
  query.bind("uri",new XQuery("'id/0x010D.xml'",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<project ??reated-by=\"{{build_number}}\"/>",false));
}
