{
  if (dateTime.length != 20)   return EMPTY;
  int i=0;
  for (; i < 4; i++) {
    if (!digit(dateTime[i]))     return EMPTY;
  }
  if (dateTime[i] != ':')   return EMPTY;
  dateTime[i++]='-';
  for (; i < 7; i++) {
    if (!digit(dateTime[i]))     return EMPTY;
  }
  if (dateTime[i] != ':')   return EMPTY;
  dateTime[i++]='-';
  for (; i < 10; i++) {
    if (!digit(dateTime[i]))     return EMPTY;
  }
  if (dateTime[i] != ' ')   return EMPTY;
  dateTime[i++]='T';
  for (; i < 13; i++) {
    if (!digit(dateTime[i]))     return EMPTY;
  }
  if (dateTime[i++] != ':')   return EMPTY;
  for (; i < 16; i++) {
    if (!digit(dateTime[i]))     return EMPTY;
  }
  if (dateTime[i++] != ':')   return EMPTY;
  for (; i < 19; i++) {
    if (!digit(dateTime[i]))     return EMPTY;
  }
  return dateTime;
}
