{
  if (year.length != 4)   return Token.EMPTY;
  for (int i=0; i < 4; i++) {
    if (year[i] < '0' || year[i] > '9') {
      BaseX.debug("Invalid date value: %",year);
      return Token.EMPTY;
    }
  }
  byte[] y=new byte[6];
  System.arraycopy(year,0,y,0,4);
  y[4]='-';
  y[5]='-';
  return y;
}
