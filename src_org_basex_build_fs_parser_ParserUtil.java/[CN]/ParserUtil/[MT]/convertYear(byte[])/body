{
  if (isEmpty(year) || year.length != 4)   return null;
  for (int i=0; i < 4; i++) {
    if (year[i] < '0' || year[i] > '9') {
      BaseX.debug("Invalid date value: " + Token.string(year));
      return null;
    }
  }
  byte[] y=new byte[6];
  System.arraycopy(year,0,y,0,4);
  y[4]='-';
  y[5]='-';
  return y;
}
