{
  int count=0;
  byte[] value={0,0,0,0,'-','-'};
  for (int i=0, max=year.length; i < max && count < 4; i++) {
    if (Token.digit(year[i]))     value[count++]=year[i];
 else     if (year[i] != 0) {
      return Token.EMPTY;
    }
  }
  if (count < 4) {
    return Token.EMPTY;
  }
  return value;
}
