{
  final ByteList bl=new ByteList();
  while (true) {
    final byte[] line=readLine();
    if (line == null || eq(line,sep))     break;
    if (eq(line,end)) {
      while (readLine() != null)       ;
      break;
    }
    bl.add(line).add(CRLF);
  }
  return new TextInput(new IOContent(bl.finish())).encoding(enc).content();
}
