{
  final ArrayList<Dependency> procs=new ArrayList<Dependency>();
  for (  final Dependency dep : pkg.dep) {
    if (dep.pkg == null && dep.processor == null)     PKGDESCINV.thrw(info,MISSSECOND);
    if (dep.pkg != null && depPkg(dep) == null)     NECPKGNOTINST.thrw(info,dep.pkg);
    if (dep.processor != null)     procs.add(dep);
  }
  if (procs.size() != 0)   checkProcs(procs);
}
