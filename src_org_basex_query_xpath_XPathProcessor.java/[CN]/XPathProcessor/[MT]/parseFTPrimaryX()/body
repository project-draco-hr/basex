{
  Expr e=null;
  if (curr('"') || curr('"')) {
    e=parseFTWords();
    consumeWS();
    FTPositionFilter ftps=parseFTTimesX();
    if (ftps != null)     return new FTPrimary(new Expr[]{e},ftps);
    return new FTPrimary(new Expr[]{e});
  }
 else   if (consume('(')) {
    consumeWS();
    e=parseFTSelectionX();
    consumeWS();
    if (!consume(')')) {
      error(") expected");
    }
  }
 else {
    error("\', \" or ( expected");
  }
  return e;
}
