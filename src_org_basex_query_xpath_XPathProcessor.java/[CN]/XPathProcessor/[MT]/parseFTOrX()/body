{
  Expr e=parseFTAndX();
  consumeWS();
  if (!consume(FTOR))   return e;
  ExprList el=new ExprList();
  el.add(e);
  consumeWS();
  if (consume(FTNOT))   error("An \"ftnot\" FTUnaryNot expression may only appear as " + "a direct right operand of an \"ftand\" (FTAnd) operation.");
  el.add(parseFTAndX());
  consumeWS();
  while (consume(FTOR)) {
    consumeWS();
    el.add(parseFTAndX());
  }
  return new FTOr(el.get());
}
