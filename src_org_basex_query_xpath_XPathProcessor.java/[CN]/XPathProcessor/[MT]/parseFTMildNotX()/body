{
  Expr e=parseFTUnaryNotX();
  consumeWS();
  if (!consume(NOTIN))   return e;
  ExprList el=new ExprList();
  el.add(e);
  consumeWS();
  if (consume(FTNOT))   error("An \"ftnot\" FTUnaryNot expression may only appear as " + "a direct right operand of an \"ftand\" (FTAnd) operation.");
  el.add(parseFTUnaryNotX());
  consumeWS();
  while (consume(NOTIN)) {
    consumeWS();
    el.add(parseFTUnaryNotX());
  }
  return new FTMildNot(el.get());
}
