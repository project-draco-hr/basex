{
  final XQuery query=new XQuery("\n" + "        <XMark-result-Q6> { \n" + "            let $auction := (/) return for $b in $auction//site/regions return count($b//item) } \n"+ "        </XMark-result-Q6>",ctx);
  try {
    query.context(node(file("app/XMark/XMarkAuction.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<XMark-result-Q6>647</XMark-result-Q6>",false));
}
