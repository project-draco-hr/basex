{
  new Thread(){
    @Override public void run(){
      process(new Insert(CmdUpdate.ELEMENT,"//members","aa"),session1);
      new Thread(){
        @Override public void run(){
          String result=process(new Delete("//aa"),session2);
          if (result != null)           err("test failed conUpdate: " + result);
          done=true;
        }
      }
.start();
    }
  }
.start();
  while (!done) {
    Performance.sleep(200);
  }
  running=false;
}
