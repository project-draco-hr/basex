{
  start=0;
  ok(new CreateDB(FILE),session1);
  checkPin(1);
  ok(new CreateDB(FILE),session1);
  checkPin(0);
  no(new CreateDB(FILE),session2);
  checkPin(0);
  no(new CreateDB(FILE),session2);
  checkPin(0);
  no(new DropDB(NAME),session2);
  checkPin(0);
  ok(new DropDB(NAME),session1);
  checkPin(-1);
}
