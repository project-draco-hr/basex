{
  consumeWs('{',true);
  h.openObject();
  if (!consumeWs('}',false)) {
    do {
      h.openEntry(spec != Spec.LIBERAL || curr() == '"' ? string() : unquoted());
      consumeWs(':',true);
      value(h);
      h.closeEntry();
    }
 while (consumeWs(',',false) && !(spec == Spec.LIBERAL && curr() == '}'));
    consumeWs('}',true);
  }
  h.closeObject();
}
