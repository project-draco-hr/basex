{
  int kind=data.kind(p);
  if (kind == Data.ATTR)   return;
  int pos=0;
  final int size=data.size;
  int pre=p + data.size(p,kind);
  while (pre != size) {
    kind=data.kind(pre);
    if (test.eval(data,pre,kind) && ++pos == posPred) {
      preds.posEval(ctx,pre,result);
      return;
    }
    pre+=data.attSize(pre,kind);
  }
}
