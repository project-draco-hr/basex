{
  int toScan=i;
  int[] subContentDir=FSUtils.getAllOfDir(data,toScan);
  IntList allDir=new IntList();
  for (  int j : subContentDir) {
    if (!cFlag) {
      out.print(FSUtils.getPath(data,j));
      out.print(NL);
    }
    if (FSUtils.isDir(data,j)) {
      allDir.add(j);
    }
    ++filesfound;
  }
  while (allDir.size > 0) {
    printDir(allDir.remove(0));
  }
}
