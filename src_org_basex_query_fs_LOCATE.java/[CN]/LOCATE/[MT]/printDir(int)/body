{
  int toScan=i;
  int[] subContentDir=FSUtils.getAllOfDir(data,toScan);
  int[] allDir=FSUtils.getAllDir(data,toScan);
  for (  int j : subContentDir) {
    if (!cFlag) {
      out.print(FSUtils.getPath(data,j));
      out.print(NL);
    }
    ++filesfound;
  }
  for (int x=0; x < allDir.length; x++) {
    printDir(allDir[x]);
  }
}
