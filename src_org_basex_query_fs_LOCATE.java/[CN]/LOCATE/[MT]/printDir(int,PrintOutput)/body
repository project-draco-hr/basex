{
  final int toScan=i;
  final int[] subContentDir=FSUtils.getChildren(data,toScan);
  final IntList allDir=new IntList();
  for (  final int j : subContentDir) {
    if (!cFlag) {
      out.print(FSUtils.getPath(data,j));
      out.print(NL);
    }
    if (FSUtils.isDir(data,j)) {
      allDir.add(j);
    }
    ++filesfound;
  }
  while (allDir.size > 0) {
    printDir(allDir.remove(0),out);
  }
}
