{
  final XQuery query=new XQuery("\n" + "        <frequent_bidder> { \n" + "            for $u in $users//user_tuple \n"+ "            where every $item in $items//item_tuple \n"+ "                  satisfies some $b in $bids//bid_tuple \n"+ "                            satisfies ($item/itemno = $b/itemno and $u/userid = $b/userid) \n"+ "            return $u/name \n"+ "        } </frequent_bidder>\n"+ "      ",ctx);
  try {
    query.bind("$users",node(file("docs/users.xml")));
    query.bind("$items",node(file("docs/items.xml")));
    query.bind("$bids",node(file("docs/bids.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<frequent_bidder/>",false));
}
