{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) as xs:integer { $x }; \n" + "      \tmin(local:f(4) to local:f(10))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"4"));
}
