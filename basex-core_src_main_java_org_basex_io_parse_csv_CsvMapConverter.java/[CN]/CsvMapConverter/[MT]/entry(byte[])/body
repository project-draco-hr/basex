{
  if (headers.isEmpty()) {
    record.add(Str.get(value));
  }
 else {
    byte[] name=headers.get(col++);
    if (name == null)     name=ENTRY;
    try {
      record.set(0,((Map)record.get(0)).insert(Str.get(name),Str.get(value),null));
    }
 catch (    final QueryException ex) {
      throw new QueryIOException(ex);
    }
  }
}
