{
  atts.reset();
  atts.add(NAME,token(f.getName()));
  atts.add(SIZE,EMPTY);
  int sizeAttId=builder.startElem(DIR_NS,atts) + 2;
  sizeStack[++lvl]=0;
  parse(f);
  final long size=sizeStack[lvl];
  addFSElems(f,size);
  addFSAtts(sizeAttId,f,size);
  builder.endElem(DIR_NS);
  sizeStack[--lvl]+=size;
}
