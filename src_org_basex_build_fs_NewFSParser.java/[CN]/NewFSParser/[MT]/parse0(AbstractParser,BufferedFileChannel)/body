{
  if (Prop.fsmeta) {
    builder.nodeAndText(Element.TYPE.get(),atts.reset(),parser.getType());
    builder.nodeAndText(Element.FORMAT.get(),atts,parser.getFormat());
    bfc.reset();
    parser.readMeta(bfc,this);
    bfc.finish();
  }
  if (Prop.fscont) {
    bfc.reset();
    parser.readContent(bfc,this);
    bfc.finish();
  }
}
