{
  final XQuery query=new XQuery("\n" + "        declare(::)base-uri(::)\"http://example.com/declareBaseURITest\"; \n" + "        static-base-uri() eq 'http://example.com/declareBaseURITest'\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
