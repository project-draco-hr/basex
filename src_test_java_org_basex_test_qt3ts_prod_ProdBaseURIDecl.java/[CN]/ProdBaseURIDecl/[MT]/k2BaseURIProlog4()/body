{
  final XQuery query=new XQuery("declare base-uri \"abc\"; declare function local:isAbsolute($uri as xs:string?) as xs:boolean { fn:matches($uri, \"[a-zA-Z0-9\\-.]*:/\") }; local:isAbsolute(fn:static-base-uri())",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertBoolean(true) || error("XPST0001")));
}
