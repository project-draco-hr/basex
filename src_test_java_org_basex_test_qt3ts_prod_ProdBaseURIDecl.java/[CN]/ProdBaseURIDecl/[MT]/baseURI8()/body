{
  final XQuery query=new XQuery("\n" + "        declare namespace eg = \"http://example.org\"; \n" + "        declare function eg:noContextFunction() { \n"+ "            if (fn:static-base-uri() eq fn:resolve-uri(\"examples\")) then \"true\" else \"true\" \n"+ "        }; \n"+ "        eg:noContextFunction()",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"true") || error("FONS0005")));
}
