{
  final XQuery query=new XQuery("declare base-uri \"c:\\windows\"; fn:doc(\"example.com.xml\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQST0046") || error("FODC0005") || error("FODC0002")));
}
