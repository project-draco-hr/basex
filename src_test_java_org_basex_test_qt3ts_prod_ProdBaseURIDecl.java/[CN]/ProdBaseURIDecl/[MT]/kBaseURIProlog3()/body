{
  final XQuery query=new XQuery("\n" + "        declare(::)base-uri(::)\"http://example.com/declareBaseURITest\"(::); \n" + "        declare(::)base-uri(::)\"http://example.com/declareBaseURITest\"(::); 1 eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0032"));
}
