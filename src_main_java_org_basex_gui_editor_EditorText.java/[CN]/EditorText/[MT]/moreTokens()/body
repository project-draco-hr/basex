{
  final byte[] txt=text;
  final int tl=txt.length;
  int ppe=pe;
  if (ppe >= tl)   return false;
  ps=ppe;
  int ch=cp(txt,ppe);
  ppe+=cl(txt,ppe);
  if (ftChar(ch)) {
    while (ppe < tl) {
      ch=cp(txt,ppe);
      if (!ftChar(ch))       break;
      ppe+=cl(txt,ppe);
    }
  }
  pe=ppe;
  return true;
}
