{
  try {
    for (int i=0; i < runs; ++i) {
      Performance.sleep((long)(50 * RND.nextDouble()));
      final int n=RND.nextInt() % MAX + 1;
      final String qu=Util.info(QUERY,n);
      new XQuery(qu).execute(CONTEXT);
    }
  }
 catch (  final BaseXException ex) {
    ex.printStackTrace();
  }
}
