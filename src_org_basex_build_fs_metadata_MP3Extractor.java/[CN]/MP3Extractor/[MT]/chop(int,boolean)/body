{
  int i=s;
  while (--i >= 0 && content[i] == 0)   ;
  if (i == -1)   return EMPTY;
  int j=i;
  while (--j >= 0 && content[j] != 0)   ;
  if (j == -1)   return Array.finish(content,i + 1);
  if (!conv)   return null;
  for (int k=i; k > j; k--)   if (content[k] == 0)   content[k]=' ';
  return Array.create(content,j + 1,i - j);
}
