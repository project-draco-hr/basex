{
  int i=0;
  while (i < tp.length && tp[i] == -1)   i++;
  int p=tp[i];
  int j=i + 1;
  while (j < tp.length && tp[j] == -1)   j++;
  while (p < tp[tp.length - 1]) {
    final byte[] txt=ti.readBytes(p,p + i);
    final int oc=getDataSize(p,i);
    stats.add(txt,oc);
    p+=i + 4L + 5L;
    if (p == tp[j]) {
      i=j;
      while (j + 1 < tp.length && tp[++j] == -1)       ;
    }
  }
}
