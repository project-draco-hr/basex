{
  int i=0;
  while (i < tp.length && tp[i] == -1)   i++;
  int p=tp[i];
  int j=i + 1;
  while (j < tp.length && tp[j] == -1)   j++;
  while (p < tp[tp.length - 1]) {
    if (stats.adding(getDataSize(p,i)))     stats.add(ti.readBytes(p,p + i));
    p+=i + 9;
    if (p == tp[j]) {
      i=j;
      while (j + 1 < tp.length && tp[++j] == -1)       ;
    }
  }
}
