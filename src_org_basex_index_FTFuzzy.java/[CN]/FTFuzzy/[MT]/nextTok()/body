{
  if (tp[tp.length - 1] == ptok)   return new byte[]{};
  if (tp[ntl] == ptok || ntl == 0) {
    ctl++;
    while (tp[ctl] == -1)     ctl++;
    ntl=ctl + 1;
    while (tp[ntl] == -1)     ntl++;
  }
  if (ctl == tp.length)   return new byte[]{};
  final byte[] tok=ti.readBytes(ptok,ptok + ctl);
  pftd=ti.read5(ti.pos());
  fts=ti.read4();
  ptok=(int)ti.pos();
  return tok;
}
