{
  GetOpts g=new GetOpts(cmd,"ah",1);
  int ch=g.getopt();
  while (ch != -1) {
switch (ch) {
case 'a':
      fPrintAll=true;
    break;
case 'h':
  printHelp();
return;
case ':':
out.print("ls: missing argument");
return;
case '?':
out.print("ls: illegal option");
return;
}
ch=g.getopt();
}
if (g.getPath() != null) {
curDirPre=FSUtils.goToDir(context.data(),curDirPre,g.getPath());
if (curDirPre == -1) {
out.print("du " + g.getPath() + "No such file or directory. ");
}
}
du(".",curDirPre);
}
