{
  final boolean active=r.w >= 16 && r.h >= 16 && my - r.y < 16 && mx - r.x < 16 && fs.isFile(r.pre) && gfs.mime(fs.name(r.pre)) != GUIFS.Type.IMAGE;
  if (active && click) {
    try {
      fs.launch(view.gui.context.focused);
    }
 catch (    final IOException ex) {
      Dialog.warn(view.gui,NODEFAULTAPP);
      ;
    }
  }
  return active;
}
