{
  final int s=size << 1;
  final int[] tmp=new int[s];
  for (  final int b : bucket) {
    int id=b;
    while (id != 0) {
      final int p=hash(keys[id]) & s - 1;
      final int nx=next[id];
      next[id]=tmp[p];
      tmp[p]=id;
      id=nx;
    }
  }
  bucket=tmp;
  next=Arrays.copyOf(next,s);
  final byte[][] k=new byte[s][];
  System.arraycopy(keys,0,k,0,size);
  keys=k;
}
