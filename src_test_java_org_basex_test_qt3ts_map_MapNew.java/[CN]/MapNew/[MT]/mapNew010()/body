{
  final XQuery query=new XQuery("let $foo := map:new(for $i in 1 to 20 return map:entry($i, $i*$i)), $bar := map:new(), $foobar := map:new(($foo, $bar))\n" + "              return $foobar",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:integer, xs:integer)")&& assertQuery("map:size($result) eq 20")&& assertQuery("$result(8) eq 64")));
}
