{
  final XQuery query=new XQuery("map:new((map:entry(3, \"three\"), map:entry(3.0e0, \"threeD\"), map:entry(xs:float('3.0'), \"threeF\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:float, xs:string)")&& assertQuery("map:size($result) eq 1")&& assertQuery("$result(3.0) eq \"threeF\"")));
}
