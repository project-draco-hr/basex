{
  final XQuery query=new XQuery("map:new(map:entry(\"foo\", 1 to 5))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:string, xs:integer+)")&& assertType("function(xs:anyURI) as xs:integer*")));
}
