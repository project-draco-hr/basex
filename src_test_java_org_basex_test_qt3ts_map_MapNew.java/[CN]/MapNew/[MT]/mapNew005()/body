{
  final XQuery query=new XQuery("map:new(for $i in 1 to 20 return map:entry($i, $i*$i))",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:integer, xs:integer)")&& assertQuery("map:size($result) eq 20")&& assertQuery("map:get($result, 9) eq 81")));
}
