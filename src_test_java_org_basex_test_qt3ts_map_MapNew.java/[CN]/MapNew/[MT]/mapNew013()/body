{
  final XQuery query=new XQuery("map:new(for $n in 1 to 20 return map:entry($n, map{$n:=string($n), $n+1:=string($n+1), $n+2:=string($n+2)}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("map(xs:integer, map(xs:integer, xs:string))") && assertQuery("$result(1)(3) eq \"3\"")));
}
