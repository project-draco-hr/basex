{
  final XQuery query=new XQuery("map:new((map:entry(\"foo\", 3), map:entry(\"foo\", 4)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:string, xs:integer)")&& assertQuery("map:size($result) eq 1")&& assertQuery("map:get($result, \"foo\") eq 4")));
}
