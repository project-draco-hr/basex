{
  final XQuery query=new XQuery("let $foo := map:new(for $i in 1 to 20 return map:entry($i, $i*$i)), $bar := map:entry(8, 63), $foobar := map:new(($foo, $bar))\n" + "              return ($foobar, $bar, $foo)[2]",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:integer, xs:integer)")&& assertQuery("map:size($result) eq 1")&& assertQuery("$result(8) eq 63")));
}
