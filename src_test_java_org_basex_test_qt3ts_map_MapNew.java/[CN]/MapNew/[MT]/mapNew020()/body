{
  final XQuery query=new XQuery("deep-equal(map:new(for $i in 1 to 1000 return map:entry($i, $i+1)),\n" + "                         map:new(for $i in 2 to 1001 return map:entry($i, $i+1)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
