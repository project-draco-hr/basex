{
  final XQuery query=new XQuery("map:new(())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:date, element()+)")));
}
