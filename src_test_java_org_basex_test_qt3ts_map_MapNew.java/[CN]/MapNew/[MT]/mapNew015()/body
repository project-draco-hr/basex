{
  final XQuery query=new XQuery("map:new(map{\"abc\":=1,\"xyz\":=2}, \"collation/codepoint\")",ctx);
  query.baseURI("http://www.w3.org/2005/xpath-functions/");
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("map:collation($result) eq \"http://www.w3.org/2005/xpath-functions/collation/codepoint\""));
}
