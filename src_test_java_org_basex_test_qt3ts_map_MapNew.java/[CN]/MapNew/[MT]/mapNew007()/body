{
  final XQuery query=new XQuery("let $foo := map:entry(\"foo\", 3), $bar := map:entry(\"foo\", 4), $foobar := map:new(($foo, $bar))\n" + "              return ($foobar, $bar, $foo)[3]",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertCount(1) && assertType("map(*)") && assertType("map(xs:string, xs:integer)")&& assertQuery("map:size($result) eq 1")&& assertQuery("map:get($result, \"foo\") eq 3")));
}
