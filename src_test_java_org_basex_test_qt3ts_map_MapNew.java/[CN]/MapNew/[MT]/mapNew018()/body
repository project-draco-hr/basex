{
  final XQuery query=new XQuery("deep-equal(map{\"a\":=1,\"b\":=2,\"c\":=(3,4,5)}, map{\"c\":=(3,4,5),\"a\":=1,\"b\":=2})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
