{
  final String file=DATAFTX;
  ti=new DataAccess(db,file + "y");
  dat=new DataAccess(db,file + "z");
  data=d;
  li=new DataAccess(db,file + "x");
  for (int i=0; i < tp.length; i++)   tp[i]=-1;
  int is=li.read();
  while (--is >= 0) {
    final int p=li.read();
    tp[p]=li.readInt();
  }
  tp[tp.length - 1]=(int)ti.length();
}
