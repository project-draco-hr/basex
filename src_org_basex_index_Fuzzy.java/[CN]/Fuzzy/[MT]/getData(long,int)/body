{
  final boolean cmp=data.meta.fcompress;
  final int[][] d={cmp ? dat.readNums(p,s) : dat.readInts(p,p + s * 4L),cmp ? dat.readNums(s) : dat.readInts(p + s * 4L,p + 2 * s * 4L)};
  return new IndexIterator(){
    /** 
 * Counter. 
 */
    private int c=-1;
    /** 
 * Pre value flag. 
 */
    private boolean pre;
    @Override public boolean more(){
      return ++c < s;
    }
    @Override public int next(){
      return d[(pre^=true) ? 0 : 1][c];
    }
  }
;
}
