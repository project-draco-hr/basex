{
  final int df=(tok1.length - tok2.length < 0) ? tok2.length - tok1.length : tok1.length - tok2.length;
  if (df > e)   return false;
  int d;
  d=(tok1.length > tok2.length) ? Levenshtein.levenshtein(tok1,sp,tok1.length,tok2,e) : Levenshtein.levenshtein(tok2,sp,tok2.length,tok1,e);
  return d <= e;
}
