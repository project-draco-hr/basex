{
  int c=0;
  for (int i=0; i < ids[0].length; ) {
    final int id=ids[0][i];
    ftdb.init(d.text(id));
    while (i < ids[0].length && id == ids[0][i] && ftdb.more()) {
      ftdb.cs=false;
      if (!Token.eq(Token.lc(tok),ftdb.get())) {
        continue;
      }
      ftdb.cs=true;
      if (Token.eq(tok,ftdb.get())) {
        ids[0][c]=id;
        ids[1][c++]=ids[1][i];
      }
      i++;
    }
  }
  return c;
}
