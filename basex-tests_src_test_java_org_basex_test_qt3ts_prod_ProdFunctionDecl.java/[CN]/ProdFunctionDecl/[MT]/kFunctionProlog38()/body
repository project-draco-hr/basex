{
  final XQuery query=new XQuery("\n" + "        declare function local:MyFunction($myArg) { $myArg2 };\n" + "        declare function local:MyFunction2($myArg2) { 0 };\n"+ "        1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
