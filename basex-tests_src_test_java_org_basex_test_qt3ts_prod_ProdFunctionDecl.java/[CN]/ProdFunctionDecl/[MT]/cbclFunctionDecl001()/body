{
  final XQuery query=new XQuery("declare function local:nothing() as empty-sequence() { () }; empty(local:nothing())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
