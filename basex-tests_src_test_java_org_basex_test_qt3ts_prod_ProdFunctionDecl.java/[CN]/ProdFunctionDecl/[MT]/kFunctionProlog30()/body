{
  final XQuery query=new XQuery("\n" + "        declare function local:myName($var as xs:integer) as xs:integer { $var };\n" + "        declare function local:myName() as xs:integer { 1 };\n"+ "        (local:myName(4) - 3) eq local:myName()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
