{
  final XQuery query=new XQuery("\n" + "        declare function local:myFunction($arg as item()?, $arg2 as xs:integer) { $arg, $arg2 };\n" + "        local:myFunction((), 4.1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
