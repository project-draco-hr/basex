{
  final XQuery query=new XQuery("       \n" + "        declare function local:foo2($i as xs:string) as xs:string {local:foo($i)};\n" + "        declare function local:foo($i as xs:string) as xs:string {$i};\n"+ "        local:foo2(\"abc\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abc"));
}
