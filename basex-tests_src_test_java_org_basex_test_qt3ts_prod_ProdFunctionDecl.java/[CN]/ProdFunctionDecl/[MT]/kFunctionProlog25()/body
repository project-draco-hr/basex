{
  final XQuery query=new XQuery("declare variable $local:myName := 1;\n" + "        declare function local:myName() as xs:integer { 1 };\n" + "        $local:myName eq local:myName()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
