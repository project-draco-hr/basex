{
  final XQuery query=new XQuery("if (implicit-timezone() eq xs:dayTimeDuration('PT1H'))\n" + "            then xs:time(\"12:00:00+02:00\") eq xs:time(\"12:00:00\")\n" + "            else xs:time(\"12:00:00+01:00\") eq xs:time(\"12:00:00\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
