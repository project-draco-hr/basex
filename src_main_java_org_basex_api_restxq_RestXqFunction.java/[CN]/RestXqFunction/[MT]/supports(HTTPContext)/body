{
  if (methods.contains(http.method)) {
    PATHS:     for (    final String[] path : paths) {
      if (path.length != http.depth())       continue;
      for (int p=0; p < path.length; p++) {
        if (!path[p].equals(http.step(p)) && !path[p].startsWith("{")) {
          continue PATHS;
        }
      }
      return true;
    }
  }
  return false;
}
