{
  final FElem hhead=new FElem(QNm.get(HTTP_PREFIX,"header",HTTP_URI)).add("name","location").add("value",toToken(exprs[0],qc));
  final FElem hresp=new FElem(QNm.get(HTTP_PREFIX,"response",HTTP_URI)).add("status","302").add(hhead);
  return new FElem(QNm.get(REST_PREFIX,"response",REST_URI)).add(hresp);
}
