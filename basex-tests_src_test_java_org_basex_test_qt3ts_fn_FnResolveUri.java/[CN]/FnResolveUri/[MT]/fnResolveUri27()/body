{
  final XQuery query=new XQuery("string(resolve-uri(\"b.html\", \"http://www.example.com/a.html?foo=bar\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/b.html"));
}
