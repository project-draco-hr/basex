{
  final int[] v=cls.startsWith("Is") ? MAP.get(cls.substring(2)) : null;
  if (v == null)   return "\\" + incl + "{"+ cls+ "}";
  final TokenBuilder sb=new TokenBuilder().add('[');
  if (incl == 'P')   sb.add("!");
  for (int i=0; i < v.length; ) {
    sb.add(v[i++]);
    sb.add('-');
    sb.add(v[i++]);
  }
  return sb.add(']').toString();
}
