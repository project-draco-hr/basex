{
  NodeIter it=doc.child();
  Node node=null;
  while ((node=it.next()) != null) {
    if (node.type != Type.ELM)     continue;
    it=node.attr();
    while ((node=it.next()) != null) {
      final QNm name=node.qname();
      if (eq(name.pre(),XMLNS))       ns.index(name);
    }
    break;
  }
  return doc;
}
