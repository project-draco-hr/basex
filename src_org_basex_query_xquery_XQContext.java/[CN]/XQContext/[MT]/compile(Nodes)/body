{
  inf=Prop.allInfo;
  info.reset();
  firstOpt=true;
  firstEval=true;
  if (nodes != null) {
    docs=new DNode[]{addNS(new DNode(nodes.data,0,null,Type.DOC))};
    item=docs[0];
  }
  if (inf)   compInfo(QUERYCOMP);
  fun.comp(this);
  vars.comp(this);
  ftopt=ftoptions;
  root=root.comp(this);
  if (inf)   compInfo(QUERYRESULT + "%",root);
  evalTime=System.nanoTime();
  return this;
}
