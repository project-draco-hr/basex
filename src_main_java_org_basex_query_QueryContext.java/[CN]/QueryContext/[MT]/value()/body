{
  try {
    Value v=value(root);
    if (updating) {
      updates.apply();
      if (updates.size() != 0 && context.data() != null)       context.update();
      if (output.size() != 0)       v=output.value();
    }
    return v;
  }
 catch (  final StackOverflowError ex) {
    Util.debug(ex);
    throw XPSTACK.thrw(null);
  }
}
