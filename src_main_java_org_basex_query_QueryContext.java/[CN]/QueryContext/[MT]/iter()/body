{
  try {
    final Iter iter=iter(root);
    if (!updating)     return iter;
    final Item i=iter.finish();
    updates.apply(this);
    if (context.data != null)     context.update();
    return i.iter();
  }
 catch (  final StackOverflowError ex) {
    Main.debug(ex);
    Err.or(null,XPSTACK);
    return null;
  }
}
