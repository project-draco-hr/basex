{
  final XQuery query=new XQuery("\n" + "        let $f := function($a as xs:integer, $b as xs:integer) as xs:integer {\n" + "          data(<a>{$a}{$b}</a>)\n"+ "        }\n"+ "        return $f(12, 34)\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertType("xs:integer") && assertEq("1234")));
}
