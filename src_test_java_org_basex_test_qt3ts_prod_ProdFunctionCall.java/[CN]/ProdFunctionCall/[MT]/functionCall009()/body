{
  final XQuery query=new XQuery("\n" + "        declare function local:f($in as xs:decimal*) as xs:decimal {sum($in, 0.0)};\n" + "        local:f(xs:NMTOKENS('1 1.2 1.3 1.4')!xs:untypedAtomic(.))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("xs:decimal") && assertEq("4.9")));
}
