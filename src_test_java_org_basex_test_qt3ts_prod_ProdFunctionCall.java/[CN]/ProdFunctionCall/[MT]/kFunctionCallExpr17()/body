{
  final XQuery query=new XQuery("matches(format-time(current-time(), \"[H01]:[m01]\", (), (), ()), \"..:..\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0017"));
}
