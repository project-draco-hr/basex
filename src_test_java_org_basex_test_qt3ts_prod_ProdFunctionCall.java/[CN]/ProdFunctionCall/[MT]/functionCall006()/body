{
  final XQuery query=new XQuery("\n" + "         let $f := function ($in as xs:boolean) as xs:boolean { $in }\n" + "         return $f(<a>0</a>)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
