{
  final XQuery query=new XQuery("(resolve-uri('prod2', 'http://datypic.com/prod1'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://datypic.com/prod2"));
}
