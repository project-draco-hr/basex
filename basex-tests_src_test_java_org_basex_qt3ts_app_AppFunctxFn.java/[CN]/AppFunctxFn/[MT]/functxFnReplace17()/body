{
  final XQuery query=new XQuery("(replace('2315551212', '(\\d{3})(\\d{3})(\\d{4})', '($1) $2-$3'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"(231) 555-1212"));
}
