{
  final XQuery query=new XQuery("let $address := '123 Main Street\n" + "Traverse City, MI 49684' return (matches($address, 'Main Street'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
