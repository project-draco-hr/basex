{
  final XQuery query=new XQuery("(resolve-uri( 'http://datypic.com', '../base'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"http://datypic.com") || error("FORG0002")));
}
