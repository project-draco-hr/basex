{
  final XQuery query=new XQuery("fn:string-length(fn:in-scope-prefixes(<anElement>Some content</anElement>)[1])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("3"));
}
