{
  final XQuery query=new XQuery("declare default element namespace \"http://www.example.com\"; count(fn:in-scope-prefixes(<e/>))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("2"));
}
