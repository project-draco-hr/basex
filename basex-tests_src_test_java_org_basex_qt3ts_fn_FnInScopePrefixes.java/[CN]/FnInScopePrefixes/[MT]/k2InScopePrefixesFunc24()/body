{
  final XQuery query=new XQuery("declare namespace p = \"http://example.com/\"; count(in-scope-prefixes(<element/>))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
