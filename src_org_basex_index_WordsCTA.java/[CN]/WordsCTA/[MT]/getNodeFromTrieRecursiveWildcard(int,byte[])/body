{
  byte[] valueSearchNode=searchNode;
  final byte[] curNodeEntry=getNodeEntry(currentCompressedTrieNode);
  if (currentCompressedTrieNode != 0) {
    counter[1]+=curNodeEntry[0];
    int i=0;
    while (i < valueSearchNode.length && i < curNodeEntry[0] && curNodeEntry[i + 1] == valueSearchNode[i]) {
      i++;
    }
    if (curNodeEntry[0] == i) {
      if (valueSearchNode.length == i) {
        counter[0]=i;
        return currentCompressedTrieNode;
      }
 else {
        final byte[] tmp=new byte[valueSearchNode.length - i];
        for (int j=0; j < tmp.length; j++) {
          tmp[j]=valueSearchNode[i + j];
        }
        valueSearchNode=tmp;
        final int position=getInsertingPositionLinear(valueSearchNode[0],curNodeEntry);
        if (!found) {
          counter[0]=i;
          counter[1]=counter[1] - curNodeEntry[0] + i;
          return currentCompressedTrieNode;
        }
 else {
          final int id=getIdOnDataArray(curNodeEntry);
          return getNodeFromTrieRecursiveWildcard(getDataEntry(id)[position],valueSearchNode);
        }
      }
    }
 else {
      counter[0]=i;
      counter[1]=counter[1] - curNodeEntry[0] + i;
      return currentCompressedTrieNode;
    }
  }
 else {
    final int position=getInsertingPositionLinear(valueSearchNode[0],curNodeEntry);
    if (!found) {
      counter[0]=-1;
      counter[1]=-1;
      return -1;
    }
 else {
      final int id=getIdOnDataArray(curNodeEntry);
      return getNodeFromTrieRecursiveWildcard(getDataEntry(id)[position],valueSearchNode);
    }
  }
}
