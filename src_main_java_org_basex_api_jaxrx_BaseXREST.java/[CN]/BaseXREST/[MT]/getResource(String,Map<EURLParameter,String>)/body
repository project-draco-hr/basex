{
  return new StreamingOutput(){
    @Override public void write(    final OutputStream out){
      final Context ctx=new Context();
      if (new Open(resource).exec(ctx)) {
        query(params.get(EURLParameter.QUERY),out,ctx,params.get(EURLParameter.WRAP),params.get(EURLParameter.OUTPUT),params.get(EURLParameter.START),params.get(EURLParameter.COUNT));
      }
      ctx.close();
    }
  }
;
}
