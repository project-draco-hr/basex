{
  final XQuery query=new XQuery("\n" + "        validate { document { <a/>, <b/> } }\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQDY0061") || error("XQDY0084")));
}
