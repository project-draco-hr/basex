{
  final XQuery query=new XQuery("declare default order empty least; declare default order empty greatest;  \"aaa\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0069"));
}
