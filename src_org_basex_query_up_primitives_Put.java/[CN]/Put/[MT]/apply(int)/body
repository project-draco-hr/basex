{
  PrintOutput out=null;
  try {
    out=new PrintOutput(Token.string(path()));
    final XMLSerializer ser=new XMLSerializer(out);
    node.serialize(ser);
    ser.close();
  }
 catch (  final IOException ex) {
    Err.or(UPPUTERR,path());
  }
 finally {
    try {
      if (out != null)       out.close();
    }
 catch (    final IOException ex) {
    }
  }
}
