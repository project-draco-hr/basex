{
  p(NCLIENTS + " clients / " + NQUERIES+ " queries");
  p(q);
  final Client[] cl=new Client[NCLIENTS];
  for (int i=0; i < NCLIENTS; ++i)   cl[i]=new Client(q);
  for (  final Client c : cl)   c.start();
  for (  final Client c : cl)   try {
    c.join();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
}
