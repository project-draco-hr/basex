{
  final Data data=context.data();
  final CmdUpdate type=getType();
  final Nodes nodes=Prop.gui ? context.marked() : query(args[type == CmdUpdate.PI || type == CmdUpdate.ATTRIBUTE ? 2 : 1],null);
  if (nodes == null)   return false;
  boolean ok=false;
switch (type) {
case ATTRIBUTE:
    ok=attr(data,nodes);
  break;
default :
ok=node(data,nodes);
break;
}
if (!ok) return false;
data.flush();
return Prop.info ? info(UPDATEINFO,nodes.size(),perf.getTimer()) : true;
}
