{
  final XQuery query=new XQuery("let $result := analyze-string(\"banana\", \"(b)(anana)\") return string($result)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"banana"));
}
