{
  final XQuery query=new XQuery("fn:string-join(('Blow, ', 'blow, ', 'thou ', 'winter ', 'wind!'), '')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Blow, blow, thou winter wind!"));
}
