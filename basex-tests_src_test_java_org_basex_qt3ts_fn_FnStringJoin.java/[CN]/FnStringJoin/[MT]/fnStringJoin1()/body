{
  final XQuery query=new XQuery("fn:string-join(('Now', 'is', 'the', 'time', '...'), ' ')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Now is the time ..."));
}
