{
  final XQuery query=new XQuery("fn:string-join((\"string-joinstring-join\"),\"string-join\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"string-joinstring-join"));
}
