{
  contains(_CLIENT_EXECUTE.args(conn(),new ShowUsers()),Text.S_USERINFO[0]);
  query("let $a := " + conn() + ", $b := "+ conn()+ " return ("+ _CLIENT_EXECUTE.args("$a",new XQuery("1"))+ ','+ _CLIENT_EXECUTE.args("$b",new XQuery("2"))+ ')',"1 2");
  error(_CLIENT_EXECUTE.args(conn(),"x"),BXCL_COMMAND_X);
}
