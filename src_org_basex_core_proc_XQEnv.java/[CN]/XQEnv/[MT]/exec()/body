{
  String query=cmd.args();
  long pars=0;
  long comp=0;
  long eval=0;
  try {
    for (int i=0; i < Prop.runs; i++) {
      if (query.length() == 0) {
        qu=new XQueryProcessor(query);
        query="()";
      }
      qu.query=query;
      progress(qu);
      final Nodes nodes=context.current();
      qu.parse();
      pars+=per.getTime();
      qu.compile(nodes);
      comp+=per.getTime();
      result=qu.eval(nodes);
      eval+=per.getTime();
    }
    if (Prop.info) {
      info(qu.getInfo());
      info(NL + QUERYPARSE + Performance.getTimer(pars,Prop.runs));
      info(NL + QUERYCOMPILE + Performance.getTimer(comp,Prop.runs));
      info(NL + QUERYEVALUATE + Performance.getTimer(eval,Prop.runs));
    }
  }
 catch (  final QueryException ex) {
    BaseX.debug(ex);
    return error(ex.getMessage());
  }
catch (  final Exception ex) {
    ex.printStackTrace();
    final String msg=ex.getMessage();
    return error("Implementation Bug? " + ex.getClass().getSimpleName() + (msg != null ? ": " + msg : ""));
  }
  return true;
}
