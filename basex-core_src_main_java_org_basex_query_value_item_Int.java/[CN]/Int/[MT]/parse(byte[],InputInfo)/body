{
  final byte[] val=Token.trim(value);
  boolean valid=true;
  for (  final byte v : val) {
    if (!Token.digit(v) && v != '+' && v != '-') {
      valid=false;
      break;
    }
  }
  if (valid) {
    final long l=Token.toLong(val);
    if (l != Long.MIN_VALUE || Token.eq(val,Token.MINLONG))     return l;
  }
  throw funCastError(ii,AtomType.INT,val);
}
