{
  final IntList il=new IntList();
  int p=0;
  final int sz=size();
  for (  final byte[] key : new TokenList(map).sort()) {
    p=get(key,p,sz);
    if (p < 0)     throw Util.notExpected("Key does not exist: '%'",key);
    if (deleteIds(p,key,map.get(key).sort().finish()))     il.add(p);
    p++;
  }
  deleteKeys(il);
}
