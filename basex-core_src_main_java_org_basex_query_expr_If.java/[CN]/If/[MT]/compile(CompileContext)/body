{
  cond=cond.compile(cc);
  final int[] branches=cond.isValue() ? new int[]{branch(cc.qc)} : new int[]{0,1};
  for (  final int b : branches) {
    try {
      exprs[b]=exprs[b].compile(cc);
    }
 catch (    final QueryException ex) {
      exprs[b]=FnError.get(ex,seqType,cc.sc());
    }
  }
  return optimize(cc);
}
