{
  final int s=ctx.vars.size();
  for (  final Let c : copies) {
    checkUp(c,ctx);
    ctx.vars.add(c.var);
  }
  for (int e=0; e != expr.length; e++)   expr[e]=expr[e].comp(ctx);
  if (!expr[0].uses(Use.UPD,ctx) && !expr[0].v())   Err.or(UPEXPECT);
  checkUp(expr[1],ctx);
  ctx.vars.reset(s);
  return this;
}
