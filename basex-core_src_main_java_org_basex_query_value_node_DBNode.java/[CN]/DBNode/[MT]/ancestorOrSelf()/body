{
  return new BasicNodeIter(){
    private final DBNode node=copy();
    int curr=pre, kind=data.kind(curr);
    @Override public ANode next(){
      if (curr == -1)       return null;
      kind=data.kind(curr);
      node.set(curr,kind);
      curr=data.parent(curr,kind);
      return node;
    }
  }
;
}
