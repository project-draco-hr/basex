{
  return new AxisIter(){
    int k=data.kind(pre), p=pre + data.attSize(pre,k);
    final int s=pre + data.size(pre,k);
    final DBNode node=copy();
    @Override public DBNode next(){
      if (p == s)       return null;
      k=data.kind(p);
      node.set(p,k);
      p+=data.attSize(p,k);
      return node;
    }
  }
;
}
