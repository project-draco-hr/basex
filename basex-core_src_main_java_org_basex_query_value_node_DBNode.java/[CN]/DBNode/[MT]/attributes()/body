{
  return new BasicNodeIter(){
    final DBNode node=copy();
    final int last=pre + data.attSize(pre,data.kind(pre));
    int curr=pre + 1;
    @Override public DBNode next(){
      if (curr == last)       return null;
      final DBNode n=node;
      n.set(curr++,Data.ATTR);
      return n;
    }
  }
;
}
