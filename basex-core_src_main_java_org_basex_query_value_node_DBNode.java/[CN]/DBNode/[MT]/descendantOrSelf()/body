{
  return new BasicNodeIter(){
    final DBNode node=copy();
    final int last=pre + data.size(pre,data.kind(pre));
    int curr=pre;
    @Override public ANode next(){
      if (curr == last)       return null;
      final int k=data.kind(curr);
      node.set(curr,k);
      curr+=data.attSize(curr,k);
      return node;
    }
  }
;
}
