{
  if (!(t instanceof FuncType))   return t == AtomType.ITEM;
  if (t instanceof MapType)   return false;
  final FuncType ft=(FuncType)t;
  for (int i=0; i < ft.ann.size(); i++)   if (!ann.contains(ft.ann.names[i],ft.ann.values[i]))   return false;
  if (this == ft || ft == ANY_FUN)   return true;
  if (this == ANY_FUN || args.length != ft.args.length || !type.instanceOf(ft.type))   return false;
  for (int a=0; a < args.length; a++) {
    if (!ft.args[a].instanceOf(args[a]))     return false;
  }
  return true;
}
