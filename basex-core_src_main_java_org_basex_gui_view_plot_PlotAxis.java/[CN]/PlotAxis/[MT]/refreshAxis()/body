{
  final Data data=plotData.context.data();
  final Stats key=elem ? data.elmindex.stat(attrID) : data.atnindex.stat(attrID);
  if (key == null)   return;
  type=key.type;
  if (type == null)   return;
  if (type == StatsType.CATEGORY)   type=StatsType.TEXT;
  final int[] items=plotData.pres;
  if (items.length < 1)   return;
  co=new double[items.length];
  final byte[][] vals=new byte[items.length][];
  for (int i=0; i < items.length; ++i) {
    byte[] value=getValue(items[i]);
    if (type == StatsType.TEXT && value.length > TEXTLENGTH) {
      value=substring(value,0,TEXTLENGTH);
    }
    vals[i]=lc(value);
  }
  if (type == StatsType.TEXT) {
    textToNum(vals);
  }
 else {
    minMax(vals);
    if (!log)     prepareLinAxis();
    for (int i=0; i < vals.length; ++i)     co[i]=calcPosition(vals[i]);
  }
}
