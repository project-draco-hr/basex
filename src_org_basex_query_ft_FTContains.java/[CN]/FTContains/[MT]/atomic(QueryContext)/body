{
  if (div == 0)   div=++ctx.ftcount;
  final Iter iter=expr.iter(ctx);
  final Tokenizer tmp=ctx.fttoken;
  ctx.fttoken=ft;
  double s=0;
  Item it;
  while ((it=iter.next()) != null) {
    ft.init(it.str());
    final FTNode node=ftexpr.atomic(ctx);
    final double d=node.score();
    s=ctx.score.and(s,d);
    if (d > 0 && ctx.ftpos != null && node.pos.length != 0 && it instanceof DBNode)     ctx.ftpos.add(node.pos,((DBNode)it).pre,div);
  }
  ctx.fttoken=tmp;
  return Bln.get(s);
}
