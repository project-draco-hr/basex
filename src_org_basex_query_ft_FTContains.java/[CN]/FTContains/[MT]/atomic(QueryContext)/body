{
  if (div == 0)   div=++ctx.ftcount;
  final Iter iter=expr.iter(ctx);
  final Tokenizer tmp=ctx.fttoken;
  final IntList[] ftd=ctx.ftd;
  ctx.fttoken=ft;
  double s=0;
  Item i;
  while ((i=iter.next()) != null) {
    ft.init(i.str());
    final double d=ftexpr.atomic(ctx).score();
    s=ctx.score.and(s,d);
    if (ctx.ftpos != null && ctx.ftd != null && d > 0 && i instanceof DBNode)     ctx.ftpos.add(ctx.ftd,((DBNode)i).pre,div);
  }
  ctx.fttoken=tmp;
  ctx.ftd=ftd;
  return Bln.get(s);
}
