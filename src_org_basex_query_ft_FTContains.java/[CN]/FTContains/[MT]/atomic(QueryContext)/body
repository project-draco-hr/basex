{
  if (div == 0)   div=++ctx.ftcount;
  final Iter iter=expr.iter(ctx);
  final Tokenizer tmp=ctx.fttoken;
  final IntList[] ftd=ctx.ftd;
  double d=0;
  Item i;
  ctx.fttoken=ft;
  while ((i=iter.next()) != null) {
    ft.init(i.str());
    final double d2=ftexpr.atomic(ctx).score();
    d=ctx.score.and(d,d2);
    if (ctx.ftpos != null && ctx.ftd != null && d2 > 0 && i instanceof DBNode)     ctx.ftpos.addConvSeqData(ctx.ftd,((DBNode)i).pre,div);
  }
  ctx.fttoken=tmp;
  ctx.ftd=ftd;
  return Bln.get(d);
}
