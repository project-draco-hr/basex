{
  try {
    for (int e=0; e < OP.length; e++) {
      if (Token.eq(k,OP[e].nm))       return OP[e].op.newInstance();
    }
  }
 catch (  final Exception ex) {
    final QueryException qe=new QueryException(ex.toString());
    qe.initCause(ex);
    throw qe;
  }
  throw new QueryException(PFSIMPLE,k);
}
