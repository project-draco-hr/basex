{
  final CmdUpdate type=getType();
  final StringBuilder sb=new StringBuilder(Cmd.INSERT + " " + type);
  sb.append(quote(args[2]));
  if (args.length == 4)   sb.append(quote(args[3]));
  sb.append(" " + INTO + " "+ args[1]);
  if (pos != 0)   sb.append(" " + AT + " "+ pos);
  return sb.toString();
}
