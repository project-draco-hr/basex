{
  try {
    final XMLParser parser=val.startsWith("<") ? new XMLParser(new CachedInput(Token.token(val))) : new XMLParser(val);
    final Data tmp=new MemBuilder().build(parser,"tmp");
    final Data data=context.data();
    for (int i=nodes.size - 1; i >= 0; i--) {
      if (data.kind(nodes.pre[i]) != Data.ELEM)       return error(COPYTAGS);
    }
    for (int i=nodes.size - 1; i >= 0; i--) {
      final int par=nodes.pre[i];
      data.insert(Insert.pre(par,pos,data),par,tmp);
    }
    data.flush();
    return Prop.info ? timer(BaseX.info(INSERTINFO,nodes.size)) : true;
  }
 catch (  final IOException ex) {
    BaseX.debug(ex);
    return error(ex.getMessage());
  }
}
