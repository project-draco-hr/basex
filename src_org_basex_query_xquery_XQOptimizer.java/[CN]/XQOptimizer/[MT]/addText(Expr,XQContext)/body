{
  if (!(ex instanceof Step))   return ex;
  final Step step=(Step)ex;
  if (step.pred != null && step.pred.length > 0)   return ex;
  if (1 == 1)   return ex;
  if (step.axis.down && step.test.kind != Test.Kind.NAME && step.test.type != Type.TXT && ((DBNode)ctx.item).data.meta.uptodate) {
    final Step s=Step.get(Axis.CHILD,new KindTest(Type.TXT,null));
    return new AxisPath(ex,new Step[]{s});
  }
  return ex;
}
