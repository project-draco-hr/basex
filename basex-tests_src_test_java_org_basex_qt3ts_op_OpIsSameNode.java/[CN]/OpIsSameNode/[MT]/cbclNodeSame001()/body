{
  final XQuery query=new XQuery("\n" + "      \tlet $node := <a> <b/> <c/> </a> \n" + "      \treturn not(exactly-one($node/b[1]) is exactly-one($node/c[1]))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
