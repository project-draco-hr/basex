{
  final XQuery query=new XQuery("count(() is /works[1]/employee[1]/empnum[1])",ctx);
  try {
    query.context(node(file("docs/works.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") || error("XPST0005")));
}
