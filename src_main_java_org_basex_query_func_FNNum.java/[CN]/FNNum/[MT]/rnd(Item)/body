{
  final double d=it.dbl();
  if (it.type != Type.DEC) {
    return num(it,d,d == d && d != 0 && d >= Long.MIN_VALUE && d < Long.MAX_VALUE ? d >= -.5d && d < 0 ? -0d : Math.round(d) : d);
  }
  final BigDecimal bd=it.dec();
  return Dec.get(bd.setScale(0,bd.signum() > 0 ? BigDecimal.ROUND_HALF_UP : BigDecimal.ROUND_HALF_DOWN));
}
