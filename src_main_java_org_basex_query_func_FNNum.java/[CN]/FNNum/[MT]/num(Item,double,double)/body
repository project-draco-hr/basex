{
  final Item i=it.isUntyped() ? Dbl.get(n) : it;
  if (n == d)   return i;
  if (it.type instanceof AtomType) {
switch ((AtomType)it.type) {
case DEC:
      return Dec.get(d);
case DBL:
    return Dbl.get(d);
case FLT:
  return Flt.get((float)d);
case ITR:
return Int.get((long)d);
default :
break;
}
}
return Dbl.get(d);
}
