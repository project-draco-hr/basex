{
  final Item i=it.unt() ? Dbl.get(n) : it;
  if (n == d)   return i;
  if (it.type instanceof AtomType) {
switch ((AtomType)it.type) {
case DEC:
      return Dec.get(d);
case DBL:
    return Dbl.get(d);
case FLT:
  return Flt.get((float)d);
default :
return Itr.get((long)d);
}
}
return Itr.get((long)d);
}
