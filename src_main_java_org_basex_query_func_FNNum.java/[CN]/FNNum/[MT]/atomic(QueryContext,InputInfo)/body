{
  final Item it=expr[0].atomic(ctx,input);
  if (it == null)   return null;
  if (!it.unt() && !it.num())   Err.number(this,it);
  final double d=it.dbl(input);
switch (func) {
case ABS:
    return abs(it,input);
case CEIL:
  return num(it,d,Math.ceil(d));
case FLOOR:
return num(it,d,Math.floor(d));
case RND:
return rnd(it,d,false,ctx);
case RNDHLF:
return rnd(it,d,true,ctx);
default :
return super.atomic(ctx,ii);
}
}
