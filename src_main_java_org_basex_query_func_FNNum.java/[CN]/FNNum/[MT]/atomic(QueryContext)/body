{
  final Item it=expr[0].atomic(ctx);
  if (it == null)   return null;
  if (!it.u() && !it.n())   Err.num(info(),it);
  final double d=it.dbl();
switch (func) {
case ABS:
    return abs(it);
case CEIL:
  return num(it,d,Math.ceil(d));
case FLOOR:
return num(it,d,Math.floor(d));
case RND:
return rnd(it);
case RNDHLF:
return rnd(it,d,ctx);
default :
return super.atomic(ctx);
}
}
