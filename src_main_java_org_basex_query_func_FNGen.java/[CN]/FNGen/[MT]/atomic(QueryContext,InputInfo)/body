{
switch (def) {
case DOC:
    return doc(ctx);
case DOCAVL:
  return docAvailable(ctx);
case PARSETXT:
return unparsedText(ctx);
case PARSETXTAVL:
return unparsedTextAvailable(ctx);
case PUT:
return put(ctx);
case PARSE:
case PARSEXML:
return parseXml(ctx);
case SERIALIZE:
return serialize(ctx);
default :
return super.atomic(ctx,ii);
}
}
