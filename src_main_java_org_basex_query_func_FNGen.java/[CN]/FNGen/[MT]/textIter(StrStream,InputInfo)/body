{
  final byte[] str=si.string(ii);
  return new Iter(){
    int p=-1;
    @Override public Item next(){
      final ByteList bl=new ByteList();
      while (++p < str.length && str[p] != '\n')       bl.add(str[p]);
      return p + 1 < str.length || !bl.isEmpty() ? Str.get(bl.toArray()) : null;
    }
  }
;
}
