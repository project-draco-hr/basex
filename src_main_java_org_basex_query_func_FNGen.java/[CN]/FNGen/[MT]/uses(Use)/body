{
  return u == Use.CNS && sig == Function.PARSE_XML || u == Use.UPD && sig == Function.PUT || u == Use.X30 && (sig == Function.DATA && expr.length == 0 || oneOf(sig,UNPARSED_TEXT,UNPARSED_TEXT_LINES,UNPARSED_TEXT_AVAILABLE,Function.PARSE_XML,URI_COLLECTION,SERIALIZE)) || u == Use.CTX && (sig == Function.DATA && expr.length == 0 || sig == Function.PUT) || super.uses(u);
}
