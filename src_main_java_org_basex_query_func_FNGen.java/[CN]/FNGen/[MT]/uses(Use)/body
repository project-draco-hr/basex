{
  return u == Use.UPD && def == FunDef.PUT || u == Use.X30 && (def == FunDef.DATA && expr.length == 0 || def == FunDef.PARSE || def == FunDef.PARSETXT || def == FunDef.PARSETXTAVL || def == FunDef.PARSEXML || def == FunDef.URICOLL || def == FunDef.SERIALIZE) || u == Use.CTX && def == FunDef.DATA && expr.length == 0 || super.uses(u);
}
