{
  return u == Use.CNS && sig == PARSE_XML || u == Use.UPD && sig == PUT || u == Use.X30 && (sig == DATA && expr.length == 0 || oneOf(sig,UNPARSED_TEXT,UNPARSED_TEXT_LINES,UNPARSED_TEXT_AVAILABLE,PARSE_XML,URI_COLLECTION,SERIALIZE)) || u == Use.CTX && (sig == DATA && expr.length == 0 || sig == PUT) || super.uses(u);
}
