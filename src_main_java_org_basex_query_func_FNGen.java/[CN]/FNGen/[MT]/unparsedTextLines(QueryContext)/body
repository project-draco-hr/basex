{
  final byte[] str=unparsedText(ctx).atom();
  return new Iter(){
    int p=-1;
    @Override public Item next(){
      final ByteList bl=new ByteList();
      while (++p < str.length) {
        if (str[p] == 0x0a)         break;
        if (str[p] == 0x0d) {
          if (p + 1 < str.length && str[p + 1] == 0x0a)           p++;
          break;
        }
        bl.add(str[p]);
      }
      return p + 1 < str.length || bl.size() != 0 ? Str.get(bl.toArray()) : null;
    }
  }
;
}
