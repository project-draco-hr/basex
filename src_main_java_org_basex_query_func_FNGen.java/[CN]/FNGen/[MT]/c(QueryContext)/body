{
  if (func == FunDef.DOC)   return expr[0].item() ? atomic(ctx,input) : this;
  if (func == FunDef.COLL)   return expr.length != 0 && expr[0].item() ? iter(ctx).finish() : this;
  if (func == FunDef.PARSETXT) {
    return expr[0].item() && (expr.length == 1 || expr[1].item()) ? atomic(ctx,input) : this;
  }
  return this;
}
