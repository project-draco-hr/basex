{
  if (func == FunDef.DOC)   return expr[0].i() ? atomic(ctx) : this;
  if (func == FunDef.COLL)   return expr.length != 0 && expr[0].i() ? iter(ctx).finish() : this;
  if (func == FunDef.PARSETXT) {
    return expr[0].i() && (expr.length == 1 || expr[1].i()) ? atomic(ctx) : this;
  }
  return this;
}
