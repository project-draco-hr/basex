{
  final Iter iter=ctx.iter(expr[0]);
switch (func) {
case DOC:
    Item it=iter.next();
  return it == null ? null : it.type == Type.DOC ? it : ctx.doc(checkStr(it),false,false);
case DOCAVAILABLE:
it=iter.next();
if (it != null) {
final byte[] file=checkStr(it);
try {
  ctx.doc(file,false,false);
  return Bln.TRUE;
}
 catch (final QueryException ex) {
}
}
return Bln.FALSE;
default :
return super.atomic(ctx);
}
}
