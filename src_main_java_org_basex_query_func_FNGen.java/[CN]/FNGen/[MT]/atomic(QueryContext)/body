{
switch (func) {
case DOC:
    Item it=expr[0].atomic(ctx);
  return it == null ? null : ctx.doc(checkStr(it),false,false);
case DOCAVL:
it=expr[0].atomic(ctx);
if (it != null) {
final byte[] file=checkStr(it);
try {
  ctx.doc(file,false,false);
  return Bln.TRUE;
}
 catch (final QueryException ex) {
}
}
return Bln.FALSE;
default :
return super.atomic(ctx);
}
}
