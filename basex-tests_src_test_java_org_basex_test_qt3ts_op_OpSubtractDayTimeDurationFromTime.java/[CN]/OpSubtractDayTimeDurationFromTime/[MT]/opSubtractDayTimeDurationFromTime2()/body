{
  final XQuery query=new XQuery("xs:time(\"08:20:00-05:00\") - xs:dayTimeDuration(\"P23DT10H10M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"22:10:00-05:00"));
}
