{
  final StringBuilder tb=new StringBuilder();
  consumeWS();
  char q=0;
  while (more()) {
    char ch=curr();
    if (q != 0 && ch == '\\') {
      consume();
      if (more())       ch=curr();
    }
 else {
      if ((spc && ch <= ' ' || ch == ';') && q == 0)       break;
      if (quote(ch))       q=q == 0 ? ch : q == ch ? 0 : q;
    }
    tb.append(ch);
    consume();
  }
  return finish(cmd,tb);
}
