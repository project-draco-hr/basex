{
  final StringList list=new StringList();
  final String t=i == null ? "" : i.toUpperCase();
  for (  final Enum<?> e : en.getEnumConstants()) {
    if (e instanceof Cmd) {
      final Cmd c=(Cmd)e;
      if (c.dummy() || c.hidden() || c.internal())       continue;
    }
    if (e.name().startsWith(t))     list.add(e.name().toLowerCase());
  }
  list.sort(true);
  return list;
}
