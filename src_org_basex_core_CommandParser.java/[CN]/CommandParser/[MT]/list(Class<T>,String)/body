{
  Enum<?>[] list=new Enum<?>[0];
  final String t=i == null ? "" : i.toUpperCase();
  for (  final Enum<?> e : en.getEnumConstants()) {
    if (e instanceof Cmd) {
      final Cmd c=(Cmd)e;
      if (c.help() || c.hidden() || c.internal())       continue;
    }
    if (e.name().startsWith(t)) {
      list=Array.add(list,e);
    }
  }
  return list;
}
