{
  final XQuery query=new XQuery("1.7976931348623157E+308 cast as xs:integer",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOCA0003") || assertQuery("string-length(string($result)) gt 300")));
}
