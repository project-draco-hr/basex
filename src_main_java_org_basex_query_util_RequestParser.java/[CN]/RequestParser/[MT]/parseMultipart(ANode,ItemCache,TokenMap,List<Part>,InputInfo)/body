{
  parseAttrs(multipart,attrs);
  if (attrs.get(MEDIATYPE) == null)   REQINV.thrw(ii,"Attribute media-type of http:multipart is mandatory");
  ANode n;
  final NodeMore i=multipart.children();
  if (contItems == null) {
    while ((n=i.next()) != null)     parts.add(parsePart(n,null,ii));
  }
 else {
    while ((n=i.next()) != null)     parts.add(parsePart(n,contItems.next(),ii));
  }
}
