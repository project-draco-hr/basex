{
  XQExpression xqe;
  XQSequence xqs;
  XQItem xqi;
  xqe=xqc.createExpression();
  xqs=xqe.executeQuery("'Hello world!'");
  xqs.next();
  xqi=xqs.getItem();
  try {
    xqi.close();
    xqi.close();
  }
 catch (  final XQException e) {
    fail("A-XQE-2.1: closing item failed with message: " + e.getMessage());
  }
  xqe.close();
}
