{
  final Item it=(expr.length == 2 ? expr[1] : checkCtx(ctx)).atomic(ctx,input);
  if (it == null)   Err.or(input,XPEMPTYPE,desc(),Type.NOD);
  final Nod node=checkNode(it);
switch (func) {
case ID:
    return id(ctx.iter(expr[0]),node);
case IDREF:
  return idref(ctx.iter(expr[0]),node);
case ELID:
return elid(ctx.iter(expr[0]),node);
default :
return super.iter(ctx);
}
}
