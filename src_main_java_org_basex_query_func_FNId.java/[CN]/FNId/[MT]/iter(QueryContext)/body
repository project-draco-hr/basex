{
  final Item it=checkEmptyType((expr.length == 2 ? expr[1] : checkCtx(ctx)).item(ctx,input),NodeType.NOD);
  final Nod node=checkNode(it);
switch (def) {
case ID:
    return id(ctx.iter(expr[0]),node);
case IDREF:
  return idref(ctx.iter(expr[0]),node);
case ELID:
return elid(ctx.iter(expr[0]),node);
default :
return super.iter(ctx);
}
}
