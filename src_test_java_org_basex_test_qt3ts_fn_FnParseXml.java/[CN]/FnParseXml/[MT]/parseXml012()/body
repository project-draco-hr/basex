{
  final XQuery query=new XQuery("parse-xml(\"<a>foo</a>\") is parse-xml(\"<a>foo</a>\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || assertBoolean(false)));
}
