{
  final XQuery query=new XQuery("parse-xml(unparsed-text(\"../docs/atomic.xml\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("document-node(element(*,xs:untyped))"));
}
