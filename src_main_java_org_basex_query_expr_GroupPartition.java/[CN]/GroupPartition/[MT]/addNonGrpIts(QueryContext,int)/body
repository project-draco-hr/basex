{
  if (items.size() <= p)   items.add(new HashMap<Var,ItemList>());
  HashMap<Var,ItemList> sq=items.get(p);
  for (  final Var v : ngv) {
    if (sq == null)     sq=new HashMap<Var,ItemList>();
    if (sq.get(v) == null)     sq.put(v,new ItemList());
    sq.get(v).add(ctx.vars.get(v).item);
  }
}
