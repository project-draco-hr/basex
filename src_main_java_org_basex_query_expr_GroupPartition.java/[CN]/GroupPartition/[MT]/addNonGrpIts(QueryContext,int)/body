{
  if (p == items.size())   items.add(new ItemIter[ngv.length]);
  final ItemIter[] sq=items.get(p);
  for (int i=0; i < ngv.length; ++i) {
    ItemIter ir=sq[i];
    if (ir == null) {
      ir=new ItemIter();
      sq[i]=ir;
    }
 else     if (!among && order.uses(ngv[i]))     Err.or(input,QueryText.XPSORT);
    ir.add(ngv[i].iter(ctx));
  }
}
