{
  gv=gvs;
  cgv=gvs;
  ngv=new Var[fls.length - gv.length];
  int i=0;
  for (  final Var v : fls) {
    boolean skip=false;
    for (    final Var g : gv) {
      if (v.eq(g)) {
        skip=true;
        break;
      }
    }
    if (skip)     continue;
    ngv[i++]=v;
  }
  partitions=new ArrayList<GroupNode>();
  items=ngv.length != 0 ? new ArrayList<ItemIter[]>() : null;
  order=ob;
  among=false;
  input=ii;
}
