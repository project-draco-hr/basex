{
  gv=gv1;
  ngv=new Var[fl1.length - gv.length];
  GroupNode.varlen=gv.length;
  int i=0;
  for (  final Var v : fl1) {
    boolean skip=false;
    for (    final Var g : gv)     if (v.eq(g)) {
      skip=true;
      break;
    }
    if (skip)     continue;
    ngv[i++]=v;
  }
  partitions=new ArrayList<GroupNode>();
  items=new ArrayList<HashMap<Var,ItemList>>();
}
