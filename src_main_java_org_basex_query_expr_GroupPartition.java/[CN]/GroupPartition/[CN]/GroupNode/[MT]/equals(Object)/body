{
  if (!(o instanceof GroupNode))   return false;
  final GroupNode c=(GroupNode)o;
  if (its.length != c.its.length || gv.length != c.its.length)   return false;
  for (int i=0; i < its.length; i++) {
    try {
      if (!its[i].equive(c.its[i]))       return false;
    }
 catch (    QueryException e) {
      return false;
    }
  }
  return true;
}
