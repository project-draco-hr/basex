{
  final XQuery query=new XQuery("/.",ctx);
  try {
    query.context(node(file("prod/AxisStep/nw_Customers.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertType("document-node(element(Root))") && assertQuery("exists($result//Country[.='Poland'])")));
}
