{
  final XQuery query=new XQuery("xs:anyURI(\"http://www.example.org/#:@\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.org/#:@"));
}
