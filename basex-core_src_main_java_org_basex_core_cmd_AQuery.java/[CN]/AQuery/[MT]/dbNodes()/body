{
  try {
    final Result res=qp(args[0],context).execute();
    if (res instanceof DBNodes)     return (DBNodes)res;
    if (res.size() == 0)     return new DBNodes(context.data());
  }
 catch (  final QueryException ex) {
    error(Util.message(ex));
  }
 finally {
    qp.close();
    qp=null;
  }
  return null;
}
