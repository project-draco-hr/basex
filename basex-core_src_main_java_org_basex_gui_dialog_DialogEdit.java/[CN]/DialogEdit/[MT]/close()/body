{
  super.close();
  ok=false;
  if (old1 != null) {
    result.add(input1.getText());
    ok|=!input1.getText().equals(old1);
  }
  if (old2 != null) {
    result.add(input2.getText());
    ok|=!input2.getText().equals(old2);
  }
  if (old3 != null) {
    final String text=string(input3.getText());
    result.add(text);
    ok|=!text.equals(old3);
  }
}
