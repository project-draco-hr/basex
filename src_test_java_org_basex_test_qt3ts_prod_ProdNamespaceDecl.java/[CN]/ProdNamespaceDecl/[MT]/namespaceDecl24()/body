{
  final XQuery query=new XQuery("declare namespace abc = \"mailto:someuser@someserver.com\"; <abc:someElement>some content</abc:someElement>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<abc:someElement xmlns:abc=\"mailto:someuser@someserver.com\">some content</abc:someElement>",false));
}
