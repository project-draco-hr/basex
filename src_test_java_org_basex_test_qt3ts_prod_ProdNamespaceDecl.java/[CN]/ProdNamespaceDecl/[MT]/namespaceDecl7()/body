{
  final XQuery query=new XQuery("declare namespace xx = \"http://example.org\"; let $i := <foo:bar xmlns:foo = \"http://example.org\"> <foo:bing> Lentils </foo:bing> </foo:bar> return $i/xx:bing",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<foo:bing xmlns:foo=\"http://example.org\"> Lentils </foo:bing>",false));
}
