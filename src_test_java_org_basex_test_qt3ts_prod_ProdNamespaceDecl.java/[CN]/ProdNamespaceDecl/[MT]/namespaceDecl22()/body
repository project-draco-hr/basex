{
  final XQuery query=new XQuery("declare namespace abc = \"gopher://spinaltap.micro.umn.edu/00/Weather/California/somefile\"; <abc:someElement>some content</abc:someElement>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<abc:someElement xmlns:abc=\"gopher://spinaltap.micro.umn.edu/00/Weather/California/somefile\">some content</abc:someElement>",false));
}
