{
  final XQuery query=new XQuery("declare namespace px1 = \"http://www.example.com/examples\"; declare namespace px2 = \"http://www.example.com/examples\"; let $var := <px1:someElement>some context</px1:someElement> return $var",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<px1:someElement xmlns:px1=\"http://www.example.com/examples\">some context</px1:someElement>",false));
}
