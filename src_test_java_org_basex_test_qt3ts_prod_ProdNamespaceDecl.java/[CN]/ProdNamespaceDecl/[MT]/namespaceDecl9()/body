{
  final XQuery query=new XQuery("declare namespace xs = \"http://www.example.com/examples\"; let $var := <xs:someElement>some context</xs:someElement> return $var",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<xs:someElement xmlns:xs=\"http://www.example.com/examples\">some context</xs:someElement>",false));
}
