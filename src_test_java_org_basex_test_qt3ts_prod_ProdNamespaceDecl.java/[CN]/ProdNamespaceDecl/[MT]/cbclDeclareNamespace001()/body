{
  final XQuery query=new XQuery("\n" + "      \tdeclare default element namespace \"http://www.example.org\"; \n" + "      \tdeclare namespace test=\"\"; \n"+ "      \t<test:a />\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0081"));
}
