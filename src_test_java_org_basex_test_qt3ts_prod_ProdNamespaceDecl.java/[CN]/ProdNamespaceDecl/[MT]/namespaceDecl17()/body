{
  final XQuery query=new XQuery("declare namespace px = \"http://www.example.com/abc\"; let $var := <px:someElement>some context</px:someElement> return namespace-uri-from-QName(node-name($var)) eq xs:anyURI(\"http://www.example.com/abc\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
