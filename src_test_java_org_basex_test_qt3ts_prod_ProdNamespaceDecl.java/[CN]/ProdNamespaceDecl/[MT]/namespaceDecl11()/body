{
  final XQuery query=new XQuery("declare namespace fn = \"http://www.example.com/examples\"; let $var := <fn:someElement>some context</fn:someElement> return $var",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<fn:someElement xmlns:fn=\"http://www.example.com/examples\">some context</fn:someElement>",false));
}
