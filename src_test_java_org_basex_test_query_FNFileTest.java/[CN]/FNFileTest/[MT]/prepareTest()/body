{
  dir1.mkdir();
  dir2.mkdir();
  dirDel.mkdirs();
  try {
    final BufferedOutputStream outCopy=new BufferedOutputStream(new FileOutputStream(fileCopy));
    try {
      outCopy.write(Token.token(doc));
    }
  finally {
      outCopy.close();
    }
    final BufferedOutputStream outCopyOver=new BufferedOutputStream(new FileOutputStream(fileCopyOver));
    try {
      outCopyOver.write(Token.token(doc));
    }
  finally {
      outCopyOver.close();
    }
    final BufferedOutputStream outMove=new BufferedOutputStream(new FileOutputStream(fileMove));
    try {
      outMove.write(Token.token(doc));
    }
  finally {
      outMove.close();
    }
    final BufferedOutputStream outDel=new BufferedOutputStream(new FileOutputStream(fileDel));
    try {
      outDel.write(Token.token(doc));
    }
  finally {
      outDel.close();
    }
  }
 catch (  final IOException ex) {
    ex.printStackTrace();
  }
}
