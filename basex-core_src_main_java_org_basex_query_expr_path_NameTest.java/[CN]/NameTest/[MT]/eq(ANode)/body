{
  if (node.type != type)   return false;
switch (kind) {
case WILDCARD:
    return true;
case NAME:
  return Token.eq(local,Token.local(node.name()));
case URI:
return Token.eq(name.uri(),node.qname(tmpq).uri());
default :
return type == NodeType.ATT && name.uri().length == 0 ? Token.eq(local,node.name()) : name.eq(node.qname(tmpq));
}
}
