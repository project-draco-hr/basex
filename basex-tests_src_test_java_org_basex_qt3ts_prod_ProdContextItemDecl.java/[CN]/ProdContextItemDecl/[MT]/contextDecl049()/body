{
  final XQuery query=new XQuery("\n" + "        import module namespace m=\"http://www.w3.org/TestModules/libmodule2\"; \n" + "        declare context item as xs:date := current-date();\n"+ "        . gt xs:date('1900-01-01')\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/libmodule2",file("prod/ContextItemDecl/libmodule-2.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
