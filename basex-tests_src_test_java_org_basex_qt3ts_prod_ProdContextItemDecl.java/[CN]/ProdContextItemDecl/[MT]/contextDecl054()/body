{
  final XQuery query=new XQuery("\n" + "      \timport module namespace m=\"http://www.w3.org/TestModules/libmodule2\"; \n" + "        xs:date(.) gt xs:date('1900-01-01')\n"+ "      ",ctx);
  try {
    query.context(new XQuery("current-dateTime()",ctx).value());
    query.addModule("http://www.w3.org/TestModules/libmodule2",file("prod/ContextItemDecl/libmodule-2.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
