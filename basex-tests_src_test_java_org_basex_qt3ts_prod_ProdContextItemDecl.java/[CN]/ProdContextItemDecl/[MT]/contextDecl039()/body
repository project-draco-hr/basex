{
  final XQuery query=new XQuery("\n" + "        declare context item as xs:double := 1.234;\n" + "        .\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
