{
  final XQuery query=new XQuery("format-integer((), 'Ww')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("''") && assertType("xs:string") && assertQuery("count($result) = 1")));
}
