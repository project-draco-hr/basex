{
  final XQuery query=new XQuery("string-join(for $i in 1 to 10 return format-integer($i, 'Ww'), '|')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("'One|Two|Three|Four|Five|Six|Seven|Eight|Nine|Ten'"));
}
