{
  final TokenBuilder tb=new TokenBuilder(context.info());
  try {
    if (Prop.xmlplan) {
      tb.add(QUERYPLAN + NL,PLANXML);
      context.planXML(PLANXML);
    }
    if (Prop.dotplan) {
      tb.add(QUERYPLAN + NL,PLANDOT);
      context.planDot(PLANDOT);
    }
  }
 catch (  Exception ex) {
    BaseX.debug(ex);
  }
  if (Prop.allInfo) {
    tb.add(NL + QUERYSTRING);
    tb.add(query);
    tb.add(NL);
  }
  return tb.toString();
}
