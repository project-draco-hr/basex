{
  try {
    if (Prop.xmlplan) {
      context.info.add(QUERYPLAN + NL);
      context.planXML();
    }
    if (Prop.dotplan)     context.planDot();
  }
 catch (  final Exception ex) {
    ex.printStackTrace();
  }
  final TokenBuilder tb=new TokenBuilder(context.info());
  if (Prop.allInfo) {
    tb.add(NL + QUERYSTRING);
    tb.add(query);
    tb.add(NL);
  }
  return tb.toString();
}
