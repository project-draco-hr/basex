{
  final XQuery query=new XQuery("(: Name: XQueryComment018 :)\n" + "(: Description: Invalid comment :)\n" + "\n"+ "\n"+ "-- Wrong comment format\n"+ "<empty/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
