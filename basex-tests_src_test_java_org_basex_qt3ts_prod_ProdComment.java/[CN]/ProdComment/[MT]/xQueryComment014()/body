{
  final XQuery query=new XQuery("(: Name: XQueryComment014 :)\n" + "(: Description: Comments inside a cast expression :)\n" + "\n"+ "\n"+ "\"10\" cast as (: type comment :) xs:integer ?\n"+ "\n"+ "",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("10"));
}
