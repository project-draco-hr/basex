{
  final XQuery query=new XQuery("\n" + "(: Name: XQueryComment010 :)\n" + "(: Description: Comments inside a conditional expression :)\n"+ "if (: comment :) \n"+ "  ( //fs:Folder[1]/fs:FolderName/text() = \"Folder00000000000\" ) \n"+ "then (: this is the then case :) ( true() )\n"+ "else (: this is the else case :) ( false() )\n"+ "\n"+ "",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx_NS.xml")));
    query.namespace("fs","http://www.example.com/filesystem");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
