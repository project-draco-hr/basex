{
  final XQuery query=new XQuery("(: Name: XQueryComment008 :)\n" + "(: Description: Simple example of embedded comments :)\n" + "\n"+ "\n"+ "(: this is a comment (: this is an embedded comment :):)\n"+ "<result/>\n"+ "",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result/>",false));
}
