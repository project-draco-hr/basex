{
  final XQuery query=new XQuery("(: Name: XQueryComment013 :)\n" + "(: Description: Comments inside a sequence expression :)\n" + "\n"+ "\n"+ "(1, 2, (: comment :) 3, 4)\n"+ "\n"+ "",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2 3 4"));
}
