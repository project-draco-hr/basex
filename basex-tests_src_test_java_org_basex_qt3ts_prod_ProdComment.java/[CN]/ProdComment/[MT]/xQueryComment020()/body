{
  final XQuery query=new XQuery("(: Name: XQueryComment020 :)\n" + "(: Description: Comment containing an enclosed expression :)\n" + "\n"+ "\n"+ "(: { \"comment\" } :)\n"+ "<result/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result/>",false));
}
