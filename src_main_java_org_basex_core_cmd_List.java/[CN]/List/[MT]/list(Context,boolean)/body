{
  final StringList db=new StringList();
  for (  final IOFile f : ctx.mprop.dbpath().children()) {
    String name=f.name();
    if (backups && name.endsWith(IO.ZIPSUFFIX)) {
      name=dbname(name);
      if (!db.contains(name))       db.add(name);
    }
 else     if (f.isDir() && !name.startsWith(".")) {
      db.add(name);
    }
  }
  db.sort(false,true);
  return db;
}
