{
  XQExpression xqe;
  XQSequence xqs;
  XQItem xqi;
  xqe=xqc.createExpression();
  xqs=xqe.executeQuery("'Hello world!'");
  xqs.next();
  xqi=xqs.getItem();
  assertFalse("A-XQE-1.1: isClosed() on open item",xqi.isClosed());
  xqi.close();
  assertTrue("A-XQE-1.2: isClosed() on closed item",xqi.isClosed());
  xqe.close();
}
