{
  final XQuery query=new XQuery("let $i2 := (//incision)[2] for $a in (//action)[. >> $i2][position()<=2] return $a//instrument",ctx);
  try {
    query.context(node(file("docs/report1.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<instrument>Hasson trocar</instrument><instrument>trocar</instrument>",false));
}
