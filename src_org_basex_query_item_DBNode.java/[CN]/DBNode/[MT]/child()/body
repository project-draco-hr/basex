{
  return new NodeMore(){
    final DBNode node=copy();
    boolean more;
    int p;
    int s;
    @Override public boolean more(){
      if (!more) {
        final int k=data.kind(pre);
        p=pre + data.attSize(pre,k);
        s=pre + data.size(pre,k);
        more=true;
      }
      return p != s;
    }
    @Override public Nod next(){
      if (!more())       return null;
      final int k=data.kind(p);
      node.set(p,k);
      p+=data.size(p,k);
      return node;
    }
  }
;
}
