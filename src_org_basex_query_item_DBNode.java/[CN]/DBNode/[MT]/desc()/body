{
  return new NodeIter(){
    final DBNode node=copy();
    final int s=pre + data.size(pre,data.kind(pre));
    int p=pre + data.attSize(pre,data.kind(pre));
    @Override public Nod next(){
      if (p == s)       return null;
      final int k=data.kind(p);
      node.set(p,k);
      p+=data.attSize(p,k);
      return node;
    }
  }
;
}
