{
  return new NodeIter(){
    /** 
 * First call. 
 */
    private boolean more;
    @Override public Nod next(){
      if (more)       return null;
      more=true;
      final Nod node=parent();
      if (node != null) {
        node.score(node.score * SCORESTEP);
      }
      return node;
    }
  }
;
}
