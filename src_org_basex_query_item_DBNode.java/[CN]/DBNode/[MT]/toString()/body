{
switch (type) {
case ATT:
    return type + "{" + string(data.attName(pre))+ "=\""+ string(data.attValue(pre))+ "\"}";
case DOC:
  return type + "{\"" + data.meta.dbname+ "\"}";
case ELM:
return type + "{" + string(data.tag(pre))+ ","+ pre+ "}";
default :
return type + "{" + Err.chop(str())+ "}";
}
}
