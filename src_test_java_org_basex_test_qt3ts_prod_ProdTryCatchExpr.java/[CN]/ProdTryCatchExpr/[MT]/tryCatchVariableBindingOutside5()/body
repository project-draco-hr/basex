{
  final XQuery query=new XQuery("for tumbling window $x as xs:string in (2, 4, 6, 8, 10)\n" + "      start $s at $spos previous $sprev next $snext when true() end $e at\n" + "      $epos previous $eprev next $enext when true() return try { $x } catch err:XPTY0004 { 0 }",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test(error("XPTY0004"));
}
