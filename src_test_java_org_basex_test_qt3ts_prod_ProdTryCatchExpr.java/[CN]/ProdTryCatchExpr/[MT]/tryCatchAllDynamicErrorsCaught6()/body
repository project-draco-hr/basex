{
  final XQuery query=new XQuery("try { attribute xmlns {} } catch err:XQDY0044 { \"Invalid attribute.\" }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Invalid attribute."));
}
