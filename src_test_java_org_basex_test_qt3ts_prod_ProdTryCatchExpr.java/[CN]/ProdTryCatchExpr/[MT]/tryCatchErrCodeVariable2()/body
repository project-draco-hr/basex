{
  final XQuery query=new XQuery("try { 1 div 0 } catch * { fn:prefix-from-QName($err:code) }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"err"));
}
