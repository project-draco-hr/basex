{
  final XQuery query=new XQuery("try { fn:error() } catch * { if (count($err:module) eq 1) then $err:module else \"\" }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("xs:string"));
}
