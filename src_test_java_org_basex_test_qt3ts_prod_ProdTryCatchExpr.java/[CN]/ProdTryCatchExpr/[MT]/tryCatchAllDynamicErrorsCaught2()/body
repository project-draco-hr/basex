{
  final XQuery query=new XQuery("try { \"\" treat as element() } catch err:XPDY0050 { \"Sequence type mismatch.\" }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Sequence type mismatch."));
}
