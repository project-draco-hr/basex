{
  final XQuery query=new XQuery("\n" + "      try { validate { document { <a/>, <b/> }} } catch err:XQDY0061 { \"Invalid document.\" }\n" + "      ",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"Invalid document."));
}
