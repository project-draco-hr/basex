{
  final XQuery query=new XQuery("\n" + "      declare function local:f() { a };\n" + "      try {local:f()} catch err:XPDY0002 {true()}\n"+ "    ",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPTY0004")));
}
