{
  final XQuery query=new XQuery("try { fn:error() } catch * { 0 }, try { fn:error() } catch err:FOER0000 { 1}",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("0 1",false));
}
