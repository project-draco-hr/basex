{
  final XQuery query=new XQuery("fn:error(fn:QName(\"http://www.example.com/\", \"example:EXER3141\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("Q{http://www.example.com/}EXER3141"));
}
