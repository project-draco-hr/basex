{
  final XQuery query=new XQuery("\n" + "        try { doc('rubbish.xml') } \n" + "        catch err:FODC0001 {<caught-error code=\"FODC0001\"/>} \n"+ "        catch err:FODC0002 {<caught-error code=\"FODC0002\"/>} \n"+ "        catch err:FODC0005 {<caught-error code=\"FODC0005\"/>} \n"+ "        catch err:* {<caught-error code=\"other\"/>}\n"+ "      ",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<caught-error code=\"FODC0002\"/>",false));
}
