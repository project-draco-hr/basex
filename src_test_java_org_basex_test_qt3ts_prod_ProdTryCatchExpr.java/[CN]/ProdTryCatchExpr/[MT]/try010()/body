{
  final XQuery query=new XQuery("\n" + "        declare function local:f($d as xs:integer) { 10 div $d };\n" + "        try { local:f(0) } catch * {local-name-from-QName($err:code)}\n"+ "      ",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"FOAR0001"));
}
