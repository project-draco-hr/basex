{
  final XQuery query=new XQuery("let $x := \"\" return try {\n" + "      $x cast as xs:integer\n" + "      }\n"+ "      catch err:FORG0001 {\n"+ "      0\n"+ "      }",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test(assertEq("0"));
}
