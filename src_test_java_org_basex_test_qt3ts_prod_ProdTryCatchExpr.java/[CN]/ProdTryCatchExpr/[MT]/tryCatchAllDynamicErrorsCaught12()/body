{
  final XQuery query=new XQuery("try { let $x := (1,2)[position() < 3] group by $x return $x } catch err:XPTY0004 { \"More than a grouping item.\" }",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"More than a grouping item.") || error("XPTY0004")));
}
