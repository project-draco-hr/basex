{
  final XQuery query=new XQuery("try { element { QName(\"http://www.w3.org/2000/xmlns/\", \"xmlns:name\") } {} } catch err:XQDY0096 { \"Invalid element.\" }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Invalid element."));
}
