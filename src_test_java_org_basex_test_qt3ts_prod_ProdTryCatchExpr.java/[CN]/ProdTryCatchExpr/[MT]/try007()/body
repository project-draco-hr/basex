{
  final XQuery query=new XQuery("\n" + "        let $doc := doc('rubbish.xml') \n" + "        return try { $doc } catch * {<caught-error/>}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FODC0002"));
}
