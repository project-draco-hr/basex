{
  final XQuery query=new XQuery("try { fn:error(fn:QName('http://www.w3.org/2005/xqt-errors', 'err:FOER0001')) } catch * { fn:namespace-uri-from-QName($err:code) }",ctx);
  query.namespace("err","http://www.w3.org/2005/xqt-errors");
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.w3.org/2005/xqt-errors"));
}
