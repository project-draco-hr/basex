{
  final XQuery query=new XQuery("try { 9999999999999999999999999999999999999999999999999999999999999999999999\n" + "                idiv\n" + "                9999999999999999999999999999999999999999999999999999999999999999999999 }\n"+ "        catch err:FOAR0002 {1}\n"+ "    ",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
