{
  final XQuery query=new XQuery("fn:round-half-to-even((xs:short(\"-32768\") div xs:short(\"-5324\")),5)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("6.15477"));
}
