{
  runQuery("geo:exteriorRing(<gml:Polygon><gml:outerBoundaryIs><gml:LinearRing>" + "<gml:coordinates>11,11 18,11 18,18 11,18 11,11</gml:coordinates></gml:LinearRing></gml:outerBoundaryIs></gml:Polygon>)","<gml:LineString srsName=\"0\"><gml:coordinates>" + "11.0,11.0 18.0,11.0 18.0,18.0 11.0,18.0 11.0,11.0</gml:coordinates></gml:LineString>");
  runError("geo:exteriorRing(<gml:Point><gml:coordinates>2.0,3.0</gml:coordinates></gml:Point>)",new QNm("GEO0003"));
  runError("geo:exteriorRing(<gml:unknown><gml:coordinates>1,1</gml:coordinates></gml:unknown>)",new QNm("GEO0001"));
  runError("geo:exteriorRing()",new QNm("XPST0017"));
  runError("geo:exteriorRing(text {'a'})",new QNm("FORG0006"));
}
