{
  runQuery("geo:isSimple(<gml:LineString><gml:coordinates>1,1 20,1 10,4 20,-10" + "</gml:coordinates></gml:LineString>)","false");
  runError("geo:isSimple(text {'a'})",new QNm("FORG0006"));
  runError("geo:isSimple(<gml:unknown/>)",new QNm("GEO0001"));
  runError("geo:isSimple(<gml:LinearRing><gml:coordinates>1,1 55,99 2,1" + "</gml:coordinates></gml:LinearRing>)",new QNm("GEO0002"));
}
