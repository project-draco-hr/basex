{
  runQuery("geo:crosses(" + "<gml:Point><gml:coordinates>10,11</gml:coordinates></gml:Point>, " + "<gml:LineString><gml:coordinates>0,0 2,2</gml:coordinates></gml:LineString>)","false");
  runError("geo:crosses(a, text {'a'})",new QNm("XPDY0002"));
  runError("geo:crosses(<gml:Point><gml:coordinates>10,10 12,11" + "</gml:coordinates></gml:Point>, " + "<gml:LinearRing><gml:coordinates>1,1 2,1 5,3 1,1</gml:coordinates>"+ "</gml:LinearRing>)",new QNm("GEO0002"));
  runError("geo:crosses(<gml:Point><gml:coordinates>1,1</gml:coordinates></gml:Point>)",new QNm("XPST0017"));
}
