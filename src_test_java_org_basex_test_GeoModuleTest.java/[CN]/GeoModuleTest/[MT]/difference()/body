{
  runQuery("geo:difference(" + "<gml:Point><gml:coordinates>20,1</gml:coordinates></gml:Point>," + "<gml:LinearRing><gml:coordinates>0,0 20,20 20,30 0,20 0,0"+ "</gml:coordinates></gml:LinearRing>)","<gml:Point srsName=\"0\"><gml:coordinates>20.0,1.0</gml:coordinates>" + "</gml:Point>");
  runError("geo:difference(" + "<gml:Point><gml:coordinates></gml:coordinates></gml:Point>," + "<gml:Point><gml:coordinates>2,3</gml:coordinates></gml:Point>)",new QNm("GEO0002"));
  runError("geo:difference(text {'a'}, <gml:Point><gml:coordinates>2,3" + "</gml:coordinates></gml:Point>)",new QNm("FORG0006"));
}
