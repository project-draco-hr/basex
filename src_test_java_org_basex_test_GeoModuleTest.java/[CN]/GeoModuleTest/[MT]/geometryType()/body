{
  runQuery("geo:geometryType(<gml:MultiPoint><gml:Point>" + "<gml:coordinates>1,1</gml:coordinates></gml:Point>" + "<gml:Point><gml:coordinates>1,2</gml:coordinates></gml:Point>"+ "</gml:MultiPoint>)","gml:MultiPoint");
  runError("geo:geometryType(text {'srsName'})",new QNm("FORG0006"));
  runError("geo:geometryType(<gml:unknown/>)",new QNm("GEO0001"));
  runError("geo:geometryType(<gml:Point><gml:coordinates>1 2</gml:coordinates>" + "</gml:Point>)",new QNm("GEO0002"));
}
