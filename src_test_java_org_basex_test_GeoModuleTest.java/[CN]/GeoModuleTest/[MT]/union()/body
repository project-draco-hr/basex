{
  runQuery("geo:union(<gml:Point><gml:coordinates>2</gml:coordinates></gml:Point>," + "<gml:Point><gml:coordinates>2,3</gml:coordinates></gml:Point>)","<gml:Point srsName=\"0\"><gml:coordinates>2.0,NaN" + "</gml:coordinates></gml:Point>");
  runQuery("geo:union(<gml:Point><gml:coordinates>2</gml:coordinates></gml:Point>," + "<gml:Point><gml:coordinates>3</gml:coordinates></gml:Point>)","<gml:MultiPoint srsName=\"0\"><gml:pointMember><gml:Point srsName=\"0\">" + "<gml:coordinates>2.0,NaN</gml:coordinates></gml:Point></gml:pointMember>" + "<gml:pointMember><gml:Point srsName=\"0\"><gml:coordinates>"+ "3.0,NaN</gml:coordinates></gml:Point></gml:pointMember></gml:MultiPoint>");
  runError("geo:union(<gml:Point><gml:coordinates></gml:coordinates></gml:Point>," + "<gml:Point><gml:coordinates>2,3</gml:coordinates></gml:Point>)",new QNm("GEO0002"));
  runError("geo:union(text {'a'}, <gml:Point><gml:coordinates>2,3" + "</gml:coordinates></gml:Point>)",new QNm("FORG0006"));
}
