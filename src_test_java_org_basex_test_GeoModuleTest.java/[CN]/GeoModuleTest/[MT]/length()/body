{
  runQuery("geo:length(<gml:Point><gml:coordinates>2,1,3</gml:coordinates></gml:Point>)","0");
  runQuery("geo:length(<gml:Polygon><gml:outerBoundaryIs><gml:LinearRing>" + "<gml:coordinates>1,1 2,1 5,3 1,1</gml:coordinates></gml:LinearRing>" + "</gml:outerBoundaryIs></gml:Polygon>)","9.07768723046357");
  runQuery("geo:length(<gml:MultiPoint><gml:Point><gml:coordinates>1,1" + "</gml:coordinates></gml:Point><gml:Point><gml:coordinates>1,2" + "</gml:coordinates></gml:Point></gml:MultiPoint>)","0");
  runError("geo:length(<gml:LinearRing><gml:coordinates>0,0 0,20 0,0</gml:coordinates></gml:LinearRing>)",new QNm("GEO0002"));
  runError("geo:length(<gml:Point><gml:coordinates></gml:coordinates></gml:Point>)",new QNm("GEO0002"));
  runError("geo:length(<gml:geo><gml:coordinates>2,1</gml:coordinates></gml:geo>)",new QNm("GEO0001"));
}
