{
  runQuery("geo:within(<gml:LinearRing><gml:coordinates>1,1 2,1 5,3 1,1" + "</gml:coordinates></gml:LinearRing>, " + "<gml:LinearRing><gml:coordinates>1,1 20,1 50,30 1,1</gml:coordinates>"+ "</gml:LinearRing>)","false");
  runError("geo:within()",new QNm("XPST0017"));
  runError("geo:within(" + "<gml:unknown><gml:coordinates>1,1</gml:coordinates></gml:unknown>, " + "<gml:LinearRing><gml:coordinates>1,1 2,1 5,3 1,1"+ "</gml:coordinates></gml:LinearRing>)",new QNm("GEO0001"));
  runError("geo:within(<gml:Point><gml:coordinates>1,1</gml:coordinates></gml:Point>)",new QNm("XPST0017"));
}
