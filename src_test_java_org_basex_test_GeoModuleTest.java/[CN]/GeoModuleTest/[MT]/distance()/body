{
  runQuery("geo:distance(<gml:LinearRing><gml:coordinates>10,400 20,200 30,100 " + "20,100 10,400</gml:coordinates></gml:LinearRing>, " + "<gml:Polygon><gml:outerBoundaryIs><gml:LinearRing><gml:coordinates>"+ "10,10 20,10 30,40 20,40 10,10</gml:coordinates></gml:LinearRing>"+ "</gml:outerBoundaryIs></gml:Polygon>)","60");
  runError("geo:distance()",new QNm("XPST0017"));
  runError("geo:distance(<gml:LinearRing><gml:coordinates>1,1 55,99 2,1</gml:coordinates></gml:LinearRing>," + "<gml:LineString></gml:LineString>)",new QNm("GEO0002"));
  runError("geo:distance(<gml:Point><gml:coordinates>1,1</gml:coordinates></gml:Point>)",new QNm("XPST0017"));
}
