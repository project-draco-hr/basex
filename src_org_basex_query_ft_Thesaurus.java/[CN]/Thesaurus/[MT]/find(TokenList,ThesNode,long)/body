{
  if (lev > max || node == null)   return;
  for (int n=0; n < node.size; n++) {
    if (rel.length == 0 || Token.eq(node.rs[n],rel)) {
      final byte[] term=node.nodes[n].term;
      if (!list.contains(term)) {
        list.add(term);
        find(list,node.nodes[n],lev + 1);
      }
    }
  }
}
