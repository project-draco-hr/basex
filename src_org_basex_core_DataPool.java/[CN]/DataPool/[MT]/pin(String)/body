{
  Iterator<Map.Entry<Data,Integer>> i=DBPOOL.entrySet().iterator();
  while (i.hasNext()) {
    Entry<Data,Integer> next=i.next();
    if (next.getKey().meta.dbname.equals(db)) {
      int j=next.getValue();
      j++;
      DBPOOL.put(next.getKey(),j);
      return next.getKey();
    }
  }
  return null;
}
