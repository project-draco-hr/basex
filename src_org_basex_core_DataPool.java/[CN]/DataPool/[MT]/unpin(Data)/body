{
  for (int i=0; i < size; i++) {
    if (data[i] == d) {
      System.out.println("UNPIN");
      final boolean close=--pins[i] == 0;
      if (close) {
        System.out.println("CLOSE");
        Array.move(data,i + 1,-1,size - i - 1);
        Array.move(pins,i + 1,-1,size - i - 1);
        size--;
      }
      return close;
    }
  }
  return true;
}
