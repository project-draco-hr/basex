{
  final XQuery query=new XQuery("let $var := xs:float(-100) return fn:abs($var cast as xs:double treat as xs:double)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("100"));
}
