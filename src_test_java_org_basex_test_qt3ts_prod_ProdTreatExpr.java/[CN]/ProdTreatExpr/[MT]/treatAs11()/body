{
  final XQuery query=new XQuery("let $var := xs:time(\"01:23:00+05:00\") return fn:hours-from-time($var treat as xs:time)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
