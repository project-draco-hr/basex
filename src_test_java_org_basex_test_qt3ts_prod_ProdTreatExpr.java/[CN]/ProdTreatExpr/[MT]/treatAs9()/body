{
  final XQuery query=new XQuery("let $var := xs:date(\"2000-01-01+05:00\") return fn:year-from-date($var treat as xs:date)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("2000"));
}
