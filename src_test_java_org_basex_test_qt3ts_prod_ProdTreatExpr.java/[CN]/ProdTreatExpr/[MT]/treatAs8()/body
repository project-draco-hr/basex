{
  final XQuery query=new XQuery("let $var := xs:boolean(fn:true()) return fn:not($var treat as xs:boolean)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
