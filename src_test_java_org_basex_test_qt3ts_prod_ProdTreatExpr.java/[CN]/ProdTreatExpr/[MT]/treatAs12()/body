{
  final XQuery query=new XQuery("let $var := 100 return ($var treat as xs:integer, $var treat as xs:integer, $var treat as xs:integer)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"100 100 100"));
}
