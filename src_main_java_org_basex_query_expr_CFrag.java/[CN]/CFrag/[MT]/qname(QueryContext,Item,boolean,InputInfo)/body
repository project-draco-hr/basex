{
  QNm n=null;
  if (it.type == AtomType.QNM) {
    n=(QNm)it;
  }
 else {
    final byte[] nm=it.string(ii);
    if (!XMLToken.isQName(nm)) {
      (it.isString() || it.isUntyped() ? INVNAME : INVQNAME).thrw(input,nm);
    }
    n=new QNm(nm);
  }
  if (!n.hasUri()) {
    n.uri(att && !n.ns() ? EMPTY : ctx.ns.uri(n.pref(),n != it,input));
  }
  return n;
}
