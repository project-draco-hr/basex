{
  final int al=pres1.length, bl=pres2.length;
  final IntList c=new IntList();
  int a=0, b=0;
  while (a != al && b != bl) {
    final int d=pres1[a] - pres2[b];
    if (d < 0)     c.add(pres1[a]);
 else     ++b;
    if (d <= 0)     ++a;
  }
  while (a != al)   c.add(pres1[a++]);
  return c.toArray();
}
