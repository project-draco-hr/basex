{
  final XQuery query=new XQuery("\n" + "         let $vA  := (\"B STRING\", current-time(), string(\"content\"))[1] treat as xs:string,\n" + "         $vB  := (\"b string\", current-time(), string(\"content\"))[1] treat as xs:string\n"+ "         return ends-with(upper-case($vA), upper-case($vB))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
