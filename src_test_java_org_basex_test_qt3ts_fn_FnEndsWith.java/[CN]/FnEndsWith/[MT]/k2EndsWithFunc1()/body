{
  final XQuery query=new XQuery("let $vA := (\"B STRING\", current-time(), string(\"content\"))[1] treat as xs:string, \n" + "        $vB := (\"b string\", current-time(), string(\"content\"))[1] treat as xs:string\n" + "        return ends-with(lower-case($vA), lower-case($vB))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
