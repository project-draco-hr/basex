{
  final String count="count(collection('" + WEEK1 + "/week/monday')/root/monday/text[text() = 'text']),"+ " count(collection('"+ WEEK2+ "/week/tuesday')/root/monday/text[text() = 'text']) ";
  final QueryProcessor qp=new QueryProcessor(count,CTX);
  String result=qp.execute().toString();
  assertEquals("3 1",result);
  qp.close();
}
