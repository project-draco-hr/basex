{
  query("delete node doc('d16')/a/b","");
  try {
    new Open("d16").execute(CONTEXT);
    assertEquals("",CONTEXT.data.ns.toString());
  }
 catch (  final Exception ex) {
    fail(ex.getMessage());
  }
 finally {
    try {
      new Close().execute(CONTEXT);
    }
 catch (    final BaseXException ex) {
    }
  }
}
