{
  try {
    new Open("d2").execute(CONTEXT);
    CONTEXT.data.delete(0);
    assertEquals(true,CONTEXT.data.ns.rootEmpty());
  }
 catch (  final Exception ex) {
    fail(ex.getMessage());
  }
 finally {
    try {
      new Close().execute(CONTEXT);
    }
 catch (    final BaseXException ex) {
    }
  }
}
