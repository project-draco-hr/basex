{
  query("delete node doc('d12')/a/b","");
  try {
    new Open("d12").execute(CONTEXT);
    assertEquals("\n" + "  Pre[2] xmlns=\"B\" ",CONTEXT.data.ns.toString());
  }
 catch (  final Exception ex) {
    fail(ex.getMessage());
  }
 finally {
    try {
      new Close().execute(CONTEXT);
    }
 catch (    final BaseXException ex) {
    }
  }
}
