{
  try {
    for (    final Field f : getClass().getFields()) {
      final Object obj=f.get(null);
      if (!(obj instanceof Object[]))       continue;
      final Object[] arr=(Object[])obj;
      props.put(arr[0].toString(),arr[1]);
    }
  }
 catch (  final Exception ex) {
    Util.notexpected(ex);
  }
  system();
}
