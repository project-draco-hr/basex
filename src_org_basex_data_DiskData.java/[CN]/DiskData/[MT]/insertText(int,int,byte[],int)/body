{
  final long id=++meta.lastid;
  final long txt=texts.length();
  texts.writeBytes(txt,val);
  table.insert(pre,new byte[]{(byte)kind,0,0,(byte)(txt >> 32),(byte)(txt >> 24),(byte)(txt >> 16),(byte)(txt >> 8),(byte)txt,(byte)(dis >> 24),(byte)(dis >> 16),(byte)(dis >> 8),(byte)dis,(byte)(id >> 24),(byte)(id >> 16),(byte)(id >> 8),(byte)id});
  meta.size++;
}
