{
  final File file=meta.prop.dbfile(meta.name,DATAINFO);
  final DataOutput out=new DataOutput(file);
  meta.write(out);
  out.writeString(DBTAGS);
  tags.write(out);
  out.writeString(DBATTS);
  atts.write(out);
  out.writeString(DBPATH);
  path.write(out);
  out.writeString(DBNS);
  ns.write(out);
  out.write(0);
  out.close();
}
