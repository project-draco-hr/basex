{
  int kind=kind(pre);
  final int siz=size(pre,kind);
  int par=pre;
  if (kind == ATTR) {
    par=parent(par,ATTR);
    attSize(par,ELEM,attSize(par,ELEM) - 1);
    size(par,ELEM,size(par,ELEM) - 1);
  }
  while (par > 0) {
    par=parent(par,ELEM);
    size(par,ELEM,size(par,ELEM) - siz);
  }
  table.delete(pre,siz);
  size-=siz;
  int p=pre;
  while (p < size) {
    kind=kind(p);
    dist(p,kind,dist(p,kind) - siz);
    p+=size(p,kind);
  }
}
