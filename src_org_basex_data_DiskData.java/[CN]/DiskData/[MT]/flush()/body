{
  try {
    table.flush();
    texts.flush();
    values.flush();
    final DataOutput out=new DataOutput(meta.dbname,DATAINFO);
    meta.write(out);
    tags.write(out);
    atts.write(out);
    path.write(out);
    ns.write(out);
    out.close();
    meta.dirty=false;
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
}
