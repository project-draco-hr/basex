{
  int n=(int)i;
  if (n == 0)   return Token.ZERO;
  int j=Token.numDigits(n);
  final byte[] num=new byte[j];
  while (n > 81919) {
    final int q=n / 10;
    num[--j]=(byte)(n - (q << 3) - (q << 1) + '0');
    n=q;
  }
  while (n != 0) {
    final int q=(n * 52429) >>> 19;
    num[--j]=(byte)(n - (q << 3) - (q << 1) + '0');
    n=q;
  }
  return num;
}
