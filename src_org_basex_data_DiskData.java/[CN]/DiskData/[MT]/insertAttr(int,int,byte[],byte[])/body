{
  final long len=values.length();
  values.writeBytes(len,val);
  final int att=atts.index(name,val,false);
  final long id=++meta.lastid;
  table.insert(pre,new byte[]{ATTR,(byte)(att >> 8),(byte)att,(byte)(len >> 32),(byte)(len >> 24),(byte)(len >> 16),(byte)(len >> 8),(byte)len,0,0,0,(byte)dis,(byte)(id >> 24),(byte)(id >> 16),(byte)(id >> 8),(byte)id});
  meta.size++;
}
