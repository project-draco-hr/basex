{
  try {
    return new IOFile(new URI(url));
  }
 catch (  Exception ex) {
    Util.debug(ex);
  }
  String file=url.replace('\\','/');
  if (file.startsWith(FILEPREF))   file=url.substring(FILEPREF.length());
  if (file.matches("^/*[a-zA-Z]:/.*"))   file=file.replace("^/+","");
  return new IOFile(file);
}
