{
  final Data data=checkData(qc);
  final MetaData meta=data.meta;
  if (!(text ? meta.textindex : meta.attrindex))   throw BXDB_INDEX_X.get(info,meta.name,(text ? IndexType.TEXT : IndexType.ATTRIBUTE).toString().toLowerCase(Locale.ENGLISH));
  return new ValueAccess(info,exprs[1],text,null,new IndexContext(data,false));
}
