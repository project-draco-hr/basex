{
  final ByteList bl=new ByteList();
  while (true) {
    final byte[] next=readLine(io);
    if (next == null || eq(next,sep))     break;
    if (eq(next,end)) {
      while (readLine(io) != null)       ;
      break;
    }
    bl.add(next).add('\n');
  }
  return TextInput.content(new IOContent(bl.toArray()),charset).finish();
}
