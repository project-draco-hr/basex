{
  final ByteList bl=new ByteList();
  while (true) {
    final byte[] next=readLine(io);
    if (next == null) {
      return bl.toArray();
    }
    if (eq(next,sep))     return bl.toArray();
    if (eq(next,end))     break;
    bl.add(next).add('\n');
  }
  while (readLine(io) != null)   ;
  return bl.toArray();
}
