{
  try {
    f.buffer(10);
  }
 catch (  final EOFException e) {
    return;
  }
  if (!check(f))   return;
  final Metadata meta=new Metadata();
  meta.setInt(IntField.PIXEL_WIDTH,f.get() + f.get() << 8);
  parser.metaEvent(meta);
  meta.setInt(IntField.PIXEL_HEIGHT,f.get() + f.get() << 8);
  parser.metaEvent(meta);
}
