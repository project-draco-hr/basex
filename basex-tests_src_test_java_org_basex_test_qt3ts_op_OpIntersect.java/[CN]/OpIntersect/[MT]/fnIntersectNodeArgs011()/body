{
  final XQuery query=new XQuery("(/bib/book intersect /bib/book)/string(@year)",ctx);
  try {
    query.context(node(file("op/union/bib2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1994 1992 2000 1999"));
}
