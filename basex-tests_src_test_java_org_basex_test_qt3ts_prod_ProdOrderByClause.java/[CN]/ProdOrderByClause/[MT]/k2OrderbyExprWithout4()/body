{
  final XQuery query=new XQuery("let $i as xs:integer* := (1, 2, 3) order by 1 collation \"http:\\\\invalid%>URI\\someURI\" return $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0046"));
}
