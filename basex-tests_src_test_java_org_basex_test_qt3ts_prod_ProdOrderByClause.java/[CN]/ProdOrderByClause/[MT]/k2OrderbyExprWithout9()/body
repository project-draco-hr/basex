{
  final XQuery query=new XQuery("let $i := (1, 3, 2), $b := (4, 6, 5) order by $b return $b",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"4 6 5") || error("XPTY0004")));
}
