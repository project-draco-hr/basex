{
  final XQuery query=new XQuery("let $i := (1, 2, 3) order by $i return $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"1 2 3") || error("XPTY0004")));
}
