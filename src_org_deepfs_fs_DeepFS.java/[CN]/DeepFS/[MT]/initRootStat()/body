{
  rootStat=new DeepStat();
  rootStat.statimespec=System.currentTimeMillis();
  rootStat.stctimespec=rootStat.statimespec;
  rootStat.stmtimespec=rootStat.statimespec;
  rootStat.stmode=getSIFDIR() | 0755;
  rootStat.stsize=0;
  rootStat.stuid=sUID;
  rootStat.stgid=sGID;
  rootStat.stnlink=0;
  rootStat.stino=1;
}
