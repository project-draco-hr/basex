{
  final ListIterator<Clause> iter=clauses.listIterator();
  try {
    while (iter.hasNext())     iter.next().compile(qc,scp);
  }
 catch (  final QueryException qe) {
    iter.remove();
    clauseError(qe,iter);
  }
  try {
    ret=ret.compile(qc,scp);
  }
 catch (  final QueryException qe) {
    clauseError(qe,iter);
  }
  return optimize(qc,scp);
}
