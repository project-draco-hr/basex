{
  tba.insert(3,getTestEntries(entries - 1));
  assertEquals(size + entries - 1,tba.size());
  assertEntrysEqual(0,0,4);
  assertAreInserted(4,entries - 1);
  assertEntrysEqual(4,4 + entries - 1,size - 4);
  closeAndReload();
  assertEquals(size + entries - 1,tba.size());
  assertEntrysEqual(0,0,4);
  assertAreInserted(4,entries - 1);
  assertEntrysEqual(4,4 + entries - 1,size - 4);
}
