{
  tba.delete((size / nodesPerBlock) * nodesPerBlock,size % nodesPerBlock);
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodesPerBlock - (size % nodesPerBlock));
  closeAndReload();
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodesPerBlock - (size % nodesPerBlock));
}
