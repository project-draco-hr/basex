{
  final XQuery query=new XQuery("try { 10000000000000000000000000001 - 10000000000000000000000000000 } catch err:FAOR0002 { 1 }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
