{
  final XQuery query=new XQuery("try { . } catch err:XPDY0002 { \"Context item not set.\" }",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Context item not set."));
}
