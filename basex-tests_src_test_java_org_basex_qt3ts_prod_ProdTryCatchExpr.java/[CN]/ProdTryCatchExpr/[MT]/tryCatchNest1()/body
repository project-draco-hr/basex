{
  final XQuery query=new XQuery("try { try { 1 div 0 } catch * { \"Division by zero\" } } catch * { \"Should not be reached.\" }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Division by zero"));
}
