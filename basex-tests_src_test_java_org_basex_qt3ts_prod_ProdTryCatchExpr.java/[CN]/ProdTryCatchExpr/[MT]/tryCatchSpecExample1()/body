{
  final XQuery query=new XQuery("let $x := \"\" return try {\n" + "      $x cast as xs:integer\n" + "      }\n"+ "      catch * {\n"+ "      0\n"+ "      }",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
