{
  final TokenBuilder tb=new TokenBuilder();
  for (int i=0; i < size; ++i) {
    tb.add('%').add(names[i].string());
    final long s=values[i].size();
    if (s != 0) {
      tb.add('(');
      for (int a=0; a < s; a++) {
        if (a != 0)         tb.add(',');
        tb.add(values[i].itemAt(a).toString());
      }
      tb.add(')');
    }
    tb.add(' ');
  }
  return tb.toString();
}
