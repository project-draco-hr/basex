{
  if (XSLTMODE)   IO.get(TMPCTX).write(Token.token("<x/>"));
  final BufferedReader br=new BufferedReader(new InputStreamReader(new FileInputStream(QUERIES),Token.UTF8));
  while (true) {
    final String line=br.readLine();
    if (line == null)     break;
    if (line.length() == 0 || line.startsWith("#"))     continue;
    query(line);
  }
  br.close();
  IO.get(TMP).delete();
  IO.get(TMPCTX).delete();
}
