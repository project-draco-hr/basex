{
  final XQuery query=new XQuery("declare variable $root := <root> <b d=\"\"/> <c> <c d=\"\"/> <c/> </c> </root>; declare function local:function($object) { $root/b[@d = $object/@d] }; $root/c/c/local:function(.)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<b d=\"\"/>",false));
}
