{
  final XQuery query=new XQuery("declare variable $root := <root> <b d=\"\"/> <c d=\"\"/> </root>; declare function local:function($object) { $root[$object/@d] }; $root//local:function(c)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<root><b d=\"\"/><c d=\"\"/></root>",false));
}
