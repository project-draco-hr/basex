{
  final XQuery query=new XQuery("declare function local:myFunc() { e[true()] }; local:myFunc()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPDY0002") || error("XPST0005")));
}
