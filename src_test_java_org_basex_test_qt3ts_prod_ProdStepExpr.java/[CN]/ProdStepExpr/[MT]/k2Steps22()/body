{
  final XQuery query=new XQuery("declare variable $root := <a> <b e=\"B\"/> <c e=\"B\"/> </a>; declare function local:function($arg) { $root[exactly-one($arg/@e)] }; $root/local:function(c)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<a><b e=\"B\"/><c e=\"B\"/></a>",false));
}
