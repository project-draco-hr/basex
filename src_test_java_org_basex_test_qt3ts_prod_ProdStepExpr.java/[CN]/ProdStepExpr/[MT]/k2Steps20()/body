{
  final XQuery query=new XQuery("<e> <a/> <b/> </e>/((b, a)/., (.), (*, *))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e><a/><b/></e><a/><b/>",false));
}
