{
  final XQuery query=new XQuery("declare variable $root := ( <b d=\"\"/>, <c> <c d=\"\"/> </c> ); declare function local:function($object) { $root[@d eq $object/@d] }; $root/local:function(c)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<b d=\"\"/>",false));
}
