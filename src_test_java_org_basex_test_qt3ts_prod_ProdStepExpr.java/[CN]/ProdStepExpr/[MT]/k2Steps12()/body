{
  final XQuery query=new XQuery("declare variable $e := ()/.; declare variable $b := <b/>/.; $e, <b/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<b/>",false) || error("XPST0005")));
}
