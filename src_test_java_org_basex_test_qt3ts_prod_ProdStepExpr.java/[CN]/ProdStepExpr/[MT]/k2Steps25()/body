{
  final XQuery query=new XQuery("\n" + "        declare variable $root := <root> <b d=\"\"/> <c> <c d=\"\"/> <c/> </c> </root>; \n" + "        declare function local:function($object) { $root/b[@d = $object/@d] }; \n"+ "        $root//local:function(c)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<b d=\"\"/>",false));
}
