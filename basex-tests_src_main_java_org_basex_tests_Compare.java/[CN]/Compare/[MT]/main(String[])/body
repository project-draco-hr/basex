{
  if (xsltMode)   new IOFile(TMPCTX).write(Token.token("<x/>"));
  try (final BufferedReader br=new BufferedReader(new InputStreamReader(new FileInputStream(QUERIES),Strings.UTF8))){
    while (true) {
      final String line=br.readLine();
      if (line == null)       break;
      if (!line.isEmpty() && line.charAt(0) != '#')       query(line);
    }
  }
   new IOFile(TMP).delete();
  new IOFile(TMPCTX).delete();
}
