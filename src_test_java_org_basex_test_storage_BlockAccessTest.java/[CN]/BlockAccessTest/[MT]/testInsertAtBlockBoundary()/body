{
  tba.insert(nodes,getTestEntries(nodes));
  assertEquals(size + nodes,tba.size());
  assertEquals(blocks + 1,tba.blocks());
  assertEntrysEqual(0,0,nodes);
  assertAreInserted(nodes,nodes);
  assertEntrysEqual(nodes,2 * nodes,size - nodes);
  closeAndReload();
  assertEquals(size + nodes,tba.size());
  assertEquals(blocks + 1,tba.blocks());
  assertEntrysEqual(0,0,nodes);
  assertAreInserted(nodes,nodes);
  assertEntrysEqual(nodes,2 * nodes,size - nodes);
}
