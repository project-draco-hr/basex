{
  tba.delete(nodes,nodes);
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodes);
  assertEntrysEqual(2 * nodes,nodes,size - 2 * nodes);
  closeAndReload();
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodes);
  assertEntrysEqual(2 * nodes,nodes,size - 2 * nodes);
}
