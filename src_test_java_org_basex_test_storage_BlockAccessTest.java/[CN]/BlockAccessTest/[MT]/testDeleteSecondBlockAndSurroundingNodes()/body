{
  tba.delete(nodes - 1,nodes + 2);
  assertEquals(size - 2 - nodes,tba.size());
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodes - 1);
  assertEntrysEqual(2 * nodes + 1,nodes - 1,size - 2 * nodes - 1);
  closeAndReload();
  assertEquals(size - 2 - nodes,tba.size());
  assertEquals(blocks - 1,tba.blocks());
  assertEntrysEqual(0,0,nodes - 1);
  assertEntrysEqual(2 * nodes + 1,nodes - 1,size - 2 * nodes - 1);
}
