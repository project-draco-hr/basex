{
  final XQuery query=new XQuery(" \n" + "            let $millenium := year-from-date(current-date()) idiv 1000 \n" + "            return map-pairs(function($a as xs:integer, $b as xs:integer) as xs:integer{$a + $b + $millenium}, 1 to 5, 2 to 6)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertDeepEq("5, 7, 9, 11, 13"));
}
