{
  final XQuery query=new XQuery(" let $in := 1 to 5 return map-pairs(function($a, $b){$a+$b}, $in, tail($in)) ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("3, 5, 7, 9"));
}
