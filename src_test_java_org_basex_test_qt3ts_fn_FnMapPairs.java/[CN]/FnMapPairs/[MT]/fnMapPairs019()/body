{
  final XQuery query=new XQuery("fn:map-pairs( concat#2, fn:error(), ())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEmpty() || error("FOER0000") || error("XPST0005")));
}
