{
  final XQuery query=new XQuery("collection() | collection(())",ctx);
  query.addCollection("",new String[]{"docs/books.xml","docs/bib.xml","docs/reviews.xml"});
  final QT3Result res=result(query);
  result=res;
  test(((assertQuery("count($result) = 3") && assertType("document-node()*")) || error("FODC0003")));
}
