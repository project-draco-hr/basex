{
  return new IndexArrayIterator(){
    boolean f=true;
    long pos=p;
    int lpre, c;
    FTNode n;
    @Override public boolean more(){
      if (c == s)       return false;
      if (f) {
        lpre=da.readNum(pos);
        pos=da.pos();
        f=false;
      }
      final IntList il=new IntList();
      int pre=lpre;
      il.add(pre);
      il.add(da.readNum(pos));
      while (++c < s && (lpre=da.readNum()) == pre)       il.add(da.readNum());
      pos=da.pos();
      n=new FTNode(il);
      return true;
    }
    @Override public FTNode node(){
      n.genPointer(toknum);
      n.setToken(tok);
      return n;
    }
    @Override public int next(){
      return n.pre();
    }
  }
;
}
