{
  super(NodeType.ELM);
  name=nm;
  children=ch;
  atts=at;
  ns=nsp;
  if (ch != null) {
    final long nl=(int)ch.size();
    for (int n=0; n < nl; ++n)     ch.get(n).parent(this);
  }
  if (at != null) {
    final long al=(int)at.size();
    for (int a=0; a < al; ++a)     at.get(a).parent(this);
  }
}
