{
  final XQuery query=new XQuery("declare ordering ordered; (//overtime) intersect (//overtime)",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<overtime>\n     <day>Monday</day>\n     <day>Tuesday</day>\n   </overtime>",false));
}
