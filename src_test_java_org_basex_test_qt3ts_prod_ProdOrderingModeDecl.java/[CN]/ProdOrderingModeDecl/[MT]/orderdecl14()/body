{
  final XQuery query=new XQuery("declare ordering unordered; for $x in (/works/employee[4]/hours) return $x",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertSerialization("<hours>20</hours><hours>40</hours>",false) || assertSerialization("<hours>40</hours><hours>20</hours>",false)));
}
