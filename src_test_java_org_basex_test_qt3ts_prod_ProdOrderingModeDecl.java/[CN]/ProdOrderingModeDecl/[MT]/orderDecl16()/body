{
  final XQuery query=new XQuery("declare ordering unordered; for $x in (//day) return $x",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test((assertSerialization("<day>Monday</day><day>Tuesday</day>",false) || assertSerialization("<day>Tuesday</day><day>Monday</day>",false)));
}
