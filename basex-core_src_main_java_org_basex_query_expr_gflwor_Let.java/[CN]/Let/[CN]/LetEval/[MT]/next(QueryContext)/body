{
  if (!sub.next(qc))   return false;
  for (  final Let let : lets) {
    final boolean s=qc.scoring;
    try {
      qc.scoring=let.scoring;
      qc.set(let.var,let.scoring ? score(let.expr.iter(qc)) : qc.value(let.expr),let.info);
    }
  finally {
      qc.scoring=s;
    }
  }
  return true;
}
