{
  if (args.length == 1) {
    new FSWalker(new TreePrinter()).traverse(new File(Prop.WORK));
    return;
  }
  if (args.length == 2) {
    final File d=new File(args[1]);
    if (d.isDirectory()) {
      new FSWalker(new TreePrinter()).traverse(d);
      return;
    }
    Main.errln("No such directory %",d.getAbsolutePath());
  }
  help(new String[]{"help","tree"});
}
