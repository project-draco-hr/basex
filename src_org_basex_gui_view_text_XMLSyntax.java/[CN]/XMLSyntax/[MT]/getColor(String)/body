{
  final char ch=word.charAt(0);
  final boolean qu=ch == '"' || ch == '\'';
  if (quote != 0 || qu) {
    if (qu)     quote=quote == ch ? 0 : ch;
    return GUIConstants.COLORERROR;
  }
  if (tag) {
    if (ch == '>')     tag=false;
    return GUIConstants.COLORQUOTE;
  }
  if (ch == '<') {
    tag=true;
    return GUIConstants.COLORQUOTE;
  }
  return Color.black;
}
