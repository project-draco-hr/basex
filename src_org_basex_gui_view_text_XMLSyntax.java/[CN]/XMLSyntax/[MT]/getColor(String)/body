{
  final char ch=word.length() != 0 ? word.charAt(0) : 0;
  if (tag) {
    if (quote != 0) {
      if (quote == ch)       quote=0;
      return GUIConstants.COLORERROR;
    }
    if (ch == '"' || ch == '\'') {
      quote=ch;
      return GUIConstants.COLORERROR;
    }
    if (ch == '>')     tag=false;
    return GUIConstants.COLORQUOTE;
  }
  if (ch == '<') {
    tag=true;
    return GUIConstants.COLORQUOTE;
  }
  return Color.black;
}
