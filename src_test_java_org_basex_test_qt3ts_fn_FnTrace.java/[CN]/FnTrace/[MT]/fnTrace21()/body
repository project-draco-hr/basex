{
  final XQuery query=new XQuery("fn:trace((for $var in (fn:upper-case(\"a\"),fn:lower-case(\"B\")) return (fn:lower-case($var),fn:upper-case($var))) ,\"The value of the complex expression on the other argument is:\")",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"a A b B"));
}
