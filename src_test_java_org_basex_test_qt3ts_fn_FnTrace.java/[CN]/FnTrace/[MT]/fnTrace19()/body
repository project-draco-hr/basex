{
  final XQuery query=new XQuery("fn:count(fn:trace(() ,\"The value of the empty sequence is:\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"0") || error("XPST0005")));
}
