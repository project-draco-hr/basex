{
  final XQuery query=new XQuery("for $var in (2,3,4,5) return fn:trace($var * 2 ,\"The Value of $var * 2 is: \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"4 6 8 10"));
}
