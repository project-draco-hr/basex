{
  final XQuery query=new XQuery("for $var in (2,3,4,5) return fn:trace($var - 1,\"The Value of $var - 1 is: \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2 3 4"));
}
