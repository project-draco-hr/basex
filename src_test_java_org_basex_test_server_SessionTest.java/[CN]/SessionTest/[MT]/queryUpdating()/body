{
  Query query=session.query("12345678");
  assertFalse(query.updating());
  check("12345678",query.execute());
  assertFalse(query.updating());
  query.close();
  query=session.query("insert node <a/> into <b/>");
  assertTrue(query.updating());
  check("",query.execute());
  assertTrue(query.updating());
  query.close();
}
