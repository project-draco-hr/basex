{
  final Query query1=session.query("1 to 2");
  final Query query2=session.query("reverse(3 to 4)");
  check("1",query1.next());
  check("4",query2.next());
  check("2",query1.next());
  check("3",query2.next());
  assertNull(query1.next());
  assertNull(query2.next());
  query1.close();
  query2.close();
}
