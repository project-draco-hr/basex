{
  final XQuery query=new XQuery("\n" + "      import module namespace module = \"http://www.w3.org/XQueryTest/RequireProhibitFeature\";\n" + "      declare option prohibit-feature \"all-optional-features\";\n"+ "      declare option require-feature \"module\";\n"+ "      for $f in module:one() return 1\n"+ "    ",ctx);
  try {
    query.addModule("http://www.w3.org/XQueryTest/RequireProhibitFeature",file("prod/RequireProhibitFeature/uses.xquery"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("1") || error("XQST0128")));
}
