{
  final IO io=IO.get(args[0]);
  if (!io.exists())   return error(FILEWHICH,context.user.perm(User.CREATE) ? io : io.name());
  context.query=io;
  try {
    return query(Token.string(io.content()));
  }
 catch (  final IOException ex) {
    Util.debug(ex);
    return error(ex.getMessage());
  }
}
