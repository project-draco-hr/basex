{
  found=false;
  if (next[currentPosition] == null) {
    return 0;
  }
  final int[] nextNodes=next[currentPosition];
  if (nodes[nextNodes[0]][0] > toInsert) {
    touchedElements++;
    return 0;
  }
  int i=0;
  for (; i < nextNodes.length; i++) {
    touchedElements++;
    if (nodes[nextNodes[i]][0] == toInsert) {
      found=true;
      return i;
    }
    if (nodes[nextNodes[i]][0] > toInsert) {
      return i;
    }
  }
  return i;
}
