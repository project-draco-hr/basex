{
  if (data[node] == null || dataSize[node] == ((int[][])data[node])[0].length) {
    return;
  }
  final int[][] newData=new int[2][dataSize[node]];
  System.arraycopy(((int[][])data[node])[0],0,newData[0],0,dataSize[node]);
  System.arraycopy(((int[][])data[node])[1],0,newData[1],0,dataSize[node]);
  data[node]=newData;
}
