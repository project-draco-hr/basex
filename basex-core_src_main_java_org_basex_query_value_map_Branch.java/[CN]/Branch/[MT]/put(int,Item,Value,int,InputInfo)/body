{
  final int key=key(h,l);
  final TrieNode sub=kids[key], nsub;
  final int bs, rem;
  if (sub != null) {
    nsub=sub.put(h,k,v,l + 1,ii);
    if (nsub == sub)     return this;
    bs=used;
    rem=sub.size;
  }
 else {
    nsub=new Leaf(h,k,v);
    bs=used | 1 << key;
    rem=0;
  }
  final TrieNode[] ks=copyKids();
  ks[key]=nsub;
  return new Branch(ks,bs,size - rem + nsub.size);
}
