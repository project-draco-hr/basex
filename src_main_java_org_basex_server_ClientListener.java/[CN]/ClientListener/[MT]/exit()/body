{
  running=false;
  log.write(this,"LOGOUT " + context.user.name,OK);
  try {
    if (events) {
      esocket.close();
      for (      final Sessions s : context.events.values())       s.remove(this);
    }
    new Close().execute(context);
    if (command != null)     command.stop();
    context.delete(this);
    socket.close();
  }
 catch (  final Exception ex) {
    log.write(ex.getMessage());
    Util.stack(ex);
  }
}
