{
  final Performance perf=new Performance();
  final String name=in.readString();
  log.write(this,CREATE + " " + CmdCreate.DATABASE+ " "+ name+ " [...]");
  final ClientInputStream wis=new ClientInputStream(in);
  try {
    final String info=wis.curr() == -1 ? CreateDB.create(name,Parser.emptyParser(),context) : CreateDB.create(name,wis,context);
    info(true,info,perf);
  }
 catch (  final BaseXException ex) {
    wis.close();
    info(false,ex.getMessage(),perf);
  }
}
