{
  var=ctx.vars.get(var);
  if (var.expr == null)   return this;
  final NSLocal lc=ctx.ns;
  ctx.ns=lc.copy();
  if (ctx.nsElem.length != 0)   ctx.ns.add(new QNm(EMPTY,Uri.uri(ctx.nsElem)));
  Expr e=var.expr;
  if (lc.size() != 0 || var.type != null || e instanceof CAttr || e instanceof CElem || e instanceof CPI || e instanceof CComm || e instanceof CText || e instanceof FunCall) {
    e=var.item(ctx);
  }
  ctx.ns=lc;
  return e;
}
