{
  final XQuery query=new XQuery("let $x := (0,0.1e-1,2.0,'a',\"cat\",'',true()) return $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0 0.01 2 a cat  true"));
}
