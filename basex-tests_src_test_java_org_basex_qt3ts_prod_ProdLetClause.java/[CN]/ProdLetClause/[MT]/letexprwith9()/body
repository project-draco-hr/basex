{
  final XQuery query=new XQuery("let $var as xs:time := xs:time(\"21:23:00Z\") return $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"21:23:00Z"));
}
