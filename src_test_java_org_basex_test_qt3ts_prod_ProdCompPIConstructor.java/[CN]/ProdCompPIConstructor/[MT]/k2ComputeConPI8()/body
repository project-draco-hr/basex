{
  final XQuery query=new XQuery("string(processing-instruction {\"thename\"} {\"asdas? >\"})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"asdas? >"));
}
