{
  final XQuery query=new XQuery("fn:data(processing-instruction pi {'a', element a {}, 'b'})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"a  b"));
}
