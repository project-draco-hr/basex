{
  for (int s=0; s < steps.size() - 1; s++) {
    final Step curr=steps.get(s);
    if (curr.preds.size() != 0 || curr.axis != Axis.ANC && curr.axis != Axis.ANCORSELF && curr.axis != Axis.DESC && curr.axis != Axis.SELF && curr.axis != Axis.DESCORSELF && curr.axis != Axis.CHILD && curr.axis != Axis.PARENT)     return false;
  }
  return true;
}
