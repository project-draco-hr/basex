{
  final XQuery query=new XQuery("\n" + "declare boundary-space preserve;\n" + "<A>  A   {\"B\"}   C   {\"D  \"}</A>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<A>  A   B   C   D  </A>",false));
}
