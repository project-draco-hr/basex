{
  final XQuery query=new XQuery("if (implicit-timezone() eq xs:dayTimeDuration('PT1H'))\n" + "            then xs:gDay(\"---31+01:01\") eq xs:gDay(\"---31\")\n" + "            else xs:gDay(\"---31+01:00\") eq xs:gDay(\"---31\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
