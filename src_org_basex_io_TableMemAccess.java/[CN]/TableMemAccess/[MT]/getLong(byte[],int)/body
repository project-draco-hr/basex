{
  final int j=i << 3;
  return ((long)(array[j] & 0xFF) << 56) + ((long)(array[j + 1] & 0xFF) << 48) + ((long)(array[j + 2] & 0xFF) << 40)+ ((long)(array[j + 3] & 0xFF) << 32)+ ((long)(array[j + 4] & 0xFF) << 24)+ ((array[j + 5] & 0xFF) << 16)+ ((array[j + 6] & 0xFF) << 8)+ (array[j + 7] & 0xFF);
}
