{
  int s=0;
  for (int ai=0, bi=0; ai < pre.size && bi < pre2.size; ) {
    int d=pre.get(ai) - pre2.get(bi);
    if (d == 0) {
      d=pos.get(ai) - pos2.get(bi) + w;
      if (d == 0) {
        pre.set(pre.get(ai),s);
        pos.set(pos.get(ai),s++);
      }
    }
    if (d <= 0)     ai++;
    if (d >= 0)     bi++;
  }
  pre.size=s;
  pos.size=s;
}
