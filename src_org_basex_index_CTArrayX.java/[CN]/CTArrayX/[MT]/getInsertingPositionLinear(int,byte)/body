{
  found=false;
  if (nodes[currentPosition][nodes[currentPosition][0] + 1] > 0) {
    return 0;
  }
  final int[] nextNodes=getNextNodes(currentPosition);
  if (nodes[nextNodes[0]][1] > toInsert) {
    return 0;
  }
  int i=0;
  for (; i < nextNodes.length; i++) {
    if (nodes[nextNodes[i]][1] == toInsert) {
      found=true;
      return i;
    }
    if (nodes[nextNodes[i]][1] > toInsert) {
      return i;
    }
  }
  return i;
}
