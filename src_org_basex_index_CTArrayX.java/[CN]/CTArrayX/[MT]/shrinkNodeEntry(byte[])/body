{
  final byte[] b=calculateFirst2Byte(nodeEntry[0],nodeEntry[1],nodeEntry[nodeEntry[0] + 1]);
  final byte[] newEntry=new byte[nodeEntry.length - 1];
  newEntry[0]=b[0];
  newEntry[1]=b[1];
  if (nodeEntry[0] > 1)   System.arraycopy(nodeEntry,2,newEntry,2,nodeEntry[0] - 1);
  System.arraycopy(nodeEntry,nodeEntry[0] + 2,newEntry,nodeEntry[0] + 1,nodeEntry.length - nodeEntry[0] - 2);
  return newEntry;
}
