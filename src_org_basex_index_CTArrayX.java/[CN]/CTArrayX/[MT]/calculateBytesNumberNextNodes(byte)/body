{
  if (numberNextNodes > 26) {
    System.out.println("can't index word - " + "more than 26 child for one node!!");
    System.exit(113);
  }
  final byte[] returnByte=new byte[2];
  byte tmp;
  tmp=(byte)(numberNextNodes & 0x3);
  tmp=(byte)(tmp << 6);
  returnByte[0]=(byte)(returnByte[0] | tmp);
  tmp=(byte)(numberNextNodes & 0x1c);
  tmp=(byte)(tmp << 3);
  returnByte[1]=(byte)(returnByte[1] | tmp);
  return returnByte;
}
