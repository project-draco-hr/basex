{
  final BufferedReader br=new BufferedReader(new FileReader(file));
  String path=file.getCanonicalPath().replaceAll("\\\\","/");
  path=path.substring(0,path.lastIndexOf("/") + 1);
  String name;
  while ((name=br.readLine()) != null) {
    test(name.replaceAll(".*\\?",""),path);
  }
  br.close();
}
