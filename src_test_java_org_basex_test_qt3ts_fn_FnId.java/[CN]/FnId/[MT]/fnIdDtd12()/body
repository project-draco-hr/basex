{
  final XQuery query=new XQuery("fn:id(fn:substring(\"1id3\",2), /IDS[1])/name()",ctx);
  try {
    query.context(node(file("fn/id/iddtd.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"elementwithid-3"));
}
