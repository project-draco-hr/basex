{
  final XQuery query=new XQuery("\n" + "        import module namespace copy=\"http://www.w3.org/QT3/copy\";\n" + "        id(\"id\", copy:copy((//comment())[1]))\n"+ "      ",ctx);
  query.context(node(file("docs/auction.xml")));
  query.addModule("http://www.w3.org/QT3/copy",file("fn/id/copy.xq"));
  final QT3Result res=result(query);
  result=res;
  test(error("FODC0001"));
}
