{
  final XQuery query=new XQuery("\n" + "        declare variable $x external := /works/employee[@name eq \"Jane Doe 1\"];\n" + "        fn:count($x)\n"+ "      ",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
