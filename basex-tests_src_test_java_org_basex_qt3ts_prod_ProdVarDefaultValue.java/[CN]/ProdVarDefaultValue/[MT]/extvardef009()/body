{
  final XQuery query=new XQuery("declare variable $x external; \"result\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"result") || error("XPDY0002")));
}
