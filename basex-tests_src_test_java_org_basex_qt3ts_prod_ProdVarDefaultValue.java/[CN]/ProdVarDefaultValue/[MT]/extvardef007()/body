{
  final XQuery query=new XQuery("\n" + "        declare variable $ext as xs:date external := current-date() + xs:dayTimeDuration('P30D'); \n" + "        $ext gt xs:date('2008-12-30')\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
