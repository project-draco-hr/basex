{
  try {
    if (data != null) {
      final Data d=data;
      data=null;
      current=null;
      marked=null;
      copied=null;
      d.close();
      if (Prop.mainmem)       Performance.gc(1);
    }
    return true;
  }
 catch (  final IOException ex) {
    BaseX.debug(ex);
    return false;
  }
}
