{
  try {
    if (data != null) {
      final Data d=data;
      data=null;
      current=null;
      marked=null;
      copied=null;
      d.close();
      if (Prop.mainmem || Prop.onthefly)       Performance.gc(1);
      if (Prop.fuse) {
        BaseX.err("[basex_diskdata_close]");
        d.fs.nativeShutDown();
      }
    }
    return true;
  }
 catch (  final IOException ex) {
    BaseX.debug(ex);
    return false;
  }
}
