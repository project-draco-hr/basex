{
  for (int i=0; i < l; i++)   sb.append("  ");
  String nm="";
switch (kind) {
case Data.ELEM:
    nm=Token.string(d.tags.key(name));
  break;
case Data.ATTR:
nm="@" + Token.string(d.atts.key(name));
break;
case Data.TEXT:
nm="TEXT";
break;
case Data.COMM:
nm="COMM";
break;
case Data.PI:
nm="PI";
break;
}
sb.append(nm + ": " + count+ "x\n");
for (final Node c : ch) c.dump(sb,l + 1,d);
}
