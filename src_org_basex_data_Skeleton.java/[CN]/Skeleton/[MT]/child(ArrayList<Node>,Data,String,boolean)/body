{
  final ArrayList<Node> ch=new ArrayList<Node>();
  if (name.startsWith("@"))   return ch;
  final int t=data.tagID(Token.token(name));
  for (  final Node r : n) {
    if (name.length() != 0 && r.name != t)     continue;
    for (    final Node c : r.ch) {
      if (desc)       c.child(ch);
 else       ch.add(c);
    }
  }
  return ch;
}
