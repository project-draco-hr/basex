{
  if (!(cmp.exprs[1] instanceof AStr))   return cmp;
  final byte[] d=((Item)cmp.exprs[1]).string(cmp.info);
  final Expr e=cmp.exprs[0];
  if (e.seqType().mayBeArray())   return cmp;
switch (cmp.op.op) {
case GE:
    return new CmpSR(e,d,true,null,true,cmp.coll,cmp.info);
case GT:
  return new CmpSR(e,d,false,null,true,cmp.coll,cmp.info);
case LE:
return new CmpSR(e,null,true,d,true,cmp.coll,cmp.info);
case LT:
return new CmpSR(e,null,true,d,false,cmp.coll,cmp.info);
default :
return cmp;
}
}
