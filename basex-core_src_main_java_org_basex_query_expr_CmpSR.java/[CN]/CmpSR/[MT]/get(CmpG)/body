{
  if (!(ex.exprs[1] instanceof AStr))   return ex;
  final byte[] d=((Item)ex.exprs[1]).string(ex.info);
  final Expr e=ex.exprs[0];
switch (ex.op.op) {
case GE:
    return new CmpSR(e,d,true,null,true,ex.coll,ex.info);
case GT:
  return new CmpSR(e,d,false,null,true,ex.coll,ex.info);
case LE:
return new CmpSR(e,null,true,d,true,ex.coll,ex.info);
case LT:
return new CmpSR(e,null,true,d,false,ex.coll,ex.info);
default :
return ex;
}
}
