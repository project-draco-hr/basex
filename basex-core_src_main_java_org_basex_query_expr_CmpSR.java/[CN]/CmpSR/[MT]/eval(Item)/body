{
  if (!it.type.isStringOrUntyped())   throw INVTYPECMP.get(info,it.type,AtomType.STR);
  final byte[] s=it.string(info);
  final int mn=min == null ? 1 : collation == null ? Token.diff(s,min) : collation.compare(s,min);
  final int mx=max == null ? -1 : collation == null ? Token.diff(s,max) : collation.compare(s,max);
  return (mni ? mn >= 0 : mn > 0) && (mxi ? mx <= 0 : mx < 0);
}
