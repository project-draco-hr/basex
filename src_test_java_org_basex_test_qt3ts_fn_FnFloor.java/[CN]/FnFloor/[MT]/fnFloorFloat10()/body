{
  final XQuery query=new XQuery("floor(xs:float(xs:float('-INF')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"-INF") && assertType("xs:float")));
}
