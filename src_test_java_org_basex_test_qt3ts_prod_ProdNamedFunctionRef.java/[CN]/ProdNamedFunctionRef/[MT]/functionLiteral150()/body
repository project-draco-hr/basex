{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}matches#3('string', 'STRING', 'i')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
