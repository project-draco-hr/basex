{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions/math}cos#1(1e0)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0.5403023058681398"));
}
