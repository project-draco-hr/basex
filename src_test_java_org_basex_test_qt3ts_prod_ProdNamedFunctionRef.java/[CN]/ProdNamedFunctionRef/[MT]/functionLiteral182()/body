{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}years-from-duration#1(xs:yearMonthDuration(\"P20Y15M\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"21"));
}
