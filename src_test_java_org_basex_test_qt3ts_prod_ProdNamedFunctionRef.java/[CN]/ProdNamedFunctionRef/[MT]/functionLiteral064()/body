{
  final XQuery query=new XQuery("format-number(Q{http://www.w3.org/2005/xpath-functions/math}exp#1(1e0), '#.0000000')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2.7182818"));
}
