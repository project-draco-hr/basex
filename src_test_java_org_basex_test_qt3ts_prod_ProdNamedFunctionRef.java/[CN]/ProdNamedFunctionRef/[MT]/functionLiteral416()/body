{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}static-base-uri#0()",ctx);
  try {
    query.baseURI("http://www.example.com");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertType("xs:anyURI?"));
}
