{
  final XQuery query=new XQuery("Q{http://www.w3.org/2005/xpath-functions}map#2(xs:int#1, (\"23\", \"29\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("(23, 29)"));
}
