{
  final XQuery query=new XQuery("(2, 4, 6)!Q{http://www.w3.org/2005/xpath-functions}last#0()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"3 3 3"));
}
