{
  return new Iterator<XQItem>(){
    /** 
 * Current item. 
 */
    private XQItem next;
    @Override public boolean hasNext(){
      if (next == null)       next=XQuery.this.next();
      return next != null;
    }
    @Override public XQItem next(){
      final XQItem it=hasNext() ? next : null;
      next=null;
      return it;
    }
    @Override public void remove(){
      Util.notexpected();
    }
  }
;
}
