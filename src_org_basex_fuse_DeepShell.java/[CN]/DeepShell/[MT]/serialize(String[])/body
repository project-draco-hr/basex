{
  try {
    Nodes n=new Nodes(0,fs.data);
    n=new QueryProcessor("/",n).queryNodes();
    final ConsoleOutput out=new ConsoleOutput(System.out);
    final XMLSerializer xml=new XMLSerializer(out,false,true);
    n.serialize(xml);
    xml.close();
  }
 catch (  final Exception e) {
    e.printStackTrace();
  }
}
