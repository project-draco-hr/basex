{
  final XQuery query=new XQuery("\n" + "        import module namespace copy=\"http://www.w3.org/QT3/copy\";\n" + "        generate-id(copy:copy((//@*)[1]))\n"+ "      ",ctx);
  query.context(node(file("docs/works-mod.xml")));
  query.addModule("http://www.w3.org/QT3/copy",file("fn/id/copy.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertQuery("matches($result, '^[A-Za-z][A-Za-z0-9]*$')"));
}
