{
  final XQuery query=new XQuery("\n" + "        import module namespace copy=\"http://www.w3.org/QT3/copy\";\n" + "        let $att := (//@*)[1] return generate-id(copy:copy($att)) eq generate-id($att)\n"+ "      ",ctx);
  query.context(node(file("docs/works-mod.xml")));
  query.addModule("http://www.w3.org/QT3/copy",file("fn/id/copy.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(false));
}
