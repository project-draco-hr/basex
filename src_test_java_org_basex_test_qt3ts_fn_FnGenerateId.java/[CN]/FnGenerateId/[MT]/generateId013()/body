{
  final XQuery query=new XQuery("let $nodes := collection()//*\n" + "            return count($nodes) eq count(distinct-values($nodes/generate-id()))",ctx);
  query.addCollection("",new String[]{"docs/auction.xml","docs/works-mod.xml"});
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
