{
  final XQuery query=new XQuery("for $h in (/works/employee[7]) return $h/child::*[self::pnum or self::empnum]",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<empnum>E2</empnum><pnum>P1</pnum>",false));
}
