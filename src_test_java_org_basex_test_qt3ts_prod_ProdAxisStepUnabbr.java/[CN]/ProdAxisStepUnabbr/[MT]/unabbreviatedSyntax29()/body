{
  final XQuery query=new XQuery("for $h in (/works) return $h/child::employee[attribute::gender eq 'female'][fn:position() = 5]",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<employee name=\"Jane Doe 9\" gender=\"female\">\n   <empnum>E3</empnum>\n   <pnum>P2</pnum>\n   <hours>20</hours>\n  </employee>",false));
}
