{
  final XQuery query=new XQuery("for $h in (/works) return $h/child::employee[fn:position() = fn:last()]",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<employee name=\"Jane Doe 13\" gender=\"female\" type=\"FT\">\n   <empnum>E4</empnum>\n   <pnum>P5</pnum>\n   <hours>80</hours>\n   <status>active</status>\n  </employee>",false));
}
