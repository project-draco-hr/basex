{
  final XQuery query=new XQuery("for $h in (/works/employee[1]) return $h/child::*",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<empnum>E1</empnum><pnum>P1</pnum><hours>40</hours>",false));
}
