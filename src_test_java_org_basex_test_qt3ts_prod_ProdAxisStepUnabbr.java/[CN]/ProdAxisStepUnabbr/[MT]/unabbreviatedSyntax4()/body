{
  final XQuery query=new XQuery("for $h in (/works/employee[1]) return $h/child::node()",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("\n   <empnum>E1</empnum>\n   <pnum>P1</pnum>\n   <hours>40</hours>\n  ",false));
}
