{
switch (sig) {
case _DATETIME_CURRENT_TIME:
    return dateTime(info)[0];
case _DATETIME_CURRENT_DATE:
  return dateTime(info)[1];
case _DATETIME_CURRENT_DATETIME:
return dateTime(info)[2];
case _DATETIME_TIMESTAMP:
return Int.get(System.currentTimeMillis());
default :
return super.item(ctx,ii);
}
}
