{
  hexQuery(_BIN_JOIN.args("()"),"");
  hexQuery(_BIN_JOIN.args('(' + base64("") + ')'),"");
  hexQuery(_BIN_JOIN.args('(' + base64("FF") + ')'),"FF");
  hexQuery(_BIN_JOIN.args('(' + base64("FF") + ','+ base64("FF")+ ')'),"FFFF");
  hexQuery(_BIN_JOIN.args(" (1 to 3) ! " + base64("11")),"111111");
}
