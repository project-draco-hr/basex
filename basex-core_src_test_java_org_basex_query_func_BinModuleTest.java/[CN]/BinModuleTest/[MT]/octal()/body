  hexQuery(_BIN_OCTAL.args("000"),"0000");
  hexQuery(_BIN_OCTAL.args("007"),"0007");
  hexQuery(_BIN_OCTAL.args("1"),"01");
  hexQuery(_BIN_OCTAL.args("10"),"08");
