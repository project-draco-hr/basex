{
  final XQuery query=new XQuery("let $var := (\"a\",\"b\",\"c\") where fn:boolean($var) = fn:true() return $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPTY0004") || error("FORG0006")));
}
