{
  final XQuery query=new XQuery("for $file in (//Folder)[1]/File where ($file, 1) return $file/FileName",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPTY0004") || error("FORG0006")));
}
