{
  String ini=null;
  try {
    final StringBuilder sb=new StringBuilder();
    final BufferedReader br=new BufferedReader(new InputStreamReader(Util.class.getResourceAsStream("/" + filename)));
    String line;
    while ((line=br.readLine()) != null) {
      sb.append(line + "\n");
    }
    ini=sb.toString();
    br.close();
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
  return ini;
}
