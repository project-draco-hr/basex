{
  final XQuery query=new XQuery("( fn:function-arity( if (current-date() eq xs:date('1900-01-01'))\n" + "                                 then fn:dateTime#2\n" + "                                 else () ),\n"+ "              fn:function-arity( if (current-date() eq xs:date('1900-01-01'))\n"+ "                                 then ()\n"+ "                                 else fn:dateTime#2 ) )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
