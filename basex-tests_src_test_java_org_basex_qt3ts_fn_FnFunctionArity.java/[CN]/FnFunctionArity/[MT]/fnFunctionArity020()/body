{
  final XQuery query=new XQuery("let $initial := fn:substring(?, 1, 1) \n" + "            return fn:function-arity( $initial )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1"));
}
