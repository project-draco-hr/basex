{
  return !new LockQuery(http,"declare variable $path external;" + "declare variable $owner external;" + "w:conflicting-locks("+ "<w:lockinfo>"+ "<w:path>{ $path }</w:path>"+ "<w:scope>exclusive</w:scope>"+ "<w:depth>infinite</w:depth>"+ "<w:owner>{ $owner }</w:owner>"+ "</w:lockinfo>)").bind("path",db + SEP + p).bind("owner",http.user).execute().isEmpty();
}
