{
  for (int o=0; o < K_N_OPT.length; o++)   if (options.containsKey(K_N_OPT[o]))   nprops.put(N_OPT[o],toInt(options.get(K_N_OPT[o])));
  for (int o=0; o < K_B_OPT.length; o++)   if (options.containsKey(K_B_OPT[o]))   nprops.put(B_OPT[o],Util.yes(options.get(K_B_OPT[o])));
  for (int o=0; o < K_S_OPT.length; o++)   if (options.containsKey(K_S_OPT[o]))   nprops.put(S_OPT[o],options.get(K_S_OPT[o]));
}
