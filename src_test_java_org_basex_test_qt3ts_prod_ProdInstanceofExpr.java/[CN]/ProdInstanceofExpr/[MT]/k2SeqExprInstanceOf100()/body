{
  final XQuery query=new XQuery("<a><b/></a>/(b instance of element(), * instance of element())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"true true"));
}
