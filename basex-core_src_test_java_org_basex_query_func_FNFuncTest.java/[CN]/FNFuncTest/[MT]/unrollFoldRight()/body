{
  check("fn:fold-right(1 to 9, 10, function($a,$b) {$a+$b})","55","empty(//" + Util.className(FNFunc.class) + ')',"exists(*/" + Util.className(Int.class) + ')');
  check("fn:fold-right(1 to 9, 10, function($a,$b) {0*random:integer($a)+$b})","10","empty(//" + Util.className(FNFunc.class) + ')',"empty(*/" + Util.className(Int.class) + ')',"count(//" + Util.className(Arith.class) + "[@op = '+']) eq 9");
  check("fn:fold-right(0 to 9, 10, function($a,$b) {$a+$b})","55","exists(//" + Util.className(FNFunc.class) + "[contains(@name, 'fold-right')])");
}
