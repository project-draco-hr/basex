{
  return new ValueIter(){
    private boolean req;
    @Override public Item next(){
      if (req)       return null;
      req=true;
      return Item.this;
    }
    @Override public long size(){
      return 1;
    }
    @Override public Item get(    final long i){
      return Item.this;
    }
    @Override public boolean reset(){
      req=false;
      return true;
    }
    @Override public Value value(){
      return Item.this;
    }
  }
;
}
