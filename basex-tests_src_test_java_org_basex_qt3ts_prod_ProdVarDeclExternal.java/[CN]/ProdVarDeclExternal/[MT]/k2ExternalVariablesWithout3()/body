{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"\"; \n" + "        declare namespace prefix = \"\"; \n"+ "        declare variable $prefix:input-context external; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0081"));
}
