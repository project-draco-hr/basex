{
  final XQuery query=new XQuery("declare variable $input-context1 external; declare variable $input-context1 := 1; 1",ctx);
  try {
    query.context(node(file("prod/AxisStep/TopMany.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0049"));
}
