{
  final XQuery query=new XQuery("\n" + "        declare variable $global := count(*); \n" + "        <e/>/($global, $global)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
