{
  final XQuery query=new XQuery("declare variable $v as element(elementName, xs:anyType?)+ := <elementName/>; 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
