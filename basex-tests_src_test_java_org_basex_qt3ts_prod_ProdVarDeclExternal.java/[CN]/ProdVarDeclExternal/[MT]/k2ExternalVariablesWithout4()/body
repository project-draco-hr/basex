{
  final XQuery query=new XQuery("\n" + "        \n" + "        declare variable $exampleComThisVarIsNotRecognized external; $exampleComThisVarIsNotRecognized",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
