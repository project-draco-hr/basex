{
  final XPContext ctx=(XPContext)xpc;
  final FTPosFilter tmp=ctx.ftpos;
  ctx.ftpos=ftpos;
  FTNode ftn=exprs[0].next(ctx);
  ftpos.pos.init(ctx.ftitem);
  if (ftn.size > 0) {
    init(ftn,ctx);
    while (!seqEval()) {
      if (more()) {
        ftn=exprs[0].next(ctx);
        if (ftn.size == 0)         return new FTNode();
        init(ftn,ctx);
      }
 else {
        ctx.ftpos=tmp;
        return new FTNode();
      }
    }
    ctx.ftpos=tmp;
    return ftn;
  }
  ctx.ftpos=tmp;
  return ftn;
}
