{
  int distance=-1;
  int pos;
  int pos1=p1;
  int pos2=p2;
  if (pos1 != Math.min(pos1,pos2)) {
    final int tmp=pos1;
    pos1=pos2;
    pos2=tmp;
  }
  pos=pos1;
  while (pos < pos2) {
    pos=getBytePositionNScopsFollowing(pos,data,1,ftpos.pos.dunit);
    distance++;
  }
  if (pos2 == pos) {
    return distance;
  }
 else {
    return -1;
  }
}
