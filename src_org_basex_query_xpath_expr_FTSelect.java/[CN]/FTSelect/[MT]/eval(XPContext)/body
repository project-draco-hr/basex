{
  final FTPositionFilter tmp=ctx.ftpos;
  ctx.ftpos=ftpos;
  ctx.ftpos.st=ctx.ftpos.pos.content || ctx.ftpos.pos.different || ctx.ftpos.pos.end|| ctx.ftpos.pos.ordered|| ctx.ftpos.pos.same|| ctx.ftpos.pos.start;
  ftpos.pos.init(ctx.ftitem);
  final Item i=exprs[0].eval(ctx);
  ctx.ftpos=tmp;
  return i;
}
