{
  if (type == 0) {
    final ArrayList<PathNode> list=new ArrayList<PathNode>();
    list.add(skel.root);
    stack.push(list);
  }
 else   if (type == 1) {
    ArrayList<PathNode> list=null;
    if (stack.size() == 0) {
      if (!ctx.root())       return;
      list=new ArrayList<PathNode>();
      list.add(skel.root);
    }
 else {
      list=skel.desc(stack.peek(),0,Data.ELEM,false);
    }
    stack.push(list);
  }
}
