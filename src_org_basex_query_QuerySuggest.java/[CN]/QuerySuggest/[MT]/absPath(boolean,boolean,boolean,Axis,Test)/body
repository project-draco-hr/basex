{
  if (!nslash) {
    final ArrayList<SkelNode> list=new ArrayList<SkelNode>();
    list.add(skel.root);
    stack.push(list);
  }
  if (root) {
    if (!desc) {
      checkStep(axis,test);
    }
 else {
      checkStep(Axis.DESCORSELF,Test.NODE);
      checkStep(axis,test);
    }
  }
 else   if (nslash) {
    checkStep(axis,test);
  }
 else {
    if (desc)     checkStep(Axis.DESCORSELF,Test.NODE);
    checkStep(axis,test);
  }
}
