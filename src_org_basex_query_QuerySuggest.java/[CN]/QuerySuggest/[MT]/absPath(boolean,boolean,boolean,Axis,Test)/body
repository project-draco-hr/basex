{
  final ArrayList<SkelNode> list=new ArrayList<SkelNode>();
  list.add(skel.root);
  stack.push(list);
  if (root) {
    olds.clear();
    if (!desc) {
      checkStep(axis,test);
      filter(false);
    }
 else {
      checkStep(Axis.DESCORSELF,Test.NODE);
      checkStep(axis,test);
      Object[] step=new Object[]{Axis.DESCORSELF,Test.NODE};
      olds.add(step);
    }
  }
 else   if (nslash) {
  }
 else {
    if (olds.size() > 0) {
      for (int i=0; i < olds.size(); i++) {
        checkStep((Axis)olds.get(i)[0],(Test)olds.get(i)[1]);
      }
    }
    checkStep(axis,test);
    filter(false);
  }
}
