{
  if (t.type == Type.TXT) {
    return TEXT;
  }
  if (t.type == Type.COM) {
    return COMM;
  }
  if (t.type == Type.PI) {
    return PI;
  }
  if (t instanceof NameTest && t.name != null) {
    final byte[] name=t.name.ln();
    return a == Axis.ATTR ? Token.concat(ATT,name) : name;
  }
  return Token.EMPTY;
}
