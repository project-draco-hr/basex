{
  if (qe == null) {
    try {
      super.compile(ctx);
      if (expr.isValue())       return optPre(expr,ctx);
    }
 catch (    final QueryException ex) {
      qe=ex;
    }
  }
  for (  final Catch c : ctch)   c.compile(ctx);
  type=expr.type();
  for (  final Catch c : ctch)   type=type.intersect(c.type());
  return this;
}
