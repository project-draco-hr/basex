{
  for (int c=0; c < ctch.length; ++c) {
    ctch[c]=((Catch)checkUp(ctch[c],ctx)).comp(ctx);
  }
  checkUp(expr,ctx);
  Expr e=this;
  try {
    e=super.comp(ctx);
  }
 catch (  final QueryException ex) {
    qe=ex;
  }
  type=expr.type();
  for (  final Catch c : ctch)   type=type.intersect(c.type());
  return e;
}
