{
  final int s=ctx.vars.size();
  try {
    try {
      return ctx.value(expr);
    }
 catch (    final QueryException ex) {
      for (      final Catch c : ctch) {
        final Value val=c.value(ctx,ex);
        if (val != null)         return val;
      }
      throw ex;
    }
  }
  finally {
    ctx.vars.size(s);
  }
}
