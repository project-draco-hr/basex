{
switch (def) {
case FUNCARITY:
    return Itr.get(getFun(0,FunType.ANY,ctx).arity());
case FUNCNAME:
  return getFun(0,FunType.ANY,ctx).fName();
case PARTAPP:
return partApp(ctx,ii);
case HOFID:
return expr[0].item(ctx,ii);
case CONST:
return expr[0].item(ctx,ii);
default :
return super.item(ctx,ii);
}
}
