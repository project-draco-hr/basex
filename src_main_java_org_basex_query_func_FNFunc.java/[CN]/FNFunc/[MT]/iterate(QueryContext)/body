{
  final FunItem f=withArity(0,1,ctx);
  return new Iter(){
    /** 
 * Current value. 
 */
    Value v=expr[1].value(ctx);
    long i, len=v.size();
    @Override public Item next() throws QueryException {
      while (i >= len) {
        v=f.invValue(ctx,input,v);
        i=0;
        len=v.size();
      }
      return v.itemAt(i++);
    }
  }
;
}
