{
  expr=expr.compile(ctx);
  type=expr.type();
  size=expr.size();
  if (ctx.grouping) {
    var.ret=SeqType.get(type.type,Occ.ZERO_MORE);
  }
 else {
    var.size=Math.min(1,size);
    var.ret=type.type.seqType();
  }
  ctx.vars.add(var);
  if (pos != null)   ctx.vars.add(pos);
  if (score != null)   ctx.vars.add(score);
  return this;
}
