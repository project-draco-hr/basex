{
switch (func) {
case _HOF_SORT_WITH:
    return sortWith(qc);
case _HOF_FOLD_LEFT1:
  return foldLeft1(qc);
case _HOF_UNTIL:
return until(qc);
case _HOF_ID:
case _HOF_CONST:
return qc.value(exprs[0]);
case _HOF_TOP_K_BY:
return topKBy(qc);
case _HOF_TOP_K_WITH:
return topKWith(qc);
default :
return super.value(qc);
}
}
