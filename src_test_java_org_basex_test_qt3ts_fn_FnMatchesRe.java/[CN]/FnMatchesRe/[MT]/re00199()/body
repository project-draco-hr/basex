{
  final XQuery query=new XQuery("(every $s in tokenize('??', ',') satisfies matches($s, '^(?:\\P{Z}*)$')) and (every $s in tokenize('\u1680\u3000\u2028\u2028\u2029\u2029', ',') satisfies not(matches($s, '^(?:\\P{Z}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
