{
  final XQuery query=new XQuery("(every $s in tokenize('22a22z', ',') satisfies matches($s, '^(?:(\\d*){2,}?a\\1z)$')) and (every $s in tokenize('22a22', ',') satisfies not(matches($s, '^(?:(\\d*){2,}?a\\1z)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
