{
  final XQuery query=new XQuery("(every $s in tokenize('\u0d00???,\u0d00\u0d01??????\u0d04????????????????????????\u0d0d?????????\u0d11???????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u0d3b\u0d3c????????????????????????\u0d45?????????\u0d49???????????????\u0d4f\u0d50\u0d51\u0d52\u0d53\u0d54\u0d55\u0d56???\u0d58\u0d59\u0d5a\u0d5b\u0d5c\u0d5d\u0d5e\u0d5f????????????\u0d64\u0d65????????????????????????????????????????????????\u0d76\u0d77\u0d78?????????????????????', ',') satisfies matches($s, '^(?:\\p{IsMalayalam}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsMalayalam}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
