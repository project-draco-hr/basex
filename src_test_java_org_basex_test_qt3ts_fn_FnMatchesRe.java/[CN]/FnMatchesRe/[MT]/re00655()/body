{
  final XQuery query=new XQuery("(every $s in tokenize('foo?? bar??', ',') satisfies matches($s, '^(?:(\\w+)\\s+(\\w+))$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:(\\w+)\\s+(\\w+))$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
