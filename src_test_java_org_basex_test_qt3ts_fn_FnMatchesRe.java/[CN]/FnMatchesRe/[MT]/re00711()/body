{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:[\\d-[13579\\s]]+)$')) and (every $s in tokenize('  ??02468??', ',') satisfies not(matches($s, '^(?:[\\d-[13579\\s]]+)$')))",ctx);
