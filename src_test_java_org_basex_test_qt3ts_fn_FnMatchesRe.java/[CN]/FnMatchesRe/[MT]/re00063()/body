{
  final XQuery query=new XQuery("(every $s in tokenize('a', ',') satisfies matches($s, '^(?:[^\\P{IsBasicLatin}])$')) and (every $s in tokenize('??', ',') satisfies not(matches($s, '^(?:[^\\P{IsBasicLatin}])$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
