{
  final XQuery query=new XQuery("(every $s in tokenize('\t', ',') satisfies matches($s, '^(?:\\P{So}*)$')) and (every $s in tokenize('???????', ',') satisfies not(matches($s, '^(?:\\P{So}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
