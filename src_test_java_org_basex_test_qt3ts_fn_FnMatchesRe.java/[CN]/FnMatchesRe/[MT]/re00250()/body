{
  final XQuery query=new XQuery("(every $s in tokenize('\u1800\u18af,\u1800\u1801\u1802\u1803\u1804\u1805\u1806\u1807\u1808\u1809\u180a?????????\u180e\u180f??????????????????????????????\u181a\u181b\u181c\u181d\u181e\u181f????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u1878\u1879\u187a\u187b\u187c\u187d\u187e\u187f?????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u18ab\u18ac\u18ad\u18ae\u18af', ',') satisfies matches($s, '^(?:\\p{IsMongolian}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsMongolian}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
