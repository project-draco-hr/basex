{
  final XQuery query=new XQuery("(every $s in tokenize('\u1680', ',') satisfies matches($s, '^(?:\\P{S}*)$')) and (every $s in tokenize('??????????????????????????????????', ',') satisfies not(matches($s, '^(?:\\P{S}*)$')))",ctx);
