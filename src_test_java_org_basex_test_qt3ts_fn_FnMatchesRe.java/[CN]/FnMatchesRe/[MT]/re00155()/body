{
  final XQuery query=new XQuery("(every $s in tokenize('\u301c\uff0d', ',') satisfies matches($s, '^(?:\\p{Pd}*)$')) and (every $s in tokenize('\u301d', ',') satisfies not(matches($s, '^(?:\\p{Pd}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
