{
  final XQuery query=new XQuery("(every $s in tokenize('???\u1fff,??????????????????????????????????????????????????????????????????\u1f16\u1f17??????????????????\u1f1e\u1f1f??????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u1f46\u1f47??????????????????\u1f4e\u1f4f????????????????????????\u1f58???\u1f5a???\u1f5c???\u1f5e?????????????????????????????????????????????????????????????????????????????????????????????\u1f7e\u1f7f???????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u1fb5?????????????????????????????????????????????\u1fc5??????????????????????????????????????????\u1fd4\u1fd5??????????????????\u1fdc?????????????????????????????????????????????????????????\u1ff0\u1ff1?????????\u1ff5???????????????????????????\u1fff', ',') satisfies matches($s, '^(?:\\p{IsGreekExtended}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsGreekExtended}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
