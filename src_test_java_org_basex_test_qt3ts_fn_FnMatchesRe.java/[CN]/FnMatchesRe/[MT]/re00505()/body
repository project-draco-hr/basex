{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:\\w)$')) and (every $s in tokenize('\uf8ff,\u070f,\u007f,\u2010,\ufe37,},\u201c,??,\u0f04, ,\u2028,\u2029', ',') satisfies not(matches($s, '^(?:\\w)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
