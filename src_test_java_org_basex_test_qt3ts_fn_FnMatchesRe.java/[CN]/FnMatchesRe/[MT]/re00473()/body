{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:\\p{IsDeseret})$')) and (every $s in tokenize('\ud800\udf4f', ',') satisfies not(matches($s, '^(?:\\p{IsDeseret})$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
