{
  final XQuery query=new XQuery("(every $s in tokenize('\udbc0\udc00', ',') satisfies matches($s, '^(?:\\p{IsPrivateUse}?)$')) and (every $s in tokenize('???,\u007f', ',') satisfies not(matches($s, '^(?:\\p{IsPrivateUse}?)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
