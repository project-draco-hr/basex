{
  final XQuery query=new XQuery("(every $s in tokenize('\u3100\u312f,\u3100\u3101\u3102\u3103\u3104???????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u312e\u312f', ',') satisfies matches($s, '^(?:\\p{IsBopomofo}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsBopomofo}+)$')))",ctx);
