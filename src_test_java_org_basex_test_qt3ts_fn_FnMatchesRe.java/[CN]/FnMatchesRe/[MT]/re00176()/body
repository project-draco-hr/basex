{
  final XQuery query=new XQuery("(every $s in tokenize('_,???', ',') satisfies matches($s, '^(?:\\P{L}*)$')) and (every $s in tokenize('aAbB,A????aa??????????????????????????', ',') satisfies not(matches($s, '^(?:\\P{L}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
