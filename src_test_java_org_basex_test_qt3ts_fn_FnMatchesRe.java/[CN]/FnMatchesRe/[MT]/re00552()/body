{
  final XQuery query=new XQuery("(every $s in tokenize('??ello ??orld', ',') satisfies matches($s, '^(?:(\\p{Lt}\\w*)\\s(\\p{Lt}*\\w*))$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:(\\p{Lt}\\w*)\\s(\\p{Lt}*\\w*))$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
