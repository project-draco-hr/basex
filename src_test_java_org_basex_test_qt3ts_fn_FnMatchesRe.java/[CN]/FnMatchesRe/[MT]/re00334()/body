{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:\\p{IsGreekExtended}?)$')) and (every $s in tokenize('\u2000', ',') satisfies not(matches($s, '^(?:\\p{IsGreekExtended}?)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
