{
  final XQuery query=new XQuery("(every $s in tokenize('\u0b80\u0bff,\u0b80\u0b81??????\u0b84??????????????????\u0b8b\u0b8c\u0b8d?????????\u0b91????????????\u0b96\u0b97\u0b98??????\u0b9b???\u0b9d??????\u0ba0\u0ba1\u0ba2??????\u0ba5\u0ba6\u0ba7?????????\u0bab\u0bac\u0bad????????????????????????????????????\u0bba\u0bbb\u0bbc\u0bbd???????????????\u0bc3\u0bc4\u0bc5?????????\u0bc9????????????\u0bce\u0bcf???\u0bd1\u0bd2\u0bd3\u0bd4\u0bd5\u0bd6???\u0bd8\u0bd9\u0bda\u0bdb\u0bdc\u0bdd\u0bde\u0bdf\u0be0\u0be1\u0be2\u0be3\u0be4\u0be5???????????????????????????????????????????????????????????????\u0bfb\u0bfc\u0bfd\u0bfe\u0bff', ',') satisfies matches($s, '^(?:\\p{IsTamil}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsTamil}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
