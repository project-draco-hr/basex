{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:\\p{IsHangulSyllables})$')) and (every $s in tokenize('\ua4cf', ',') satisfies not(matches($s, '^(?:\\p{IsHangulSyllables})$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
