{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:\\p{IsArabicPresentationForms-B})$')) and (every $s in tokenize('\ufe6f', ',') satisfies not(matches($s, '^(?:\\p{IsArabicPresentationForms-B})$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
