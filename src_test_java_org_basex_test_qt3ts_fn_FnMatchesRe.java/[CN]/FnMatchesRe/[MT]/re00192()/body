{
  final XQuery query=new XQuery("(every $s in tokenize('\u301c', ',') satisfies matches($s, '^(?:\\P{Pc}*)$')) and (every $s in tokenize('\u203f\uff65', ',') satisfies not(matches($s, '^(?:\\P{Pc}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
