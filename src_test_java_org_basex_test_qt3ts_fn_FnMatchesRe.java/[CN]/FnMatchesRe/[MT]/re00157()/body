{
  final XQuery query=new XQuery("(every $s in tokenize('\u301e\uff63', ',') satisfies matches($s, '^(?:\\p{Pe}*)$')) and (every $s in tokenize('??', ',') satisfies not(matches($s, '^(?:\\p{Pe}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
