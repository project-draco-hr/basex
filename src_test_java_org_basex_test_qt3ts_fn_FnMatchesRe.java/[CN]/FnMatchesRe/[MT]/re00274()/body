{
  final XQuery query=new XQuery("(every $s in tokenize('\u3000???,\u3000\u3001\u3002\u3003????????????\u3008\u3009\u300a\u300b\u300c\u300d\u300e\u300f\u3010\u3011??????\u3014\u3015\u3016\u3017\u3018\u3019\u301a\u301b\u301c\u301d\u301e\u301f????????????????????????????????????????????????\u3030????????????????????????????????????\u303d??????', ',') satisfies matches($s, '^(?:\\p{IsCJKSymbolsandPunctuation}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsCJKSymbolsandPunctuation}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
