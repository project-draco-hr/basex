{
  final XQuery query=new XQuery("(every $s in tokenize('\u0590\u05ff,\u0590??????????????????????????????????????????????????????????????????????????????????????????\u05be??\u05c0????\u05c3????\u05c6??\u05c8\u05c9\u05ca\u05cb\u05cc\u05cd\u05ce\u05cf??????????????????????????????????????????????????????\u05eb\u05ec\u05ed\u05ee\u05ef??????\u05f3\u05f4\u05f5\u05f6\u05f7\u05f8\u05f9\u05fa\u05fb\u05fc\u05fd\u05fe\u05ff', ',') satisfies matches($s, '^(?:\\p{IsHebrew}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsHebrew}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
