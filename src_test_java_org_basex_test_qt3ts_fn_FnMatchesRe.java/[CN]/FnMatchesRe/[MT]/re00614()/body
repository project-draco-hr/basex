{
  final XQuery query=new XQuery("matches('qwerty','(foo) (?#foo) \\s+ (?#followed by 1 or more whitespace')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FORX0002"));
}
