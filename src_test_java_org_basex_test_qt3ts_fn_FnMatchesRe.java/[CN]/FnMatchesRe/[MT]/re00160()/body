{
  final XQuery query=new XQuery("(every $s in tokenize('??\uff64', ',') satisfies matches($s, '^(?:\\p{Po}*)$')) and (every $s in tokenize('\u1680', ',') satisfies not(matches($s, '^(?:\\p{Po}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
