{
  final XQuery query=new XQuery("(every $s in tokenize('\u301d\uff62', ',') satisfies matches($s, '^(?:\\p{Ps}*)$')) and (every $s in tokenize('\u301e', ',') satisfies not(matches($s, '^(?:\\p{Ps}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
