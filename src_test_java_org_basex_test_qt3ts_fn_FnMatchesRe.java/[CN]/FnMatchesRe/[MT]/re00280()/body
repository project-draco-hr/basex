{
  final XQuery query=new XQuery("(every $s in tokenize('???\u31bf,?????????????????????????????????????????????????????????????????????????????????\u31bb\u31bc\u31bd\u31be\u31bf', ',') satisfies matches($s, '^(?:\\p{IsBopomofoExtended}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsBopomofoExtended}+)$')))",ctx);
