{
  final XQuery query=new XQuery("(every $s in tokenize('?????????????????????????????', ',') satisfies matches($s, '^(?:\\p{M}*)$')) and (every $s in tokenize('??', ',') satisfies not(matches($s, '^(?:\\p{M}*)$')))",ctx);
