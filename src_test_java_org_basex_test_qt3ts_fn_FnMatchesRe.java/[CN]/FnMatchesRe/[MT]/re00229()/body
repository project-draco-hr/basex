{
  final XQuery query=new XQuery("(every $s in tokenize('\u0980\u09ff,\u0980?????????\u0984????????????????????????\u098d\u098e??????\u0991\u0992??????????????????????????????????????????????????????????????????\u09a9?????????????????????\u09b1???\u09b3\u09b4\u09b5????????????\u09ba\u09bb???????????????????????????\u09c5\u09c6??????\u09c9\u09ca????????????\u09cf\u09d0\u09d1\u09d2\u09d3\u09d4\u09d5\u09d6???\u09d8\u09d9\u09da\u09db??????\u09de???????????????\u09e4\u09e5??????????????????????????????????????????????????????????????????\u09fc\u09fd\u09fe\u09ff', ',') satisfies matches($s, '^(?:\\p{IsBengali}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsBengali}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
