{
  final XQuery query=new XQuery("(every $s in tokenize('???\u209f,??????\u2072\u2073???????????????????????????\u207d\u207e??????????????????????????????????????????\u208d\u208e\u208f???????????????????????????????????????\u209d\u209e\u209f', ',') satisfies matches($s, '^(?:\\p{IsSuperscriptsandSubscripts}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsSuperscriptsandSubscripts}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
