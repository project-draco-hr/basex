{
  final XQuery query=new XQuery("(every $s in tokenize('???', ',') satisfies matches($s, '^(?:\\P{C}*)$')) and (every $s in tokenize('\t\u070f\u070f\udb40\udc78\ue000\ue000\udbc0\udc00\udb80\udc00\udbbf\udffd\udbff\udffd', ',') satisfies not(matches($s, '^(?:\\P{C}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
