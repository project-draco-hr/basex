{
  final XQuery query=new XQuery("(every $s in tokenize('\u0530\u058f,\u0530????????????????????????????????????????????????????????????????????????????\u0557\u0558??\u055a\u055b\u055c\u055d\u055e\u055f\u0560??????????????????????????????????????????????????????????????????????????????\u0588\u0589\u058a\u058b\u058c\u058d\u058e\u058f', ',') satisfies matches($s, '^(?:\\p{IsArmenian}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsArmenian}+)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
