{
  final XQuery query=new XQuery("(every $s in tokenize('??', ',') satisfies matches($s, '^(?:\\P{Pi}*)$')) and (every $s in tokenize('??\u2039', ',') satisfies not(matches($s, '^(?:\\P{Pi}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
