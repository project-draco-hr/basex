{
  final XQuery query=new XQuery("(every $s in tokenize('???', ',') satisfies matches($s, '^(?:\\P{N}*)$')) and (every $s in tokenize('??????????????????????????', ',') satisfies not(matches($s, '^(?:\\P{N}*)$')))",ctx);
