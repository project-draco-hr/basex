{
  final XQuery query=new XQuery("(every $s in tokenize('???', ',') satisfies matches($s, '^(?:\\P{Co}*)$')) and (every $s in tokenize('\ue000\udbc0\udc00\udb80\udc00\udbbf\udffd\udbff\udffd', ',') satisfies not(matches($s, '^(?:\\P{Co}*)$')))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
