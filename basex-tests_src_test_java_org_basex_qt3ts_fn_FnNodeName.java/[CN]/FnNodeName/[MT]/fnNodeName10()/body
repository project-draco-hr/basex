{
  final XQuery query=new XQuery("fn:local-name-from-QName(fn:node-name(element elementName { element achild {\"some text\"}}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"elementName"));
}
