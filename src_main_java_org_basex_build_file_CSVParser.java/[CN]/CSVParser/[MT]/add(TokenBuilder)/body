{
  if (row == 0 && !simple) {
    addHeader(tb.finish());
    tb.reset();
    return;
  }
  byte[] tag=simple ? ENTRY : headers.get(col);
  if (tag == null) {
    addHeader(COLUMN);
    tag=headers.get(col);
  }
  if (tb.size() != 0 || simple) {
    builder.startElem(tag,atts);
    builder.text(tb.finish());
    builder.endElem(tag);
    tb.reset();
  }
  ++col;
}
