{
  if (row == 0 && !simple) {
    addHeader(tb.finish());
    tb.reset();
    return;
  }
  final byte[] t;
  if (simple) {
    t=ENTRY;
  }
 else {
    if (col == headers.size())     addHeader(COLUMN);
    t=headers.get(col);
  }
  if (!tb.isEmpty() || simple) {
    builder.startElem(t,atts);
    builder.text(tb.finish());
    builder.endElem();
    tb.reset();
  }
  ++col;
}
