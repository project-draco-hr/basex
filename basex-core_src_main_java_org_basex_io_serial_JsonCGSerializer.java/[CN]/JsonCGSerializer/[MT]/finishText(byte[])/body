{
  final byte[] type=types.get(level - 1);
  if (eq(type,T_STRING)) {
    print('"');
    for (    final byte ch : text)     encode(ch);
    print('"');
  }
 else   if (eq(type,T_BOOLEAN,T_NUMBER)) {
    print(text);
  }
 else   if (trim(text).length != 0) {
    error("Element <%> is typed as \"%\" and cannot have a value",tags.get(level - 1),type);
  }
}
