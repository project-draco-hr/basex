def hasNext(self):
    if self.__open:
        self.__session.send(((('\x01' + self.__id) + '\x00') + '\x00'))
        if self.__session.check():
            self.__next = self.__session.res()
            return True
        else:
            if self.__session.check():
                raise IOError(self.__session.res())
            self.__open = False
            return False
