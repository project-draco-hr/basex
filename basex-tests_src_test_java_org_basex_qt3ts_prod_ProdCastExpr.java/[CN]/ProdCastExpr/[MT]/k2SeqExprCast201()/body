{
  final XQuery query=new XQuery("namespace-uri-from-QName(xs:QName(\"ncname\"))",ctx);
  try {
    query.namespace("","http://example.com/defelementns");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://example.com/defelementns"));
}
