{
  final XQuery query=new XQuery("\n" + "      declare function local:clarkname($q as xs:QName) as xs:string { \n" + "      concat('{', namespace-uri-from-QName($q), '}', local-name-from-QName($q)) \n"+ "      }; \n"+ "      local:clarkname(xs:untypedAtomic(\"xs:integer\"))\n"+ "    \n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0117"));
}
