{
  final XQuery query=new XQuery("\"START\", \n" + "      \tstring(QName(\"http://example.com\", \n" + "      \t\"prefix:localName\")), \n"+ "      \tstring(QName(\"http://example.com\", \"localName\")), \n"+ "      \tstring(QName(\"http://example.com\", \"localName\")), \n"+ "      \tstring(QName(\"\", \"localName\")), \n"+ "      \tstring(QName((), \"localName\")), \n"+ "      \t\"END\" ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("START prefix:localName localName localName localName localName END",false));
}
