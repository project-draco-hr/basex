{
  final XQuery query=new XQuery("let $var := xs:unsignedShort(120) cast as xs:unsignedInt return $var instance of xs:unsignedInt",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
