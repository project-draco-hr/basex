{
  final XQuery query=new XQuery("xs:untypedAtomic(\"an arbitrary string(untypedAtomic source)\") castable as xs:string",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
