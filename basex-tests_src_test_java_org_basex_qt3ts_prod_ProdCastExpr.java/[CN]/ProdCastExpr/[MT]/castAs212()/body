{
  final XQuery query=new XQuery("round-half-to-even(xs:double(\"-1.75e-3\") cast as xs:decimal,5)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-0.00175"));
}
