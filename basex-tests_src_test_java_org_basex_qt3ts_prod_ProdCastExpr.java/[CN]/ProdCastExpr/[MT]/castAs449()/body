{
  final XQuery query=new XQuery("xs:gYearMonth(\"-0012-12-05:00\") cast as xs:gYearMonth",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-0012-12-05:00"));
}
