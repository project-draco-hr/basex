{
  final XQuery query=new XQuery("\" white space left alone\" cast as xs:string eq xs:string(\" white space left alone\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
