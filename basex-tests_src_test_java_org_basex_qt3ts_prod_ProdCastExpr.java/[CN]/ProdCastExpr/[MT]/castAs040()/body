{
  final XQuery query=new XQuery("xs:untypedAtomic(\"2000-01-16T00:00:00Z\") cast as xs:dateTime",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2000-01-16T00:00:00Z"));
}
