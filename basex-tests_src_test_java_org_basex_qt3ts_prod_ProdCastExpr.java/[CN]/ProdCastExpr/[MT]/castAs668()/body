{
  final XQuery query=new XQuery("let $var := xs:unsignedInt(120) cast as xs:unsignedShort return $var instance of xs:unsignedShort",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
