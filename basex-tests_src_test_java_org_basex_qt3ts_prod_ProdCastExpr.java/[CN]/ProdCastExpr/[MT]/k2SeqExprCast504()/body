{
  final XQuery query=new XQuery("xs:NCName(codepoints-to-string((3648, 3592, 3617, 3626, 3660)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"???????????????"));
}
