{
  final XQuery query=new XQuery("xs:dateTime(\"-1999-05-31T13:20:00+14:00\") cast as xs:gMonthDay",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"--05-31+14:00"));
}
