{
  final XQuery query=new XQuery("xs:untypedAtomic(\"an arbitrary string(untypedAtomic source)\") cast as xs:QName",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
