{
  final XQuery query=new XQuery("let $var := xs:int(120) cast as xs:long return $var instance of xs:long",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
