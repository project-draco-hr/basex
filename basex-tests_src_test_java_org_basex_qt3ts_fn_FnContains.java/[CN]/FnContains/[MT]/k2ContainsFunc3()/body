{
  final XQuery query=new XQuery("let $vA  := (\"B STRING\", current-time(), string(\"content\"))[1] treat as xs:string,\n" + "                $vB := (\"no match\", current-time(), string(\"content\"))[1] treat as xs:string\n" + "         return contains(lower-case($vA), lower-case($vB))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
