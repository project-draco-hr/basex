{
switch (sig) {
case DOC:
    return doc(ctx);
case DOC_AVAILABLE:
  return docAvailable(ctx);
case UNPARSED_TEXT:
return unparsedText(ctx,false);
case UNPARSED_TEXT_AVAILABLE:
return unparsedText(ctx,true);
case PUT:
return put(ctx);
case PARSE_XML:
return parseXml(ctx,false);
case PARSE_XML_FRAGMENT:
return parseXml(ctx,true);
case SERIALIZE:
return serialize(ctx);
default :
return super.item(ctx,ii);
}
}
