{
  try {
    return Bln.get(doc(ctx) != null);
  }
 catch (  final QueryException ex) {
    final Err err=ex.err();
    if (err != null) {
      final String num=err.code.length() == 8 ? err.code.substring(4) : "";
      if (err.is(ErrType.FODC) && (num.equals("0002") || num.equals("0004")) || err.is(ErrType.BXDB) && num.equals("0006"))       return Bln.FALSE;
    }
    throw ex;
  }
}
