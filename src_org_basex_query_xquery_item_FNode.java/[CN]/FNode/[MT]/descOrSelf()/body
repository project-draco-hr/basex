{
  return new NodeIter(){
    /** 
 * Iterator. 
 */
    private NodeNext it;
    /** 
 * First call. 
 */
    private boolean more;
    @Override public Node next() throws XQException {
      if (!more) {
        final NodIter ch=new NodIter();
        ch.add(FNode.this);
        addDesc(child(),ch);
        it=new NodeNext(ch);
        more=true;
      }
      return it.next();
    }
  }
;
}
