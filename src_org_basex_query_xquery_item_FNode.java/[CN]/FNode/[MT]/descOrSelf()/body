{
  return new NodeIter(){
    /** 
 * Iterator. 
 */
    private NodIter it;
    /** 
 * First call. 
 */
    private boolean more;
    @Override public Node next() throws XQException {
      if (!more) {
        it=new NodIter();
        it.add(FNode.this);
        addDesc(child(),it);
        more=true;
      }
      return it.next();
    }
  }
;
}
