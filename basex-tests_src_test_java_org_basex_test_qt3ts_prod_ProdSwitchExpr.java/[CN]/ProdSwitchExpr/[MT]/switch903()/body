{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := 2; \n" + "        <out>{ switch ($in) default return \"Baa\" }</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
