{
  final XQuery query=new XQuery("xquery version \"3.0\"; \n" + "        declare variable $in := 25; \n" + "        declare variable $zero := 0; \n"+ "        <out>{ switch ($in) \n"+ "            case 42 return $in div $zero \n"+ "            case 25 return \"Baa\" \n"+ "            case 39 return $in div $zero \n"+ "            default return \"Woof\" }</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>Baa</out>",false));
}
