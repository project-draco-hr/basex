{
  tarjan(0);
  for (  final Scope[] comp : result) {
    if (comp.length > 1) {
      for (      final Scope scp : comp) {
        if (scp instanceof StaticVar) {
          throw Err.CIRCVAR.thrw(((StaticVar)scp).info,scp);
        }
      }
      comp[comp.length - 1].compile(ctx);
    }
 else {
      comp[0].compile(ctx);
    }
  }
}
