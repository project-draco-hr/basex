{
  final XQuery query=new XQuery("resolve-uri(\"http://www.example.com/\", \"relative/uri.ext\", \"wrong param\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0017"));
}
