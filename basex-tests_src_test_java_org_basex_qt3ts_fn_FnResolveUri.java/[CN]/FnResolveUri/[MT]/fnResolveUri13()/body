{
  final XQuery query=new XQuery("fn:string(fn:resolve-uri(\"examples\",fn:concat(\"http://www.example\",\".com/\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/examples"));
}
