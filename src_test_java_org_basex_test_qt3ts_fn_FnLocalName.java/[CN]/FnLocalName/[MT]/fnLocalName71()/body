{
  final XQuery query=new XQuery("string-join((fn:local-name(./works[1]/employee[1]),fn:local-name(./works[1]/employee[2])),\n" + "         ' ')",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"employee employee"));
}
