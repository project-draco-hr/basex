{
  final XQuery query=new XQuery("declare namespace p1 = \"http://example.org\"; \n" + "        fn:string(fn:local-name(attribute p1:anAttribute {\"Attribute Value\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"anAttribute"));
}
