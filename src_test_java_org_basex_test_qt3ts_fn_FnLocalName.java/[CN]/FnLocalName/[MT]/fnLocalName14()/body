{
  final XQuery query=new XQuery("fn:string(fn:local-name(processing-instruction PITarget {\"PIcontent\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"PITarget"));
}
