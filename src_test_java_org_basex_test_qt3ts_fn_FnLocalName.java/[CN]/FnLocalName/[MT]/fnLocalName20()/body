{
  final XQuery query=new XQuery("fn:lower-case(fn:string(fn:local-name(<anElement>Some content</anElement>)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"anelement"));
}
