{
  final XQuery query=new XQuery("for $h in (./works/employee[2]) return\n" + "         fn:local-name($h/descendant-or-self::empnum[position() = 1])",ctx);
  query.context(node(file("docs/works-mod.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"empnum"));
}
