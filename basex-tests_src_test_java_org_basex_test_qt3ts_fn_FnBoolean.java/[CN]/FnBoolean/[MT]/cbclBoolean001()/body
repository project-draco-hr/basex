{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:generate($count as xs:integer) {\n" + "      \t\tif ($count < 0) then \"string\" \n"+ "      \t\telse for $x in 1 to $count \n"+ "      \t\t\t return \n"+ "      \t\t\t \tif ($x mod 3 = 0) then <a /> \n"+ "      \t\t\t \telse if ($x mod 3 = 1) then <b /> \n"+ "      \t\t\t \telse <c /> \n"+ "      \t}; \n"+ "      \tfn:boolean(fn:reverse( local:generate(5) ))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
