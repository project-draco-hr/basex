{
  int s=find(pre);
  if (s == -1 || children[s].pr != pre)   ++s;
  final int upper=pre + size;
  int num=0;
  for (int i=s; i < sz && children[i].pr < upper; ++i, ++num)   ;
  sz-=num;
  if (sz == 0)   children=new NSNode[0];
 else   if (num > 0)   System.arraycopy(children,s + num,children,s,sz - s);
}
