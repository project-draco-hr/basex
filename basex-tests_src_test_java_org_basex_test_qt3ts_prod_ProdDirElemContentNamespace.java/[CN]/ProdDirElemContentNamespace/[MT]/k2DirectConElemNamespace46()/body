{
  final XQuery query=new XQuery("let $i := <e xml:space=\"preserve\"/> return <a>{$i/@*}</a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a xml:space=\"preserve\"/>",false));
}
