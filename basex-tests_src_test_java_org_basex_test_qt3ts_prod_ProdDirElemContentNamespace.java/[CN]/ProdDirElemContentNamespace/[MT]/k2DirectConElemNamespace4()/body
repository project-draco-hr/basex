{
  final XQuery query=new XQuery("declare namespace p = \"http://example.com/QuiteWeirdNamespace\"; empty(p:e[1])",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPDY0002")));
}
