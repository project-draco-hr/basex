{
  final XQuery query=new XQuery("\n" + "      import module namespace module = \"http://www.w3.org/XQueryTest/RequireProhibitFeature\";\n" + "      module:one()()\n"+ "    ",ctx);
  try {
    query.addModule("http://www.w3.org/XQueryTest/RequireProhibitFeature",file("prod/RequireProhibitFeature/requires.xquery"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
