{
  final XQuery query=new XQuery("\n" + "      declare namespace example = \"http://www.example.com/does/not/exist\";\n" + "      declare option require-feature \"example:does-not-exist\";\n"+ "      ()\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0123"));
}
