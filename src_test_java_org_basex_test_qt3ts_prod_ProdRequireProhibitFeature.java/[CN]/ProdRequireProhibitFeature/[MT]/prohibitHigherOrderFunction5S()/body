{
  final XQuery query=new XQuery("\n" + "      declare option prohibit-feature \"higher-order-function\";\n" + "      for $f in () return $f(1, ?)\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XQST0129") || error("XQST0128")));
}
