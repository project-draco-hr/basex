{
  final XQuery query=new XQuery("\n" + "      import module namespace module = \"http://www.w3.org/XQueryTest/RequireProhibitFeature\";\n" + "      declare option require-feature \"higher-order-function\";\n"+ "      let $f := () return $f()\n"+ "    ",ctx);
  try {
    query.addModule("http://www.w3.org/XQueryTest/RequireProhibitFeature",file("prod/RequireProhibitFeature/prohibits.xquery"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("1") || error("XQST0128")));
}
