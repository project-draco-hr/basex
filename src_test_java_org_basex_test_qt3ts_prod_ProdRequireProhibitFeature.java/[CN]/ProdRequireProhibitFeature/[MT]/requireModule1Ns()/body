{
  final XQuery query=new XQuery("\n" + "      import module namespace m = \"http://www.w3.org/XQueryTest/RequireProhibitFeature\";\n" + "      declare option require-feature \"module\";\n"+ "      ()\n"+ "    ",ctx);
  try {
    query.addModule("http://www.w3.org/XQueryTest/RequireProhibitFeature",file("prod/RequireProhibitFeature/dummy.xquery"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0120"));
}
