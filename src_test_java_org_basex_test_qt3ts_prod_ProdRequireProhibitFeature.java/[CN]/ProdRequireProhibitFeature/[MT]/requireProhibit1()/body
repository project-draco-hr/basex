{
  final XQuery query=new XQuery("\n" + "      declare option require-feature \"schema-aware\";\n" + "      declare option prohibit-feature \"schema-aware\";\n"+ "      ()\n"+ "    ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0127"));
}
