{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:generate($count as xs:integer) {\n" + "      \t\tfor $x in 1 to $count \n"+ "      \t\treturn \n"+ "      \t\t\tif ($x mod 3 = 0) then <a /> \n"+ "      \t\t\telse if ($x mod 3 = 1) then <b /> \n"+ "      \t\t\telse <c /> \n"+ "      \t}; \n"+ "      \tfn:boolean(fn:count( local:generate(5) ))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
