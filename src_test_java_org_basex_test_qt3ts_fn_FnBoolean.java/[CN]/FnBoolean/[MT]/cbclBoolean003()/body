{
  final XQuery query=new XQuery("declare function local:f() { (<a/>, \"a\") }; boolean(local:f())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FORG0006")));
}
