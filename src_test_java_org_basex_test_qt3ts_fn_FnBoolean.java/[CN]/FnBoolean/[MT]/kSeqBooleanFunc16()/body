{
  final XQuery query=new XQuery("true() eq boolean(remove((xs:hexBinary(\"FF\"), 1), 1) treat as xs:integer)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
