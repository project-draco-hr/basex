{
  final Data data=gui.context.data();
  if (data == null || !data.meta.pathindex)   return;
  StringList sl=null;
  try {
    final QuerySuggest qs=new QuerySuggest(query,new QueryContext(gui.context),data);
    qs.parse(null,null);
    sl=qs.complete();
    pre=query.substring(0,qs.qm);
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    pre=query.substring(0,ex.col() - (ex.col() == 1 ? 1 : 0));
  }
  if (getCaretPosition() < pre.length())   sl=null;
  createCombo(sl);
}
