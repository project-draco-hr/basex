{
  StringList sl=null;
  final boolean excl=query.startsWith("!");
  try {
    pre=excl ? "!" : "";
    final String suf=getText().substring(pre.length());
    new CommandParser(suf,gui.context).parse();
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    if (ex.col() > 1) {
      pre=query;
    }
  }
  createCombo(sl);
}
