{
  StringList sl=null;
  final boolean excl=query.startsWith("!");
  try {
    pre=excl ? "!" : "";
    final String suf=getText().substring(pre.length());
    new CommandParser(suf,gui.context).parse(true);
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    if (ex.col2() > 0)     pre=query.substring(0,ex.col2());
  }
  createCombo(sl);
}
