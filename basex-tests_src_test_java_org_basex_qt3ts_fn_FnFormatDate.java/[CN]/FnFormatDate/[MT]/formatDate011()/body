{
  final XQuery query=new XQuery("\n" + "        for $i in 1 to 48, $d in $t + xs:dayTimeDuration('P3D')*$i \n" + "        return concat(\"[\", $d, \":\", format-date($d, '[w]', (), 'ISO', ()), ']')",ctx);
  try {
    query.bind("t",new XQuery("xs:date('2005-15-01')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n            [2005-15-04:1] [2005-15-07:2] [2005-15-10:2] [2005-15-13:3] [2005-15-16:3] [2005-15-19:4] [2005-15-22:4] \n            [2005-15-25:4] [2005-15-28:5] [2005-15-31:5] [2006-01-03:1] [2006-01-06:1] [2006-01-09:2] [2006-01-12:2] \n            [2006-01-15:2] [2006-01-18:3] [2006-01-21:3] [2006-01-24:4] [2006-01-27:4] [2006-01-30:5] [2006-02-02:1] \n            [2006-02-05:1] [2006-02-08:2] [2006-02-11:2] [2006-02-14:3] [2006-02-17:3] [2006-02-20:4] [2006-02-23:4] \n            [2006-02-26:4] [2006-03-01:1] [2006-03-04:1] [2006-03-07:2] [2006-03-10:2] [2006-03-13:3] [2006-03-16:3] \n            [2006-03-19:3] [2006-03-22:4] [2006-03-25:4] [2006-03-28:5] [2006-03-31:5] [2006-04-03:1] [2006-04-06:1] \n            [2006-04-09:1] [2006-04-12:2] [2006-04-15:2] [2006-04-18:3] [2006-04-21:3] [2006-04-24:4]            \n         "));
}
