{
  final XQuery query=new XQuery("string-join( for $z in -28 to +28 return format-date(adjust-date-to-timezone($t,\n" + "         $z*xs:dayTimeDuration('PT30M')), '[Z99]'), '; ')",ctx);
  try {
    query.bind("t",new XQuery("xs:date('1985-03-01Z')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n         \t-14; -13:30; -13; -12:30; -12; -11:30; -11; -10:30; -10; -09:30; -09; -08:30; -08;\n         \t-07:30; -07; -06:30; -06; -05:30; -05; -04:30; -04; -03:30; -03; -02:30; -02; -01:30;\n         \t-01; -00:30; +00; +00:30; +01; +01:30; +02; +02:30; +03; +03:30; +04; +04:30; +05;\n         \t+05:30; +06; +06:30; +07; +07:30; +08; +08:30; +09; +09:30; +10; +10:30; +11; +11:30;\n         \t+12; +12:30; +13; +13:30; +14\n\t\t "));
}
