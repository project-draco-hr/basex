{
  final XQuery query=new XQuery("string-join( for $z in -28 to +28 return format-date(adjust-date-to-timezone($t,\n" + "         $z*xs:dayTimeDuration('PT30M')), '[Z0t]'), '; ')",ctx);
  try {
    query.bind("t",new XQuery("xs:date('1985-03-01Z')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"\n         \t-14; -13:30; -13; -12:30; -12; -11:30; -11; -10:30; -10; -9:30; -9; -8:30; -8;\n         \t-7:30; -7; -6:30; -6; -5:30; -5; -4:30; -4; -3:30; -3; -2:30; -2; -1:30; -1; -0:30;\n         \tZ; +0:30; +1; +1:30; +2; +2:30; +3; +3:30; +4; +4:30; +5; +5:30; +6; +6:30; +7;\n         \t+7:30; +8; +8:30; +9; +9:30; +10; +10:30; +11; +11:30; +12; +12:30; +13;\n         \t+13:30; +14\n\t\t "));
}
