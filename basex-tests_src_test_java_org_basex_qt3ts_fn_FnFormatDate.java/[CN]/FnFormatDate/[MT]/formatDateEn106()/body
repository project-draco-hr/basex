{
  final XQuery query=new XQuery("\n" + "        for $i in 1 to 12 \n" + "        return let $d2 := $d + xs:yearMonthDuration('P1M')*$i \n"+ "        return format-date($d2, '[MNn,3-3]', 'en', (), ()) \n"+ "      ",ctx);
  try {
    query.bind("d",new XQuery("xs:date('2003-12-07')",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(true,"Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec"));
}
