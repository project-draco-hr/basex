{
  try {
    final List<BXResource> dbs=new ArrayList<BXResource>();
    final Session s=session.login();
    try {
      for (      final String d : listDBs(s))       dbs.add(new BXDatabase(d,session));
      return dbs;
    }
  finally {
      s.close();
    }
  }
 catch (  final Exception ex) {
    Util.errln(ex);
  }
  return null;
}
