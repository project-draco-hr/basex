{
  String ar=token.toString().trim();
  int l=ar.length();
  int c=l == 0 ? 0 : ar.charAt(0);
  if (c == '"' || c == '\'')   ar=ar.substring(1);
  l=ar.length();
  c=l == 0 ? 0 : ar.charAt(l - 1);
  if (c == '"' || c == '\'')   ar=ar.substring(0,--l);
  if (l != 0 || q)   list.add(ar);
  token.setLength(0);
}
