{
  final XQuery query=new XQuery("let $var := fn:min((xs:long(22),xs:short(10))) return $var instance of xs:integer",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
