{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) { if ($x = 0) then false() else true() }; \n" + "      \tmin(for $x in (1,2,3) return local:f($x))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
