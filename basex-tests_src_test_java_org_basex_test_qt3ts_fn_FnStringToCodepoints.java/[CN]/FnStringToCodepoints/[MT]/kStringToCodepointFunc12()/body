{
  final XQuery query=new XQuery("deep-equal(string-to-codepoints(\"Th??r??se\"), (84, 104, 233, 114, 232, 115, 101))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
