{
  query.confirm();
  final boolean max=getExtendedState() == MAXIMIZED_BOTH;
  prop.set(GUIProp.MAXSTATE,max);
  if (!max) {
    prop.set(GUIProp.GUILOC,new int[]{getX(),getY()});
    prop.set(GUIProp.GUISIZE,new int[]{getWidth(),getHeight()});
  }
  super.dispose();
  prop.write();
  context.close();
}
