{
  QueryProcess tmp=null;
  int t=Integer.valueOf(in.readString());
  for (  QueryProcess q : queries) {
    if (q.id == t) {
      tmp=q;
      break;
    }
  }
  if (tmp == null) {
    send(false);
  }
 else {
    if (in.readByte() == 1) {
      tmp.close();
    }
 else {
      tmp.more();
    }
  }
}
