{
  for (  final QueryProcess q : queries.values()) {
    try {
      q.close(true);
    }
 catch (    final IOException ex) {
    }
  }
  if (triggers.size() > 0) {
    context.triggers.remove(this,triggers);
  }
  try {
    new Close().execute(context);
    if (cmd != null)     cmd.stop();
    context.delete(this);
    socket.close();
  }
 catch (  final Exception ex) {
    log.write(ex.getMessage());
    Util.stack(ex);
  }
}
