{
  for (  final QueryProcess q : queries.values()) {
    try {
      q.close();
    }
 catch (    final IOException ex) {
    }
  }
  new Close().exec(context);
  if (proc != null)   proc.stop();
  context.delete(this);
  try {
    socket.close();
  }
 catch (  final IOException ex) {
    log.write(ex.getMessage());
    ex.printStackTrace();
  }
}
