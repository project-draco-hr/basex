{
  for (Object x; (x=queue.poll()) != null; ) {
    final BucketEntry e=(BucketEntry)x;
    final int i=indexFor(e.hash,buckets.length);
    BucketEntry prev=buckets[i];
    BucketEntry p=prev;
    while (p != null) {
      final BucketEntry next=p.next;
      if (p == e) {
        delete(i,e,prev,next);
        break;
      }
      prev=p;
      p=next;
    }
  }
}
