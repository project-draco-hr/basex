{
  byte[] type=types.get(level - 1);
  if (eq(type,STR)) {
    print('"');
    for (    final byte ch : text)     code(ch);
    print('"');
  }
 else   if (eq(type,BOOL,NUM)) {
    print(text);
  }
 else   if (trim(text).length != 0) {
    error("Element <%> is typed as \"%\" and cannot have a value",tags.get(level - 1),type);
  }
}
