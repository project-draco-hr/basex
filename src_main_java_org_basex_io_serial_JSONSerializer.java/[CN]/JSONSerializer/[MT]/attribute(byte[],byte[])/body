{
  if (level == 0) {
    final int tl=typeCache.length;
    for (int t=0; t < tl; t++) {
      if (eq(name,ATTRS[t])) {
        for (        final byte[] b : split(value,' '))         typeCache[t].add(b);
        return;
      }
    }
  }
  if (eq(name,TYPE)) {
    types.set(level,value);
    if (!eq(value,TYPES))     error("Element <%> has invalid type \"%\"",tag,value);
  }
 else {
    error("Element <%> has invalid attribute \"%\"",tag,name);
  }
}
