{
switch (ch) {
case '\b':
    print("\\b");
  break;
case '\f':
print("\\f");
break;
case '\n':
print("\\n");
break;
case '\r':
print("\\r");
break;
case '\t':
print("\\t");
break;
case '"':
print("\\\"");
break;
case '/':
print("\\/");
break;
case '\\':
print("\\\\");
break;
default :
print(ch);
break;
}
}
