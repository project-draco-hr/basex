{
  final long id=++meta.lastid;
  return new byte[]{(byte)(as << 3 | ELEM),(byte)((ne ? 1 << 7 : 0) | (byte)(tn >> 8)),(byte)tn,(byte)u,(byte)(dis >> 24),(byte)(dis >> 16),(byte)(dis >> 8),(byte)dis,(byte)(s >> 24),(byte)(s >> 16),(byte)(s >> 8),(byte)s,(byte)(id >> 24),(byte)(id >> 16),(byte)(id >> 8),(byte)id};
}
