{
  final long i=++meta.lastid;
  final int n=ne ? 1 << 7 : 0;
  b.s(as << 3 | ELEM);
  b.s(n | (byte)(tn >> 8));
  b.s(tn);
  b.s(u);
  b.s(d >> 24);
  b.s(d >> 16);
  b.s(d >> 8);
  b.s(d);
  b.s(s >> 24);
  b.s(s >> 16);
  b.s(s >> 8);
  b.s(s);
  b.s(i >> 24);
  b.s(i >> 16);
  b.s(i >> 8);
  b.s(i);
}
