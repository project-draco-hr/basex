{
  final long len=index(val,pre,false);
  final long id=++meta.lastid;
  table.insert(pre,new byte[]{(byte)(dis << 3 | ATTR),(byte)(name >> 8),(byte)name,(byte)(len >> 32),(byte)(len >> 24),(byte)(len >> 16),(byte)(len >> 8),(byte)len,0,0,0,(byte)u,(byte)(id >> 24),(byte)(id >> 16),(byte)(id >> 8),(byte)id});
}
