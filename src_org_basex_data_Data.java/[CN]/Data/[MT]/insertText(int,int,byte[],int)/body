{
  final long id=++meta.lastid;
  final long len=index(val,pre,true);
  table.insert(pre,new byte[]{(byte)k,0,0,(byte)(len >> 32),(byte)(len >> 24),(byte)(len >> 16),(byte)(len >> 8),(byte)len,(byte)(dis >> 24),(byte)(dis >> 16),(byte)(dis >> 8),(byte)dis,(byte)(id >> 24),(byte)(id >> 16),(byte)(id >> 8),(byte)id});
}
