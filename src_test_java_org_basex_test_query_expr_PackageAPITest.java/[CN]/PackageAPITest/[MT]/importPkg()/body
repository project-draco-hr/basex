{
  final QueryProcessor qp1=new QueryProcessor("import module namespace ns3='ns3'; ns3:test()",ctx);
  assertEquals(qp1.execute().toString(),"pkg2mod2");
  qp1.execute();
  final QueryProcessor qp2=new QueryProcessor("import module namespace ns2='ns2'; ns2:test()",ctx);
  assertEquals(qp2.execute().toString(),"pkg2mod2");
  qp2.execute();
}
