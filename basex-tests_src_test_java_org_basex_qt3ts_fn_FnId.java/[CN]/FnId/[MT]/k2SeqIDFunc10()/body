{
  final XQuery query=new XQuery("for $i in id((\"short\", \"positiveInteger\")) return $i/@name/string()",ctx);
  try {
    query.context(node(file("fn/id/UsingXMLId.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"positiveInteger short"));
}
