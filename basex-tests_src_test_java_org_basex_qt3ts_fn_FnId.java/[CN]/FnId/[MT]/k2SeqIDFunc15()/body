{
  final XQuery query=new XQuery("fn:id(string-join(reverse(//b/@ref), '\t'))/data(exactly-one(@*))",ctx);
  try {
    query.context(node(file("fn/id/XMLIDMany.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"a b c d e f i"));
}
