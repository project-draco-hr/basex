{
  tb.add(LI_ENTRIES + size + NL);
  int m=0;
  int c=0;
  for (int o=0; o < max; ++o) {
    int tl=txtMin[o].length;
    if (tl == 0)     ++c;
 else     if (m < tl)     m=tl;
    tl=txtMax[o].length;
    if (tl == 0)     ++c;
 else     if (m < tl)     m=tl;
  }
  print(tb,txtMax,occMax,m + 2);
  if (c == 0)   tb.add("  " + DOTS + NL);
  print(tb,txtMin,occMin,m + 2);
}
