{
  final Expr ex=super.comp(ctx);
  if (ex != this)   return ex;
  if (fl.length == 1 && where == null && fl[0].simple() && ret instanceof VarRef) {
    final Var v=((VarRef)ret).var;
    if (v.type == null && fl[0].var.eq(v)) {
      ctx.compInfo(OPTFLWOR);
      return fl[0].expr;
    }
  }
  return this;
}
