{
  final SingleParser p;
  final MainParser mp=options.get(MainOptions.PARSER);
switch (mp) {
case HTML:
    p=new HtmlParser(in,options);
  break;
case TEXT:
p=new TextParser(in,options);
break;
case JSON:
p=new JsonParser(in,options);
break;
case CSV:
p=new CsvParser(in,options);
break;
default :
p=xmlParser(in,options);
break;
}
p.target(target);
return p;
}
