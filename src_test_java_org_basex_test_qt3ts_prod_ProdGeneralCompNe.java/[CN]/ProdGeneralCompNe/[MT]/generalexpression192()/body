{
  final XQuery query=new XQuery("($works/works/employee[1]/hours[1],$staff/staff/employee[6]/hours[1]) != (10000,50000)",ctx);
  query.bind("$works",node(file("docs/works.xml")));
  query.bind("$staff",node(file("docs/staff.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
