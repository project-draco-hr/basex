{
  final XQuery query=new XQuery("let $in := <a><b>ABC</b><b>XYZ</b></a> return $in//string-to-codepoints(.)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"65 66 67 88 89 90 65 66 67 65 66 67 88 89 90 88 89 90"));
}
