{
  final XQuery query=new XQuery("for $h in (/works) return $h/*/hours/string()",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"40 70 20 80 20 40 20 30 12 40 80 20 20 20 40 80"));
}
