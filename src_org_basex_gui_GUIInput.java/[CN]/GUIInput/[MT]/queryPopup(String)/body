{
  StringList sl=null;
  try {
    final QuerySuggest parser=new QuerySuggest(new QueryContext());
    parser.init(query);
    parser.parse(null,true);
    sl=parser.complete();
    pre=query.substring(0,xPos(query) + 1);
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    pre=query.substring(0,Math.min(query.length(),ex.col()));
  }
  createCombo(sl);
}
