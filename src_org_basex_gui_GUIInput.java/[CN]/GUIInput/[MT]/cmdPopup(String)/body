{
  StringList sl=new StringList();
  try {
    pre=query.startsWith("!") ? "!" : "";
    final String suf=getText().substring(pre.length());
    new CommandParser(suf).parse();
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    pre=query.substring(0,ex.col() - 1);
  }
  createCombo(sl);
}
