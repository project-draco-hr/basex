{
  final Context ctx=GUI.context;
  StringList sl=null;
  try {
    final XPParser parser=new XPParser(query,ctx.current());
    parser.parse();
    sl=parser.complete();
    pre=query.substring(0,xPos(query) + 1);
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    pre=query.substring(0,ex.col() - 1);
  }
  createCombo(sl);
}
