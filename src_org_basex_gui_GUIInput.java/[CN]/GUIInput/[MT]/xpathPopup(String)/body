{
  StringList sl=null;
  try {
    final XPSuggest parser=new XPSuggest(query,GUI.context);
    parser.parse();
    sl=parser.complete();
    pre=query.substring(0,xPos(query) + 1);
  }
 catch (  final QueryException ex) {
    sl=ex.complete();
    pre=query.substring(0,Math.min(query.length(),ex.col()));
  }
  createCombo(sl);
}
