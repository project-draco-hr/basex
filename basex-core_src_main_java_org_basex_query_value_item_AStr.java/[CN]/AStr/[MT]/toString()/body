{
  try {
    final ByteList tb=new ByteList();
    tb.add('"');
    for (    final byte v : string(null)) {
      if (v == '&')       tb.add(E_AMP);
 else       if (v == '\r')       tb.add(E_0D);
 else       if (v == '\n')       tb.add(E_0A);
 else       tb.add(v);
      if (v == '"')       tb.add('"');
    }
    return tb.add('"').toString();
  }
 catch (  final QueryException ex) {
    Util.debug(ex);
    return "";
  }
}
