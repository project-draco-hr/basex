{
  final XQuery query=new XQuery("fn:unparsed-text-lines(\"http://www.w3.org/fots/unparsed-text/invalid-xml.xml\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertDeepEq("'<?xml version=\"1.0\" encoding=\"utf-16\"?><text>hello??world'") || error("FOUT1200")));
}
