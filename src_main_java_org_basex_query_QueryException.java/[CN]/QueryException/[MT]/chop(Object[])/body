{
  for (int i=0; i < t.length; i++) {
    if (t[i] instanceof byte[]) {
      t[i]=Token.string((byte[])t[i]);
    }
 else     if (!(t[i] instanceof String)) {
      t[i]=t[i].toString();
    }
    final String s=t[i].toString();
    t[i]=s.length() > 256 ? s.substring(0,256) + DOTS : s;
  }
  return t;
}
