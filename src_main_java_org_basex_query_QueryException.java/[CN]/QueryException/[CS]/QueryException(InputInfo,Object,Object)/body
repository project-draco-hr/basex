{
  super(Main.info(s,chop(e)));
  if (ii == null)   return;
  line=1;
  col=1;
  final int qp=Math.min(ii.pos - 1,ii.query.length());
  for (int i=0; i < qp; ++i) {
    final char ch=ii.query.charAt(i);
    if (ch == 0x0A) {
      line++;
      col=1;
    }
 else     if (ch != 0x0D) {
      col++;
    }
  }
}
