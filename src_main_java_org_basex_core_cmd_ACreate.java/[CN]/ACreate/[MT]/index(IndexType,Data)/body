{
  if (d instanceof MemData)   return;
  IndexBuilder b=null;
switch (i) {
case TEXT:
    b=new ValueBuilder(d,true);
  break;
case ATTV:
b=new ValueBuilder(d,false);
break;
case FTXT:
b=FTBuilder.get(d,d.meta.wildcards);
break;
case PATH:
b=new PathBuilder(d);
break;
default :
break;
}
d.closeIndex(i);
d.meta.dirty=true;
progress(b);
d.setIndex(i,b.build());
}
