{
  xquery10();
  final XQuery query=new XQuery("xquery version \"1.0\" encoding \"US-ASCII\"; 1,2",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2"));
}
