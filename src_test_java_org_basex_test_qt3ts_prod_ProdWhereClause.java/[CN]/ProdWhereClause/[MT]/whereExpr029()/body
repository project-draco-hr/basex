{
  final XQuery query=new XQuery("\n" + "         let $x := 1 to 10\n" + "         where count($x) = 1\n"+ "         return $x\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
