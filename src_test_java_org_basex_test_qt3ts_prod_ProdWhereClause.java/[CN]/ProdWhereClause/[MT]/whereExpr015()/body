{
  final XQuery query=new XQuery("<empty> { for $file in (//Folder)[1]/File where false() return $file/FileName } </empty>",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<empty/>",false) || error("XPST0005")));
}
