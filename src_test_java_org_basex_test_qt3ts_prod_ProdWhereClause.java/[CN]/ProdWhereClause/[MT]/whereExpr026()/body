{
  final XQuery query=new XQuery("\n" + "         let $doc := (/)\n" + "         return (11 to 15)!\n"+ "            (for $file in $doc//File\n"+ "            count $c\n"+ "            where $c = position()\n"+ "            return $file/FileName)\n"+ "      ",ctx);
  query.context(node(file("prod/ForClause/fsx.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"File00000000000 File00000000001 File00000000002 File00000000003 File00000000004"));
}
