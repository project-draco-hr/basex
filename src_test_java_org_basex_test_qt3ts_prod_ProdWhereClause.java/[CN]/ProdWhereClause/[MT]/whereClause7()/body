{
  final XQuery query=new XQuery("let $var := \"String\" where fn:string-length($var) = 6 return fn:string-length($var)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("6"));
}
