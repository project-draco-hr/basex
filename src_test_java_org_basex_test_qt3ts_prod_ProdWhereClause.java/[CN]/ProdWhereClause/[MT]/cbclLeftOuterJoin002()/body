{
  final XQuery query=new XQuery("<e> { for $x in 1 to 10 return <a>{for $y in 1 to 10 where $x > 7 and $y = $x return $y}</a> } </e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e><a/><a/><a/><a/><a/><a/><a/><a>8</a><a>9</a><a>10</a></e>",false));
}
