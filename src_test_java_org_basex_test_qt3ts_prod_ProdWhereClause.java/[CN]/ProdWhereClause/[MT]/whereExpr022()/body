{
  final XQuery query=new XQuery("\n" + "         for $file in //File \n" + "         let $namelen := string-length($file/FileName)\n"+ "         where ($namelen = 15 and $file//Allow/user = 'system\\Hyungjeong') \n"+ "         return $file/FileName\n"+ "      ",ctx);
  query.context(node(file("prod/ForClause/fsx.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"File00000000000"));
}
