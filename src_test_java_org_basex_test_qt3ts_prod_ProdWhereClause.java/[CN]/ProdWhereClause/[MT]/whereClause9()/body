{
  final XQuery query=new XQuery("let $var := <anElement>Some content</anElement> where $var is $var return $var is $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
