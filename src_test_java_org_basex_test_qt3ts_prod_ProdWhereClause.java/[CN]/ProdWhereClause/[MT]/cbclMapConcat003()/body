{
  final XQuery query=new XQuery("declare function local:f($x) { if ($x) then true() else () }; for $x in local:f(true()), $y in 1 to 10 return ($x, $y)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"true 1 true 2 true 3 true 4 true 5 true 6 true 7 true 8 true 9 true 10"));
}
