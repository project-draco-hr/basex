{
  final XQuery query=new XQuery("\n" + "         declare variable $n external := 10;\n" + "         for $x allowing empty in 1 to $n\n"+ "         where $x lt 5\n"+ "         return $x\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"1 2 3 4"));
}
