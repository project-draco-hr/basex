{
  final XQuery query=new XQuery("for $d in /MyComputer/Drive4 where $d/Folder[@id=\"128\"] return <FolderName>{$d/Folder/@name}</FolderName>",ctx);
  query.context(node(file("prod/ForClause/fsx.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<FolderName name=\"Folder00000000047\" />",false));
}
