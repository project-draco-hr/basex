{
  final XQuery query=new XQuery("\n" + "         for $x allowing empty in 1 to 5\n" + "         where $x lt 0\n"+ "         return empty($x)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEmpty());
}
