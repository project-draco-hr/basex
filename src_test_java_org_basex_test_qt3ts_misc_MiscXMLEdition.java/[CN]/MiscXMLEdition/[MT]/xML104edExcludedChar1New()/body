{
  final XQuery query=new XQuery("xquery version \"1.0\" encoding \"utf-8\"; <\u037fnode/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<\u037fnode/>",false));
}
