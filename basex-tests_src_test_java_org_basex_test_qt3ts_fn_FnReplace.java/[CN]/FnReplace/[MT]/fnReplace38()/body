{
  final XQuery query=new XQuery("fn:replace(\"abracadabra\", \"(a)\", \"\\$$1\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"$abr$ac$ad$abr$a"));
}
