{
  final XQuery query=new XQuery("fn:replace(\"abracadabraabracadabraabracadabra\", \"aa{1,2}\",\"with\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"abracadabrwithbracadabrwithbracadabra"));
}
