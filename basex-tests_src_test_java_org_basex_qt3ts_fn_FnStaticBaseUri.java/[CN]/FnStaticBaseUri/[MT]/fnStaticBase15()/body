{
  final XQuery query=new XQuery("static-base-uri()",ctx);
  try {
    query.baseURI("http://www.example.com");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com"));
}
