{
  final XQuery query=new XQuery("declare base-uri \"tel:+1-816-555-1212\"; fn:string(fn:static-base-uri())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"tel:+1-816-555-1212"));
}
