{
  InputStream bais=new ByteArrayInputStream("<xml>Hello World!</xml>".getBytes());
  session1.create("database",bais);
  bais=new ByteArrayInputStream("<xml>Hello World!</xml>".getBytes());
  session1.create("database2",bais);
  session1.execute("drop db database");
  bais=new ByteArrayInputStream("<xml>Hello World!</xml>".getBytes());
  session1.create("database2",bais);
  session1.execute("drop db database2");
}
