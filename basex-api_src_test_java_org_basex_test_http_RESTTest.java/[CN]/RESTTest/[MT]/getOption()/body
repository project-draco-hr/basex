{
  assertEquals("2",get("?query=switch(1)+case+1+return+2+default+return+3&" + Options.XQUERY3.key + "=true"));
  try {
    get("?query=switch(1)+case+1+return+2+default+return+3&" + Options.XQUERY3.key + "=false");
    fail("Error expected.");
  }
 catch (  final IOException ex) {
    assertContains(ex.getMessage(),"[XPST0003]");
  }
}
