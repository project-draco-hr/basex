{
  final NodeCache s=new NodeCache();
  ANode i=n.next();
  while (i != null) {
    if (i.type == NodeType.TXT) {
      final TokenBuilder tb=new TokenBuilder();
      while (i != null && i.type == NodeType.TXT) {
        tb.add(i.atom());
        i=n.next();
      }
      s.add(new FTxt(tb.finish(),null));
    }
 else {
      s.add(i);
      i=n.next();
    }
  }
  return s;
}
