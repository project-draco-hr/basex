{
  final XQuery query=new XQuery("\n" + "        for $i in 1 to 24 return\n" + "        format-dateTime($t + xs:dayTimeDuration('PT1H')*$i, '[h].[m]')",ctx);
  query.bind("t",new XQuery("xs:dateTime('2003-09-07T09:15:06.456')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"10.15 11.15 12.15 1.15 2.15 3.15 4.15 5.15 6.15 7.15 8.15 9.15 10.15 11.15 12.15 \n         1.15 2.15 3.15 4.15 5.15 6.15 7.15 8.15 9.15"));
}
