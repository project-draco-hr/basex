{
  final XQuery query=new XQuery("\n" + "        format-dateTime($d1, '[Y][EN]', 'en', (), ()),\n" + "        format-dateTime($d2, '[Y][EN]', 'en', (), ())\n"+ "      ",ctx);
  query.bind("d1",new XQuery("xs:dateTime('1990-12-01T12:00:00')",ctx).value());
  query.bind("d2",new XQuery("xs:dateTime('-0055-12-01T12:00:00')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test((assertDeepEq("\"1990AD\", \"55BC\"") || assertDeepEq("\"1990CE\", \"55BCE\"")));
}
