{
  final XQuery query=new XQuery("xs:time(\"23:12:00+03:00\") + xs:dayTimeDuration(\"P1DT3H15M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"02:27:00+03:00"));
}
