{
  final XQuery query=new XQuery("fn:distinct-values((xs:float('-INF'), xs:float('INF')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertPermutation("xs:float('-INF'), xs:float('INF')"));
}
