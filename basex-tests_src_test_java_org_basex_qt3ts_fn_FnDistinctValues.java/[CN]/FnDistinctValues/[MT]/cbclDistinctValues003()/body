{
  final XQuery query=new XQuery("distinct-values(\n" + "              (xs:dateTime(\"2008-01-01T13:00:00\"),\n" + "               adjust-dateTime-to-timezone(xs:dateTime(\"2008-01-01T13:00:00\"))))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertCount(1));
}
