{
  write(out,STR_BIN);
  write(out,BYTE_BIN);
  write(out,LONG_BIN);
  write(out,INT_BIN);
  write(out,CINT5_BIN);
  write(out,CINT4_BIN);
  write(out,CINT2_BIN);
  write(out,CINT1_BIN);
  final long off=(out.getFilePointer() >>> 12) << 12;
  out.seek(off + BLOCK_BOUNDARY_POS);
  write(out,STR_BIN);
}
