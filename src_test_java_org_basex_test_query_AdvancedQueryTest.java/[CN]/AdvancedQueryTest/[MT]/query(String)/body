{
  final QueryProcessor qp=new QueryProcessor(query,CONTEXT);
  try {
    return qp.execute().toString().replaceAll("(\\r|\\n) *","");
  }
 catch (  final QueryException ex) {
    fail("Query failed:\n" + query + "\nMessage: "+ ex.getMessage());
    return null;
  }
 finally {
    try {
      qp.close();
    }
 catch (    final IOException e) {
    }
  }
}
