{
  final Data data=context.data();
  final MetaData m=data.meta;
  size=m.size;
  if (!startUpdate(data))   return false;
  boolean ok=true;
  try {
    optimize(data,this);
  }
 catch (  final IOException ex) {
    ok=error(Util.message(ex));
  }
  return stopUpdate(data) && ok && info(DB_OPTIMIZED_X,m.name,perf);
}
