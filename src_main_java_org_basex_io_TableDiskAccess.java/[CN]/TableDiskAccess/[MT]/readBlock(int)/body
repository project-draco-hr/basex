{
  index=i;
  fpre=fpres[i];
  npre=i + 1 >= blocks ? meta.size : fpres[i + 1];
  final int b=pages[i];
  final boolean ch=bm.cursor(b);
  bf=bm.current();
  if (ch) {
    try {
      if (bf.dirty)       writeBlock(bf);
      bf.pos=b;
      data.seek(bf.pos * IO.BLOCKSIZE);
      data.readFully(bf.data);
    }
 catch (    final IOException ex) {
      Util.stack(ex);
    }
  }
}
