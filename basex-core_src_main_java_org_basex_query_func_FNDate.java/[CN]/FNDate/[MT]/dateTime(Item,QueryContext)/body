{
  final Item zon=expr.length == 2 ? expr[1].item(ctx,info) : null;
  if (zon == null)   return null;
  final Dat d=date.type.isUntyped() ? new Dat(date.string(info),info) : (Dat)checkType(date,AtomType.DAT);
  final Tim t=zon.type.isUntyped() ? new Tim(zon.string(info),info) : (Tim)checkType(zon,AtomType.TIM);
  return new Dtm(d,t,info);
}
