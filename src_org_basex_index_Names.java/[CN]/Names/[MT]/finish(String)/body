{
  final DataOutput out=new DataOutput(db,tag ? DATATAG : DATAATN);
  out.writeBytesArray(keys);
  out.writeNums(next);
  out.writeNums(bucket);
  out.writeNums(counter);
  out.writeBooleans(noleaf);
  out.writeNum(size);
  out.writeBool(stats);
  for (int s=1; s < size; s++)   stat[s].write(out);
  out.close();
}
