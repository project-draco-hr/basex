{
  final DataInput in=new DataInput(db,t ? DATATAG : DATAATN);
  keys=in.readBytesArray();
  next=in.readNums();
  bucket=in.readNums();
  counter=in.readNums();
  noleaf=in.readBooleans();
  size=in.readNum();
  stats=in.readBool();
  stat=new StatsKey[next.length];
  for (int s=1; s < size; s++)   stat[s]=new StatsKey(in);
  tag=t;
  in.close();
}
