{
  for (  final PathNode c : ch) {
    if (c.kind == k && c.name == n) {
      c.size++;
      return c;
    }
  }
  final PathNode pn=new PathNode(n,k,this);
  final int cs=ch.length;
  final PathNode[] tmp=new PathNode[cs + 1];
  System.arraycopy(ch,0,tmp,0,cs);
  tmp[cs]=pn;
  ch=tmp;
  return pn;
}
