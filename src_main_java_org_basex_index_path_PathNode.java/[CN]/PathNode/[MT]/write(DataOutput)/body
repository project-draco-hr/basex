{
  out.writeNum(name);
  out.write1(kind);
  out.writeNum(0);
  out.writeNum(ch.length);
  out.writeDouble(1);
  boolean leaf=stats.isLeaf();
  for (  final PathNode c : ch) {
    leaf&=c.kind == Data.TEXT || c.kind == Data.ATTR;
  }
  stats.setLeaf(leaf);
  stats.write(out);
  for (  final PathNode c : ch)   c.write(out);
}
