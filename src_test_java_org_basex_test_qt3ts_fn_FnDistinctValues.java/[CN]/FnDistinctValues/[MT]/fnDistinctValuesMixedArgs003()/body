{
  final XQuery query=new XQuery("fn:distinct-values(( 1, 1.0e0))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertEq("1") && (assertType("xs:integer") || assertType("xs:double"))));
}
