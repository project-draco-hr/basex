{
  final XQuery query=new XQuery("fn:distinct-values((xs:double('-INF'), xs:double('INF')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"INF -INF") || assertStringValue(false,"-INF INF")));
}
