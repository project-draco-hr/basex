{
  final XQuery query=new XQuery("\n" + "        let $input := (xs:float('1.0'), xs:decimal('1.0000000000100000000001'), \n" + "                       xs:double( '1.00000000001'), xs:float('2.0'), \n"+ "                       xs:decimal('2.0000000000100000000001'), xs:double( '2.00000000001')), \n"+ "            $distinct := distinct-values($input) \n"+ "        return ( (every $n in $input satisfies $n = $distinct) and \n"+ "        (every $bool in (for $d1 at $p in $distinct, $d2 in $distinct [position() > $p] return $d1 eq $d2) satisfies not($bool)) )",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
