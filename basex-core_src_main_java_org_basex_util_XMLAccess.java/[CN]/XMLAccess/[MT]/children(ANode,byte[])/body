{
  final AxisIter children=node.children();
  return new AxisIter(){
    @Override public ANode next(){
      for (ANode child; (child=children.next()) != null; ) {
        if (child.type == NodeType.ELM && (name == null || eq(child.qname().id(),name)))         return child;
      }
      return null;
    }
  }
;
}
