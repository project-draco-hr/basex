{
  final ItemIter it=new ItemIter();
  try {
    String str=string(xml).trim();
    if (!doc)     str="<X>" + str + "</X>";
    final Data d=CreateDB.xml(IO.get(str),context);
    for (int p=doc ? 0 : 2; p < d.meta.size; p+=d.size(p,d.kind(p)))     it.add(new DBNode(d,p));
  }
 catch (  final IOException e) {
  }
  return it;
}
