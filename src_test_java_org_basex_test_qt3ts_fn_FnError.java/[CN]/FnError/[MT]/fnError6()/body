{
  final XQuery query=new XQuery("fn:error((), 'err:FOER0000','error raised by this test by setting first argument to empty sequence')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FOER0000"));
}
