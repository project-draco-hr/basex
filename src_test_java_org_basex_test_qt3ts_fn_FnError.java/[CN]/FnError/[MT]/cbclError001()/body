{
  final XQuery query=new XQuery("declare function local:ignore($arg) { true() }; local:ignore( fn:error() )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOER0000")));
}
