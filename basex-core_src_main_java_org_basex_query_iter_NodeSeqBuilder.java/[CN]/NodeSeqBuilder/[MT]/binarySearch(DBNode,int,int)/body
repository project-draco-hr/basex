{
  if (size == 0 || n.data != nodes[0].data())   return -start - 1;
  int l=start, r=start + length - 1;
  while (l <= r) {
    final int m=l + r >>> 1;
    final int npre=((DBNode)nodes[m]).pre;
    if (npre == n.pre)     return m;
    if (npre < n.pre)     l=m + 1;
 else     r=m - 1;
  }
  return -(l + 1);
}
