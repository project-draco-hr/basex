{
  final HashSet<Data> datas=new HashSet<Data>(dbUpdates.size());
  for (  final DataUpdates up : dbUpdates.values()) {
    if (tmp == null)     tmp=new MemData(up.data().meta.options);
    up.prepare(tmp);
    datas.add(up.data());
  }
  for (  final NameUpdates up : nameUpdates.values())   up.prepare();
  return datas;
}
