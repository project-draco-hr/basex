{
  check();
  if (!it.node())   throw new BXQException("Current item is not a node.");
  final org.basex.query.xquery.item.Node node=(org.basex.query.xquery.item.Node)it;
  try {
    return new URI(Token.string(node.base()));
  }
 catch (  final URISyntaxException ex) {
    throw new BXQException(ex.toString());
  }
}
