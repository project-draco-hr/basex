{
  testQueries(new XQuery(Q),new XQuery(Q),true);
  testQueries(new XQuery(f("(doc('%s'), %s)",NAME,Q)),new XQuery(f("(doc('%s'), %s)",NAME,Q)),true);
  testQueries(new XQuery(f("(doc('%s'), %s)",NAME,Q)),new XQuery(f("(doc('%s1'), %s)",NAME,Q)),true);
  testQueries(new XQuery(f("insert node %s into doc('%s')",Q,NAME)),new XQuery(f("insert node %s into doc('%s')",Q,NAME)),false);
  testQueries(new XQuery(f("insert node %s into doc('%s')",Q,NAME)),new XQuery(f("insert node %s into doc('%s1')",Q,NAME)),true);
  testQueries(new XQuery(f("(doc('%s'), %s)",NAME,Q)),new XQuery(f("insert node %s into doc('%s')",Q,NAME)),false);
  testQueries(new XQuery(f("(doc('%s'), %s)",NAME,Q)),new XQuery(f("insert node %s into doc('%s1')",Q,NAME)),true);
  testQueries(new XQuery(f("(doc('%s'), %s)",NAME,Q)),new XQuery(f("for $i in ('%s') return insert node %s into doc($i)",NAME,Q)),false);
  testQueries(new XQuery(f("for $i in ('%s') return insert node %s into doc($i)",NAME,Q)),new XQuery(f("for $i in ('%s') return insert node %s into doc($i)",NAME,Q)),false);
  testQueries(new XQuery(f("import module namespace qm='java:org.basex.test.query.func.QueryModuleTest';" + "qm:writeLock(), %s",Q)),new XQuery(f("import module namespace qm='java:org.basex.test.query.func.QueryModuleTest';" + "qm:writeLock(), %s",Q)),false);
}
