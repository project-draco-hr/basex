{
  checkCreate(ctx);
  final RepoManager rm=new RepoManager(ctx.context,ii);
  final String pkg=expr.length == 0 ? null : string(checkStr(expr[0],ctx));
switch (sig) {
case _REPO_INSTALL:
    rm.install(pkg);
  return null;
case _REPO_DELETE:
rm.delete(pkg);
return null;
default :
return super.item(ctx,ii);
}
}
