{
  checkAdmin(ctx);
  final RepoManager rm=new RepoManager(ctx.context.repo);
  final String pkg=expr.length == 0 ? null : string(checkStr(expr[0],ctx));
switch (sig) {
case _REPO_INSTALL:
    rm.install(pkg,ii);
  return null;
case _REPO_DELETE:
rm.delete(pkg,ii);
return null;
default :
return super.item(ctx,ii);
}
}
