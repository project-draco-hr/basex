{
  try {
    for (    final byte[] sl : split(fl.read(),'\n')) {
      byte[] val=null;
      for (      final byte[] st : split(norm(sl),' ')) {
        if (val == null)         val=st;
 else         add(st,val);
      }
    }
    return true;
  }
 catch (  final IOException ex) {
    return false;
  }
}
