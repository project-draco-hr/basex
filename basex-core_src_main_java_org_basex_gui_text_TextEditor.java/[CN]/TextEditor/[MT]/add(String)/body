{
  final int cl=str.length();
  final TokenBuilder tb=new TokenBuilder(cl);
  for (int c=0; c < cl; ++c) {
    int ch=str.charAt(c);
    if (ch == '\r' || ch < ' ' && !ws(ch))     continue;
    if (Character.isHighSurrogate((char)ch) && c + 1 < cl) {
      ch=Character.toCodePoint((char)ch,str.charAt(++c));
    }
    tb.add(ch);
  }
  add(tb.finish(),pos,pos);
  pos+=tb.size();
}
