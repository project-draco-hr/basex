{
  final XQuery query=new XQuery("fn:unparsed-text-available(\"surely-nobody-supports-this:/path.txt\", \"utf-8\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
