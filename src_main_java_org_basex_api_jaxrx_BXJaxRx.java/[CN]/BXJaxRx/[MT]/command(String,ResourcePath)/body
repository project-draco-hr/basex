{
  return new BXOutput(rp){
    @Override void code() throws IOException {
      final ArrayOutput ao=new ArrayOutput();
      exec(cmd,ao);
      final XMLSerializer xml=new XMLSerializer(out,new SerializerProp(params(path)));
      xml.text(Token.delete(ao.toArray(),'\r'));
      xml.close();
    }
  }
;
}
