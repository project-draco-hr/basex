{
  new BXCode(){
    @Override void code() throws IOException {
      final File file=cache(input);
      try {
        cs.execute(new CreateDB(root(rp),file.toString()));
      }
 catch (      final BaseXException ex) {
        throw new JaxRxException(400,ex.getMessage());
      }
 finally {
        file.delete();
      }
    }
  }
.run();
}
