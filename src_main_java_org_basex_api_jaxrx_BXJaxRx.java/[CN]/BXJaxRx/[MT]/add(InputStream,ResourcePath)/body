{
  return new BXCode(path){
    @Override String code(){
      try {
        session.execute(new Open(db(path)));
      }
 catch (      final BaseXException ex) {
        throw new JaxRxException(404,ex.getMessage());
      }
      add(input,path,session);
      return session.info();
    }
  }
.run();
}
