{
  new BXCode(){
    @Override void code() throws IOException {
      try {
        cs.execute(new Open(root(rp)));
      }
 catch (      final BaseXException ex) {
        throw new JaxRxException(404,ex.getMessage());
      }
      final File file=cache(input);
      try {
        cs.execute(new Add(file.toString()));
      }
 catch (      final BaseXException ex) {
        file.delete();
        throw new JaxRxException(404,ex.getMessage());
      }
      file.delete();
    }
  }
.run();
}
