{
  final Query q=s.query("if(" + DBISRAW.args("$db","$path") + ") then "+ DBSTORE.args("$tdb","$tpath",DBRETRIEVE.args("$db","$path"))+ " else "+ DBADD.args("$tdb",DBOPEN.args("$db","$path"),"$tpath"));
  q.bind("db",db);
  q.bind("path",path);
  q.bind("tdb",tdb);
  q.bind("tpath",tpath);
  q.execute();
}
