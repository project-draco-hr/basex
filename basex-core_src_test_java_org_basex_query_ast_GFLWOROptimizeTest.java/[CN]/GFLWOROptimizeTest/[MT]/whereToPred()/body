  check("for $i in 1 to 10 where <x/>[$i] and $i < 3 return $i","1","exists(//*[ends-with(name(), 'Filter')]/" + Util.className(BaseXItemAt.class) + ")");
  check("for $i in 1 to 10 where (<a/>)[$i] return $i","1","exists(//*[ends-with(name(), 'Filter')]/" + Util.className(BaseXItemAt.class) + ")");
  check("for $i in 1 to 3 " + "where count(for $j in 1 to $i group by $k := $j mod 2 return $i) > 1 " + "return $i","2\n3","empty(//Where)","exists(//*[ends-with(name(), 'Filter')])");
}
