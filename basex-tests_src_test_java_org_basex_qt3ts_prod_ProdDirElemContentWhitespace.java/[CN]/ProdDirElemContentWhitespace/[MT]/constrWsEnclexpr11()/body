{
  final XQuery query=new XQuery("declare boundary-space strip; <elem> <a/> <b/> </elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem><a/><b/></elem>",false));
}
