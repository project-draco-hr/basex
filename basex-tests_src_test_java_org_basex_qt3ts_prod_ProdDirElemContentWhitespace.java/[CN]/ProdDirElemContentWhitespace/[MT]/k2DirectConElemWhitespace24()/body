{
  final XQuery query=new XQuery("string(<elem> content <![CDATA[ content ]]> content </elem>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false," content  content  content "));
}
