{
  final XQuery query=new XQuery("declare boundary-space strip; <elem> &#x30; </elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem> 0 </elem>",false));
}
