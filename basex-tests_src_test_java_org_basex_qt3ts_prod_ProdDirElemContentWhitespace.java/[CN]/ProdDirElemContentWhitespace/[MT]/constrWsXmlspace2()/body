{
  final XQuery query=new XQuery("declare boundary-space preserve; <elem xml:space=\"strip\"> </elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<elem xml:space=\"strip\"> </elem>",false) || error("XQDY0092")));
}
