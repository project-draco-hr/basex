{
  final XQuery query=new XQuery(queryFile(file("prod/DirElemContent.whitespace/Constr-ws-enclexpr-17.xq")),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>1\r\n\r\n\r\n2</elem>",false));
}
