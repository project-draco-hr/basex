{
  final XQuery query=new XQuery("string(<e>123<!-- a comment -->ab<!-- another comment -->c</e>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"123abc"));
}
