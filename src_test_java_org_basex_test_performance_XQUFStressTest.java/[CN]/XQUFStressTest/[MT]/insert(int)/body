{
  for (int r=0; r < runs; r++) {
    new CreateDB(NAME,"<doc/>").execute(context);
    new XQuery("for $i in 1 to " + NRNODES + " return insert node "+ "<section><page/></section> into /doc").execute(context);
    new XQuery("for $page in //page " + "let $par := $page/.. " + "return (delete node $page, insert node $page before $par)").execute(context);
    new DropDB(NAME).execute(context);
  }
}
