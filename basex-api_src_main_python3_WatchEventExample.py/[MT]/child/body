def child():
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    session.execute(('xquery db:event("MY_EVENT", "fired MY_EVENT from child, at %s")' % str(datetime.now())))
    time.sleep(3)
    session.execute(('xquery db:event("MY_EVENT", "fired MY_EVENT from child, at %s")' % str(datetime.now())))
    session.close()
