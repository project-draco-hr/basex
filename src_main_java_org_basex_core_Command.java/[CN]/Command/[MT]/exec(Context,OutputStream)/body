{
  final Data dt=ctx.data();
  if (dt == null && data)   return error(NO_DB_OPENED);
  if (!ctx.perm(perm,dt != null ? dt.meta : null))   return error(PERM_REQUIRED_X,perm);
  updating=updating(ctx);
  try {
    ctx.register(this);
    return run(ctx,os);
  }
 catch (  final RuntimeException th) {
    Util.stack(th);
    throw th;
  }
 finally {
    ctx.unregister(this);
  }
}
