{
  final Data d=ctx.data();
  if (d == null && data)   return error(NO_DB_OPENED);
  if (!ctx.perm(perm,d != null ? d.meta : null))   return error(PERM_REQUIRED_X,perm);
  updating=updating(ctx);
  try {
    ctx.register(this);
    return run(ctx,os);
  }
  finally {
    ctx.unregister(this);
  }
}
