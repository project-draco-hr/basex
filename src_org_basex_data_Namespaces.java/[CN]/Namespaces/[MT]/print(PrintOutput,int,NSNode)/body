{
  final byte[] quote={'"'};
  for (int i=0; i < n.key.length; i++) {
    out.print(s,token(n.pre));
    out.print(s + 1,token(n.pre - n.par.pre));
    out.print(' ');
    out.print(concat(quote,key(n.key[i]),quote),11);
    out.print(key(n.val[i]));
    out.println(" (" + n.val[i] + ")");
  }
  for (  final NSNode c : n.ch)   print(out,s,c);
}
