{
  if (n.ch.length == 0)   return n;
  final Node[] ch=n.ch;
  int l=0, m=0, h=ch.length - 1;
  while (l <= h) {
    m=(l + h) >>> 1;
    int v=ch[m].pre;
    if (v < p)     l=m + 1;
 else     if (v > p)     h=m - 1;
 else     break;
  }
  while (p >= ch[m].pre && ++m < ch.length)   ;
  return find(ch[Math.max(0,m - 1)],p);
}
