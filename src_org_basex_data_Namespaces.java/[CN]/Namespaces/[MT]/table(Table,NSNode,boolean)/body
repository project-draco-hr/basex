{
  for (int i=0; i < n.key.length; i++) {
    StringList sl=new StringList();
    sl.add(n.pre);
    sl.add(n.pre - n.par.pre);
    sl.add("\"" + string(keys[n.key[i]]) + "\"");
    sl.add(string(keys[n.val[i]]) + " (" + n.val[i]+ ")");
    t.contents.add(sl);
  }
  if (all || n.key.length == 0)   for (  final NSNode c : n.ch)   table(t,c,all);
}
