{
  final XQuery query=new XQuery("sum(xs:duration(\"P1Y1M1D\"), xs:duration(\"PT0S\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FORG0006") || error("XPTY0004")));
}
