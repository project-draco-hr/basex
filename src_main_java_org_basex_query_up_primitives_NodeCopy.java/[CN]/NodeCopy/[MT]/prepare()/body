{
  md=new MemData(data);
  final NodeSeqBuilder seq=new NodeSeqBuilder();
  for (int i=0; i < insert.size(); i++) {
    final NodeSeqBuilder nc=insert.get(i);
    for (ANode n; (n=nc.next()) != null; ) {
      seq.add(n);
      size++;
    }
    insert.set(i,null);
  }
  insert=null;
  new DataBuilder(md).build(mergeNodeCacheText(seq));
}
