{
  md=new MemData(data);
  final ANodeList seq=new ANodeList();
  for (int i=0; i < insert.size(); i++) {
    final ANodeList nl=insert.get(i);
    final int ns=nl.size();
    for (int n=0; n < ns; n++)     seq.add(nl.get(n));
    size+=ns;
    insert.set(i,null);
  }
  insert=null;
  new DataBuilder(md).build(mergeNodeCacheText(seq));
}
