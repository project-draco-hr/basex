{
  int ns=nl.size();
  final ANodeList s=new ANodeList(ns);
  if (ns == 0)   return s;
  ANode n=nl.get(0);
  for (int c=0; c < ns; ) {
    if (n.type == NodeType.TXT) {
      final TokenBuilder tb=new TokenBuilder();
      while (n.type == NodeType.TXT) {
        tb.add(n.string());
        if (++c == ns)         break;
        n=nl.get(c);
      }
      s.add(new FTxt(tb.finish()));
    }
 else {
      s.add(n);
      if (++c < ns)       n=nl.get(c);
    }
  }
  return s;
}
