{
  expr=checkUp(expr,ctx).comp(ctx).addText(ctx);
  final boolean fast=ctx.ftfast;
  ctx.ftfast=ctx.ftfast && ctx.ftpos == null;
  ftexpr=ftexpr.comp(ctx);
  ctx.ftfast=fast;
  ft=new Tokenizer(ctx.context.prop);
  if (expr.empty()) {
    ctx.compInfo(OPTPRE,this);
    return Bln.FALSE;
  }
  return this;
}
