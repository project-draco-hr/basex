{
  final int tl=t.length;
  for (int i=0; i < tl; i+=cl(t,i)) {
    final int c=cp(t,i);
    if (c != norm(c)) {
      final TokenBuilder tb=new TokenBuilder();
      tb.add(t,0,i);
      for (int j=i; j < tl; j+=cl(t,j))       tb.add(norm(cp(t,j)));
      return tb.finish();
    }
  }
  return t;
}
