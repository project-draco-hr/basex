{
  roots=new TokenList();
  for (  final byte[] k : data.pthindex.desc(EMPTY,data,true,true)) {
    int c=0;
    for (    final byte[] kk : data.pthindex.desc(k,data,true,false)) {
      final Names nm=startsWith(kk,'@') ? data.atts : data.tags;
      if (nm.stat(nm.id(delete(kk,'@'))).leaf)       ++c;
    }
    if (c > 2)     roots.add(k);
  }
  init(data,-1);
}
