{
  if (root != null) {
    if (root instanceof BufferNode) {
      ((BufferNode<E,E>)root).toString(sb);
    }
 else {
      boolean first=true;
      for (      final E e : (FingerTree<E,E>)root) {
        if (!first)         sb.append(", ");
 else         first=false;
        sb.append(e);
      }
    }
  }
}
