{
  final Nod ns=ctx.item;
  ctx.item=tmp;
  tmp.set(pre);
  tmp.currSize=-1;
  boolean more=true;
  for (int j=0; j != size; j++) {
    final Pred pred=preds[j];
    if (!pred.eval(ctx,tmp,++pos[j])) {
      ctx.item=ns;
      return pred.more;
    }
    more&=pred.more;
  }
  result.add(pre);
  ctx.item=ns;
  return more;
}
