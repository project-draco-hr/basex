{
  final Item it=exprs[0].item(ctx,info);
  final Item it2=exprs.length == 2 ? exprs[1].item(ctx,info) : null;
switch (func) {
case RESOLVE_QNAME:
    return resolveQName(it,it2);
case QNAME:
  return qName(it,it2);
case LOCAL_NAME_FROM_QNAME:
return lnFromQName(ctx,it);
case PREFIX_FROM_QNAME:
return prefixFromQName(ctx,it);
case NAMESPACE_URI_FOR_PREFIX:
return nsUriForPrefix(it,it2);
case RESOLVE_URI:
return resolveUri(it,it2);
default :
return super.item(ctx,ii);
}
}
