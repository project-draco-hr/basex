{
  consumeWS();
  Expr name;
  if (XMLToken.isXMLLetter(curr())) {
    name=Str.get(ncName(null));
  }
 else {
    if (!consume(BRACE1))     return null;
    name=check(expr(),PIWRONG);
    check(BRACE2);
  }
  check(BRACE1);
  Expr expr=expr();
  if (expr == null)   expr=Seq.EMPTY;
  check(BRACE2);
  return new CPI(name,expr);
}
