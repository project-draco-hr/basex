{
  check(MODULE);
  check(NAMESPACE);
  module=new QNm(ncName(null));
  check(IS);
  module.uri=Uri.uri(stringLiteral());
  if (module.uri == Uri.EMPTY)   Err.or(NSMODURI);
  if (u != Uri.EMPTY && !u.eq(module.uri))   Err.or(WRONGMODULE,module.uri,file);
  ctx.ns.add(module);
  skipWS();
  check(';');
  prolog1();
  prolog2();
}
