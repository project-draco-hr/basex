{
  Expr expr=unionExpr();
  if (expr == null)   return null;
  while (true) {
    final Calc c=consume('*') ? Calc.MULT : consumeWS(DIV) ? Calc.DIV : consumeWS(IDIV) ? Calc.IDIV : consumeWS(MOD) ? Calc.MOD : null;
    if (c == null)     break;
    expr=new Clc(expr,check(unionExpr(),CALCEXPR),c);
  }
  return expr;
}
