{
  final Expr expr=ftContainsExpr();
  if (expr == null)   return null;
  for (  final CmpV.COMP c : CmpV.COMP.values())   if (consumeWS(c.name))   return new CmpV(expr,check(ftContainsExpr(),CMPEXPR),c);
  for (  final CmpN.COMP c : CmpN.COMP.values())   if (consumeWS(c.name))   return new CmpN(expr,check(ftContainsExpr(),CMPEXPR),c);
  for (  final CmpG.COMP c : CmpG.COMP.values())   if (consume(c.name))   return new CmpG(expr,check(ftContainsExpr(),CMPEXPR),c);
  return expr;
}
