{
  final Expr expr=exprSingle();
  if (expr == null) {
    if (qp != ql)     return null;
    if (alter != null)     error();
 else     Err.or(NOEXPR);
  }
  if (!consume(COMMA))   return expr;
  Expr[] list={expr};
  do   list=add(list,exprSingle());
 while (consume(COMMA));
  return new List(list);
}
