{
  consumeWS();
  Expr name;
  if (letter(curr())) {
    name=Str.get(qName(null));
  }
 else {
    if (!consume(BRACE1))     return null;
    name=check(expr(),NOTAGNAME);
    check(BRACE2);
  }
  check(BRACE1);
  final Expr expr=expr();
  check(BRACE2);
  return new CElem(name,expr == null ? new Expr[0] : new Expr[]{expr},new FAttr[]{});
}
