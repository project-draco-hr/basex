{
  final boolean elem=consumeWS(ELEMENT);
  if (!elem && !consumeWS(FUNCTION))   return false;
  check(NAMESPACE);
  final byte[] ns=stringLiteral();
  if (elem) {
    if (declElem)     Err.or(DUPLNS);
    ctx.nsElem=Uri.uri(ns);
    declElem=true;
  }
 else {
    if (declFunc)     Err.or(DUPLNS);
    ctx.nsFunc=Uri.uri(ns);
    declFunc=true;
  }
  return true;
}
