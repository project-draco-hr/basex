{
  int i=find(p);
  if (i >= 0) {
    if (i < --size)     Array.move(nodes,i + 1,-1,size - i);
  }
 else {
    if (size == 0)     nodes=new int[1];
 else     if (size == nodes.length)     nodes=Array.extend(nodes);
    i=-i - 1;
    Array.move(nodes,i,1,size++ - i);
    nodes[i]=p;
  }
  return this;
}
