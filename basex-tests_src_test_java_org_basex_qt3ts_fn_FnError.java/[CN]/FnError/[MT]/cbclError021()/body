{
  final XQuery query=new XQuery("typeswitch ( fn:error() ) case xs:integer return true() default return false()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOER0000")));
}
