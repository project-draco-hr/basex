{
  final XQuery query=new XQuery("\n" + "        declare namespace xq=\"http://www.w3.org/2012/xquery\";\n" + "        declare %private %xq:public function local:foo() { () };\n"+ "        local:foo()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0106"));
}
