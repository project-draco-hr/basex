{
  final XQuery query=new XQuery("\n" + "        import module namespace test1=\"http://www.w3.org/TestModules/test1\"; \n" + "        declare variable $test1:flag := 1; \n"+ "        <result>{test1:ok()}</result>\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/test1",file("prod/ModuleImport/test1-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0049"));
}
