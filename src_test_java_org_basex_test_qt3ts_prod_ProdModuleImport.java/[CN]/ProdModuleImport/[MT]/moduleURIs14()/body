{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"ftp://www.w3.org/TestModules/test;type=A\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  query.addModule("ftp://www.w3.org/TestModules/test;type=A",file("prod/ModuleImport/module-uris10-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result>ok</result>",false));
}
