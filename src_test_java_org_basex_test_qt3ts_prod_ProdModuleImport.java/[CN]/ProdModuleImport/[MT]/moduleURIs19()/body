{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"http:test\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  query.addModule("http:test",file("prod/ModuleImport/module-uris15-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result>ok</result>",false));
}
