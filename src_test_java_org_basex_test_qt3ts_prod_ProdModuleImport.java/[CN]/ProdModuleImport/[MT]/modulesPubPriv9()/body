{
  final XQuery query=new XQuery("\n" + "        declare %public function local:inc($i as xs:integer) {\n" + "            $i + 1\n"+ "        };\n"+ "        \n"+ "        declare function local:test() {\n"+ "            local:inc(1)\n"+ "        };\n"+ "        <a>{local:test()}</a>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<a>2</a>",false));
}
