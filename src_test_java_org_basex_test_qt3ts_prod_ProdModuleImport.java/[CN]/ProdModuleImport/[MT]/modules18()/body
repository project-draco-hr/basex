{
  final XQuery query=new XQuery("\n" + "        import module namespace mod1=\"http://www.w3.org/TestModules/module1\"; \n" + "        import module namespace mod2=\"http://www.w3.org/TestModules/module2\"; \n"+ "        mod1:x(),mod2:y()",ctx);
  query.addModule("http://www.w3.org/TestModules/module1",file("prod/ModuleImport/module1-lib.xq"));
  query.addModule("http://www.w3.org/TestModules/module2",file("prod/ModuleImport/module2-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"x y x y"));
}
