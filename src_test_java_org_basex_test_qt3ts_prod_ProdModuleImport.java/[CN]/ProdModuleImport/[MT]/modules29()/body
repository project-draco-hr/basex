{
  final XQuery query=new XQuery("\n" + "        import module namespace xmlns=\"http://www.w3.org/TestModules/test\"; \n" + "        xmlns:ok ()\n"+ "      ",ctx);
  query.addModule("http://www.w3.org/TestModules/test",file("prod/ModuleImport/test1-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(error("XQST0070"));
}
