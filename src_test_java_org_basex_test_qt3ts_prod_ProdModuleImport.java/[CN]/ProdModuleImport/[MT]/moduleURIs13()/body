{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"urn:example:animal:ferret:nose\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  query.addModule("urn:example:animal:ferret:nose",file("prod/ModuleImport/module-uris9-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result>ok</result>",false));
}
