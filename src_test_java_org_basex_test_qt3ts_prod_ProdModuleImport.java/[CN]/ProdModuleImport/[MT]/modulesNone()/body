{
  final XQuery query=new XQuery("\n" + "        import module namespace none=\"http://www.w3.org/TestModules/none\" at \"empty-lib.xq\"; \n" + "        \"abc\"\n"+ "      ",ctx);
  query.addModule("http://www.w3.org/TestModules/none",file("prod/ModuleImport/empty-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(error("XQST0059"));
}
