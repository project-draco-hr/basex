{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"ldap://[2001:db8::7]/c=GB?objectClass?one\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  query.addModule("ldap://[2001:db8::7]/c=GB?objectClass?one",file("prod/ModuleImport/module-uris21-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result>ok</result>",false));
}
