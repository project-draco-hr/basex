{
  final XQuery query=new XQuery("\n" + "        declare namespace xq=\"http://www.w3.org/2012/xquery\";\n" + "        declare %private %xq:public variable $foo := ();\n"+ "        $foo\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0116"));
}
