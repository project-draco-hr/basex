{
  final XQuery query=new XQuery("\n" + "        import module namespace test1=\"http://www.w3.org/TestModules/test1\"; \n" + "        declare namespace foo = \"http://example.org\"; \n"+ "        declare function foo:ok () { \"ok\" }; \n"+ "        let $var := fn:concat(test1:ok(),foo:ok()) \n"+ "        return $var",ctx);
  query.addModule("http://www.w3.org/TestModules/test1",file("prod/ModuleImport/test1-lib.xq"));
  query.addModule("http://www.w3.org/TestModules/test2",file("prod/ModuleImport/test1-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"okok"));
}
