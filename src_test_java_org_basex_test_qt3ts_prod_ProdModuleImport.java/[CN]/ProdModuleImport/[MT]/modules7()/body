{
  final XQuery query=new XQuery("\n" + "        import module namespace test1=\"http://www.w3.org/TestModules/test1\"; \n" + "        let $var := fn:concat(xs:string($test1:flag),xs:string(test1:ok())) \n"+ "        return $var",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/test1",file("prod/ModuleImport/test1-lib.xq"));
    query.addModule("http://www.w3.org/TestModules/test2",file("prod/ModuleImport/test1-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1ok"));
}
