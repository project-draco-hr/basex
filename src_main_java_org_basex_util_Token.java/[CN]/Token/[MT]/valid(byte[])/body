{
  final int l=token.length;
  int i=0;
  while (i < l) {
    int cl=cl2(token[i]);
    if (cl <= 0 || cl > l - i++)     return false;
    if (l == i)     return true;
    final byte b=token[i];
    if (b >= 0 && b < ' ' && !ws(b))     return false;
    while (--cl > 0)     if (cl2(token[i++]) != 0)     return false;
  }
  return true;
}
