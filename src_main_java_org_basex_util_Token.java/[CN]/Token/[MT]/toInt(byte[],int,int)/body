{
  int t=start;
  while (t < end && token[t] <= ' ')   t++;
  if (t == end)   return Integer.MIN_VALUE;
  boolean m=false;
  if (token[t] == '-' || token[t] == '+')   m=token[t++] == '-';
  if (t == end)   return Integer.MIN_VALUE;
  int v=0;
  for (; t < end; ++t) {
    final byte c=token[t];
    if (c < '0' || c > '9')     break;
    v=(v << 3) + (v << 1) + c - '0';
  }
  while (t < end && token[t] <= ' ')   t++;
  return t < end ? Integer.MIN_VALUE : m ? -v : v;
}
