{
  int t=ts;
  while (t < te && to[t] <= ' ')   t++;
  if (t == te)   return Integer.MIN_VALUE;
  boolean m=false;
  if (to[t] == '-' || to[t] == '+')   m=to[t++] == '-';
  if (t == te)   return Integer.MIN_VALUE;
  int v=0;
  for (; t < te; t++) {
    final byte c=to[t];
    if (c < '0' || c > '9')     break;
    v=(v << 3) + (v << 1) + c - '0';
  }
  while (t < te && to[t] <= ' ')   t++;
  return t < te ? Integer.MIN_VALUE : m ? -v : v;
}
