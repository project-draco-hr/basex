{
  final int v=token[pos] & 0xFF;
  if (v < 192)   return v;
  final int c=v >> 4;
  int l=pos;
  if (c == 12 || c == 13)   return (v & 0x1F) << 6 | token[++l] & 0x3F;
  if (c == 14)   return (v & 0x0F) << 12 | (token[++l] & 0x3F) << 6 | token[++l] & 0x3F;
  return (v & 0x07) << 18 | (token[++l] & 0x3F) << 12 | (token[++l] & 0x3F) << 6 | token[++l] & 0x3F;
}
