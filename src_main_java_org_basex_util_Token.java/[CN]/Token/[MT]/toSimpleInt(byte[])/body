{
  final int te=to.length;
  if (te >= 10 || te == 0)   return Integer.MIN_VALUE;
  if (to[0] == '0')   return te == 1 ? 0 : Integer.MIN_VALUE;
  int v=0;
  for (int ts=0; ts < te; ts++) {
    final byte c=to[ts];
    if (c < '0' || c > '9')     return Integer.MIN_VALUE;
    v=(v << 3) + (v << 1) + c - '0';
  }
  return v;
}
