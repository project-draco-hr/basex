{
  for (  final QNm c : codes) {
    if (c != null) {
      final byte[] cu=c.uri();
      if (err == null || cu.length != 0 && !eq(err.qname().uri(),cu))       continue;
      final byte[] nm=c.local();
      if (nm.length != 0 && !eq(code.local(),nm))       continue;
    }
    return true;
  }
  return false;
}
