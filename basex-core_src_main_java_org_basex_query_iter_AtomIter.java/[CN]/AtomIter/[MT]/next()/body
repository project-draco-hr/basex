{
  if (size != -1) {
    qc.checkStop();
    final Item it=iter.next();
    return it == null ? it : it.atomItem(info);
  }
  while (true) {
    if (atom == null) {
      final Item it=iter.next();
      if (it == null)       return it;
      atom=it.atomValue(info).iter();
    }
    qc.checkStop();
    final Item it=atom.next();
    if (it != null)     return it;
    atom=null;
  }
}
