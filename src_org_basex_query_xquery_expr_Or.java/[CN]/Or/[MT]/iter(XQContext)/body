{
  double d=0;
  boolean found=false;
  for (  final Expr e : expr) {
    final Item it=ctx.iter(e).ebv();
    if (it.bool()) {
      if (it.score() == 0)       return Bln.TRUE.iter();
      d=Scoring.or(d,it.score());
      found=true;
    }
  }
  return (!found ? Bln.FALSE : new Bln(found,d)).iter();
}
