{
  int el=expr.length;
  for (int e=0; e < el; e++) {
    expr[e]=ctx.comp(expr[e]);
    if (!expr[e].i())     continue;
    if (((Item)expr[e]).bool())     return Bln.TRUE;
    Array.move(expr,e + 1,-1,--el - e);
    --e;
  }
  return el == expr.length ? this : el == 0 ? Bln.FALSE : new Or(Array.finish(expr,el));
}
