{
  GetOpts g=new GetOpts(cmd,"Rh");
  int ch=g.getopt();
  while (ch != -1) {
switch (ch) {
case 'h':
      printHelp();
    return;
case ':':
  out.print("touch: missing argument");
return;
case '?':
out.print("touch: illegal option");
return;
}
ch=g.getopt();
}
if (g.getPath() != null) {
touch(g.getPath());
}
}
