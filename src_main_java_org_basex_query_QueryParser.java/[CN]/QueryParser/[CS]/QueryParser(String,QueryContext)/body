{
  super(q,c.base());
  ctx=c;
  final String bind=ctx.context.prop.get(Prop.BINDINGS);
  if (bind.isEmpty())   return;
  final Scanner sc=new Scanner(bind).useDelimiter(",");
  while (sc.hasNext()) {
    final String[] sp=sc.next().split("=",2);
    c.bind(sp[0],new Atm(token(sp.length > 1 ? sp[1] : "")));
  }
}
