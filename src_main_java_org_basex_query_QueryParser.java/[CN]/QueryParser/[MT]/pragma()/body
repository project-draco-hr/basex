{
  Expr[] pragmas={};
  while (wsConsumeWs(PRAGMA)) {
    final QNm name=eQName(QNAMEINV,URICHECK);
    char c=curr();
    if (c != '#' && !ws(c))     error(PRAGMAINV);
    tok.reset();
    while (c != '#' || next() != ')') {
      if (c == 0)       error(PRAGMAINV);
      tok.add(consume());
      c=curr();
    }
    pragmas=add(pragmas,new Pragma(name,tok.trim().finish(),input()));
    qp+=2;
  }
  return pragmas;
}
