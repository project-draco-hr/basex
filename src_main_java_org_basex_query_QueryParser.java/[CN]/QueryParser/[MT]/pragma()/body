{
  final ExprList el=new ExprList();
  while (wsConsumeWs(PRAGMA)) {
    final QNm name=eQName(QNAMEINV,URICHECK);
    char c=curr();
    if (c != '#' && !ws(c))     error(PRAGMAINV);
    tok.reset();
    while (c != '#' || next() != ')') {
      if (c == 0)       error(PRAGMAINV);
      tok.add(consume());
      c=curr();
    }
    add(el,new Pragma(name,tok.trim().finish(),info()));
    ip+=2;
  }
  return el.finish();
}
