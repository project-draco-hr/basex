{
  skipWS();
  final char del=curr();
  if (!quote(del))   error(NOQUOTE,found());
  consume();
  tok.reset();
  while (true) {
    while (!consume(del)) {
      if (curr() == 0)       error(NOQUOTE,found());
      entity(tok);
    }
    if (!consume(del))     break;
    tok.add(del);
  }
  return tok.finish();
}
