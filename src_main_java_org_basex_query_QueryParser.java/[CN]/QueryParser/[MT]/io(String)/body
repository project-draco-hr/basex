{
  final IO io=IO.get(fn);
  if (io.exists())   return io;
  final IO base=ctx.sc.baseIO();
  if (base != null) {
    final IO io2=base.merge(fn);
    if (!io2.eq(io) && io2.exists())     return io2;
  }
  if (input != null) {
    final IO io2=input.merge(fn);
    if (!io2.eq(io) && io2.exists())     return io2;
  }
  return io;
}
