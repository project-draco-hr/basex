{
  tok.reset();
  while (digit(curr()))   tok.add(consume());
  if (letter(curr()))   return checkDbl();
  if (!consume('.')) {
    final long l=toLong(tok.finish());
    if (l == Long.MIN_VALUE)     error(RANGE,tok);
    return Itr.get(l);
  }
  if (itr)   error(NUMBERITR);
  tok.add('.');
  return decimalLiteral();
}
