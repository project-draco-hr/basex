{
  final Expr e=stringConcat();
  final int p=qp;
  if (consume('=') && consume('>') || consume('<') && consume('-')) {
    skipWS();
  }
 else   if (!wsConsumeWs(CONTAINS) || !wsConsumeWs(TEXT)) {
    qp=p;
    return e;
  }
  final FTExpr select=ftSelection(false);
  if (wsConsumeWs(WITHOUT)) {
    wsCheck(CONTENT);
    union();
    error(FTIGNORE);
  }
  return new FTContains(e,select,input());
}
