{
  try {
    versionDecl();
    if (u != null)     return moduleDecl(u);
    final int i=ip;
    if (wsConsumeWs(MODULE,NSPACE,null))     error(MAINMOD);
    ip=i;
    final Expr expr=mainModule();
    if (expr == null) {
      if (alter != null)       error();
 else       error(EXPREMPTY);
    }
    return expr;
  }
 catch (  final QueryException ex) {
    mark();
    ex.pos(this);
    throw ex;
  }
}
