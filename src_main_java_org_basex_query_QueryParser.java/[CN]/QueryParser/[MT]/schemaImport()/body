{
  if (wsConsumeWs(NSPACE)) {
    ncName(XPNAME);
    wsCheck(IS);
  }
 else   if (wsConsumeWs(DEFAULT)) {
    wsCheck(ELEMENT);
    wsCheck(NSPACE);
  }
  final byte[] ns=stringLiteral();
  if (ns.length == 0)   error(NSEMPTY);
  if (wsConsumeWs(AT))   do   stringLiteral();
 while (wsConsumeWs(COMMA));
  error(IMPLSCHEMA);
}
