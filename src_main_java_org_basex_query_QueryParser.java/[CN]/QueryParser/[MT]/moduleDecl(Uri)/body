{
  wsCheck(MODULE);
  wsCheck(NSPACE);
  module=new QNm(ncName(XPNAME));
  wsCheck(IS);
  module.uri(stringLiteral());
  if (module.uri() == Uri.EMPTY)   error(NSMODURI);
  ctx.ns.add(module,input());
  skipWS();
  check(';');
  prolog1();
  prolog2();
  if (u != Uri.EMPTY && !u.eq(module.uri()))   error(WRONGMODULE,module.uri(),file);
}
