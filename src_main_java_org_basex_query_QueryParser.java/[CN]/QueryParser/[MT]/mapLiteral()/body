{
  wsCheck(BRACE1);
  Expr[] args={};
  if (!wsConsume(BRACE2)) {
    do {
      args=Array.add(args,check(single(),INVMAPKEY));
      wsCheck(ASSIGN);
      args=Array.add(args,check(single(),INVMAPVAL));
    }
 while (wsConsume(COMMA));
    wsCheck(BRACE2);
  }
  return new LitMap(input(),args);
}
