{
  final boolean elem=wsConsumeWs(ELEMENT);
  if (!elem && !wsConsumeWs(FUNCTION))   return false;
  wsCheck(NSPACE);
  final byte[] ns=stringLiteral();
  if (elem) {
    if (declElem)     error(DUPLNS);
    declElem=true;
    ctx.nsElem=ns;
  }
 else {
    if (declFunc)     error(DUPLNS);
    declFunc=true;
    ctx.nsFunc=ns;
  }
  return true;
}
