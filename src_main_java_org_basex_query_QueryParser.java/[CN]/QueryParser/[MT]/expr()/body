{
  final Expr e=single();
  if (e == null) {
    if (more())     return null;
    if (alter != null)     error();
 else     error(NOEXPR);
  }
  if (!wsConsume(COMMA))   return e;
  final ExprList el=new ExprList(e);
  do   add(el,single());
 while (wsConsume(COMMA));
  return new List(info(),el.finish());
}
