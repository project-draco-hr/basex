{
  if (wsConsumeWs(VALIDATE)) {
    if (!wsConsumeWs(STRICT) && !wsConsumeWs(LAX) && wsConsumeWs(TYPE)) {
      final QNm qnm=eQName(QNAMEINV,SKIPCHECK);
      names.add(new QNmCheck(qnm));
      error(NOSCHEMA,qnm);
    }
    wsCheck(BRACE1);
    check(single(),NOVALIDATE);
    wsCheck(BRACE2);
    error(IMPLVAL);
  }
}
