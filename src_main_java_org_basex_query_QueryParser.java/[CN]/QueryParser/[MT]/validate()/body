{
  final int i=ip;
  if (wsConsumeWs(VALIDATE)) {
    if (wsConsumeWs(TYPE)) {
      final QNm qnm=eQName(QNAMEINV,SKIPCHECK);
      names.add(new QNmCheck(qnm));
      error(NOSCHEMA,qnm);
    }
    if (wsConsumeWs(STRICT) || wsConsumeWs(LAX)) {
      wsCheck(BRACE1);
      check(single(),NOVALIDATE);
      wsCheck(BRACE2);
      error(IMPLVAL);
    }
  }
  ip=i;
}
