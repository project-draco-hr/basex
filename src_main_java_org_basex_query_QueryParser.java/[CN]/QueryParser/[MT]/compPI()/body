{
  skipWS();
  Expr name;
  if (XMLToken.isNCStartChar(curr())) {
    name=Str.get(ncName(null));
  }
 else {
    if (!wsConsume(BRACE1))     return null;
    name=check(expr(),PIWRONG);
    wsCheck(BRACE2);
  }
  if (!wsConsume(BRACE1))   return null;
  final Expr e=expr();
  wsCheck(BRACE2);
  return new CPI(input(),name,e == null ? Empty.SEQ : e);
}
