{
  skipWS();
  Expr name;
  if (XMLToken.isXMLLetter(curr())) {
    name=Str.get(ncName(null));
  }
 else {
    if (!consumeWS2(BRACE1))     return null;
    name=check(expr(),PIWRONG);
    check(BRACE2);
  }
  if (!consumeWS2(BRACE1))   return null;
  final Expr e=expr();
  check(BRACE2);
  return new CPI(input(),name,e == null ? Seq.EMPTY : e);
}
