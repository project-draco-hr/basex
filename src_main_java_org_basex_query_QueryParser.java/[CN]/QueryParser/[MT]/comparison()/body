{
  final Expr e=ftContains();
  if (e != null) {
    for (    final OpV c : OpV.values())     if (wsConsumeWs(c.name))     return new CmpV(info(),e,check(ftContains(),CMPEXPR),c);
    for (    final OpN c : OpN.values())     if (wsConsumeWs(c.name))     return new CmpN(info(),e,check(ftContains(),CMPEXPR),c);
    for (    final OpG c : OpG.values())     if (wsConsumeWs(c.name))     return new CmpG(info(),e,check(ftContains(),CMPEXPR),c);
  }
  return e;
}
