{
  final Expr e=ftContains();
  if (e != null) {
    for (    final CmpV.Op c : CmpV.Op.values())     if (consumeWS(c.name))     return new CmpV(input(),e,check(ftContains(),CMPEXPR),c);
    for (    final CmpN.Op c : CmpN.Op.values())     if (consumeWS(c.name))     return new CmpN(input(),e,check(ftContains(),CMPEXPR),c);
    for (    final CmpG.Op c : CmpG.Op.values())     if (consumeWS2(c.name))     return new CmpG(input(),e,check(ftContains(),CMPEXPR),c);
  }
  return e;
}
