{
  if (!ctx.xquery3)   return null;
  skipWS();
  Expr name;
  final byte[] str=ncName(null);
  if (str.length != 0) {
    name=Str.get(str);
  }
 else {
    if (!wsConsume(BRACE1))     return null;
    name=check(expr(),NSWRONG);
    wsCheck(BRACE2);
  }
  if (!wsConsume(BRACE1))   return null;
  final Expr e=expr();
  wsCheck(BRACE2);
  return new CNSpace(info(),name,e == null ? Empty.SEQ : e);
}
