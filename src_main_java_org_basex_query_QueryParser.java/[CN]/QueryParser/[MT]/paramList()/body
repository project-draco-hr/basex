{
  Var[] args={};
  skipWS();
  while (true) {
    if (curr() != '$') {
      if (args.length == 0)       break;
      check('$');
    }
    final Var var=typedVar();
    ctx.vars.add(var);
    for (    final Var v : args)     if (v.name.eq(var.name))     error(FUNCDUPL,var);
    args=Array.add(args,var);
    if (!consume(','))     break;
    skipWS();
  }
  return args;
}
