{
  while (true) {
    final int p=qp;
    if (!consumeWS(DECLARE))     return;
    if (consumeWS(VARIABLE)) {
      varDecl();
    }
 else     if (consumeWS(UPDATING)) {
      ctx.updating=true;
      check(FUNCTION);
      functionDecl(true);
    }
 else     if (consumeWS(FUNCTION)) {
      functionDecl(false);
    }
 else     if (consumeWS(OPTION)) {
      optionDecl();
    }
 else     if (consumeWS(DEFAULT)) {
      error(PROLOGORDER);
    }
 else {
      qp=p;
      return;
    }
    skipWS();
    check(';');
  }
}
