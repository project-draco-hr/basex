{
  if (!consume('e') && !consume('E'))   error(NUMBERWS);
  tok.add('e');
  if (curr() == '+' || curr() == '-')   tok.add(consume());
  boolean dig=false;
  while (digit(curr())) {
    tok.add(consume());
    dig=true;
  }
  if (!dig)   error(NUMBERINC);
  if (letter(curr()))   error(NUMBERWS);
  return Dbl.get(tok.finish());
}
