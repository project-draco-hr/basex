{
  skipWS();
  Expr name;
  if (XMLToken.isXMLLetter(curr())) {
    name=new QNm(qName(null));
  }
 else {
    if (!consumeWS2(BRACE1))     return null;
    name=check(expr(),NOTAGNAME);
    check(BRACE2);
  }
  if (!consumeWS2(BRACE1))   return null;
  final Expr e=expr();
  check(BRACE2);
  return new CElem(name,e == null ? new Expr[0] : new Expr[]{e},new Atts());
}
