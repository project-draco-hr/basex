{
  file(ctx.sc.baseIO(),ctx.context);
  checkValidChars();
  try {
    versionDecl();
    wsCheck(MODULE);
    wsCheck(NSPACE);
    skipWS();
    final byte[] name=ncName(XPNAME);
    wsCheck(IS);
    final byte[] uri=stringLiteral();
    if (uri.length == 0)     error(NSMODURI);
    module=new QNm(name,uri);
    ctx.sc.ns.add(name,uri,info());
    skipWS();
    check(';');
    prolog1();
    prolog2();
    finish(null,check);
    return module;
  }
 catch (  final QueryException ex) {
    mark();
    ex.pos(this);
    throw ex;
  }
}
