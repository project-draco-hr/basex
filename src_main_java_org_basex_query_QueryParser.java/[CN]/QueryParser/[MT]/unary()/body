{
  boolean minus=false;
  boolean found=false;
  do {
    skipWS();
    if (consume('-')) {
      minus^=true;
      found=true;
    }
 else     if (consume('+')) {
      found=true;
    }
 else {
      final Expr e=value();
      return found ? new Unary(info(),check(e,EVALUNARY),minus) : e;
    }
  }
 while (true);
}
