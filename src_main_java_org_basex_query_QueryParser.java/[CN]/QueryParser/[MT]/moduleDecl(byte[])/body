{
  wsCheck(MODULE);
  wsCheck(NSPACE);
  skipWS();
  final byte[] pref=ncName(XPNAME);
  wsCheck(IS);
  final byte[] uri=stringLiteral();
  if (uri.length == 0)   error(NSMODURI);
  module=new QNm(pref,uri);
  ctx.ns.add(pref,uri,input());
  skipWS();
  check(';');
  prolog1();
  prolog2();
  if (!eq(u,module.uri()))   error(WRONGMODULE,module.uri(),file);
}
