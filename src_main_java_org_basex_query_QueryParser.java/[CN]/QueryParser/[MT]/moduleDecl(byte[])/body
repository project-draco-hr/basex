{
  wsCheck(MODULE);
  wsCheck(NSPACE);
  skipWS();
  final byte[] name=ncName(XPNAME);
  wsCheck(IS);
  final byte[] uri=stringLiteral();
  if (uri.length == 0)   error(NSMODURI);
  module=new QNm(name,uri);
  ctx.sc.ns.add(name,uri,info());
  skipWS();
  check(';');
  prolog1();
  prolog2();
  if (u.length != 0 && !eq(u,uri))   error(WRONGMODULE,module.uri(),file);
  return module;
}
