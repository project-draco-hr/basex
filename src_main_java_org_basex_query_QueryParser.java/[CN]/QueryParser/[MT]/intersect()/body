{
  final Expr e=instanceoff();
  if (consumeWS(INTERSECT)) {
    Expr[] list={e};
    do     list=add(list,instanceoff());
 while (consumeWS(INTERSECT));
    return new InterSect(info(),list);
  }
 else   if (consumeWS(EXCEPT)) {
    Expr[] list={e};
    do     list=add(list,instanceoff());
 while (consumeWS(EXCEPT));
    return new Except(info(),list);
  }
 else {
    return e;
  }
}
