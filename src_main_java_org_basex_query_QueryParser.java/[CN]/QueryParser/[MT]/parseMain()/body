{
  file(ctx.sc.baseIO(),ctx.context);
  checkValidChars();
  try {
    String doc=moduleDoc("");
    versionDecl();
    doc=moduleDoc(doc);
    final int i=ip;
    if (wsConsumeWs(MODULE,NSPACE,null))     error(MAINMOD);
    ip=i;
    doc=moduleDoc(doc);
    prolog1();
    prolog2();
    scope=new VarScope();
    final Expr e=expr();
    if (e == null) {
      if (alter != null)       error();
 else       error(EXPREMPTY);
    }
    if (doc.equals(xqdoc.toString()))     doc=null;
    final MainModule mm=new MainModule(e,scope,doc);
    scope=null;
    finish(mm,true);
    return mm;
  }
 catch (  final QueryException ex) {
    mark();
    ex.pos(this);
    throw ex;
  }
}
