{
  Expr expr=null;
  try {
    versionDecl();
    if (u == null) {
      expr=mainModule();
      if (expr == null)       if (alter != null)       error();
 else       error(EXPREMPTY);
    }
 else {
      moduleDecl(u);
    }
    if (c && more()) {
      if (alter != null)       error();
      error(QUERYEND,rest());
    }
  }
 catch (  final QueryException ex) {
    mark();
    ex.pos(this);
    throw ex;
  }
  ctx.funcs.check();
  ctx.vars.check();
  ctx.ns.finish(ctx.nsElem);
  final QNm empty=new QNm(EMPTY);
  if (ctx.decFormats.get(empty) == null) {
    ctx.decFormats.put(empty,new DecimalFormat());
  }
  return expr;
}
