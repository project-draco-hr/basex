{
  final QNm name=eQName(QNAMEINV,ctx.sc.nsFunc);
  final ItemCache ic=new ItemCache(1);
  if (wsConsumeWs(PAR1)) {
    do {
      final Item it=literal();
      if (it == null)       error(ANNVALUE);
      ic.add(it);
    }
 while (wsConsumeWs(COMMA));
    wsCheck(PAR2);
  }
  skipWS();
  addAnnotation(ann,name,ic.value());
}
