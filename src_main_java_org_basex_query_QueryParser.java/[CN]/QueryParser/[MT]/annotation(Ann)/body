{
  skipWS();
  final QNm name=eQName(QNAMEINV,XQURI);
  final ValueBuilder vb=new ValueBuilder();
  if (wsConsumeWs(PAR1)) {
    do {
      final Item it=literal();
      if (it == null)       error(ANNVALUE);
      vb.add(it);
    }
 while (wsConsumeWs(COMMA));
    wsCheck(PAR2);
  }
  skipWS();
  ann.add(name,vb.value());
}
