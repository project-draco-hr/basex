{
  final Ann ann=new Ann();
  while (consume('%')) {
    final QNm name=eQName(QNAMEINV,ctx.sc.nsFunc);
    final ItemCache ic=new ItemCache(1);
    if (wsConsumeWs(PAR1)) {
      do       ic.add(literal());
 while (wsConsumeWs(COMMA));
      wsCheck(PAR2);
    }
    ann.add(name,ic.value());
  }
  skipWS();
  return ann;
}
