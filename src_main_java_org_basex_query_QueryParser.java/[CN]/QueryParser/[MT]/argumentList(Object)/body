{
  if (!wsConsume(PAR1))   return null;
  Expr[] args={};
  if (!wsConsume(PAR2)) {
    do {
      Expr arg=null;
      if (!wsConsume(PLHOLDER) && (arg=single()) == null)       error(FUNCMISS,name);
      args=add(args,arg);
    }
 while (wsConsume(COMMA));
    if (!wsConsume(PAR2))     error(FUNCMISS,name);
  }
  return args;
}
