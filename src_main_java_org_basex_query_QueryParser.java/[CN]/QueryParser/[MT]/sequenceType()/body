{
  skipWS();
  final Type t=itemType();
  skipWS();
  final Occ occ=consume('?') ? Occ.ZO : consume('+') ? Occ.OM : consume('*') ? Occ.ZM : Occ.O;
  if (t == AtomType.EMP && occ != Occ.O)   error(EMPTYSEQOCC,t);
  final KindTest kt=tok.isEmpty() ? null : kindTest(t,tok.finish());
  tok.reset();
  skipWS();
  return SeqType.get(t,occ,kt == null ? null : kt.extype == null || t == kt.extype || !kt.extype.isNode() ? kt.name : new QNm());
}
