{
  final char c=curr();
  if (digit(c))   return numericLiteral();
  if (c == '.' && next() != '.') {
    consume('.');
    if (!digit(curr()))     return new Context(info());
    tok.reset();
    tok.add('.');
    return decimalLiteral();
  }
  return quote(c) ? Str.get(stringLiteral()) : null;
}
