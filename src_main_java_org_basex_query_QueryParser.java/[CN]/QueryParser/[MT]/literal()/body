{
  final char c=curr();
  if (digit(c) || c == '.')   return numericLiteral(false);
  if (!quote(c))   return null;
  final int i=pos;
  final byte[] s=stringLiteral();
  final int p2=pos;
  if (consume(':')) {
    if (!consume('=')) {
      pos=i;
      return null;
    }
    pos=p2;
  }
  return Str.get(s);
}
