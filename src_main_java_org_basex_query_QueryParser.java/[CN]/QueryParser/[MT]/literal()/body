{
  final char c=curr();
  if (digit(c) || c == '.')   return numericLiteral(false);
  if (!quote(c))   return null;
  final int i=ip;
  final byte[] s=stringLiteral();
  final int p2=ip;
  if (consume(':')) {
    if (!consume('=')) {
      ip=i;
      return null;
    }
    ip=p2;
  }
  return Str.get(s);
}
