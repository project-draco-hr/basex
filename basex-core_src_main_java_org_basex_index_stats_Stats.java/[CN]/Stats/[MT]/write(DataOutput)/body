{
  StatsType t=type;
  if ((t == StatsType.NONE || t == StatsType.STRING) && values != null && !values.isEmpty())   t=StatsType.CATEGORY;
  out.writeNum(t.ordinal() | 0x10);
  if (t == StatsType.INTEGER || t == StatsType.DOUBLE) {
    out.writeDouble(min);
    out.writeDouble(max);
  }
 else   if (t == StatsType.CATEGORY) {
    values.write(out);
  }
  out.writeNum(count);
  out.writeBool(leaf);
  out.writeDouble(0);
}
