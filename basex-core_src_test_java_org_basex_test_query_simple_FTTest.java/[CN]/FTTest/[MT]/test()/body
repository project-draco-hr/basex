{
  final Options opts=context.options;
  if (ALL) {
    for (int a=0; a < 2; ++a) {
      opts.set(Options.FTINDEX,a == 0);
      super.test();
    }
  }
 else {
    opts.set(Options.FTINDEX,true);
    opts.set(Options.STEMMING,true);
    opts.set(Options.DIACRITICS,true);
    opts.set(Options.CASESENS,true);
    super.test();
  }
}
