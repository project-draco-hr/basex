{
  final int tl=tok.length;
  if (tree[tl] == null) {
    isize++;
    tree[tl]=new FTHash();
  }
  if (wp instanceof ScoringTokenizer) {
    final int score=((ScoringTokenizer)wp).score(tok);
    if (score > -1) {
      tree[tl].index(tok,id,score);
    }
  }
 else   tree[tl].index(tok,id,wp.pos);
}
