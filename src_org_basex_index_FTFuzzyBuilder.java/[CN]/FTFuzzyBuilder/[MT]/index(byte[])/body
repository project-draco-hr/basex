{
  final int tl=tok.length;
  if (tree[tl] == null) {
    isize++;
    tree[tl]=new FTHash();
  }
  if (wp instanceof ScoringTokenizer) {
    Main.outln(new String(tok) + " " + ((ScoringTokenizer)wp).score(tok));
    tree[tl].index(tok,id,((ScoringTokenizer)wp).score(tok));
  }
 else   tree[tl].index(tok,id,wp.pos);
}
