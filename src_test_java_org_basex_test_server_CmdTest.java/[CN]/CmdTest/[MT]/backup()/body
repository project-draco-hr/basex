{
  no(new Backup(NAME));
  ok(new CreateDB(NAME));
  ok(new CreateDB(NAME2));
  ok(new Backup(NAME));
  ok(new Close());
  ok(new Restore(NAME));
  ok(new Backup(NAME));
  ok(new DropBackup(NAME));
  ok(new Backup(NAME + "*"));
  ok(new Restore(NAME2));
  ok(new DropBackup(NAME + "*"));
  no(new Restore(":"));
}
