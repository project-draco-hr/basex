{
  ok(new CreateUser(NAME2,"test"));
  ok(new CreateUser(NAME,"test"));
  no(new Grant("something",NAME2));
  ok(new Grant("none",NAME + "*","*"));
  no(new Grant("all",NAME2));
  no(new Grant("all",":*?",":*:"));
  ok(new DropUser(NAME + "," + NAME2));
  no(new Grant("all",NAME));
  no(new Grant("all",NAME + "*",":"));
}
