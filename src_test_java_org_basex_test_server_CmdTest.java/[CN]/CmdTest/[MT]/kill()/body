{
  no(new Kill("admin"));
  no(new Kill("admin2"));
  no(new Kill("ha*"));
  ok(new CreateUser(NAME2,"test"));
  BaseXServer bxs=new BaseXServer();
  ClientSession cs=new ClientSession(CONTEXT,NAME2,"test");
  ok(new Kill(NAME2));
  no(new Kill(NAME2 + "?"));
  ok(new Kill(NAME + "?"));
  cs.close();
  bxs.quit(false);
  ok(new DropUser(NAME2));
}
