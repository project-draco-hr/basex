{
  final String fun=check(Function.DBFULLTEXT);
  new DropIndex("fulltext").execute(CONTEXT);
  error(fun + "('" + DB+ "', 'assignments')",Err.NOIDX);
  new CreateIndex("fulltext").execute(CONTEXT);
  query(fun + "('" + DB+ "', 'assignments')","Assignments");
  query(fun + "('" + DB+ "', 'XXX')","");
}
