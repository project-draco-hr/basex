{
  final String fun=check(Function.DBISXML);
  query("db:add('" + DB + "', '<a/>', 'xml')");
  query("db:store('" + DB + "', 'raw', 'bla')");
  query(fun + "('" + DB+ "', 'xml')","true");
  query(fun + "('" + DB+ "', 'raw')","false");
  query(fun + "('" + DB+ "', 'xxx')","false");
  query(fun + "('" + DB+ "', 'xxx')","false");
}
