{
  final String dbname=NAME + "DBCreate";
  error(query(_DB_CREATE.args(dbname,FILE,"in/") + ",insert node <dummy/> into doc('" + dbname+ "'),"+ _DB_DROP.args(dbname)),Err.WHICHRES);
  query(_DB_CREATE.args(dbname));
  query(_DB_CREATE.args(dbname,"<dummy/>","t1.xml") + ",insert node <dummy/> into doc('" + dbname+ "'),"+ _DB_DROP.args(dbname));
  query("doc('" + dbname + "')/root()","<dummy/>");
}
