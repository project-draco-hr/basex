{
  final String fun=check(Function.DBISRAW);
  query("db:add('" + DB + "', '<a/>', 'xml')");
  query("db:store('" + DB + "', 'raw', 'bla')");
  query(fun + "('" + DB+ "', 'xml')","false");
  query(fun + "('" + DB+ "', 'raw')","true");
  query(fun + "('" + DB+ "', 'xxx')","false");
  query(fun + "('" + DB+ "', 'xxx')","false");
}
