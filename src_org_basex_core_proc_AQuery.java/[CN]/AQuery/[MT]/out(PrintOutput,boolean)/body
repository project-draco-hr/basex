{
  for (int i=0; i < Prop.runs; i++) {
    final XMLSerializer xml=new XMLSerializer(i == 0 && Prop.serialize ? o : new NullOutput(!Prop.serialize),Prop.xmloutput,p);
    result.serialize(xml);
    if (Prop.xmloutput)     xml.closeElement();
  }
  if (Prop.runs > 0) {
    if (Prop.info)     outInfo(o,result.size());
    qp.close();
  }
}
