{
  XMLSerializer ser=new XMLSerializer(Prop.serialize ? o : new NullOutput(),Prop.xmloutput,p);
  for (int i=0; i < Prop.runs; i++) {
    if (i != 0)     ser=new XMLSerializer(new NullOutput(!Prop.serialize),Prop.xmloutput,Prop.xqformat);
    result.serialize(ser);
    ser.close();
  }
  if (Prop.info)   outInfo(o,result.size());
  qp.close();
}
