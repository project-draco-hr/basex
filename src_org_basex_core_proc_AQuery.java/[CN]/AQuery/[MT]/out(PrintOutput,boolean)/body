{
  for (int i=0; i < Prop.runs; i++) {
    final XMLSerializer xml=i == 0 ? new XMLSerializer(Prop.serialize ? o : new NullOutput(),Prop.xmloutput,p) : new XMLSerializer(new NullOutput(!Prop.serialize),Prop.xmloutput,Prop.xqformat);
    result.serialize(xml);
    xml.close();
  }
  if (Prop.info)   outInfo(o,result.size());
  qp.close();
}
