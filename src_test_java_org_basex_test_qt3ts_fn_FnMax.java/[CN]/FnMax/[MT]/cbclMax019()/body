{
  final XQuery query=new XQuery("if (max(for $x in 1 to 10 return $x mod 9 = 0)) then true() else false()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
