{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) {\n" + "      \t\t (xs:decimal(1.1), xs:float(1.2), xs:double(0.4), xs:string(\"2\"))[$x] \n"+ "      \t}; \n"+ "      \tmax(for $x in (1,4,2,3) return local:f($x))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FORG0006"));
}
