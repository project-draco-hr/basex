{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f($x as xs:integer) { \n" + "      \t\tif ($x = 2) then xs:time(\"12:00:00-01:00\") else xs:time(\"12:00:00+01:00\") \n"+ "      \t}; \n"+ "      \tmax(for $x in (1,2,3) return local:f($x))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"12:00:00-01:00"));
}
