{
  if (!parsed) {
    final Scanner sc=new Scanner(ctx.context.prop.get(Prop.BINDINGS));
    sc.useDelimiter(",");
    while (sc.hasNext()) {
      final String[] sp=sc.next().split("=",2);
      bind(sp[0],new Atm(token(sp.length > 1 ? sp[1] : "")));
    }
    ctx.parse(query);
    parsed=true;
  }
}
