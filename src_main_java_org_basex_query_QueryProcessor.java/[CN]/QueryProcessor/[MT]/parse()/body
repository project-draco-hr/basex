{
  if (parsed)   return;
  parsed=true;
  final String bind=ctx.context.prop.get(Prop.BINDINGS);
  if (!bind.isEmpty()) {
    final Scanner sc=new Scanner(bind);
    sc.useDelimiter(",");
    while (sc.hasNext()) {
      final String[] sp=sc.next().split("=",2);
      bind(sp[0],new Atm(token(sp.length > 1 ? sp[1] : "")));
    }
  }
  ctx.parse(query);
}
