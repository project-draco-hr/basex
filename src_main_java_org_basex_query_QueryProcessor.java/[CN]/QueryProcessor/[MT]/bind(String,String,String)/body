{
  Object obj=o;
  if (t != null && !t.isEmpty()) {
    final QNm type=new QNm(Token.token(t));
    if (type.ns())     type.uri(ctx.ns.uri(type.pref(),false,null));
    final Type typ=Types.find(type,true);
    if (typ != null)     obj=typ.e(o,null);
 else     NOTYPE.thrw(null,type);
  }
  bind(n,obj);
}
