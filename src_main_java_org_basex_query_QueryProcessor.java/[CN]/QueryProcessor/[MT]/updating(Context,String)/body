{
  for (  final String s : UPDATES) {
    if (qu.indexOf(s) != -1) {
      try {
        final QueryProcessor qp=new QueryProcessor(qu,ctx);
        qp.parse();
        return qp.ctx.updating;
      }
 catch (      final QueryException ex) {
        return true;
      }
    }
  }
  return false;
}
