{
  final XQuery query=new XQuery("fn:normalize-space(\"This text should contains\n" + "no newline characters.\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"This text should contains no newline characters."));
}
