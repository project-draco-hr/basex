{
  final XQuery query=new XQuery("fn:normalize-space(\"This\ttext should contains no tabs\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"This text should contains no tabs"));
}
