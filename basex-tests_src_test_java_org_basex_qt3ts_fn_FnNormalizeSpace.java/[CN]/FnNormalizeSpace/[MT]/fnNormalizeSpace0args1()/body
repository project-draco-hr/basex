{
  final XQuery query=new XQuery("//doc/normalize-space(zero-or-one(a[normalize-space() = 'Hello, How are you?']))",ctx);
  try {
    query.context(node(file("fn/normalize-space/textWithSpaces.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Hello, How are you?"));
}
