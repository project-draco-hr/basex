{
  final XQuery query=new XQuery("normalize-space(normalize-space((\"foo\", current-time())[1])) eq \"foo\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPTY0004")));
}
