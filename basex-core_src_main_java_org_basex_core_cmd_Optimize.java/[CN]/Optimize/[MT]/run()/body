{
  final Data data=context.data();
  final MetaData m=data.meta;
  size=m.size;
  if (!startUpdate())   return false;
  try {
    optimize(data,options,this);
    return info(DB_OPTIMIZED_X,m.name,perf);
  }
 catch (  final IOException ex) {
    return error(Util.message(ex));
  }
 finally {
    finishUpdate();
  }
}
