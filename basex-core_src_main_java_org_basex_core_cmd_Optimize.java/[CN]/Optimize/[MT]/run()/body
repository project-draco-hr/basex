{
  final Data data=context.data();
  final MetaData meta=data.meta;
  size=meta.size;
  if (!startUpdate())   return false;
  boolean ok=true;
  try {
    optimize(data,options,this);
    ok=info(DB_OPTIMIZED_X,meta.name,perf);
  }
 catch (  final IOException ex) {
    ok=error(Util.message(ex));
  }
 finally {
    ok&=finishUpdate();
  }
  return ok;
}
