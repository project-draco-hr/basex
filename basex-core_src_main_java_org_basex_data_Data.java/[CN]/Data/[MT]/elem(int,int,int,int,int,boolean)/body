{
  final int i=newID();
  final int n=nsFlag ? 1 << 7 : 0;
  s(Math.min(IO.MAXATTS,asize) << 3 | ELEM);
  s(n | (byte)(nameId >> 8));
  s(nameId);
  s(uriId);
  s(dist >> 24);
  s(dist >> 16);
  s(dist >> 8);
  s(dist);
  s(size >> 24);
  s(size >> 16);
  s(size >> 8);
  s(size);
  s(i >> 24);
  s(i >> 16);
  s(i >> 8);
  s(i);
}
