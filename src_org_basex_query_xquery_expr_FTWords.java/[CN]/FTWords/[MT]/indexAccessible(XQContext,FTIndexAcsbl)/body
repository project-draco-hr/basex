{
  ia.io=ia.io && checkItr(ctx.iter(occ[0])) == 1 && checkItr(ctx.iter(occ[1])) == Long.MAX_VALUE;
  if (!ia.io)   return;
  if (query instanceof Str) {
    tok=((Str)query).str();
    ctx.ftopt.sb.text=tok;
    int i=Integer.MAX_VALUE;
    while (ctx.ftopt.sb.more()) {
      final int n=ia.data.nrIDs(ctx.ftopt.sb);
      if (n == 0)       ia.indexSize=0;
      i=Math.min(i,n);
    }
    ia.indexSize=i;
    ia.iu=true;
  }
 else {
    ia.io=false;
  }
}
