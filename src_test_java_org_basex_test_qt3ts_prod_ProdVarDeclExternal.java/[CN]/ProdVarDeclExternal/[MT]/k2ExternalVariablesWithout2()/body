{
  final XQuery query=new XQuery("declare variable $input-context external; declare variable $input-context external; $input-context",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0049"));
}
