{
  final XQuery query=new XQuery("declare variable $a as attribute()* := (attribute name1 {()}, attribute name2 {()}, attribute name3 {()}); declare variable $b as attribute()* := (attribute name1 {()}, attribute name2 {()}, attribute name3 {()}); $a/(let $p := position() return . is $b[$p])",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"false false false"));
}
