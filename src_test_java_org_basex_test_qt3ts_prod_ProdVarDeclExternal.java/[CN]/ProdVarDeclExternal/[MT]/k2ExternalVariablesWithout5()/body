{
  final XQuery query=new XQuery("\n" + "        declare namespace e = \"http://example.com/ANamespace\"; \n" + "        declare variable $e:exampleComThisVarIsNotRecognized external; \n"+ "        $e:exampleComThisVarIsNotRecognized",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
