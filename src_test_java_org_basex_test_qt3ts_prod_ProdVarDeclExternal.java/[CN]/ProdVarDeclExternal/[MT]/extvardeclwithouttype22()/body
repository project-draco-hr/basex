{
  final XQuery query=new XQuery("declare variable $x external; fn:not($x)",ctx);
  try {
    query.bind("x",new XQuery("0 + 1",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || error("FORG0006")));
}
