{
  final XQuery query=new XQuery("declare variable $i as xs:string := xs:anyURI(\"http://www.example.com/\") ; $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
