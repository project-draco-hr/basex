{
  check();
  final BXXMLResource del=checkXML(res);
  final Data data=ctx.data();
  if (del.data != data && del.data != null)   throw new XMLDBException(ErrorCodes.NO_SUCH_RESOURCE,ERR_UNKNOWN + data.meta.dbname);
  ctx.data().delete(((BXXMLResource)getResource(del.getId())).pos);
  ctx.data().flush();
}
