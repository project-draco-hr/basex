{
  final XQuery query=new XQuery("<elem attr1=\"val1\" attr2=\"val2\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem attr1=\"val1\" attr2=\"val2\"/>",false));
}
