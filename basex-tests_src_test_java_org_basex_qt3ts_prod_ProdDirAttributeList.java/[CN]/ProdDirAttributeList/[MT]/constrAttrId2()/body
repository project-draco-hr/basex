{
  final XQuery query=new XQuery("<elem xml:id=\" a{'b c d',' '}\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<elem xml:id=\"ab c d\"/>",false) || error("XQDY0091")));
}
