{
  if (size == nodes.length) {
    nodes=Array.extend(nodes);
    pos=Array.extend(pos);
    poi=Array.extend(poi);
  }
  if (!sort && size != 0) {
    final int d=pre - nodes[size - 1];
    if (d == 0) {
      pos[size - 1]=Array.merge(pos[size - 1],po);
      poi[size - 1]=Array.merge(poi[size - 1],pi);
      return;
    }
    sort=d <= 0;
  }
  nodes[size++]=pre;
  pos[size - 1]=po;
  poi[size - 1]=pi;
}
