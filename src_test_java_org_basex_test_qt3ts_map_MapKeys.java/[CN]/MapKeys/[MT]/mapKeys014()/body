{
  final XQuery query=new XQuery("map:keys(map:new(for $n in 1 to 500000 return map:entry($n, $n+1)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("$result = 1") && assertQuery("$result = 500000") && assertCount(500000)));
}
