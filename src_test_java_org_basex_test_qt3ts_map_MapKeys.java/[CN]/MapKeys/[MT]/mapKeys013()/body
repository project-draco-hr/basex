{
  final XQuery query=new XQuery("map:keys(map:remove(map{\"a\":=1,\"b\":=2}, \"c\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("$result = \"a\"") && assertQuery("$result = \"b\"") && assertCount(2)));
}
