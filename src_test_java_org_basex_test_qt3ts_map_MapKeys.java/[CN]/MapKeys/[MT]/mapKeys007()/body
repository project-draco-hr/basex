{
  final XQuery query=new XQuery("map:keys(map{\"a\":=1, \"a\":=2})",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("xs:string") && assertEq("\"a\"")));
}
