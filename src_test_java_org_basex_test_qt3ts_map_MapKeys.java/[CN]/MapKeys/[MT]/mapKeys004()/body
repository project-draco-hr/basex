{
  final XQuery query=new XQuery("map:keys(map:entry(\"a\", \"1\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertCount(1) && assertType("xs:string") && assertEq("\"a\"")));
}
