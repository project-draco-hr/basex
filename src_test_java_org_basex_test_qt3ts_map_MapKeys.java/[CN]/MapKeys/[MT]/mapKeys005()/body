{
  final XQuery query=new XQuery("map:keys(map:new((map:entry(\"a\", \"1\"), map:entry(\"b\", 2))))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertQuery("$result = \"a\"") && assertQuery("$result = \"b\"") && assertCount(2)));
}
