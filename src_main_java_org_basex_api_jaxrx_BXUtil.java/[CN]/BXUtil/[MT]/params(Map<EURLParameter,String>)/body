{
  String ser=p.get(EURLParameter.OUTPUT);
  if (ser == null)   ser="";
  final String wrap=p.get(EURLParameter.WRAP);
  if (wrap == null || wrap.equals(DataText.YES)) {
    ser+="," + SerializerProp.S_WRAP_PRE[0] + "="+ JAXRX+ ","+ SerializerProp.S_WRAP_URI[0]+ "="+ URL;
  }
 else   if (!wrap.equals(DataText.NO)) {
    badRequest(SerializerProp.error(EURLParameter.WRAP.toString(),DataText.YES,DataText.NO));
  }
  return ser;
}
