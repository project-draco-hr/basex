{
  String ser=p.get(EURLParameter.OUTPUT);
  if (ser == null)   ser="";
  final String wrap=p.get(EURLParameter.WRAP);
  if (wrap == null || wrap.equals(SerializeProp.YES)) {
    ser+="," + SerializeProp.S_WRAP_PRE[0] + "="+ JAXRX+ ","+ SerializeProp.S_WRAP_URI[0]+ "="+ URL;
  }
 else   if (!wrap.equals(SerializeProp.NO)) {
    badRequest(SerializeProp.error(EURLParameter.WRAP.toString(),SerializeProp.YES,SerializeProp.NO));
  }
  return ser;
}
