{
  return new StreamingOutput(){
    @Override public void write(    final OutputStream out){
      final ClientSession cs=session();
      run(cs,new Code(){
        @Override public void run() throws IOException {
          if (db != null && !cs.execute(new Open(db)))           throw new JAXRXException(404,cs.info());
          final String par=params(p);
          if (!cs.execute(new Set(Prop.SERIALIZER,par)))           throw new JAXRXException(400,cs.info());
          if (p.get(EURLParameter.COMMAND) != null) {
            command(cs,out,p);
          }
 else           if (p.get(EURLParameter.RUN) != null) {
            file(cs,out,p);
          }
 else           if (p.get(EURLParameter.QUERY) != null || db != null) {
            query(cs,out,p);
          }
 else {
            list(cs,out,p);
          }
        }
      }
);
    }
  }
;
}
