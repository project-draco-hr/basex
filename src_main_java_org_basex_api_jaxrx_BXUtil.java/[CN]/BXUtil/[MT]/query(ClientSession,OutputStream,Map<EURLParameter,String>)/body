{
  final String query=par.get(EURLParameter.QUERY);
  final String start=par.get(EURLParameter.START);
  final String count=par.get(EURLParameter.COUNT);
  final int s=start != null ? Integer.valueOf(start) : 1;
  final int m=count != null ? Integer.valueOf(count) : Integer.MAX_VALUE - s;
  final String xquery="(" + (query != null ? query : ".") + ")[position() = "+ s+ " to "+ (s + m - 1)+ "]";
  if (!cs.execute(new XQuery(xquery),out))   throw JAXRXException.badRequest(cs.info());
}
