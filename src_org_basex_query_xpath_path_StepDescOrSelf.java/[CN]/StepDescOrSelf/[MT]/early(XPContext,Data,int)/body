{
  final int[] pos=new int[preds.size()];
  int kind=data.kind(p);
  final int size=p + data.size(p,kind);
  int pre=p;
  while (pre != size) {
    kind=data.kind(pre);
    if (test.eval(data,pre,kind)) {
      if (!preds.earlyEval(ctx,result,pre,pos))       return;
    }
    pre+=data.attSize(pre,kind);
  }
}
