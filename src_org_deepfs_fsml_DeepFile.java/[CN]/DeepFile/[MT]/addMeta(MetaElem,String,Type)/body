{
  if (metaFinished || value.length() == 0)   return;
  if (e.equals(MetaElem.TYPE) | e.equals(MetaElem.FORMAT)) {
    Main.debug("The metadata attributes " + MetaElem.TYPE + " and "+ MetaElem.FORMAT+ " must not be set by an addMetaElem() method."+ " Use setMetaType() and setFormat() instead.");
    return;
  }
  if (dataType != null)   e.refineDataType(dataType);
 else   e.reset();
  addMeta0(e,value);
}
