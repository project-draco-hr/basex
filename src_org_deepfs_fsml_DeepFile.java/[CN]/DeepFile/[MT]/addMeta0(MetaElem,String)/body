{
  final String s=value.trim();
  if (s.isEmpty())   return;
  final ArrayList<String> vals;
  if (metaElements.containsKey(e)) {
    if (!e.isMultiVal()) {
      Main.debug("DeepFile: Failed to add metadata value. Multiple values are " + "forbidden for " + "metadata element % (%).",e,bfc.getFileName());
      return;
    }
    vals=metaElements.get(e);
  }
 else {
    vals=new ArrayList<String>();
    metaElements.put(e,vals);
  }
  vals.add(s);
}
