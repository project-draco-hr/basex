{
  found=false;
  int l=1;
  int r=next[cn].length - 2;
  int m=r - l / 2;
  while (l < r) {
    m=r - l / 2;
    if (tokens[next[next[cn][m]][0]][0] < toi)     l=m + 1;
 else     if (tokens[next[next[cn][m]][0]][0] > toi)     r=m - 1;
 else {
      found=true;
      return m;
    }
  }
  if (l < next[cn].length - 2 && tokens[next[next[cn][m]][0]][0] == toi) {
    found=true;
    return l + 1;
  }
  return l;
}
