{
  final XQuery query=new XQuery("every $x in (1,2,3), $y in (4,5,6) satisfies xs:integer($x) = xs:integer($y)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
