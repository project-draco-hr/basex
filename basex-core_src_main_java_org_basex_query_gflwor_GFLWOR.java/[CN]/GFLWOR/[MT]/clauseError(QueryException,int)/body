{
  final ListIterator<Clause> iter=clauses.listIterator(idx);
  while (iter.hasPrevious()) {
    final Clause b4=iter.previous();
    if (b4 instanceof For || b4 instanceof Window || b4 instanceof Where) {
      iter.next();
      while (iter.hasNext()) {
        iter.next();
        iter.remove();
      }
      ret=FNInfo.error(qe,ret.type());
      return true;
    }
  }
  throw qe;
}
