{
  for (  final Expr e : list.exprs) {
    if (!(e instanceof Path))     return false;
    final Path p=(Path)e;
    if (p.root != null || !(p.steps[0] instanceof Step))     return false;
    final Step n=(Step)p.steps[0];
    if (n.axis != CHILD || n.has(Flag.FCS))     return false;
  }
  return true;
}
