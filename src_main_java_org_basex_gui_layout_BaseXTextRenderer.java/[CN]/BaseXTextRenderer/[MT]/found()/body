{
  if (search.length == 0)   return false;
  final int sl=search.length, wl=text.length(), p=text.pos();
  if (wl < sl)   return false;
  int c=0;
  for (; c < search.length; c+=Token.cl(search,c)) {
    if (Token.lc(text.next()) != Token.lc(Token.cp(search,c)))     break;
  }
  text.pos(p);
  return c == sl;
}
