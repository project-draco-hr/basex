{
  final XQuery query=new XQuery("xs:float('3.402823e38') cast as xs:integer",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("FOCA0003") || assertQuery("string-length(string($result)) gt 36")));
}
