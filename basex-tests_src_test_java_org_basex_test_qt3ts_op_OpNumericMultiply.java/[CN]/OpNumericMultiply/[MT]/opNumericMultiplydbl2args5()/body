{
  final XQuery query=new XQuery("xs:double(\"-1.0000000000\") * xs:double(\"1.7976931348623157E308\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("-1.7976931348623157E308"));
}
