{
  if (order == null) {
    end=vl.size();
    order=new int[end];
    for (int i=0; i < end; ++i)     order[i]=i;
    sort(order,0,end);
  }
  while (true) {
    if (ir != null) {
      final Item i=ir.next();
      if (i != null)       return i;
      ir=null;
    }
 else {
      if (++pos == end)       return null;
      ir=vl.get(order[pos]).iter();
    }
  }
}
