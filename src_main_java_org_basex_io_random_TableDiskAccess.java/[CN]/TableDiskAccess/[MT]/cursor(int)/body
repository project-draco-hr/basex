{
  int fp=fpre;
  int np=npre;
  if (pre < fp || pre >= np) {
    final int last=used - 1;
    int l=0;
    int h=last;
    int m=index;
    while (l <= h) {
      if (pre < fp)       h=m - 1;
 else       if (pre >= np)       l=m + 1;
 else       break;
      m=h + l >>> 1;
      fp=fpres[m];
      np=m == last ? meta.size : fpres[m + 1];
    }
    if (l > h)     Util.notexpected("Data Access out of bounds:" + "\n- pre value: " + pre + "\n- #used blocks: "+ used+ "\n- #total locks: "+ blocks+ "\n- access: "+ m+ " ("+ l+ " > "+ h+ ']');
    readIndex(m);
  }
  return pre - fpre << IO.NODEPOWER;
}
