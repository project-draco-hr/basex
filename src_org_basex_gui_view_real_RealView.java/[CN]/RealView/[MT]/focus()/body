{
  if (rects == null)   return false;
  final Iterator<RealRect[]> it=rects.iterator();
  while (it.hasNext()) {
    final RealRect[] r=it.next();
    for (int i=0; i < r.length; i++) {
      if (r[i].contains(mousePosX,mousePosY)) {
        focusedRealRect=r[i];
        return true;
      }
    }
  }
  focusedRealRect=null;
  return false;
}
