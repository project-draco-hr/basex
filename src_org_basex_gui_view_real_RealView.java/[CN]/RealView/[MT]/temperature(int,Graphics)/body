{
  realImage=createImage();
  Graphics rg=realImage.getGraphics();
  final Data data=GUI.context.data();
  rects=new ArrayList<RealRect[]>();
  int level=0;
  sumNodeSizeInLine=data.size;
  parentList=new IntList();
  parentList.add(root);
