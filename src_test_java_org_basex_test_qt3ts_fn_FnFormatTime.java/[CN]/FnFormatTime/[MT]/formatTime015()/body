{
  final XQuery query=new XQuery("string-join(\n" + "               for $z in -28 to +28\n" + "               return format-time(adjust-time-to-timezone(\n"+ "                        $t, $z*xs:dayTimeDuration('PT30M')), '[h01][m01][z]'), '; ')",ctx);
  query.bind("t",new XQuery("xs:time('09:15:06.456Z')",ctx).value());
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(true,"\n            0715GMT-14; 0745GMT-13:30; 0815GMT-13; 0845GMT-12:30; 0915GMT-12; 0945GMT-11:30; 1015GMT-11; 1045GMT-10:30;\n            1115GMT-10; 1145GMT-9:30; 1215GMT-9; 1245GMT-8:30; 0115GMT-8; 0145GMT-7:30; 0215GMT-7; 0245GMT-6:30;\n            0315GMT-6; 0345GMT-5:30; 0415GMT-5; 0445GMT-4:30; 0515GMT-4; 0545GMT-3:30; 0615GMT-3; 0645GMT-2:30;\n            0715GMT-2; 0745GMT-1:30; 0815GMT-1; 0845GMT-0:30; 0915; 0945GMT+0:30; 1015GMT+1; 1045GMT+1:30; 1115GMT+2;\n            1145GMT+2:30; 1215GMT+3; 1245GMT+3:30; 0115GMT+4; 0145GMT+4:30; 0215GMT+5; 0245GMT+5:30; 0315GMT+6;\n            0345GMT+6:30; 0415GMT+7; 0445GMT+7:30; 0515GMT+8; 0545GMT+8:30; 0615GMT+9; 0645GMT+9:30; 0715GMT+10;\n            0745GMT+10:30; 0815GMT+11; 0845GMT+11:30; 0915GMT+12; 0945GMT+12:30; 1015GMT+13; 1045GMT+13:30;\n            1115GMT+14\n         "));
}
