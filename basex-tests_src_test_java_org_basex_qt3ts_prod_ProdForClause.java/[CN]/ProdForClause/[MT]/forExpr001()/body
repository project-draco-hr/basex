{
  final XQuery query=new XQuery("for $a in (\"test\", \"test1\", \"test2\") return $a",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"test test1 test2"));
}
