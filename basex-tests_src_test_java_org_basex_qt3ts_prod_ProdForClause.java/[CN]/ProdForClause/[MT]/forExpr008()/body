{
  final XQuery query=new XQuery("for $AaBbCc.-_Dd in /MyComputer/Drive1/Folder where $AaBbCc.-_Dd/@creator = \"Mani\" return $AaBbCc.-_Dd/FolderName",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<FolderName>Folder00000000000</FolderName>",false));
}
