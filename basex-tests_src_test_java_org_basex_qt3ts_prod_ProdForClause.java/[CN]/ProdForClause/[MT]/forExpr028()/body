{
  final XQuery query=new XQuery("for $num in ( <one/>, <two/>, <three/> ) return $num",ctx);
  try {
    query.context(node(file("prod/ForClause/fsx.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<one/><two/><three/>",false));
}
