{
  for (int k=0; k < ob.length; ++k) {
    final OrderBy or=ob[k];
    final Item m=or.item(o[a]);
    final Item n=it[k];
    final boolean x=m == null;
    final boolean y=n == null;
    final int c=x ? y ? 0 : or.lst ? -1 : 1 : y ? or.lst ? 1 : -1 : m.diff(input,n);
    if (c != 0)     return or.desc ? -c : c;
  }
  return 0;
}
