{
  for (  final OrderBy l : ord) {
    final Item m=l.item(o[a]);
    final Item n=l.item(o[b]);
    final boolean x=m == null;
    final boolean y=n == null;
    final int c=x ? y ? 0 : l.lst ? -1 : 1 : y ? l.lst ? 1 : -1 : m.diff(input,n);
    if (c != 0)     return l.desc ? -c : c;
  }
  return 0;
}
