{
  final XQuery query=new XQuery("fn:not(fn:string(fn:timezone-from-date(xs:date(\"1970-01-03+02:00\"))))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
