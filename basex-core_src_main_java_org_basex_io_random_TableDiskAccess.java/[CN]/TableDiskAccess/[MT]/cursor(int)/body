{
  int fp=fpre;
  int np=npre;
  if (pre < fp || pre >= np) {
    final int last=used - 1;
    int l=0;
    int h=last;
    int m=page;
    while (l <= h) {
      if (pre < fp)       h=m - 1;
 else       if (pre >= np)       l=m + 1;
 else       break;
      m=h + l >>> 1;
      fp=fpre(m);
      np=m == last ? meta.size : fpre(m + 1);
    }
    if (l > h)     throw Util.notExpected("Data Access out of bounds:" + "\n- pre value: " + pre + "\n- #used blocks: "+ used+ "\n- #total locks: "+ blocks+ "\n- access: "+ m+ " ("+ l+ " > "+ h+ ']');
    readPage(m);
  }
  return pre - fpre << IO.NODEPOWER;
}
