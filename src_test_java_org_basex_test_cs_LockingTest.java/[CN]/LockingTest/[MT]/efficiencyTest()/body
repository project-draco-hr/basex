{
  for (int i=0; i < TESTS; ++i) {
    final ClientSession s=createSession();
    new Thread(){
      @Override public void run(){
        if (!checkRes(new XQuery(PERF),s).equals("192000"))         fail("efficiency test failed");
        closeSession(s);
        ++tdone;
      }
    }
.start();
  }
  while (tdone < TESTS)   Performance.sleep(200);
}
