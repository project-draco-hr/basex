{
  final XQuery query=new XQuery("if(true()) then true() else error(QName(\"\", \"local\"), \"description\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
