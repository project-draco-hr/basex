{
  final XQuery query=new XQuery("empty( if (current-date() lt xs:date('2009-01-01')) then () else fn:error())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOER0000")));
}
