{
  final XQuery query=new XQuery("(every $s in tokenize('\u1680\u3000', ',') satisfies matches($s, '^(?:\\p{Zs}*)$')) and (every $s in tokenize('\u2028', ',') satisfies not(matches($s, '^(?:\\p{Zs}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
