{
  final XQuery query=new XQuery("(every $s in tokenize('???', ',') satisfies matches($s, '^(?:\\P{Sm}*)$')) and (every $s in tokenize('??????', ',') satisfies not(matches($s, '^(?:\\P{Sm}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
