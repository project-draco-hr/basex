{
  final XQuery query=new XQuery("(every $s in tokenize('', ',') satisfies matches($s, '^(?:\\W)$')) and (every $s in tokenize('A,????,a,a,????,??,??,???,??,??,???,??,??,????,??,????,???,????,???,????,???,???,???,???,????,????,????,???,??,??,????,???,???,???,???,???,???,???,???,???,???,????', ',') satisfies not(matches($s, '^(?:\\W)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
