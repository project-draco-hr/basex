{
  final XQuery query=new XQuery("(every $s in tokenize('??', ',') satisfies matches($s, '^(?:\\P{Lm}*)$')) and (every $s in tokenize('?????', ',') satisfies not(matches($s, '^(?:\\P{Lm}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
