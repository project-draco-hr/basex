{
  final XQuery query=new XQuery("not(matches('', '^(?:\\n\\na\\n\\nb\\n\\n)$')) and\n" + "         (every $s in tokenize('\n" + "\n"+ "a\n"+ "\n"+ "b;\n"+ ",\n"+ "a\n"+ "\n"+ "b;\n"+ "\n"+ ",\n"+ "\n"+ "a\n"+ "\n"+ "b;\n"+ "\r', ',') \n"+ "                satisfies not(matches($s, '^(?:\\n\\na\\n\\nb\\n\\n)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
