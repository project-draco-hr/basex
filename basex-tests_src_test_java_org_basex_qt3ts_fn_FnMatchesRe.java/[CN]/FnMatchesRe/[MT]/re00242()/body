{
  final XQuery query=new XQuery("(every $s in tokenize('???\u10ff,??????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u10c6\u10c7\u10c8\u10c9\u10ca\u10cb\u10cc\u10cd\u10ce\u10cf?????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????????\u10fb???\u10fd\u10fe\u10ff', ',') satisfies matches($s, '^(?:\\p{IsGeorgian}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsGeorgian}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
