{
  final XQuery query=new XQuery("(every $s in tokenize('\u0c80\u0cff,\u0c80\u0c81??????\u0c84????????????????????????\u0c8d?????????\u0c91?????????????????????????????????????????????????????????????????????\u0ca9??????????????????????????????\u0cb4???????????????\u0cba\u0cbb???????????????????????????\u0cc5?????????\u0cc9????????????\u0cce\u0ccf\u0cd0\u0cd1\u0cd2\u0cd3\u0cd4??????\u0cd7\u0cd8\u0cd9\u0cda\u0cdb\u0cdc\u0cdd???\u0cdf????????????\u0ce4\u0ce5??????????????????????????????\u0cf0??????\u0cf3\u0cf4\u0cf5\u0cf6\u0cf7\u0cf8\u0cf9\u0cfa\u0cfb\u0cfc\u0cfd\u0cfe\u0cff', ',') satisfies matches($s, '^(?:\\p{IsKannada}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsKannada}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
