{
  final XQuery query=new XQuery("(every $s in tokenize('??????', ',') satisfies matches($s, '^(?:\\p{No}*)$')) and (every $s in tokenize('\u203f', ',') satisfies not(matches($s, '^(?:\\p{No}*)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
