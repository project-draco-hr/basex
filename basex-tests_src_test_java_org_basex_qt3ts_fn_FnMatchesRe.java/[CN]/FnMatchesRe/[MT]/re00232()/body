{
  final XQuery query=new XQuery("(every $s in tokenize('\u0b00\u0b7f,\u0b00?????????\u0b04????????????????????????\u0b0d\u0b0e??????\u0b11\u0b12??????????????????????????????????????????????????????????????????\u0b29?????????????????????\u0b31??????\u0b34???????????????\u0b3a\u0b3b???????????????????????????\u0b45\u0b46??????\u0b49\u0b4a?????????\u0b4e\u0b4f\u0b50\u0b51\u0b52\u0b53\u0b54\u0b55??????\u0b58\u0b59\u0b5a\u0b5b??????\u0b5e???????????????\u0b64\u0b65??????????????????????????????????????????????????????\u0b78\u0b79\u0b7a\u0b7b\u0b7c\u0b7d\u0b7e\u0b7f', ',') satisfies matches($s, '^(?:\\p{IsOriya}+)$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:\\p{IsOriya}+)$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
