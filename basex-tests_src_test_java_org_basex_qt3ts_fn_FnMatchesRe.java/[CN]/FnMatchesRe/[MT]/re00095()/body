{
  final XQuery query=new XQuery("(every $s in tokenize('\u0fff', ',') satisfies matches($s, '^(?:[\u0fff])$')) and (every $s in tokenize('', ',') satisfies not(matches($s, '^(?:[\u0fff])$')))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
