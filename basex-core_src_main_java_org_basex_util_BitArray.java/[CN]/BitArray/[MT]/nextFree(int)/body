{
  int wi=i >>> WORD_POWER;
  long word=~words[wi] & WORD_MASK << i;
  if (word != 0) {
    return (wi << WORD_POWER) + numberOfTrailingZeros(word);
  }
  while (++wi < words.length) {
    if ((word=~words[wi]) != 0) {
      return (wi << WORD_POWER) + numberOfTrailingZeros(word);
    }
  }
  return wi << WORD_POWER;
}
