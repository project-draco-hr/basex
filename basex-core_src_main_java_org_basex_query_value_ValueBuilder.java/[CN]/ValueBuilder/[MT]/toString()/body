{
  final StringBuilder sb=new StringBuilder(getClass().getSimpleName()).append('[');
  Iterator<Item> iter=firstValue != null ? firstValue.iterator() : builder != null ? builder.iterator() : Collections.<Item>emptyIterator();
  if (iter.hasNext()) {
    sb.append(iter.next());
    while (iter.hasNext())     sb.append(", ").append(iter.next());
  }
  return sb.append(']').toString();
}
