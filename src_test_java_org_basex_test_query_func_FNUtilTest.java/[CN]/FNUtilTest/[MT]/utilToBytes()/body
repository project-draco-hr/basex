{
  check(TO_BYTES);
  query(TO_BYTES.args("xs:base64Binary('QmFzZVggaXMgY29vbA==')"),"66 97 115 101 88 32 105 115 32 99 111 111 108");
  query(TO_BYTES.args("xs:base64Binary(xs:hexBinary('4261736558'))"),"66 97 115 101 88");
  query(TO_BYTES.args("a"),97);
  query(COUNT.args(TO_BYTES.args("a\u00f4c")),4);
  query(COUNT.args(TO_BYTES.args(123)),3);
}
