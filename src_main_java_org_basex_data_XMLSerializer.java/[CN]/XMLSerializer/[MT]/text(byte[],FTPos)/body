{
  finishElement();
  int c=-1, wl=0;
  final FTLexer ftt=new FTLexer(b);
  while (ftt.hasNext()) {
    final int end=ftt.next().start;
    ++c;
    for (int i=wl; i < end; i+=cl(b,i)) {
      final int ch=cp(b,i);
      if (ftChar(ch) && ftp.contains(c))       print((char)0x10);
      ch(ch);
    }
    wl=end;
  }
  while (wl < b.length) {
    ch(cp(b,wl));
    wl+=cl(b,wl);
  }
  ind=false;
}
