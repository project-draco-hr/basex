{
  finishElement();
  int c=-1, wl=0;
  final Tokenizer ftt=new Tokenizer(b,null);
  while (ftt.more()) {
    ++c;
    for (int i=wl; i < ftt.p; i+=cl(b,i)) {
      final int ch=cp(b,i);
      if (ftChar(ch) && ftp.contains(c))       print((char)0x10);
      ch(ch);
    }
    wl=ftt.p;
  }
  while (wl < b.length) {
    ch(cp(b,wl));
    wl+=cl(b,wl);
  }
  ind=false;
}
