{
  finishElement();
  int c=-1, wl=0;
  final FTIterator lex=new FTLexer().init(b);
  while (lex.hasNext()) {
    final int pos=lex.next().cpos;
    ++c;
    for (int i=wl; i < pos; i+=cl(b,i)) {
      final int ch=cp(b,i);
      if (ftChar(ch) && ftp.contains(c))       print((char)0x10);
      ch(ch);
    }
    wl=pos;
  }
  while (wl < b.length) {
    ch(cp(b,wl));
    wl+=cl(b,wl);
  }
  ind=false;
}
