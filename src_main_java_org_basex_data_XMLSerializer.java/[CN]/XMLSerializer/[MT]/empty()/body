{
  if (mth == M_TEXT)   return;
  if (mth == M_XML) {
    print(ELEM4);
  }
 else {
    final byte[] tag=tags.get(tags.size());
    final boolean empty=EMPTY.contains(lc(tag));
    if (mth == M_XHTML && empty) {
      print(' ');
      print(ELEM4);
    }
 else {
      print(ELEM2);
      if (mth == M_HTML && empty)       return;
      ind=false;
      close(tag);
    }
  }
}
