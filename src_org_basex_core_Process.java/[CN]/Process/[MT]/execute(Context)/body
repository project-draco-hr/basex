{
  context=ctx;
  if (data() && context.data() == null)   return error(PROCNODB);
  if (updating() && (Prop.mainmem || Prop.onthefly))   return error(PROCMM);
  try {
    final boolean ok=exec();
    if (updating() && !(this instanceof Fs))     context.flush();
    return ok;
  }
 catch (  final Throwable ex) {
    ex.printStackTrace();
    if (ex instanceof OutOfMemoryError) {
      Performance.gc(2);
      return error(PROCOUTMEM);
    }
    return error(PROCERR,this,ex.toString());
  }
}
