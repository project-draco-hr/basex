{
  final Data data=context.data();
  try {
    if (printing())     out(out);
  }
 catch (  final IOException ex) {
    if (data != null)     data.setLock(0);
    throw ex;
  }
catch (  final Exception ex) {
    out.print(ex.toString());
    Main.debug(ex);
  }
  if (data != null)   data.setLock(0);
}
