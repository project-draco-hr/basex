{
  final XQuery query=new XQuery("\n" + "      \tif(current-date() > xs:date(\"2000-01-01\")) then lang(\"en\",<a/>) else ()\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
