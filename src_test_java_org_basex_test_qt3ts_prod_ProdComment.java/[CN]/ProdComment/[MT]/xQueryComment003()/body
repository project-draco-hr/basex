{
  final XQuery query=new XQuery("(: Name: XQueryComment003 :)\n" + "(: Description: Simple use case for XQuery comment containing '-' :)\n" + "\n"+ "\n"+ "(:This is a comment-:)\n"+ "<result/>\n"+ "",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result/>",false));
}
