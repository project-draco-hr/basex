{
  final XQuery query=new XQuery("(: Name: XQueryComment012 :)\n" + "(: Description: Comments that looks like a function call :)\n" + "\n"+ "\n"+ "/south(: test :)\n"+ "\n"+ "",ctx);
  try {
    query.context(node(file("prod/AxisStep/TreeEmpty.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<south mark=\"s0\" />",false));
}
