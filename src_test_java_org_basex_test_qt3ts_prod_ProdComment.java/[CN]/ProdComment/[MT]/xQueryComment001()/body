{
  final XQuery query=new XQuery("(: Name: XQueryComment001 :)\n" + "(: Description: Simple use case for XQuery comments :)\n" + "\n"+ "\n"+ "(: This is a comment :)\n"+ "<result/>\n"+ "\n"+ "",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result/>",false));
}
