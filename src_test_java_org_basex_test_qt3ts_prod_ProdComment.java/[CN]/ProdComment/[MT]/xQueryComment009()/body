{
  final XQuery query=new XQuery("(: Name: XQueryComment009 :)\n" + "(: Description: Comments inside a conditional expression :)\n" + "\n"+ "\n"+ "\n"+ "if (:test (: yada (: neato :) :) :) (/fs:MyComputer) \n"+ "\tthen (: yada :) \"true\"\n"+ "\telse \"false\"\n"+ "",ctx);
