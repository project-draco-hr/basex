{
  final XQuery query=new XQuery("(: Name: XQueryComment011 :)\n" + "(: Description: Comments inside a conditional expression :)\n" + "\n"+ "\n"+ "\n"+ "if (:test:)(:t2:)(:t3:) (/fs:MyComputer) \n"+ "\tthen \"true\"\n"+ "\telse \"false\"\n"+ "\n"+ "",ctx);
  query.context(node(file("prod/ForClause/fsx_NS.xml")));
  query.namespace("fs","http://www.example.com/filesystem");
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"true"));
}
