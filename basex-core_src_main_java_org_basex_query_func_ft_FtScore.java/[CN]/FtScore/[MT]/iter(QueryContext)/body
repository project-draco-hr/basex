{
  final boolean s=qc.scoring;
  final Iter iter;
  try {
    qc.scoring=true;
    iter=exprs[0].iter(qc);
  }
  finally {
    qc.scoring=s;
  }
  return new Iter(){
    @Override public Dbl next() throws QueryException {
      final Item item=iter.next();
      return item == null ? null : Dbl.get(item.score());
    }
  }
;
}
