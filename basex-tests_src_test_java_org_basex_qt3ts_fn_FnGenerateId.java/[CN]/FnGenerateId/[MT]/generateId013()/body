{
  final XQuery query=new XQuery("let $nodes := collection()//*\n" + "            return count($nodes) eq count(distinct-values($nodes/generate-id()))",ctx);
  try {
    query.addCollection("",new String[]{file("docs/auction.xml"),file("docs/works-mod.xml")});
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
