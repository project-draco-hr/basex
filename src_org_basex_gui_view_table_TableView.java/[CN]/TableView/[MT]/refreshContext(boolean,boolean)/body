{
  if (tdata.invalid || !GUIProp.showtable) {
    tdata.invalid=true;
    tdata.rows=null;
    return;
  }
  tdata.context();
  scroll.pos(0);
  if (quick) {
    scroll.height(tdata.rows.size * tdata.rowH(1));
    findFocus();
    repaint();
  }
 else {
    working=true;
    new Thread(this).start();
  }
}
