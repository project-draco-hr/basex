{
  final XQuery query=new XQuery("count(insert-before((error(), 1), 1, (1, \"two\", 3))) > 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("FOER0000")));
}
