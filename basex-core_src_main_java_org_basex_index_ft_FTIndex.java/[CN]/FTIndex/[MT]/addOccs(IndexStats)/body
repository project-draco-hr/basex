{
  int i=0;
  final int tl=tp.length;
  while (i < tl && tp[i] == -1)   ++i;
  int p=tp[i], j=i + 1;
  while (j < tl && tp[j] == -1)   ++j;
  while (p < tp[tl - 1]) {
    if (stats.adding(size(p,i)))     stats.add(inY.readBytes(p,i));
    p+=i + ENTRY;
    if (p == tp[j]) {
      i=j;
      while (j + 1 < tl && tp[++j] == -1)       ;
    }
  }
}
