{
  final XQuery query=new XQuery("for $var1 in (2,4,6,8), $var2 in (3 + $var1) - (4 * $var1) \n" + "        return fn:trace($var1 + $var2 ,\"The Value of $var 1 + $var2 is: \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-1 -5 -9 -13"));
}
