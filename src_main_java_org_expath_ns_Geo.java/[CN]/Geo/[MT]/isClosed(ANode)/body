{
  final Geometry geom=gmlReader(node,Q_GML_LINEARRING,Q_GML_LINESTRING,Q_GML_MULTILINESTRING);
  if (geom == null && gmlReader(node) != null)   throw GeoErrors.lineNeeded(node.qname().local());
  return Bln.get(geom instanceof LineString ? ((LineString)geom).isClosed() : geom instanceof LinearRing ? ((LinearRing)geom).isClosed() : ((MultiLineString)geom).isClosed());
}
