{
  String[] regex=new String[5];
  regex[0]=".*\\.pdf";
  regex[1]="a.d\\.pdf";
  regex[2]="[abc]dd.pdf";
  regex[3]="[a-c]dd.pdf";
  regex[4]="[a-c].d\\.p.*";
  String[] input=new String[3];
  input[0]="add.pdf";
  input[1]="dpdf";
  input[2]="cdd.pdf";
  for (  String i : regex) {
    System.out.println("Pattern: " + i);
    for (    String j : input) {
      boolean isMatch=Pattern.matches(i,j);
      System.out.println(isMatch + " " + j);
    }
    System.out.println();
  }
}
