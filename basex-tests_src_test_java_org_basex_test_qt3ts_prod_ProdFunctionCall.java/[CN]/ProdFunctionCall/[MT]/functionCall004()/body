{
  final XQuery query=new XQuery("\n" + "         declare function local:f($in as xs:boolean) as xs:boolean { $in };\n" + "         local:f(<a>0</a>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
