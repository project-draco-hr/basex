{
  final XQuery query=new XQuery("\n" + "        declare function local:f() as xs:double* { \n" + "          if (day-from-date(current-date()) < 32) then xs:integer(3) else -1\n"+ "        };\n"+ "        local:f() + 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"4"));
}
