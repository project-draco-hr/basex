{
  final Atts at=new Atts();
  for (  final QNm name : m.keySet()) {
    if (m.get(name) > 1)     Err.or(UPATTDUPL,name);
    final byte[] an=name.pref();
    final int ai=at.get(name.pref());
    if (ai == -1) {
      at.add(an,name.uri.str());
    }
 else     if (!eq(name.uri.str(),at.val[ai])) {
      Err.or(UPNSCONFL2);
    }
  }
}
