{
  final XQuery query=new XQuery("(xs:double(1), xs:double(2), xs:double(3)) castable as xs:double*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPST0003") || error("XPST0051")));
}
