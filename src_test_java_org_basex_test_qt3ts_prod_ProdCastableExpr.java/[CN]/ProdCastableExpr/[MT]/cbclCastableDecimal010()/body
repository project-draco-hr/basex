{
  final XQuery query=new XQuery("xs:float('-3.402823e38') castable as xs:decimal",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || assertBoolean(true)));
}
