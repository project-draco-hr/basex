{
  final XQuery query=new XQuery("(\"http:\\\\invalid>URI\\someURI\") castable as xs:anyURI",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || assertBoolean(true)));
}
