{
  final XQuery query=new XQuery("for $h in (/works) return $h/employee[@name][position() = 1]/string(@name)",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"Jane Doe 1"));
}
