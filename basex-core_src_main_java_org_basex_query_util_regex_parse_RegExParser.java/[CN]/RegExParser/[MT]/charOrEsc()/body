{
  int cp=-1;
switch (jj_ntk == -1 ? jj_ntk() : jj_ntk) {
case CHAR:
case DIGIT:
    cp=XmlChar();
  break;
case SINGLE_ESC:
jj_consume_token(SINGLE_ESC);
cp=Escape.getCp(token.image);
break;
default :
jj_la1[16]=jj_gen;
jj_consume_token(-1);
throw new ParseException();
}
{
if (true) return cp;
}
throw new Error("Missing return statement in function");
}
