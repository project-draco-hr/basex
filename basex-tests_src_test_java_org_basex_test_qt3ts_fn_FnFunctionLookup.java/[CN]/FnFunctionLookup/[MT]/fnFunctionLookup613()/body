{
  final XQuery query=new XQuery("\n" + "        declare variable $n external := xs:QName('local:return-v');\n" + "        declare variable $v := function-lookup($n, 0)();\n"+ "        declare function local:return-v() {$v + 1};\n"+ "        $v\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQDY0054"));
}
