{
  final XQuery query=new XQuery("( fn:function-lookup((if (current-date() eq xs:date('1900-01-01'))\n" + "                                  then fn:QName('http://www.w3.org/2005/xpath-functions', 'node-name')\n" + "                                  else ()), 1),\n"+ "              fn:function-lookup((if (current-date() eq xs:date('1900-01-01'))\n"+ "                                  then ()\n"+ "                                  else fn:QName('http://www.w3.org/2005/xpath-functions', 'node-name')), 1))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
