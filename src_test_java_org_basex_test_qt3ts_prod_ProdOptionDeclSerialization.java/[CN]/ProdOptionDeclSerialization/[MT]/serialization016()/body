{
  final XQuery query=new XQuery("declare namespace output = \"http://www.w3.org/2010/xslt-xquery-serialization\";\n" + "         (: the charset parameter of the media type MUST NOT be specified explicitly in the value of the media-type parameter. :)\n" + "         declare option output:media-type \"text/html; charset=ISO-8859-4\";\n"+ "         <result>ok</result>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertSerialization("<result>ok</result>",false) || assertSerialError("SEPM0016")));
}
