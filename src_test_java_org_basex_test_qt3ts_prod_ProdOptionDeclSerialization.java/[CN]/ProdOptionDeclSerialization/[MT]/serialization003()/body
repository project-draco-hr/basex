{
  final XQuery query=new XQuery("\n" + "         import module namespace test=\"http://www.w3.org/TestModules/test\";\n" + "         <result>{test:ok()}</result>\n"+ "      ",ctx);
  query.addModule("http://www.w3.org/TestModules/test",file("prod/Serialization/serialization1-lib.xq"));
  final QT3Result res=result(query);
  result=res;
  test(error("XQST0108"));
}
