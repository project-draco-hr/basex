{
  final XQuery query=new XQuery("declare namespace output = \"http://www.w3.org/2010/xslt-xquery-serialization\";\n" + "         declare option output:parameter-document \"Serialization/serialization-parameters.xml\";\n" + "         declare option output:indent \"yes\";\n"+ "         <result>ok</result>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<result>ok</result>",false));
}
