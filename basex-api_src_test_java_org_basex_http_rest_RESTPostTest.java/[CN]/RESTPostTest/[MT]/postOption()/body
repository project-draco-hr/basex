{
  assertEquals("2",post("","<query xmlns=\"" + URI + "\">"+ "<text>2, delete node &lt;a/&gt;</text>"+ "<option name='"+ MainOptions.MIXUPDATES.name()+ "' value='true'/></query>",APP_XML));
  try {
    post("","<query xmlns=\"" + URI + "\">"+ "<text>1, delete node &lt;a/&gt;</text>"+ "<option name='"+ MainOptions.MIXUPDATES.name()+ "' value='false'/></query>",APP_XML);
    fail("Error expected.");
  }
 catch (  final IOException ex) {
    assertContains(ex.getMessage(),"[XUST0001]");
  }
}
