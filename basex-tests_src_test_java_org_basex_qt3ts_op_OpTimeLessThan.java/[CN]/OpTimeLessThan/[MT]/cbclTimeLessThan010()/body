{
  final XQuery query=new XQuery("if (implicit-timezone() gt xs:dayTimeDuration('PT1H'))\n" + "            then xs:time(\"00:00:01+01:00\") ge xs:time(\"00:00:00\")\n" + "            else xs:time(\"00:00:00\") ge xs:time(\"00:00:00+01:01\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
