{
  try {
    finishText();
    final int as=at.getLength();
    atts.reset();
    for (int a=0; a < as; a++) {
      atts.add(Token.token(at.getQName(a)),Token.token(at.getValue(a)));
    }
    builder.startElem(Token.token(qn),atts);
    nodes++;
  }
 catch (  final IOException ex) {
    error(ex);
  }
}
