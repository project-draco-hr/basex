{
  try {
    checkText();
    final int as=at.getLength();
    final byte[][] atts=new byte[as << 1][];
    for (int a=0; a < as; a++) {
      atts[a << 1]=Token.token(at.getQName(a));
      atts[(a << 1) + 1]=Token.token(at.getValue(a));
    }
    builder.startNode(Token.token(qn),atts);
    nodes++;
  }
 catch (  final Exception ex) {
    BaseX.debug(ex);
  }
}
