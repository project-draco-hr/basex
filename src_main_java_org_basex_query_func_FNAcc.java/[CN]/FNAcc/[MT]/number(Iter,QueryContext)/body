{
  final Item it=ir.next();
  if (it == null || ir.next() != null)   return Dbl.NAN;
  final Type t=it.type;
  if (t.isFunction())   FIATOM.thrw(info,this);
  try {
    return t == AtomType.DBL ? it : AtomType.DBL.cast(it,ctx,info);
  }
 catch (  final QueryException ex) {
    return Dbl.NAN;
  }
}
