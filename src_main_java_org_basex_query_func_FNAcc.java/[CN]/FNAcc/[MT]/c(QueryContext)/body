{
  if (expr.length == 0)   return this;
  final Item it=expr[0].item() ? (Item)expr[0] : null;
switch (func) {
case STRING:
case NUMBER:
case STRLEN:
case NORM:
    return expr[0].empty() || it != null ? atomic(ctx,input) : this;
case URIQNAME:
  return expr[0].empty() ? Seq.EMPTY : it != null ? atomic(ctx,input) : this;
default :
return this;
}
}
