{
  query("element a { [] }","<a/>");
  query("element a { [()] }","<a/>");
  query("element a { [1] }","<a>1</a>");
  query("element a { [ <b>c</b> ] }","<a><b>c</b></a>");
  query("element a { [ <b>c</b>, <b>d</b> ] }","<a><b>c</b><b>d</b></a>");
  query("element { ['a'] } { }","<a/>");
  error("element { ['a', 'b'] } { }",SEQFOUND_X);
  error("element { [] } { }",EMPTYFOUND_X);
}
