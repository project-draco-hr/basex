{
  query("element a { [] }","<a/>");
  query("element a { [()] }","<a/>");
  query("element a { [1] }","<a>1</a>");
  query("element a { [ <b>c</b> ] }","<a>c</a>");
  query("element a { [ <b>c</b>, <b>d</b> ] }","<a>c d</a>");
  query("element { ['a'] } { }","<a/>");
  error("element { ['a', 'b'] } { }",SEQFOUND_X);
  error("element { [] } { }",EMPTYFOUND_X);
}
