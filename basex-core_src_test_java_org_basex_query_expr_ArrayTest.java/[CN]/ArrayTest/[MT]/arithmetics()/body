{
  query("[] + ()","");
  query("() - []","");
  query("[] + 1","");
  query("2 + []","");
  query("[[[]]] + 2","");
  query("[2] - 1","1");
  query("[[6]] * 2","12");
  query("[[7]] idiv 2","3");
  query("[[8]] div 5","1.6");
  query("[[9]] mod [[5]]","4");
  error("[1,2] + 3",Err.SEQFOUND_X);
  error("1 + [2,3]",Err.SEQFOUND_X);
}
