{
  return new AxisIter(){
    final Data data=ictx.data;
    final int sz=data.meta.size;
    int pre=-1;
    @Override public DBNode next(){
      while (++pre < sz) {
        if (data.kind(pre) == Data.ELEM && data.size(pre,Data.ELEM) == 1)         return new DBNode(data,pre,Data.ELEM);
      }
      return null;
    }
  }
;
}
