{
  try {
    return method.equals(NEW) ? constructor(args) : method(args,qc);
  }
 catch (  final QueryException ex) {
    throw ex;
  }
catch (  final InvocationTargetException ex) {
    final Throwable cause=ex.getCause();
    throw cause instanceof QueryException ? ((QueryException)cause).info(info) : JAVAERROR_X.get(info,name(),foundArgs(args),cause);
  }
catch (  final Throwable ex) {
    throw JAVAERROR_X.get(info,name(),foundArgs(args),ex);
  }
}
