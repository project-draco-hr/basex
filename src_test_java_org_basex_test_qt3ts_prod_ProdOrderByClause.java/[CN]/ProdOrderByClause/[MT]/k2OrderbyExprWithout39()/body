{
  final XQuery query=new XQuery("for $a in (1, 4, 2) let $i := (1, 3, 2) order by $i return 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"1 1 1") || error("XPTY0004")));
}
