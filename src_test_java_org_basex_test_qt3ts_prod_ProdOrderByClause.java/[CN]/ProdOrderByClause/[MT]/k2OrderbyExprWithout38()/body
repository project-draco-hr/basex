{
  final XQuery query=new XQuery("for $i in (1, 2, 3) stable order by 1 return reverse(($i, \"FO\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"FO 1 FO 2 FO 3"));
}
