{
  final XQuery query=new XQuery("for $a in (2, 1) let $b := 1 where true() order by $a return $a",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2"));
}
