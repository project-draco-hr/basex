{
  final XQuery query=new XQuery("let $numbers := (1, 2, 1.3, 3e3, xs:double(\"NaN\"), xs:double(\"-INF\"), xs:double(\"INF\")) return (for $i in $numbers stable order by $i empty least return $i, \"SEP\", for $i in $numbers order by $i empty greatest return $i)",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"NaN -INF 1 1.3 2 3000 INF SEP -INF 1 1.3 2 3000 INF NaN"));
}
