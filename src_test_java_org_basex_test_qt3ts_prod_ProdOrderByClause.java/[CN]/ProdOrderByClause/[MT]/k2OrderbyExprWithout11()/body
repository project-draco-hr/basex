{
  final XQuery query=new XQuery("for $a in (1, 4, 2) let $i := (1, 3, 2) order by $a return $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 3 2 1 3 2 1 3 2"));
}
