{
  final XQuery query=new XQuery("for $i in (1, 3, 2) stable order by () return $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"1 3 2") || assertStringValue(false,"1 2 3") || error("XPST0005")));
}
