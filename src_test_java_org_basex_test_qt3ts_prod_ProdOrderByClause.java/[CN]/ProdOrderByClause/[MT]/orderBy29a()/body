{
  final XQuery query=new XQuery("\n" + "        declare default element namespace \"http://www.w3.org/XQueryTestOrderBy\"; \n" + "        <results> { \n"+ "            for $x in /DataValues/NegativeNumbers/orderData \n"+ "            order by ($x * -1) descending \n"+ "            return ($x * -1e0) (:force to xs:double:) \n"+ "        } </results>\n"+ "      ",ctx);
  query.context(node(file("prod/OrderByClause/orderData.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<results xmlns=\"http://www.w3.org/XQueryTestOrderBy\">1.0E17 1.0E16 1.0E15 1.0E14 1.0E13 1.0E12 1.0E11 1.0E10 1.0E9 1.0E8 1.0E7 1.0E6 100000 10000 1000 100 10 1 0</results>",false));
}
