{
  final XQuery query=new XQuery("let $i := (<e>1</e>, <e>3</e>, <e>2</e>) stable order by $i return $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<e>1</e><e>3</e><e>2</e>",false) || error("XPTY0004")));
}
