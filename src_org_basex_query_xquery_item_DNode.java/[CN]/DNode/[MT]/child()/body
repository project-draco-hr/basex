{
  return new NodeMore(){
    /** 
 * Temporary node. 
 */
    private final DNode node=copy();
    /** 
 * First call. 
 */
    private boolean more;
    /** 
 * Current pre value. 
 */
    private int p;
    /** 
 * Current size value. 
 */
    private int s;
    @Override public boolean more(){
      if (!more) {
        final int k=data.kind(pre);
        p=pre + data.attSize(pre,k);
        s=pre + data.size(pre,k);
        more=true;
      }
      return p != s;
    }
    @Override public Node next(){
      if (!more())       return null;
      final int k=data.kind(p);
      node.set(p,k);
      p+=data.size(p,k);
      return node;
    }
  }
;
}
