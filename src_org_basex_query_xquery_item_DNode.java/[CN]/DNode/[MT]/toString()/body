{
switch (type) {
case ATT:
    return type + "(" + string(data.attName(pre))+ "=\""+ string(data.attValue(pre))+ "\")";
case DOC:
  return type + "(" + data.meta.file+ ")";
case ELM:
return type + "(" + string(data.tag(pre))+ "/"+ pre+ ")";
default :
String str=string(str());
if (str.length() > 20) str=str.substring(0,20) + "...";
return type + "(" + str+ ")";
}
}
