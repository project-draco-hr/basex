{
  final XQuery query=new XQuery("string(exactly-one((for $fo in (1, 2, 3) where $fo eq 3 return $fo)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"3"));
}
