{
  final XQuery query=new XQuery("count((for $fo in (1, 2, 3) where xs:time(\"08:08:23Z\") return $fo)) eq 3",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("FORG0006"));
}
