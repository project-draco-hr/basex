{
  final XQuery query=new XQuery("let $var := (fn:false()) where fn:not($var or fn:false()) return $var or fn:false()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
