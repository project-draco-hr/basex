{
  final XQuery query=new XQuery("\n" + "         declare variable $n external := 0;\n" + "         for $x in -2 to +2\n"+ "         where ($n - $x)\n"+ "         return $x\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-2 -1 1 2"));
}
