{
  final XQuery query=new XQuery("\n" + "         declare variable $n external := 0;\n" + "         for $x allowing empty in 1 to $n\n"+ "         where not($x = 5)\n"+ "         return concat(\"[\", $x, \"]\")\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"[]"));
}
