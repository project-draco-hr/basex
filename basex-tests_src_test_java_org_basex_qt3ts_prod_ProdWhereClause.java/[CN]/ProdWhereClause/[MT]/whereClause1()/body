{
  final XQuery query=new XQuery("let $var := (fn:true()) where $var or fn:true() return $var or fn:true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
