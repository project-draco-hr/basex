{
  final XQuery query=new XQuery("\n" + "         let $var := 1 \n" + "         where every $x in (1, 1) satisfies fn:string($var) = \"1\" \n"+ "         return fn:string($var )",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1"));
}
