{
  final XQuery query=new XQuery("fn:name(<anElement name=\"attribute1\">Content</anElement>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"anElement"));
}
