{
  final XQuery query=new XQuery("<elem att=\"{<e2 a2=\"{<e3 a3=\"{<p:e/>/namespace-uri()}\"></e3>/@a3}\"></e2>/@a2}\" \n" + "                                     xmlns:p=\"http://ns.example.com/uri\"/>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem att=\"http://ns.example.com/uri\" xmlns:p=\"http://ns.example.com/uri\"/>",false));
}
