{
  final XQuery query=new XQuery("<elem attr=\"1{2,3}{4,5}6{<a>7</a>}{<a>8</a>}9\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem attr=\"12 34 56789\"/>",false));
}
