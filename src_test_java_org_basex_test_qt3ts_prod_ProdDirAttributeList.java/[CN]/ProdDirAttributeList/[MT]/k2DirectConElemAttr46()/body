{
  final XQuery query=new XQuery("<e> { <b attr=\"fo\" a=\"bo\"/>/@* } </e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e attr=\"fo\" a=\"bo\"/>",false));
}
