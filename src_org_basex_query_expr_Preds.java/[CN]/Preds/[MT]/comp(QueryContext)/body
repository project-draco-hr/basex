{
  for (int p=0; p < pred.length; p++) {
    Expr ex=pred[p].comp(ctx);
    ex=Pos.get(ex,CmpV.Comp.EQ,ex);
    if (ex.i()) {
      if (!((Item)ex).bool()) {
        ctx.compInfo(OPTFALSE,ex);
        return null;
      }
      ctx.compInfo(OPTTRUE,ex);
      pred=Array.delete(pred,p--);
    }
 else {
      pred[p]=ex;
    }
  }
  return this;
}
