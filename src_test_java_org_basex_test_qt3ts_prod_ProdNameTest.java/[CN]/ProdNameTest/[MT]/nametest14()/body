{
  final XQuery query=new XQuery("let $var := /works[1]/child::employee[12]/overtime return fn:count($var/child::* except $var/child::day)",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
