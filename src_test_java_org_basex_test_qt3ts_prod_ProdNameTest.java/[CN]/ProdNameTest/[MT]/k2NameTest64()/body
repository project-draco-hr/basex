{
  final XQuery query=new XQuery("<e>{<e foo=\"asd\"/>/attribute(*, xs:untypedAtomic)}</e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e foo=\"asd\"/>",false));
}
