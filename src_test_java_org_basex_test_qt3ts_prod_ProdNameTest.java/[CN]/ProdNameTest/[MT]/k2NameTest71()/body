{
  final XQuery query=new XQuery("<e>{<b foo=\"1\"/>/attribute(foo, xs:anyType)}</e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e foo=\"1\"/>",false));
}
