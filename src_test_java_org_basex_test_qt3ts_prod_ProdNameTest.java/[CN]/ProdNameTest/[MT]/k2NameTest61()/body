{
  final XQuery query=new XQuery("<e>{for $PI in ()/processing-instruction() return ()}</e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<e/>",false) || error("XPST0005")));
}
