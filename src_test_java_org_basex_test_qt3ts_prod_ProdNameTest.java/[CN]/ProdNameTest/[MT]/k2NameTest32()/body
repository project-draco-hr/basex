{
  final XQuery query=new XQuery("<a>{<e foo=\"content2\" bar=\"content1\"/>/attribute(foo)}</a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a foo=\"content2\"/>",false));
}
