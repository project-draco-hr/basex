{
  final XQuery query=new XQuery("subsequence(1 to 3000000000, 2147483647, 5)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"2147483647 2147483648 2147483649 2147483650 2147483651") || error("*")));
}
