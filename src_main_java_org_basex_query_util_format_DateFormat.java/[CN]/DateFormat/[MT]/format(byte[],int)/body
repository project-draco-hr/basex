{
  int cp=p;
  final int l=format.length;
  if (ch(format,cp) == 'o') {
    final TokenBuilder tb=new TokenBuilder();
    if (ch(format,++cp) == '(') {
      while (ch(format,++cp) != ')') {
        if (cp == l)         INVORDINAL.thrw(info,format);
        tb.add(ch(format,cp));
      }
      ++cp;
    }
    ordinal=tb.finish();
  }
 else   if (ch(format,cp) == 'c') {
    ++cp;
  }
 else   if (ch(format,cp) == 't' || ch(format,cp) == 'a') {
    ++cp;
  }
  return cp;
}
