{
  final XQuery query=new XQuery("\n" + "          ($staff/staff[1]/employee[1]/empnum[1]) is $works/works[1]/employee[1]/empnum[1]",ctx);
  query.bind("$works",node(file("docs/works.xml")));
  query.bind("$staff",node(file("docs/staff.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(false));
}
