{
  final XQuery query=new XQuery("empty(exactly-one(<e/>/*) is exactly-one(<e/>/*))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(false) || error("FORG0005")));
}
