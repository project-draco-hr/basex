{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:f() as node() { <a/> }; \n" + "      \tlocal:f() is local:f()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
