{
  final XQuery query=new XQuery("declare variable $e := attribute name {()}; $e is $e, <is/> is <is/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"true false"));
}
