{
  Formatter form=MAP.get(ln);
  if (form == null) {
    try {
      final String clz=Formatter.class.getSimpleName() + ln.toUpperCase();
      form=(Formatter)Class.forName(clz).newInstance();
      MAP.put(ln,form);
    }
 catch (    final Exception ex) {
      form=MAP.get(EN);
    }
  }
  return form;
}
