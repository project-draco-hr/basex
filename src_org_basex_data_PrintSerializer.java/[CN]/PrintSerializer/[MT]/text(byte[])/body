{
  for (  final byte ch : b) {
switch (ch) {
case '&':
      out.print('&');
    out.print(Token.E_AMP);
  out.print(';');
break;
case '>':
out.print('&');
out.print(Token.E_GT);
out.print(';');
break;
case '<':
out.print('&');
out.print(Token.E_LT);
out.print(';');
break;
case 0xD:
out.print('&');
out.print(E_CR);
out.print(';');
break;
default :
out.write(ch);
}
}
more=false;
}
