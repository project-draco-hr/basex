{
  final XQuery query=new XQuery("\n" + "        declare construction strip; \n" + "        fn:not(<someElement>some content</someElement> instance of element(*,xs:untyped))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
