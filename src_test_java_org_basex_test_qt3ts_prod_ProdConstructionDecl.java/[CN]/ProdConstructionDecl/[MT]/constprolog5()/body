{
  final XQuery query=new XQuery("\n" + "        declare construction strip; \n" + "        let $anElement := element anElement {\"someContent\"} \n"+ "        return $anElement instance of element(*,xs:anyType)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
