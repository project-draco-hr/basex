{
  final XQuery query=new XQuery("\n" + "        declare construction preserve; \n" + "        fn:not(element someElement{\"some content\"} instance of element(*,xs:anyType))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
