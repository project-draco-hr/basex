{
  final XQuery query=new XQuery("\n" + "        declare construction preserve; \n" + "        let $anElement := <someElement>some content</someElement> \n"+ "        return fn:not($anElement instance of element(*,xs:anyType))\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(false));
}
