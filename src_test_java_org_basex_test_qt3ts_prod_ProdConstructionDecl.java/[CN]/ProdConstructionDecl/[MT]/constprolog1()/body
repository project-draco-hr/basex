{
  final XQuery query=new XQuery("\n" + "        declare construction strip; \n" + "        declare construction strip; \n"+ "        \"abc\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0067"));
}
