{
  final XQuery query=new XQuery("\n" + "        declare construction preserve; \n" + "        let $anElement := <someElement>content</someElement> \n"+ "        return ($anElement instance of element(*,xs:anyType)) or fn:false()\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
