{
  final XQuery query=new XQuery("unordered {//part[@partof < 2] except //part[@partid = 2] }",ctx);
  try {
    query.context(node(file("docs/partlist.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<part partid=\"1\" partof=\"0\" name=\"engine\"/><part partid=\"3\" partof=\"1\" name=\"piston\"/>",false) || assertSerialization("<part partid=\"3\" partof=\"1\" name=\"piston\"/><part partid=\"1\" partof=\"0\" name=\"engine\"/>",false)));
}
