{
  final XQuery query=new XQuery("unordered {//part[@partid < 2][2]}",ctx);
  try {
    query.context(node(file("docs/partlist.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<part partid=\"1\" partof=\"0\" name=\"engine\"/>",false) || assertSerialization("<part partid=\"0\" name=\"car\"/>",false)));
}
