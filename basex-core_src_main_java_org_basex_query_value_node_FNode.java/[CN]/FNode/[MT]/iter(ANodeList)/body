{
  return new AxisMoreIter(){
    /** 
 * Child counter. 
 */
    int c;
    @Override public boolean more(){
      return iter != null && c != iter.size();
    }
    @Override public ANode next(){
      return more() ? iter.get(c++) : null;
    }
    @Override public ANode get(    final long i){
      return iter.get((int)i);
    }
    @Override public long size(){
      return iter.size();
    }
    @Override public Value value(){
      return iter.value();
    }
  }
;
}
