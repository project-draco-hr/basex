{
  final XQuery query=new XQuery("xs:string(\"hello\") , xs:integer(\"100\"), xs:anyURI(\"www.example.com\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("\"hello\", 100, \"www.example.com\""));
}
