{
  Data d=null;
  try {
    d=CreateDB.create(input.io,single,ctx.context);
  }
 catch (  final IOException ex) {
    final IO base=ctx.sc.baseIO();
    if (base != null) {
      try {
        final String in=base.merge(input.original).path();
        if (!in.equals(input.original))         d=CreateDB.create(IO.get(in),single,ctx.context);
      }
 catch (      final IOException exc) {
      }
    }
    if (d == null)     IOERR.thrw(info,ex);
  }
  input.path="";
  addData(d);
  return d;
}
