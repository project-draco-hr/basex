{
  final Data d=nodes.data;
  if (!ctx.context.perm(Perm.READ,d.meta))   BASX_PERM.thrw(null,Perm.READ);
  ctx.value=d.isEmpty() ? Empty.SEQ : DBNodeSeq.get(new IntList(nodes.pres),d,nodes.root,nodes.root);
  addCollection(nodes.root ? ctx.value : DBNodeSeq.get(d.resources.docs(),d,true,true),d.meta.name);
  addData(d);
}
