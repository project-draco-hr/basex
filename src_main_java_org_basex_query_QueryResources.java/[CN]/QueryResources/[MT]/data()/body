{
  if (ctx.value == null)   return null;
  if (docNodes())   return data[0];
  final Iter iter=ctx.value.iter();
  Data db=null;
  Item it;
  while ((it=iter.next()) != null) {
    if (!(it instanceof DBNode))     return null;
    final Data d=((DBNode)it).data;
    if (db == null)     db=d;
 else     if (db != d)     return null;
  }
  return db;
}
