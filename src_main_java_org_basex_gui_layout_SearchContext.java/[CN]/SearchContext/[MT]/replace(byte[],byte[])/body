{
  final SearchResult repl=new SearchResult();
  if (!start.isEmpty()) {
    final int ss=start.size();
    final ByteList bl=new ByteList();
    int s1=0;
    for (int p=0; p < ss; p++) {
      final int s2=start.get(p);
      bl.add(text,s1,s2);
      bl.add(replace);
      s1=end.get(p);
    }
    bl.add(text,s1,text.length);
    repl.text=bl.toArray();
    repl.nr=ss;
  }
  return repl;
}
