{
  final byte[] name=trim(checkStr(q));
  if (!XMLToken.isQName(name))   Err.value(Type.QNM,q);
  final QNm nm=new QNm(name);
  final byte[] pref=nm.pref();
  byte[] uri=((Nod)check(it,Type.ELM)).uri(pref);
  if (uri == null) {
    if (pref.length != 0)     Err.or(NSDECL,pref);
    uri=ctx.nsElem;
  }
  nm.uri=Uri.uri(uri);
  return nm;
}
