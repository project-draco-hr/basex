{
  final XQuery query=new XQuery("import schema namespace aSpace=\"http://www.youcannotfindthisschemaorg/schemas\" at \"http://www.youcannotfindithere/noschemas\";\n" + "             \"abc\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0059"));
}
