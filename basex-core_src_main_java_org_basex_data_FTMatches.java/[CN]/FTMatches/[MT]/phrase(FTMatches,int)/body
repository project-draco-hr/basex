{
  int a=0, b=0, c=0;
  while (a < size && b < all.size) {
    final int e=all.match[b].match[0].start;
    final int d=e - match[a].match[0].end - dis;
    if (d == 0) {
      match[c]=match[a];
      match[c++].match[0].end=e;
    }
    if (d >= 0)     ++a;
    if (d <= 0)     ++b;
  }
  size=c;
  return size != 0;
}
