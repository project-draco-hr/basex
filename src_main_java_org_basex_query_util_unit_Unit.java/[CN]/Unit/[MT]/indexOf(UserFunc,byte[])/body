{
  final Ann ann=func.ann;
  final int as=ann.size();
  int pos=-1;
  for (int a=0; a < as; a++) {
    final QNm nm=ann.names[a];
    if (eq(nm.uri(),QueryText.XQUNITURI) && eq(nm.local(),name)) {
      if (pos != -1)       UNIT_TWICE.thrw(info,'%',nm.local());
      pos=a;
    }
  }
  return pos;
}
