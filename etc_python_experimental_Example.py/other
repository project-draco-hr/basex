import BaseXClient, time
start = time.clock()
cmd = '1 to 2'
try:
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    query = BaseXClient.Query(session)
    if query.run(cmd):
        print query.result()
    else:
        print query.info()
    session.close()
    time = ((time.clock() - start) * 1000)
    print time, 'ms.'
except IOError as e:
    print e
