import BaseXClient, time
start = time.clock()
cmd = '1 to 10'
try:
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    query = BaseXClient.Query(session)
    print query.run(cmd)
    session.close()
    time = ((time.clock() - start) * 1000)
    print time, 'ms.'
except IOError as e:
    print e
