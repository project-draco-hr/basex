import BaseXClient, time
start = time.clock()
cmd = "for $x in doc('input')//li return $x"
try:
    session = BaseXClient.Session('localhost', 1984, 'admin', 'admin')
    query = session.query(cmd)
    if query.run():
        while query.more():
            print query.next()
    else:
        print query.info()
    session.close()
    time = ((time.clock() - start) * 1000)
    print time, 'ms.'
except IOError as e:
    print e
