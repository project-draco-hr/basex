{
  final XQuery query=new XQuery("declare base-uri \"http://example.org\"; declare base-uri \"http://example.org\"; \"aaa\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0032"));
}
