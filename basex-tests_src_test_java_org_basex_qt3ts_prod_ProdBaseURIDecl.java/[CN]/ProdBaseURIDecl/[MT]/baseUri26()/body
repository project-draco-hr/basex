{
  final XQuery query=new XQuery("declare base-uri \"abc\"; fn:ends-with(fn:string(fn:static-base-uri()),\"abc\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
