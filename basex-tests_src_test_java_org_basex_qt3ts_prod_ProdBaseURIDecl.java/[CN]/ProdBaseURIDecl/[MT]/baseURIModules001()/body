{
  final XQuery query=new XQuery("declare base-uri \"http://www.example.org/base1\";\n" + "          import module namespace m =\"http://www.w3.org/TestModules/module-001\";\n" + "          static-base-uri() eq m:static-base-uri()\n"+ "    ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/module-001",file("prod/BaseURIDecl/module-001.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
