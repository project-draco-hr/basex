{
  if (size == prepos.length) {
    prepos=Array.extend(prepos);
    poi=Array.extend(poi);
  }
  if (size == 0) {
    prepos[size]=pp;
    poi[size++]=p;
  }
 else   if (prepos[size - 1][0] == pp[0]) {
    add(size - 1,pp,p);
  }
 else {
    final int i=find(pp[0]);
    if (i >= 0)     add(i,pp,p);
 else     add(-i - 1,pp,p);
  }
}
