{
  if (size == 0 || pres == null)   return;
  final int[][] tpp=new int[pres.length][];
  final int[][] tp=new int[pres.length][];
  int p=0, t=0, i=0;
  while (i < size && p < pres.length) {
    final int d=prepos[i][0] - pres[p];
    if (d == 0) {
      tpp[t]=prepos[i];
      tp[t++]=poi[i];
      p++;
    }
 else     if (d < 0)     i++;
 else     p++;
  }
  prepos=tpp;
  poi=tp;
  size=t;
}
