{
  if (size == 0)   return;
  int l=0, r=size;
  while (l < r) {
    final int m=l + (r - l) / 2;
    final int c=prepos[m][0] - pre;
    if (c == 0) {
      removeFTData(m);
      return;
    }
 else     if (c < 0)     l=m + 1;
 else     r=m - 1;
  }
  if (r != size && l == r && prepos[l][0] == pre)   removeFTData(l);
}
