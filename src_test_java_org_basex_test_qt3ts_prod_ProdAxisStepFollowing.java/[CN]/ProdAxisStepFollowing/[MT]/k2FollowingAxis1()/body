{
  final XQuery query=new XQuery("\n" + "        let $i := <html> <head/> <body> <p attr=\"foo\"> </p> <p attr=\"boo\"> </p> <p> </p> <p> </p> <p> </p> </body> </html> \n" + "        return $i//p[1]/following::*\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<p attr=\"boo\"/><p/><p/><p/>",false));
}
