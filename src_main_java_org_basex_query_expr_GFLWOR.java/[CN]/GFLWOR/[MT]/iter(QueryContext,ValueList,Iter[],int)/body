{
  final boolean more=p + 1 != fl.length;
  while (it[p].next() != null) {
    if (more) {
      iter(ctx,vl,it,p + 1);
    }
 else     if (where == null || where.ebv(ctx,input).bool(input)) {
      if (group != null) {
        group.gp.add(ctx);
      }
 else       if (order != null) {
        order.add(ctx);
        vl.add(ret.value(ctx));
      }
    }
  }
}
