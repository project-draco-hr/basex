{
  final Iter[] iter=new Iter[fl.length];
  final int vss=ctx.vars.size();
  buildIter(iter,ctx);
  group.initgroup(fl);
  iter(ctx,iter,0);
  ctx.vars.reset(vss);
  final int vs=ctx.vars.size();
  for (  final ForLet aFl : fl)   ctx.vars.add(aFl.var);
  final ItemIter ir=new ItemIter();
  group.ret(ctx,ir,ret);
  ctx.vars.reset(vs);
  return ir;
}
