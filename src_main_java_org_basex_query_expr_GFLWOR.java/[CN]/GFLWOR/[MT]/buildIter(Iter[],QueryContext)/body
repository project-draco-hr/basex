{
  boolean[] vars=new boolean[fl.length];
  for (int f=0; f < group.groupby.length; ++f) {
    for (int p=0; p < fl.length; ++p) {
      if (fl[p].var.eq(group.groupby[f])) {
        iter[f]=ctx.iter(fl[p]);
        vars[p]=true;
      }
    }
  }
  for (int f=0; f < iter.length; ++f) {
    if (iter[f] != null)     continue;
    for (int p=0; p < fl.length; ++p) {
      if (vars[p])       continue;
      iter[f]=ctx.iter(fl[p]);
      vars[p]=true;
      break;
    }
  }
}
