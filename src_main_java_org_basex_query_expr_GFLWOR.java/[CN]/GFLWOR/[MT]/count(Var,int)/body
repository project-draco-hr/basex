{
  int c=0;
  for (int f=Math.max(0,i); f < fl.length; f++) {
    c+=fl[f].count(v);
    if ((i == -1 || f > i) && fl[f].shadows(v))     return c;
  }
  if (where != null)   c+=where.count(v);
  if (order != null)   c+=order.count(v);
  if (group != null)   c+=group.count(v);
  return c + ret.count(v);
}
