{
  final StringBuilder sb=new StringBuilder();
  sb.append("MAB2Entry:\n");
  sb.append("ID: " + Token.string(id) + "\n");
  sb.append("Pos: " + pos + "\n");
  sb.append("Size: " + size + "\n");
  sb.append("Children: \n");
  for (int s=0; s < size; s++) {
    if (s != 0)     sb.append(", ");
    sb.append(children[s]);
  }
  sb.append("\n");
  return sb.toString();
}
