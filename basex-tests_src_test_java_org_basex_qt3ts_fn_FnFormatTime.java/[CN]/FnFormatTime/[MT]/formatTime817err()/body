{
  final XQuery query=new XQuery("format-time(current-time(), '[bla]', 'en', (), ())",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XTDE1340") || error("FOFD1340")));
}
