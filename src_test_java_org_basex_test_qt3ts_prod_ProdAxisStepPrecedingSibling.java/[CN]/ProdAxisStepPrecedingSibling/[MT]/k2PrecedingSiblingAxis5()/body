{
  final XQuery query=new XQuery("declare variable $i := <root> <child> <preceding2/> <child2> <preceding1/> <child3> <leaf/> </child3> <following/> </child2> <following/> </child> <following/> </root>; root($i)//count(preceding-sibling::node())",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"0 0 0 1 0 1 0 2 2 1"));
}
