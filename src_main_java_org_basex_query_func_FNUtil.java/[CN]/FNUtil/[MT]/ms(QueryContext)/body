{
  final boolean c=expr.length == 2 && checkType(expr[1].item(ctx,input),AtomType.BLN).bool(input);
  final Performance p=new Performance();
  final Iter ir=expr[0].iter(ctx);
  if (c) {
    ItemIter.get(ir);
  }
 else {
    while (ir.next() != null)     ;
  }
  return Dbl.get(p.getTime() / 10000 / 100d);
}
