{
switch (def) {
case FORMAT:
    return format(ctx);
case MB:
  return mb(ctx);
case MS:
return ms(ctx);
case FRM_BASE:
return fromBase(ctx,ii);
case TO_BASE:
return toBase(ctx,ii);
case MD5:
return hash(ctx,"MD5");
case SHA1:
return hash(ctx,"SHA");
case CRC32:
return crc32(ctx);
case UUID:
return uuid();
case TO_STRING:
return toString(ctx);
default :
return super.item(ctx,ii);
}
}
