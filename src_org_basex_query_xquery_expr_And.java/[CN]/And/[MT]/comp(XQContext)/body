{
  super.comp(ctx);
  for (int e=0; e < expr.length; e++) {
    if (!expr[e].i())     continue;
    if (!((Item)expr[e]).bool()) {
      ctx.compInfo(OPTFALSE,expr[e]);
      return Bln.FALSE;
    }
    ctx.compInfo(OPTTRUE,expr[e]);
    expr=Array.delete(expr,e--);
    if (expr.length == 0)     return Bln.TRUE;
  }
  return this;
}
