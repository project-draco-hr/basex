{
  final TokenBuilder tb=new TokenBuilder();
  tb.add(text,0,ps);
  int cc=0;
  final int cl=str.length();
  for (int c=0; c < cl; ++c) {
    final int ch=str.charAt(c);
    if (ch != '\r') {
      tb.addUTF(ch);
      ++cc;
    }
  }
  tb.add(text,ps,size);
  text=tb.finish();
  size=tb.size();
  for (int c=0; c < cc; ++c)   next();
}
