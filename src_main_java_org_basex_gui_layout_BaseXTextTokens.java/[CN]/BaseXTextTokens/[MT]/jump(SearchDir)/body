{
  if (search.start.isEmpty()) {
    noSelect();
    return false;
  }
  int s=search.start.sortedIndexOf(pc);
switch (dir) {
case SAME:
    s=s < 0 ? -s - 1 : s;
  break;
case FORWARD:
s=s < 0 ? -s - 1 : s + 1;
break;
case BACKWARD:
s=s < 0 ? -s - 2 : s - 1;
break;
}
final int sl=search.start.size();
if (s < 0) s=sl - 1;
 else if (s == sl) s=0;
ms=search.start.get(s);
me=search.end.get(s);
pc=ms;
return true;
}
