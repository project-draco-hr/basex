{
  final XQuery query=new XQuery("( fn:innermost( if (current-date() eq xs:date('1900-01-01'))\n" + "                                 then .\n" + "                                 else 1 ),\n"+ "              fn:innermost( if (current-date() eq xs:date('1900-01-01'))\n"+ "                                 then 1\n"+ "                                 else . ) )",ctx);
  try {
    query.context(node(file("fn/innermost/innermost.xml")));
    query.bind("$doc1",node(file("fn/innermost/doc1.xml")));
    query.bind("$doc2",node(file("fn/innermost/doc2.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
