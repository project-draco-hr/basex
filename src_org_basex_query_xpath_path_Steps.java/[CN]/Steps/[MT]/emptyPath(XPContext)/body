{
  for (int s=0; s < size; s++) {
    final Step step=steps[s];
    if (step.test instanceof TestName && ((TestName)step.test).id == TestName.UNKNOWN) {
      ctx.compInfo(OPTNAME,((TestName)step.test).name);
      return true;
    }
    for (    final Pred p : step.preds) {
      if (p.posPred() == -1) {
        ctx.compInfo(OPTPOSPRED2);
        return true;
      }
    }
  }
  return false;
}
