{
  final XQuery query=new XQuery("xs:dateTime(\"1970-01-01T00:00:00Z\") - xs:yearMonthDuration(\"P2030Y12M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"-0062-01-01T00:00:00Z") || assertStringValue(false,"-0061-01-01T00:00:00Z")));
}
