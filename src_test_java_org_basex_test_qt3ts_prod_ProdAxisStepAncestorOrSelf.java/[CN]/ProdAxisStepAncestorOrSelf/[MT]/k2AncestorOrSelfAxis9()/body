{
  final XQuery query=new XQuery("<r> <a> <b> <c/> </b> </a> </r>/a/b/c/(ancestor-or-self::*[1], ancestor-or-self::*[2], ancestor-or-self::*[last()], ancestor-or-self::*[10])",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<r><a><b><c/></b></a></r><b><c/></b><c/>",false));
}
