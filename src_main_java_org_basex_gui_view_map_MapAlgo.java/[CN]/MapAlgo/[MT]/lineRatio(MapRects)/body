{
  if (r.size == 0)   return Double.MAX_VALUE;
  double ar=0;
  int dev=0;
  for (  final MapRect rect : r) {
    if (rect.w != 0 && rect.h != 0) {
      dev++;
      if (rect.w > rect.h) {
        ar+=(double)rect.w / rect.h;
      }
 else {
        ar+=(double)rect.h / rect.w;
      }
    }
  }
  return ar / dev;
}
