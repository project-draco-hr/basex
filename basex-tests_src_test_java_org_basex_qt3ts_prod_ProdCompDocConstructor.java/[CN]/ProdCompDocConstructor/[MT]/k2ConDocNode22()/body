{
  final XQuery query=new XQuery("document{document{document{document{<e/>, document{()}, <e>{document{()}}</e>}}}}//child::node()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e/><e/>",false));
}
