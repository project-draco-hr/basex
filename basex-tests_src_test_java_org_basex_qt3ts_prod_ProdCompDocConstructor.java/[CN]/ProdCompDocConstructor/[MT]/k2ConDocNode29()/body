{
  final XQuery query=new XQuery("<doo> { document { <e> { <?target data?>, attribute name {\"content\"} } </e> } } </doo>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQTY0024"));
}
