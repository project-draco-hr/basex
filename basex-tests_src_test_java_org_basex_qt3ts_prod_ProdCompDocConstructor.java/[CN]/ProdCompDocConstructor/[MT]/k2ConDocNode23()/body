{
  final XQuery query=new XQuery("string(document{ text {\"data\"}, text {\"data\"}, text {\"data\"}, text {\"data\"} })",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"datadatadatadata"));
}
