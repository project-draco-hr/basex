{
  final XQuery query=new XQuery("document {1, document {2, document {document {()}, 3, document {4}}, 5}, 6}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"123456"));
}
