{
  final XQuery query=new XQuery("1, document{document{document{document{()}}}}/child::node(), 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 1"));
}
