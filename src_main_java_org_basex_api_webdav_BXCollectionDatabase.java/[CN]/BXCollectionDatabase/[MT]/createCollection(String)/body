{
  try {
    ClientSession cs=login(user,pass);
    try {
      cs.execute(new Open(dbname));
      cs.execute(new Add("<empty/>","EMPTY.xml",newName));
    }
  finally {
      cs.close();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return new BXFolder(dbname,newName,user,pass);
}
