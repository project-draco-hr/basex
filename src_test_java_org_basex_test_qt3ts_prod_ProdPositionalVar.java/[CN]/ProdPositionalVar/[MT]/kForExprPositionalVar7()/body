{
  final XQuery query=new XQuery("deep-equal((true(), true()), for $i at $p in (1, 2) return boolean($p))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
