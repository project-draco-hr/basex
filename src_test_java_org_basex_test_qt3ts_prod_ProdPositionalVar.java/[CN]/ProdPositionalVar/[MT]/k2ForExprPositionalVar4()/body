{
  final XQuery query=new XQuery("for $i at $pos in (3 to 6) let $let := $pos + 1 return ($let, $let - 1)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2 1 3 2 4 3 5 4"));
}
