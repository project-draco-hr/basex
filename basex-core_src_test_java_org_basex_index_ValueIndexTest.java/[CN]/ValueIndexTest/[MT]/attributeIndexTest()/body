{
  final LinkedHashMap<String,Integer> tokens=new LinkedHashMap<>();
  tokens.put("context",1);
  tokens.put("baz bar blu",1);
  tokens.put("baz",0);
  tokens.put("bar",0);
  tokens.put("blu",0);
  tokens.put("",0);
  tokens.put("nonexistant",0);
  valueIndexTest(IndexType.ATTRIBUTE,tokens,paramSet);
}
