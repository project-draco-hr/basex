{
  int p=0;
  for (; p < pic.length && pic[p] != 'o'; p+=cl(pic,p))   ;
  final byte[] s=token(n);
  for (int i=p - s.length; i > 0; i--)   tb.add('0');
  tb.add(s);
  if (ord(pic,p) != null) {
    final int f=(int)(n % 10);
    tb.add(ORDSUFFIX[f > 0 && f < 4 && n % 100 / 10 != 1 ? f - 1 : 3]);
  }
}
