{
switch (def) {
case FORMINT:
    return formatInteger(ctx);
case FORMNUM:
  return formatNumber(ctx);
case FORMDTM:
return formatDate(SimpleType.DTM,ctx);
case FORMDAT:
return formatDate(SimpleType.DAT,ctx);
case FORMTIM:
return formatDate(SimpleType.TIM,ctx);
default :
return super.item(ctx,ii);
}
}
