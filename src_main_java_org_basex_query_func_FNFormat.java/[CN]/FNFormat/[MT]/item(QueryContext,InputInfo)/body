{
switch (def) {
case FORMAT_INTEGER:
    return formatInteger(ctx);
case FORMAT_NUMBER:
  return formatNumber(ctx);
case FORMAT_DATETIME:
return formatDate(AtomType.DTM,ctx);
case FORMAT_DATE:
return formatDate(AtomType.DAT,ctx);
case FORMAT_TIME:
return formatDate(AtomType.TIM,ctx);
default :
return super.item(ctx,ii);
}
}
