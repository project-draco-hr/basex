{
  final int f=gui.context.focused;
  if (f == -1)   focused=null;
  if (mainRects == null)   return;
  final int ms=mainRects.size;
  for (int mi=0; mi < ms; mi++) {
    final MapRect rect=mainRects.get(mi);
    if (f == rect.pre || mi + 1 == ms || f < mainRects.get(mi + 1).pre) {
      focused=rect;
      repaint();
      break;
    }
  }
}
