{
  if (!GUIProp.showmap) {
    mainRects=null;
    return;
  }
  final Nodes context=GUI.context.current();
  final boolean page=!more && rectHist[hist + 1] != null && rectHist[hist + 1].p == 0 || more && (context.size != 1 || focusedRect == null || context.nodes[0] != focusedRect.p);
  if (page)   focusedRect=new MapRect(0,0,getWidth(),1);
  zoom(more,quick);
}
