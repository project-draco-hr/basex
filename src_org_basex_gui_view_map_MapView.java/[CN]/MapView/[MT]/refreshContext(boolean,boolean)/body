{
  if (!GUIProp.showmap) {
    mainRects=null;
    return;
  }
  final Nodes context=gui.context.current();
  final int hist=gui.notify.hist;
  final boolean page=!more && rectHist[hist + 1] != null && rectHist[hist + 1].pre == 0 || more && (context.size != 1 || focusedRect == null || context.nodes[0] != focusedRect.pre);
  if (page)   focusedRect=new ViewRect(0,0,getWidth(),1);
  zoom(more,quick);
}
