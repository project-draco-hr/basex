{
  final double score=Math.max(item1.score(),item2.score());
  final int[] p=new int[n];
  for (  final FTMatch mi : item1.all) {
    for (    final FTStringMatch sm : mi) {
      p[sm.q - 1]=-1;
    }
  }
  int count=0;
  for (  final int pp : p)   if (pp == -1)   count++;
  return score * (count > m ? 1 : count / m);
}
