{
  final Expr a=exprs[1];
  if (!(a instanceof Item))   return this;
  final Item it=(Item)a;
  if (!it.type.isNumberOrUntyped())   return this;
  final Expr[] args=((Arr)exprs[0]).exprs;
  final StaticContext sc=cc.sc();
switch (check(comp,it)) {
case 0:
    return Bln.TRUE;
case 1:
  return Bln.FALSE;
case 2:
return Function.BOOLEAN.get(sc,info,Function.STRING.get(sc,info,args));
case 3:
return Function.NOT.get(sc,info,Function.STRING.get(sc,info,args));
default :
return this;
}
}
