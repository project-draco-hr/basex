{
  final XQuery query=new XQuery("xs:date(\"2000-10-31-05:00\") - xs:yearMonthDuration(\"P1Y1M\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1999-09-30-05:00"));
}
