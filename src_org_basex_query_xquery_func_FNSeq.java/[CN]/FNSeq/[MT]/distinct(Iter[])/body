{
  if (arg.length == 2)   checkColl(arg[1]);
  return new Iter(){
    ItemSet map=new ItemSet();
    @Override public Item next() throws XQException {
      Item i;
      while ((i=arg[0].next()) != null) {
        i=FNGen.atom(i);
        if (map.index(i))         return i;
      }
      map=null;
      return null;
    }
  }
;
}
