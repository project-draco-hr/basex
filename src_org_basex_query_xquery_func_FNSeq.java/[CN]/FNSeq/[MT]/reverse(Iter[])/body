{
  final Iter iter=arg[0];
  if (iter instanceof RangeIter) {
    ((RangeIter)iter).reverse();
    return iter;
  }
  return new Iter(){
    final SeqIter sb=new SeqIter(iter);
    int c=sb.size;
    @Override public long size(){
      return sb.size;
    }
    @Override public Item next(){
      return --c < 0 ? null : sb.item[c];
    }
  }
;
}
