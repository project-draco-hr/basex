{
  final byte[] uri=checkEStr(it);
  if (it2 == null)   XPEMPTY.thrw(info,description());
  final byte[] name=checkEStr(it2);
  final byte[] str=!contains(name,':') && eq(uri,XMLURI) ? concat(XMLC,name) : name;
  if (!XMLToken.isQName(str))   Err.value(info,AtomType.QNM,Str.get(name));
  final QNm nm=new QNm(str,uri);
  if (nm.hasPrefix() && uri.length == 0)   Err.value(info,AtomType.URI,Str.get(nm.uri()));
  return nm;
}
