{
  final XQuery query=new XQuery("<elem attr='\n" + "                            \n" + "                    \n"+ "                    \n"+ "                    \n"+ "\n"+ "                 '/>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<elem attr=\"                                                                                                               \"/>",false));
}
