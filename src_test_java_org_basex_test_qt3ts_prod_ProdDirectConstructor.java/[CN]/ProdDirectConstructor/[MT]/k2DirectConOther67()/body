{
  final XQuery query=new XQuery("<e xmlns=\"http://example.com/3\">{namespace-uri-from-QName(node-name(<e/>)), namespace-uri-from-QName(node-name(element e2 {()}))}</e>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<e xmlns=\"http://example.com/3\">http://example.com/3 http://example.com/3</e>",false));
}
