{
  final XQuery query=new XQuery(queryFile("file:///C:/Users/Leo/Documents/Workspaces/GitHub/QT3-test-suite/prod/DirectConstructor/K2-DirectConOther-51.xq"),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("true",false));
}
