{
  if (ftand != null) {
    for (int j=0; j < ftand.size; j++) {
      for (int k=0; k < ftand.list[j].length && ftand.list[j][k] > 0; k++) {
        if (i == ftand.list[j][k]) {
          int d=i - ftand.list[j][0];
          return new int[]{d,ftand.list[j][0]};
        }
      }
    }
  }
  return new int[]{0,0};
}
