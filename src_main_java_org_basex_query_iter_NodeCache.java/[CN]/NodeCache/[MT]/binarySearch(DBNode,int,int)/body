{
  if (size == 0 || nd.data != item[0].data())   return -start - 1;
  int l=start, r=start + length - 1;
  while (l <= r) {
    final int m=l + r >>> 1;
    final int npre=((DBNode)item[m]).pre;
    if (npre == nd.pre)     return m;
    if (npre < nd.pre)     l=m + 1;
 else     r=m - 1;
  }
  return -(l + 1);
}
