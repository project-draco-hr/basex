{
  if (db) {
    final DBNode dbn=(DBNode)node;
    int l=0, h=size - 1;
    while (l <= h) {
      final int m=l + h >>> 1;
      final DBNode n=(DBNode)item[m];
      final int c=n.pre - dbn.pre;
      if (c == 0)       return n.data == dbn.data ? m : -1;
      if (c < 0)       l=m + 1;
 else       h=m - 1;
    }
  }
 else {
    for (int s=0; s < size(); ++s) {
      if (item[s].is(node))       return s;
    }
  }
  return -1;
}
