{
  final XQuery query=new XQuery("\n" + "        declare function local:f() { doc('rubbish.xml') }; \n" + "        try { local:f() } catch * {\"ok\"}\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"ok"));
}
