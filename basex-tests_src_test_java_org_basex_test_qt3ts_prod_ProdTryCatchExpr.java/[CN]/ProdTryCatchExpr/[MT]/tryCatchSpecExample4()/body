{
  final XQuery query=new XQuery("\n" + "      let $output := (try {\n" + "          fn:error(fn:QName('http://www.w3.org/2005/xqt-errors', 'err:FOER0000'))\n"+ "        }\n"+ "        catch * {\n"+ "          $err:code, $err:value, \" module: \",\n"+ "          $err:module, \"(\", $err:line-number, \",\", $err:column-number, \")\"\n"+ "        })\n"+ "      return true()\n"+ "    ",ctx);
  try {
    query.namespace("err","http://www.w3.org/2005/xqt-errors");
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
