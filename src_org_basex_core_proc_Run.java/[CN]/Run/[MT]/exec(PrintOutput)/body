{
  final IO io=IO.get(args[0]);
  if (!io.exists())   return error(FILEWHICH,io);
  Prop.xquery=io;
  try {
    return query(Token.string(io.content())) && out(out);
  }
 catch (  final IOException ex) {
    Main.debug(ex);
    final String msg=ex.getMessage();
    return error(msg != null ? msg : args[0]);
  }
}
