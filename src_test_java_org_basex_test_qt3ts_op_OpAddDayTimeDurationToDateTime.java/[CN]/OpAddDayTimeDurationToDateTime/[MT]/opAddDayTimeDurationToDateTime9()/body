{
  final XQuery query=new XQuery("fn:string((xs:dateTime(\"1993-12-09T13:13:13Z\") + xs:dayTimeDuration(\"P03DT01H04M\"))) and fn:string((xs:dateTime(\"1993-12-09T13:13:13Z\") + xs:dayTimeDuration(\"P01DT01H03M\")))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
