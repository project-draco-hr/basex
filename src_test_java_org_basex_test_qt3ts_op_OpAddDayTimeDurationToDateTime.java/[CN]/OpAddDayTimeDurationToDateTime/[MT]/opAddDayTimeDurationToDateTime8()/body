{
  final XQuery query=new XQuery("(xs:dateTime(\"0001-01-01T11:11:11Z\") + xs:dayTimeDuration(\"-P11DT02H02M\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"-0001-12-21T09:09:11Z") || assertStringValue(false,"0000-12-21T09:09:11Z")));
}
