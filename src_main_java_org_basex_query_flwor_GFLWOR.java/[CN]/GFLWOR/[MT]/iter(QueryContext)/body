{
  final Iter[] iter=new Iter[fl.length];
  final int vs=ctx.vars.size();
  for (int f=0; f < fl.length; ++f)   iter[f]=ctx.iter(fl[f]);
  ObjList<Item[]> keys=null;
  ValueList vals=null;
  if (order != null) {
    keys=new ObjList<Item[]>();
    vals=new ValueList();
  }
  if (group != null)   group.init(order);
  iter(ctx,iter,0,keys,vals);
  ctx.vars.size(vs);
  for (  final ForLet f : fl)   ctx.vars.add(f.var);
  final Iter ir=group != null ? group.gp.ret(ctx,ret,keys,vals) : ctx.iter(order.set(keys,vals));
  ctx.vars.size(vs);
  return ir;
}
