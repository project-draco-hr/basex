{
switch (sig) {
case _HASH_MD5:
    return hash("MD5",ctx);
case _HASH_MD5_BINARY:
  return hashBinary("MD5",ctx);
case _HASH_SHA1:
return hash("SHA",ctx);
case _HASH_SHA1_BINARY:
return hashBinary("SHA",ctx);
case _HASH_HASH:
return hash(ctx);
case _HASH_HASH_BINARY:
return hashBinary(ctx);
default :
return super.item(ctx,ii);
}
}
