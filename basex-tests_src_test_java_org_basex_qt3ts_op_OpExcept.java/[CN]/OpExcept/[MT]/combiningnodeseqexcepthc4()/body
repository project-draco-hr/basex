{
  final XQuery query=new XQuery("for $h in ( count((//hours) except (//hours))) order by number($h) return $h",ctx);
  try {
    query.context(node(file("docs/works.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
