{
  neg=new boolean[expr.length];
  int is=0;
  int n=0;
  for (int i=0; i < expr.length; ++i) {
    if (!expr[i].indexAccessible(ic))     return false;
    neg[i]=ic.not;
    if (ic.not)     ++n;
    ic.not=false;
    if (is == 0 || ic.costs < is)     is=ic.costs;
    if (ic.costs == 0)     break;
  }
  ic.costs=is;
  return !neg[0] && n < expr.length;
}
