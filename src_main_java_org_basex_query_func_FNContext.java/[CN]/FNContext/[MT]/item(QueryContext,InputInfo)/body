{
  final Iter[] arg=new Iter[expr.length];
  for (int a=0; a < expr.length; ++a)   arg[a]=ctx.iter(expr[a]);
switch (def) {
case CURRENT_DATE:
    return currDate(ctx);
case CURRENT_DATETIME:
  return currDTM(ctx);
case CURRENT_TIME:
return currTIM(ctx);
case IMPLICIT_TIMEZONE:
return implZone();
case DEFAULT_COLLATION:
return ctx.baseURI().resolve(ctx.collation);
case STATIC_BASE_URI:
final IO base=ctx.baseIO();
return base == null ? null : Uri.uri(token(ctx.baseIO().url()));
default :
return super.item(ctx,ii);
}
}
