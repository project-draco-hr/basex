{
  final Item res=ctx.eval(expr[1]);
  if (!res.bool())   return new Nod(ctx);
  final FTTokenizer tmp=ctx.ftitem;
  ctx.ftitem=ft;
  final FTArrayExpr ftae=(FTArrayExpr)expr[1];
  final IntList il=new IntList();
  while (ftae.more()) {
    ftn=ftae.next(ctx);
    if (ftn.size == 0)     break;
    il.add(ftn.getPre());
  }
  ctx.item=new Nod(il.finish(),ctx);
  ctx.ftitem=tmp;
  return ctx.item;
}
