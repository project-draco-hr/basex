{
  final Sessions sess=get(Token.string(name));
  if (sess == null)   return false;
  for (  final ClientListener srv : sess) {
    if (srv == ctx.listener)     continue;
    try {
      srv.notify(name,msg);
    }
 catch (    final IOException ex) {
      sess.remove(srv);
    }
  }
  return true;
}
