{
  if (key == null) {
    if (CHECK && check.size() != 0) {
      final Iterator<String> it=check.keySet().iterator();
      while (it.hasNext())       BaseX.errln("%." + SUFFIX + ": '%' not used",Prop.language,it.next());
    }
    return null;
  }
  final String val=texts.get(key);
  if (val == null) {
    if (texts.size() != 0)     BaseX.errln("%." + SUFFIX + ": '%' missing",Prop.language,key);
    return "?????";
  }
  if (CHECK)   check.remove(key);
  return val;
}
