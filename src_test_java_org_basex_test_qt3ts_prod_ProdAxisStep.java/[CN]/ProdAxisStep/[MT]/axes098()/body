{
  final XQuery query=new XQuery("\n" + "        declare namespace ns1 = \"http://www.example.org/ns1\"; \n" + "        declare namespace ns2 = \"http://www.example.org/ns2\"; \n"+ "        let $element as element(*) := <ns1:foo/> \n"+ "        return count($element/self::ns2:*)",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertEq("0") || error("XPST0005")));
}
