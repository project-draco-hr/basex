{
  final XQuery query=new XQuery("let $var := <anElement>Some content</anElement> return $var/20[child::text()]",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0020"));
}
