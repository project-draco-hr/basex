{
  final XQuery query=new XQuery("\n" + "        declare namespace ns1 = \"http://www.example.org/ns1\"; \n" + "        declare namespace ns2 = \"http://www.example.org/ns2\"; \n"+ "        let $attribute as attribute(*) := attribute ns1:foo { } \n"+ "        return count($attribute/self::ns2:*)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("0") || error("XPST0005")));
}
