{
  final XQuery query=new XQuery("\n" + "        declare function local:f($x) { if ($x mod 2 = 1) then <a/> else \"a\" };\n" + "        let $y := for $x in (1 to 10) return <e>{$x}</e> return $y/local:f(.)/a",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0019"));
}
