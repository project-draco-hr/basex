{
  final XQuery query=new XQuery("<e> <b attr=\"fo\"/> <b/> </e>/descendant-or-self::node()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e><b attr=\"fo\"/><b/></e><b attr=\"fo\"/><b/>",false));
}
