{
  final XQuery query=new XQuery("\n" + "        declare namespace foo=\"test\";\n" + "        let $x := <a><foo:b/><c/><d/></a> return count($x/foo:* | $x/foo:*)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1"));
}
