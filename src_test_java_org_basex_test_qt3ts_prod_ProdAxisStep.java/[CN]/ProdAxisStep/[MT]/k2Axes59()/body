{
  final XQuery query=new XQuery("declare variable $i := <root> <child/> <child/> <child> <child2> <child3> <leaf/> </child3> </child2> </child> </root>; 1, root($i)//leaf/child::node(), 1",ctx);
  final QT3Result res=result(query);
  result=res;
  test((assertStringValue(false,"1 1") || error("XPST0005")));
}
