{
  final XQuery query=new XQuery("\n" + "        declare namespace ns1 = \"http://www.example.org/ns1\"; \n" + "        declare namespace ns2 = \"http://www.example.org/ns2\"; declare construction strip; \n"+ "        let $element as element(*, xs:untyped) := <e a=\"value\" /> \n"+ "        let $attribute as attribute(*, xs:untypedAtomic)* := $element/attribute::* \n"+ "        return count($attribute)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
