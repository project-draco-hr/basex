{
  final XQuery query=new XQuery("xquery, version, encoding, default, declare, function, option, collation, schema, import",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
