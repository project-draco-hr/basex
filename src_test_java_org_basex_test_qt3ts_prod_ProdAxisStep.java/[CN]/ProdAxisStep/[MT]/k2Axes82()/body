{
  final XQuery query=new XQuery("<a> <b id=\"person0\"> <c/> </b> </a>/*[@id eq \"person0\"]/c",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<c/>",false));
}
