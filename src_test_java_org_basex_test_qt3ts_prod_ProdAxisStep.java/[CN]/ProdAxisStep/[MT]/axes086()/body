{
  final XQuery query=new XQuery("<root> {//@*/..} </root>",ctx);
  query.context(node(file("prod/AxisStep/Tree1Text.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<root><west mark=\"w0\" /><center mark=\"c0\">Text in center</center><east mark=\"e0\">Text in east</east></root>",false));
}
