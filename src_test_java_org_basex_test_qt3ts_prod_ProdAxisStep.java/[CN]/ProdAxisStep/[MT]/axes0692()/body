{
  final XQuery query=new XQuery("fn:count(//attribute::mark)",ctx);
  try {
    query.context(node(file("prod/AxisStep/TreeEmpty.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("1"));
}
