{
  if (atts != null)   return;
  checkType(START_ELEMENT,ATTRIBUTE);
  atts=new NodeBuilder(true);
  final NodeIter iter=((Nod)item).attr();
  try {
    while (true) {
      final Nod it=iter.next();
      if (it == null)       return;
      atts.add(it);
    }
  }
 catch (  final XQException ex) {
    ex.printStackTrace();
  }
}
