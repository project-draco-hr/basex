{
  try {
    final Nod n=iter[l].next();
    if (n != null) {
      node[l]=n;
      item=n;
      type();
      if (kind == START_ELEMENT)       iter[++l]=n.child();
    }
 else {
      if (--l < 0)       return false;
      item=node[l];
      kind=END_ELEMENT;
    }
  }
 catch (  QueryException e) {
    BaseX.notexpected();
  }
  return true;
}
