{
  final ByteList result=new ByteList();
  for (int i=0; i < 100; ++i) {
    if (buffer[i] == 0)     break;
    result.add(buffer[i]);
  }
  String n=null;
  try {
    n=new String(result.toArray());
  }
 catch (  final Exception ex) {
    n=result.toString();
  }
  name=n;
  long s=0;
  boolean p=true;
  for (int i=124; i < 136; ++i) {
    final byte b=buffer[i];
    if (p && (b == ' ' || b == '0'))     continue;
    if (b == 0 || b == ' ')     break;
    s=(s << 3) + (b - '0');
    p=false;
  }
  size=s;
  type=buffer[156];
}
