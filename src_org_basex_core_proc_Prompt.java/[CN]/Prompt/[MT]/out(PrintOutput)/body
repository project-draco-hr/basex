{
  final Data data=context.data();
  if (data != null) {
    final Nodes nodes=context.current();
    final int pre=nodes.pre[0];
    if (data.deepfs && pre != 0) {
      out.print(FSUtils.getName(data,pre));
    }
 else {
      out.print(data.tag(pre));
    }
    if (nodes.size != 1)     out.print("[...]");
  }
}
