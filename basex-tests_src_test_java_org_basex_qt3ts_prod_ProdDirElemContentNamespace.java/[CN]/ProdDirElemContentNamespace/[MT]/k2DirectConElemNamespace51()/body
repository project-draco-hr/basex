{
  final XQuery query=new XQuery("declare default element namespace \"http://example.com\"; <e xmlns=\"\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e/>",false));
}
