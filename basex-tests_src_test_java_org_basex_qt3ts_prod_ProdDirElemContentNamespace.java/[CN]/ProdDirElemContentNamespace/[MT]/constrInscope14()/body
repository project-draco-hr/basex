{
  final XQuery query=new XQuery("declare namespace foo = \"http://example.com\"; <foo:new/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<foo:new xmlns:foo=\"http://example.com\"/>",false));
}
