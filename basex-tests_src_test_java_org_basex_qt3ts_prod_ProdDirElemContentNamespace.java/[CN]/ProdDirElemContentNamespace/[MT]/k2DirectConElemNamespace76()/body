{
  final XQuery query=new XQuery("<e xmlns:p=\"http://example.com/{{1}}\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<e xmlns:p=\"http://example.com/{1}\"/>",false) || error("XQST0046") || error("XQST0022")));
}
