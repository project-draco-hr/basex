{
  final XQuery query=new XQuery("namespace-uri(<p:e xmlns:p=\"http://ns.example.com/ns?val=\"\"\"\"\"\"asd\"/>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"http://ns.example.com/ns?val=\"\"\"asd") || error("XQST0046") || error("XQST0022")));
}
