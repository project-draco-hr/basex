{
  final XQuery query=new XQuery("<e a=\"{<e><b>data</b></e>/b}\" xmlns=\"http://www.example.com/\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e xmlns=\"http://www.example.com/\" a=\"data\"/>",false));
}
