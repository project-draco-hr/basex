{
  final XQuery query=new XQuery("\n" + "        declare namespace cat ='mycat'; \n" + "        <a t='cat:miau'>42</a>\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a t='cat:miau'>42</a>",false));
}
