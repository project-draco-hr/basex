{
  final XQuery query=new XQuery("<new xmlns=\"http://www.example.com/parent4\">{//*:child4}</new>",ctx);
  try {
    query.context(node(file("prod/DirElemContent.namespace/inscope.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<new xmlns=\"http://www.example.com/parent4\"><child4/></new>",false));
}
