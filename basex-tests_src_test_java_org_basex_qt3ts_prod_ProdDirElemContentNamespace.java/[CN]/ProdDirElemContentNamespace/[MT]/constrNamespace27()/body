{
  final XQuery query=new XQuery("<elem xmlns:cm=\"http://www.example.com\" attr=\"{count(//cm:b)}\"/>",ctx);
  try {
    query.context(node(file("prod/DirElemContent.namespace/MixNS.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem xmlns:cm=\"http://www.example.com\" attr=\"1\"/>",false));
}
