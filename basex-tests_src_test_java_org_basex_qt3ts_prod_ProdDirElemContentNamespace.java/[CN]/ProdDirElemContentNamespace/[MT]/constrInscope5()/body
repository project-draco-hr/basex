{
  final XQuery query=new XQuery("<new>{//*:child3}</new>",ctx);
  try {
    query.context(node(file("prod/DirElemContent.namespace/inscope.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<new><foo:child3 xmlns:foo=\"http://www.example.com/parent3\"/></new>",false));
}
