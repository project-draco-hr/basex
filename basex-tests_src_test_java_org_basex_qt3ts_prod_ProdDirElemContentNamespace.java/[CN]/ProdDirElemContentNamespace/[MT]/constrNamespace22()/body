{
  final XQuery query=new XQuery("<elem xmlns=\"http://www.example.com/parent\"><child xmlns=\"\"/></elem>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem xmlns=\"http://www.example.com/parent\"><child xmlns=\"\"/></elem>",false));
}
