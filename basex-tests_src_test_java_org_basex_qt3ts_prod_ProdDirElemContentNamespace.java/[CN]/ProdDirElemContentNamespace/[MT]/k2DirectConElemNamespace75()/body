{
  final XQuery query=new XQuery("namespace-uri(<e xmlns=\"http://example.com/{{1}}\"/>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"http://example.com/{1}") || error("XQST0046") || error("XQST0022")));
}
