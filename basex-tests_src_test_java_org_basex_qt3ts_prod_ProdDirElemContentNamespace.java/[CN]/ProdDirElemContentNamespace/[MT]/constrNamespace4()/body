{
  final XQuery query=new XQuery("<elem xmlns:foo=\"http://www.example.com\" xmlns:foo=\"http://www.example.com\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0071"));
}
