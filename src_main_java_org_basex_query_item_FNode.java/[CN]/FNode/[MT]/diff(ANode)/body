{
  if (id != node.id) {
    ANode n=this;
    while (n != null) {
      final ANode p=n.parent();
      if (p == node)       return 1;
      n=p;
    }
    n=node;
    while (n != null) {
      final ANode p=n.parent();
      if (p == this)       return -1;
      n=p;
    }
  }
  return id - node.id;
}
