{
  return new AxisIter(){
    /** 
 * Iterator. 
 */
    private AxisIter ai;
    @Override public ANode next(){
      if (ai == null) {
        final ANode r=parent();
        if (r == null)         return null;
        ai=r.children();
        ANode n;
        while ((n=ai.next()) != null && !n.is(FNode.this))         ;
      }
      return ai.next();
    }
  }
;
}
