{
  final XQuery query=new XQuery("<results>{fn:idref((\"id1\", \"id2\"), /IDS[1])}</results>",ctx);
  try {
    query.context(node(file("fn/id/iddtd.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQDY0025"));
}
