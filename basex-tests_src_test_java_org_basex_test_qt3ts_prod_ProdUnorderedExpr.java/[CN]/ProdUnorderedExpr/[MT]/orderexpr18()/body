{
  final XQuery query=new XQuery("unordered {fn:reverse((2,3))}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"2 3") || assertStringValue(false,"3 2")));
}
