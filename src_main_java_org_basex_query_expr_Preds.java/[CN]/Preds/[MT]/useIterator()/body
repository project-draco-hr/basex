{
  pos=pred[0] instanceof Pos ? (Pos)pred[0] : null;
  last=pred[0].isFun(Function.LAST);
  boolean np1=true;
  boolean np2=true;
  for (int p=0; p < pred.length; p++) {
    final boolean np=!pred[p].type().mayBeNum() && !pred[p].uses(Use.POS);
    np1&=np;
    if (p > 0)     np2&=np;
  }
  return np1 || pos != null && np2 || last && pred.length == 1;
}
