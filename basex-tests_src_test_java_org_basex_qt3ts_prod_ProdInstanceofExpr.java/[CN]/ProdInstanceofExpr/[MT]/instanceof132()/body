{
  final XQuery query=new XQuery("\n" + "         filter#2 instance of function(function(item()) as xs:boolean, item()*) as item()*\n" + "       ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
