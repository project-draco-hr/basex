{
  final byte[] nm=nname();
  name.name(nm);
  name.uri=Uri.EMPTY;
  final boolean ns=name.ns();
  if (ns || data.ns.size() != 0) {
    final int n=ns ? data.ns.uri(nm,pre) : data.uri(pre,data.kind(pre));
    name.uri=Uri.uri(n > 0 ? data.ns.uri(n) : ns ? NSGlobal.uri(pref(nm)) : EMPTY);
  }
  return name;
}
