{
switch (type) {
case ATT:
case PI:
    return type.name + " " + string(nname())+ " { \""+ string(chop(atom(),64))+ "\" }";
case ELM:
  return type.name + " " + string(nname())+ " { ... }";
case DOC:
return type.name + " { \"" + string(data.text(pre,true))+ "\" }";
default :
return type.name + " { \"" + string(chop(atom(),64))+ "\" }";
}
}
