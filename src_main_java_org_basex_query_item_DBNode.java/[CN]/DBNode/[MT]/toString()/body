{
switch (type) {
case ATT:
case PI:
    return type + " " + string(nname())+ " { \""+ string(str())+ "\" }";
case ELM:
  return type + " " + string(nname())+ " { ... }";
case DOC:
return type + " { \"" + string(data.text(pre,true))+ "\" }";
default :
return type + " { \"" + Err.chop(str())+ "\" }";
}
}
