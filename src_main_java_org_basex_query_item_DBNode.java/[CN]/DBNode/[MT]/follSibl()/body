{
  return new AxisIter(){
    private final DBNode node=copy();
    int k=data.kind(pre);
    private final int pp=data.parent(pre,k);
    final int s=pp + data.size(pp,data.kind(pp));
    int p=pre + data.size(pre,k);
    @Override public ANode next(){
      if (p == s)       return null;
      k=data.kind(p);
      node.set(p,k);
      p+=data.size(p,k);
      return node;
    }
  }
;
}
