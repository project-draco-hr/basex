{
  return new NodeIter(){
    private final DBNode node=copy();
    final int s=data.meta.size;
    int k=data.kind(pre);
    int p=pre + data.size(pre,k);
    @Override public Nod next(){
      if (p == s)       return null;
      k=data.kind(p);
      node.set(p,k);
      p+=data.attSize(p,k);
      return node;
    }
  }
;
}
