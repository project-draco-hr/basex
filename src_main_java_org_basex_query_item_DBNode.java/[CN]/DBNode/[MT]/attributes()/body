{
  return new AxisMoreIter(){
    final DBNode node=copy();
    final int s=pre + data.attSize(pre,data.kind(pre));
    int p=pre + 1;
    @Override public boolean more(){
      return p != s;
    }
    @Override public DBNode next(){
      if (!more())       return null;
      node.set(p++,Data.ATTR);
      return node;
    }
  }
;
}
