{
  return new NodeIter(){
    int k=data.kind(pre);
    int p=pre + data.attSize(pre,k);
    final int s=pre + data.size(pre,k);
    final DBNode node=copy();
    final double sc=node.score();
    @Override public DBNode next(){
      if (p == s)       return null;
      k=data.kind(p);
      node.set(p,k);
      p+=data.attSize(p,k);
      node.score(Scoring.step(sc));
      return node;
    }
  }
;
}
