{
  return new NodeIter(){
    private final DBNode node=copy();
    int p=pre;
    int k=data.kind(p);
    final double sc=node.score;
    @Override public Nod next(){
      p=data.parent(p,k);
      if (p == -1)       return null;
      k=data.kind(p);
      node.set(p,k);
      node.score(Scoring.step(sc));
      return node;
    }
  }
;
}
