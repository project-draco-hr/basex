{
  final boolean elem=consumeWS(ELEMENT);
  if (!elem && !consumeWS(FUNCTION))   return false;
  check(NSPACE);
  final byte[] ns=stringLiteral();
  if (elem) {
    if (declElem)     error(DUPLNS);
    ctx.nsElem=ns;
    declElem=true;
  }
 else {
    if (declFunc)     error(DUPLNS);
    ctx.nsFunc=ns;
    declFunc=true;
  }
  return true;
}
