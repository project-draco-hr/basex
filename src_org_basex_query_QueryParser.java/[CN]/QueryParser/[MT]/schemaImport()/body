{
  if (consumeWS(NSPACE)) {
    ncName(XPNAME);
    check(IS);
  }
 else   if (consumeWS(DEFAULT)) {
    check(ELEMENT);
    check(NSPACE);
  }
  final byte[] ns=stringLiteral();
  if (ns.length == 0)   error(NSEMPTY);
  if (consumeWS(AT))   do   stringLiteral();
 while (consumeWS(COMMA));
  error(IMPLSCHEMA);
}
