{
  skipWS();
  Expr name;
  if (XMLToken.isXMLLetter(curr())) {
    name=Str.get(ncName(null));
  }
 else {
    if (!consumeWS2(BRACE1))     return null;
    name=simple(check(expr(),PIWRONG));
    check(BRACE2);
  }
  check(BRACE1);
  final Expr e=simple(expr());
  check(BRACE2);
  return new CPI(name,e == null ? Seq.EMPTY : e);
}
