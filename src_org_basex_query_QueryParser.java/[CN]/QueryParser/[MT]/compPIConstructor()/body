{
  skipWS();
  Expr name;
  if (XMLToken.isXMLLetter(curr())) {
    name=Str.get(ncName(null));
  }
 else {
    if (!consumeWS2(BRACE1))     return null;
    name=check(expr(),PIWRONG);
    check(BRACE2);
  }
  check(BRACE1);
  Expr e=expr();
  if (e == null)   e=Seq.EMPTY;
  check(BRACE2);
  return new CPI(name,e);
}
