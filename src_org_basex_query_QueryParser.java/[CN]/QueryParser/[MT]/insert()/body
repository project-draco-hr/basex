{
  final int p=qp;
  if (!consumeWS(INSERT))   return null;
  if (!consumeWS(NODE) && !consumeWS(NODES)) {
    qp=p;
    return null;
  }
  final Expr s=check(single(),UPEXPSIMPLE);
  boolean first=false;
  boolean last=false;
  boolean into=false;
  boolean before=false;
  boolean after=false;
  if (consumeWS(AS)) {
    if (!consumeWS(FIRST)) {
      check(LAST);
      last=true;
    }
 else     first=true;
    check(INTO);
    into=true;
  }
  if (!into)   into=consumeWS(INTO);
  after=consumeWS(AFTER);
  before=consume(BEFORE);
  if (!(into ^ after ^ before))   Err.or(UPDATE,this);
  final Expr trg=check(single(),UPEXPSIMPLE);
  ctx.updating=true;
  return new Insert(s,first,last,before,after,trg);
}
