{
  final Expr e=range();
  if (ctx.context.prop.is(Prop.FTCONTAINS)) {
    if (!consumeWS(FTCONTAINS))     return e;
  }
 else {
    final int p=qp;
    if (!consumeWS(CONTAINS) || !consumeWS(TEXT)) {
      qp=p;
      return e;
    }
  }
  final FTExpr select=ftSelection(false);
  if (consumeWS2(WITHOUT)) {
    check(CONTENT);
    union();
    error(FTIGNORE);
  }
  return new FTContains(e,select);
}
