{
  final Expr e=ftContains();
  if (e == null)   return null;
  for (  final CmpV.Comp c : CmpV.Comp.values())   if (consumeWS(c.name))   return new CmpV(e,cmp(e),c);
  for (  final CmpN.Comp c : CmpN.Comp.values())   if (consumeWS(c.name))   return new CmpN(e,cmp(e),c);
  for (  final CmpG.Comp c : CmpG.Comp.values())   if (consumeWS2(c.name))   return new CmpG(e,cmp(e),c);
  return e;
}
