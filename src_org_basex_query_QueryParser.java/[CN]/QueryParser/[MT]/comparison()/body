{
  final Expr e=ftContains();
  if (e != null) {
    for (    final CmpV.Comp c : CmpV.Comp.values())     if (consumeWS(c.name))     return new CmpV(e,check(ftContains(),CMPEXPR),c);
    for (    final CmpN.Comp c : CmpN.Comp.values())     if (consumeWS(c.name))     return new CmpN(e,check(ftContains(),CMPEXPR),c);
    for (    final CmpG.Comp c : CmpG.Comp.values())     if (consumeWS2(c.name))     return new CmpG(e,check(ftContains(),CMPEXPR),c);
  }
  return e;
}
