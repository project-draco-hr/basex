{
  final int p=qp;
  if (!consumeWS(RENAME))   return null;
  if (!consumeWS(NODE)) {
    qp=p;
    return null;
  }
  if (declVar)   Err.or(UPNOUPDT,this);
  final Expr trg=check(single(),UPEXPSIMPLE);
  check(AS);
  final Expr n=check(single(),UPEXPSIMPLE);
  ctx.updating=true;
  return new Rename(trg,n);
}
