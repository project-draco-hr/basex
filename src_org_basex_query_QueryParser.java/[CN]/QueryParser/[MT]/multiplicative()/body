{
  Expr e=union();
  if (e == null)   return null;
  while (true) {
    final Calc c=consume('*') ? Calc.MULT : consumeWS(DIV) ? Calc.DIV : consumeWS(IDIV) ? Calc.IDIV : consumeWS(MOD) ? Calc.MOD : null;
    if (c == null)     break;
    e=new Clc(simple(e),simple(check(union(),CALCEXPR)),c);
  }
  return e;
}
