{
  if (!consumeWS2(PRAGMA))   return false;
  do {
    final QNm name=new QNm(qName(PRAGMAINCOMPLETE),ctx);
    if (!name.ns())     error(NSMISS,name);
    char c=curr();
    if (c != '#' && !ws(c))     error(PRAGMAINCOMPLETE);
    tok.reset();
    while (c != '#' || next() != ')') {
      if (c == 0)       error(PRAGMAINCOMPLETE);
      tok.add(consume());
      c=curr();
    }
    tok.finish();
    qp+=2;
  }
 while (consumeWS(PRAGMA));
  return true;
}
