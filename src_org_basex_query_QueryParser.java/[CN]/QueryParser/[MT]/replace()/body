{
  final int p=qp;
  if (!consumeWS(REPLACE))   return null;
  final boolean v=consumeWS(VALUEE);
  if (v)   check(OF);
  if (!consumeWS(NODE)) {
    qp=p;
    return null;
  }
  final Expr t=simple(check(single(),INCOMPLETE));
  check(WITH);
  final Expr r=simple(check(single(),INCOMPLETE));
  ctx.updating=true;
  return new Replace(t,r,v);
}
