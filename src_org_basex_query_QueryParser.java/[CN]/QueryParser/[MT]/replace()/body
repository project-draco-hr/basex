{
  final int p=qp;
  if (!consumeWS(REPLACE))   return null;
  final boolean v=consumeWS(VALUEE);
  if (v)   check(OF);
  if (!consumeWS(NODE)) {
    qp=p;
    return null;
  }
  final Expr t=check(single(),INCOMPLETE);
  check(WITH);
  final Expr e=check(single(),INCOMPLETE);
  return new Replace(t,e,v);
}
