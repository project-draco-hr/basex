{
  try {
    versionDecl();
    Expr ex=null;
    if (u == null) {
      ex=mainModule();
      if (ex == null)       if (alter != null)       error();
 else       error(EXPREMPTY);
    }
 else {
      moduleDecl(u);
    }
    if (end && more()) {
      if (alter != null)       error();
      error(QUERYEND,rest());
    }
    ctx.fun.check();
    ctx.ns.finish(ctx.nsElem);
    return ex;
  }
 catch (  final QueryException ex) {
    mark();
    ex.pos(this);
    throw ex;
  }
}
