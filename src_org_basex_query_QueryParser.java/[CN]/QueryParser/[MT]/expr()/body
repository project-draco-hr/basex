{
  final Expr e=single();
  if (e == null) {
    if (more())     return null;
    if (alter != null)     error();
 else     error(NOEXPR);
  }
  if (!consumeWS2(COMMA))   return e;
  Expr[] list={e};
  do {
    final Expr ex=single();
    if (!ex.e())     list=add(list,ex);
  }
 while (consumeWS2(COMMA));
  updating(list);
  return list.length == 1 ? e : new List(list);
}
