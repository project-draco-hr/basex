{
  final QNm type=new QNm(qName(TYPEINVALID));
  ctx.ns.uri(type);
  skipWS();
  final SeqType seq=new SeqType(type,consume('?') ? 1 : 0,false);
  if (seq.type == null) {
    final byte[] uri=type.uri.str();
    if (uri.length == 0 && type.ns())     error(PREUNKNOWN,type.pre());
    final byte[] ln=type.ln();
    error(eq(uri,Type.NOT.uri) && (eq(Type.NOT.name,ln) || eq(Type.AAT.name,ln)) ? CASTUNKNOWN : TYPEUNKNOWN,type);
  }
  return seq;
}
