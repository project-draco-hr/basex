{
  final XQuery query=new XQuery("fn:codepoint-equal(normalize-unicode(\"gar??on\", \"NFC\"), normalize-unicode(\"gar??on\", \"NFD\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
