{
  final XQuery query=new XQuery("declare function local:yes-no($arg as xs:boolean) as xs:string { if ($arg) then 'yes' else 'no' }; \n" + "      fn:codepoint-equal( local:yes-no(fn:true()), local:yes-no(fn:false()) ) = false()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
