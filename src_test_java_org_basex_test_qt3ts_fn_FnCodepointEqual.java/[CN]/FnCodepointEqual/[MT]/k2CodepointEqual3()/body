{
  final XQuery query=new XQuery("codepoint-equal(lower-case(concat(\"B STRING\", current-time())), \n" + "                            lower-case(concat(\"no match\", current-time())))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
