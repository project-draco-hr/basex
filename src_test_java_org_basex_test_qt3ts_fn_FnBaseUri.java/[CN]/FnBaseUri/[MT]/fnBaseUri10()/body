{
  final XQuery query=new XQuery("fn:string(fn:base-uri(<anElement xml:base=\"http://www.example.com\">Element content</anElement>))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com"));
}
