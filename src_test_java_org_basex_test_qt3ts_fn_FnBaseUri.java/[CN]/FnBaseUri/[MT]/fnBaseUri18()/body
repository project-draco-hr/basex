{
  final XQuery query=new XQuery("declare base-uri \"http://example.org\"; \n" + "        let $var := <anElement>With some contexnt</anElement> \n" + "        return fn:string(fn:base-uri($var))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://example.org"));
}
