{
  final XQuery query=new XQuery("fn:count(fn:base-uri(element anElement {\"An Element Node\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("1") || assertEq("0")));
}
