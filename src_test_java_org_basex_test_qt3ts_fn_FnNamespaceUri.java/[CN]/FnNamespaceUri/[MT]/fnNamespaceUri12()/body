{
  final XQuery query=new XQuery("fn:namespace-uri(/works/employee[1]/@name)",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("\"\""));
}
