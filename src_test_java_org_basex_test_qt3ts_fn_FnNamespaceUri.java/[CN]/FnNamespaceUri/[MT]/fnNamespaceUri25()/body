{
  final XQuery query=new XQuery("declare default element namespace \"http://www.example.com/examples\"; \n" + "        declare namespace ex = \"http://www.example.com/exampleswithPrefix\"; \n" + "        fn:string(fn:namespace-uri(element anElement {\"An Element Content\"}))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.com/examples"));
}
