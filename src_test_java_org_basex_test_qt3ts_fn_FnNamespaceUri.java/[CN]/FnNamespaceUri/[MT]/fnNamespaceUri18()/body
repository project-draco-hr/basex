{
  final XQuery query=new XQuery("declare namespace ex = \"http://www.example.com/examples\"; \n" + "            fn:string(fn:namespace-uri(element ex:anElement {\"An Element Content\"}))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"http://www.example.com/examples"));
}
