{
  final XQuery query=new XQuery("declare default element namespace \"http://www.example.com/examples\"; \n" + "        declare namespace ex = \"http://www.example.com/exampleswithPrefix\"; \n" + "        fn:string(fn:namespace-uri(element ex:anElement {\"An Element Content\"}))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://www.example.com/exampleswithPrefix"));
}
