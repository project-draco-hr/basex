{
  final XQuery query=new XQuery("namespace-uri(<anElement xmlns = \"http://www.example.com/examples\"/>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("\"http://www.example.com/examples\"") && assertType("xs:anyURI")));
}
