{
  final XQuery query=new XQuery("fn:min((xs:double(\"-1.7976931348623157E308\"),xs:double(\"0\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("-1.7976931348623157E308"));
}
