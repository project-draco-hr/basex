{
  final ByteList bl=new ByteList();
  while (true) {
    final byte[] next=readLine();
    if (next == null || eq(next,sep))     break;
    if (eq(next,end)) {
      while (readLine() != null)       ;
      break;
    }
    bl.add(next).add('\n');
  }
  return new TextInput(new IOContent(bl.toArray())).encoding(enc).content();
}
