{
  final XQuery query=new XQuery("not(fn:empty(fn:function-lookup(\n" + "\t  fn:QName('http://www.w3.org/2005/xpath-functions',\n" + "\t  'environment-variable') , 1)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
