{
  final XQuery query=new XQuery("fn:node-name(fn:idref(\"id2\", /IDS[1]))",ctx);
  try {
    query.context(node(file("fn/id/iddtd.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"anIdRef"));
}
