{
  final XQuery query=new XQuery("declare ordering ordered;  \n" + "        <results>{fn:idref(\"id4\", /IDS[1])}</results>",ctx);
  try {
    query.context(node(file("fn/id/iddtd.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<results anIdRef=\"id4\"/>",false));
}
