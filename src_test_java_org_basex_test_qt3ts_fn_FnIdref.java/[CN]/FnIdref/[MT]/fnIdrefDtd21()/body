{
  final XQuery query=new XQuery("fn:idref(fn:string-join((\"id\",\"1\"),\"\"), /IDS[1])/name(..)",ctx);
  try {
    query.context(node(file("fn/id/iddtd.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"elementwithidrefattr-1"));
}
