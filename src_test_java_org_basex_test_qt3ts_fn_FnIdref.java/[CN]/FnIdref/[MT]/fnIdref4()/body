{
  final XQuery query=new XQuery("\n" + "        import module namespace copy=\"http://www.w3.org/QT3/copy\";\n" + "        let $var := copy:copy(/*) return fn:idref(\"argument1\", $var)\n"+ "      ",ctx);
  query.context(node(file("docs/works-mod.xml")));
  query.addModule("http://www.w3.org/QT3/copy",file("fn/id/copy.xq"));
  final QT3Result res=result(query);
  result=res;
  test(error("FODC0001"));
}
