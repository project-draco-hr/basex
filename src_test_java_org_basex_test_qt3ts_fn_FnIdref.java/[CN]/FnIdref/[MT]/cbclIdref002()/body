{
  final XQuery query=new XQuery("\n" + "      \tlet $doc := document { <root /> } return fn:empty( fn:idref( (), $doc) )\n" + "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPST0005")));
}
