{
  final XQuery query=new XQuery("translate('---abcABCxyz---','-abcABCxyz','1ABCabcXYZ')",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"111ABCabcXYZ111"));
}
