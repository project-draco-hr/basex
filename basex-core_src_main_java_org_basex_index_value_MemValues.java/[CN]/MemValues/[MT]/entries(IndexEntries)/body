{
  final byte[] prefix=entries.get();
  return new EntryIterator(){
    final int s=size();
    int c;
    @Override public byte[] next(){
      while (++c < s) {
        final byte[] key=values.key(c);
        if (startsWith(key,prefix) && ids.get(c).length > 0)         return key;
      }
      return null;
    }
    @Override public int count(){
      return len.get(c);
    }
  }
;
}
