{
  final XQuery query=new XQuery("<c b=\"a \n" + "\n" + "\n"+ " \n"+ "string literal \n"+ "\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<c b=\"a      string literal  \"/>",false));
}
