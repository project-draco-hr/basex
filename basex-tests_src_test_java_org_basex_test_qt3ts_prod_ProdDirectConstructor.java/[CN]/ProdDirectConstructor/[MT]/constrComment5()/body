{
  final XQuery query=new XQuery("<!--<?&-&lt;&#x20;><![CDATA[x]]>-->",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<!--<?&-&lt;&#x20;><![CDATA[x]]>-->",false));
}
