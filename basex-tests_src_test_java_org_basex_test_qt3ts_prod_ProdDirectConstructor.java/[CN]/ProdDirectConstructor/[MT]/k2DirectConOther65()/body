{
  final XQuery query=new XQuery("<a xml:space=\"   preserve\"/>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertSerialization("<a xml:space=\"   preserve\"/>",false) || error("XQDY0092")));
}
