{
  final XQuery query=new XQuery("string-to-codepoints(<a>\n" + "\n" + "\n"+ " \n"+ "|&#xD; &#xD;&#xA;</a>)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"10 10 10 32 10 124 13 32 13 10"));
}
