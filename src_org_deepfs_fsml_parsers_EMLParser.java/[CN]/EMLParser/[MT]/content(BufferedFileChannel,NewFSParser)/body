{
  if (!check(f))   return;
  fsparser=parser;
  do {
    String type="";
    final Matcher m=KEYPAT.matcher(mCurrLine);
    if (m.matches()) {
      type=m.group(1);
      mCurrLine=m.group(2).trim();
      if (type.equalsIgnoreCase("content-type"))       getContentType();
 else       if (type.equalsIgnoreCase("content-transfer-encoding")) {
        getEncoding();
      }
    }
 else {
      getBoundary();
      getCharset();
    }
    if (!readLine())     return;
  }
 while (!mCurrLine.isEmpty());
  parseContent();
}
