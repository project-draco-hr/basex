{
  final XQuery query=new XQuery("\n" + "        <out>{ \n" + "\t\t  for $x at $j in 1 to 10 \n"+ "\t\t  return \n"+ "\t\t    <item><x>{$x}</x><j>{$j}</j></item> \n"+ "\t\t}</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><item><x>1</x><j>1</j></item><item><x>2</x><j>2</j></item><item><x>3</x><j>3</j></item><item><x>4</x><j>4</j></item><item><x>5</x><j>5</j></item><item><x>6</x><j>6</j></item><item><x>7</x><j>7</j></item><item><x>8</x><j>8</j></item><item><x>9</x><j>9</j></item><item><x>10</x><j>10</j></item></out>",false));
}
