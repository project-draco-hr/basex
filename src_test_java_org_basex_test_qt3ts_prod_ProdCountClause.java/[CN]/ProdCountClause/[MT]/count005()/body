{
  final XQuery query=new XQuery("\n" + "        <out>{ \n" + "\t\t  for $x in 1 to 5\n"+ "\t\t  for $y in 1 to 5\n"+ "\t\t  count $index\n"+ "\t\t  where $index mod 3 = 0\n"+ "\t\t  return \n"+ "\t\t    <item><x>{$x}</x><y>{$y}</y><index>{$index}</index></item> \n"+ "\t\t}</out>\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><item><x>1</x><y>3</y><index>3</index></item><item><x>2</x><y>1</y><index>6</index></item><item><x>2</x><y>4</y><index>9</index></item><item><x>3</x><y>2</y><index>12</index></item><item><x>3</x><y>5</y><index>15</index></item><item><x>4</x><y>3</y><index>18</index></item><item><x>5</x><y>1</y><index>21</index></item><item><x>5</x><y>4</y><index>24</index></item></out>",false));
}
