{
  final XQuery query=new XQuery("\n" + "        import module namespace test=\"http://www.w3.org/TestModules/&#x3c;&#x3d;&#x3e;&#x40;/test\";\n" + "        <result>{test:ok()}</result>\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/<=>@/test",file("prod/ModuleImport/module-uris8-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<result>ok</result>",false));
}
