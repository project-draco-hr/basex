{
  final XQuery query=new XQuery("import(::)module \"\" at \"http://example.com/\", \"http://example.com/2\"; 1 eq 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0088"));
}
