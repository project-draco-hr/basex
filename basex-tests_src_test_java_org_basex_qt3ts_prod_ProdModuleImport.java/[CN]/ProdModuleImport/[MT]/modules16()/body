{
  final XQuery query=new XQuery("\n" + "        import module namespace xml=\"http://www.w3.org/TestModules/test1\"; \n" + "        xs:string($xml:flag)\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/test1",file("prod/ModuleImport/test1-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0070"));
}
