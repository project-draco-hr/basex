{
  final XQuery query=new XQuery("\n" + "        import module namespace defs=\"http://www.w3.org/TestModules/module-pub-priv\"; \n" + "\n"+ "        declare variable $test := defs:g(42);\n"+ "\n"+ "        <a>{$test}</a>\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/module-pub-priv",file("prod/ModuleImport/module-pub-priv.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a>65</a>",false));
}
