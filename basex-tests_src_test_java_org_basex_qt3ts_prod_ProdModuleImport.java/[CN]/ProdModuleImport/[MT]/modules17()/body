{
  final XQuery query=new XQuery("\n" + "        import module namespace defs=\"http://www.w3.org/TestModules/diffns\"; \n" + "        \"abc\"\n"+ "      ",ctx);
  try {
    query.addModule("http://www.w3.org/TestModules/diffns",file("prod/ModuleImport/modulesdiffns-lib.xq"));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0048"));
}
