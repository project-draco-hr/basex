{
  final Data data=context.data();
  data.startUpdate();
  data.update(8,Data.ATTR,T_NAME,Token.EMPTY);
  data.update(8,Data.ATTR,T_JUNIT);
  data.finishUpdate();
  assertEquals(size,data.meta.size);
  assertArraysEquals(T_JUNIT,data.text(8,false));
  reload();
  assertEquals(size,data.meta.size);
  assertArraysEquals(T_JUNIT,data.text(8,false));
}
