{
  final Data data=CONTEXT.data();
  data.startUpdate();
  data.update(8,Data.ATTR,NAME,Token.EMPTY);
  data.update(8,Data.ATTR,JUNIT);
  data.finishUpdate();
  assertEquals(size,data.meta.size);
  assertArraysEquals(JUNIT,data.text(8,false));
  reload();
  assertEquals(size,data.meta.size);
  assertArraysEquals(JUNIT,data.text(8,false));
}
