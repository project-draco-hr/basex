{
  final XQuery query=new XQuery("let $var := <anElement>Some Content</anElement> return $var/ancestor-or-self::*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<anElement>Some Content</anElement>",false));
}
