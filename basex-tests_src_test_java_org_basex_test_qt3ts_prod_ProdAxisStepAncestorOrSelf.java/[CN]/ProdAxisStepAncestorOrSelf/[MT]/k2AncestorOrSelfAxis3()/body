{
  final XQuery query=new XQuery("1, (attribute name {\"content\"}/ancestor-or-self::node() instance of attribute(name)), 1",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("1, true(), 1"));
}
