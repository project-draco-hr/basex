{
  final XQuery query=new XQuery("<a> <b c=\"\"> <c/> </b> <d/> </a>//count(ancestor-or-self::*)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1 2 3 2"));
}
