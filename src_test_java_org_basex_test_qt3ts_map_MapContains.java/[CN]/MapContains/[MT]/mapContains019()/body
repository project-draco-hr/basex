{
  final XQuery query=new XQuery("map:contains(map{1:=\"Sunday\",2:=\"Monday\",3:=\"Tuesday\",4:=(),5:=\"Thursday\",6:=\"Friday\",7:=\"Saturday\"}, 4)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
