{
  final Value[] sf=ctx.stackFrame;
  ctx.stackFrame=stackSize == 0 ? null : new Value[stackSize];
  final Value cv=ctx.value;
  try {
    bindVars(ctx,ii,args);
    ctx.value=null;
    final Value v=ctx.value(expr);
    return cast != null ? cast.promote(v,ctx,ii) : v;
  }
  finally {
    ctx.value=cv;
    ctx.stackFrame=sf;
  }
}
