{
switch (func) {
case _HOF_ID:
case _HOF_CONST:
    return exprs[0];
case _HOF_FOLD_LEFT1:
  if (allAreValues() && exprs[0].size() < FNFunc.UNROLL_LIMIT) {
    qc.compInfo(QueryText.OPTUNROLL,this);
    final Value seq=(Value)exprs[0];
    if (seq.isEmpty())     throw EMPTYFOUND.get(info);
    final FItem f=checkArity(exprs[1],2,qc);
    Expr e=seq.itemAt(0);
    for (int i=1, len=(int)seq.size(); i < len; i++)     e=new DynFuncCall(info,sc,false,f,e,seq.itemAt(i)).optimize(qc,scp);
    return e;
  }
return this;
default :
return this;
}
}
