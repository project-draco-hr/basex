{
switch (func) {
case _HOF_SORT_WITH:
    return sortWith(ctx);
case _HOF_FOLD_LEFT1:
  return foldLeft1(ctx);
case _HOF_UNTIL:
return until(ctx);
case _HOF_ID:
case _HOF_CONST:
return ctx.value(exprs[0]);
case _HOF_TOP_K_BY:
return topKBy(ctx);
case _HOF_TOP_K_WITH:
return topKWith(ctx);
default :
return super.value(ctx);
}
}
