{
  final FItem lt=withArity(pos,2,qc);
  return new Comparator<Item>(){
    @Override public int compare(    final Item a,    final Item b){
      try {
        return checkType(lt.invokeItem(qc,info,a == null ? Empty.SEQ : a,b == null ? Empty.SEQ : b),AtomType.BLN).bool(info) ? -1 : 1;
      }
 catch (      final QueryException qe) {
        throw new QueryRTException(qe);
      }
    }
  }
;
}
