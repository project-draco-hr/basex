{
  if (pp == null || p == null)   return;
  if (size + 1 > prepos.length) {
    prepos=Array.extend(prepos);
    poi=Array.extend(poi);
  }
  int i=0;
  while (i < size) {
    if (prepos[i][0] < pp[0])     i++;
 else {
      add(i,pp,p);
      return;
    }
  }
  prepos[i]=pp;
  poi[i]=p;
  size++;
}
