{
  if (!gui || size == 0 || pres == null)   return;
  final int[][] tpp=new int[pres.length][];
  final int[][] tp=new int[pres.length][];
  int p=0, t=0;
  for (int i=0; i < size; i++) {
    if (prepos[i][0] == pres[p]) {
      tpp[t]=prepos[i];
      tp[t++]=poi[i];
      p++;
    }
  }
  prepos=tpp;
  poi=tp;
  size=pres.length;
}
