{
  int s=find(p);
  if (s == -1 || children[s].pre != p)   ++s;
  final int upper=p + sz;
  int num=0;
  for (int i=s; i < size && children[i].pre < upper; ++i, ++num)   ;
  size-=num;
  if (size == 0)   children=new NSNode[0];
 else   if (num > 0)   System.arraycopy(children,s + num,children,s,size - s);
}
