{
  for (int c=0; c < size; ++c)   children[c].delete(uri);
  final IntList il=new IntList(values.length);
  for (int v=0; v < values.length; v+=2) {
    if (values[v + 1] != uri) {
      il.add(values[v]);
      il.add(values[v + 1]);
    }
  }
  if (il.size() != values.length)   values=il.toArray();
}
