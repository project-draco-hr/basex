{
  int s=size;
  if (s == children.length) {
    final NSNode[] tmp=new NSNode[Math.max(s << 1,1)];
    System.arraycopy(children,0,tmp,0,s);
    children=tmp;
  }
  while (--s >= 0 && n.pre - children[s].pre <= 0)   ;
  System.arraycopy(children,++s,children,s + 1,size++ - s);
  children[s]=n;
  n.parent=this;
}
