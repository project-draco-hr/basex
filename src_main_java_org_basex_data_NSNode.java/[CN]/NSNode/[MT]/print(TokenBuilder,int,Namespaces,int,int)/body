{
  if (pre >= s && pre <= e) {
    tb.add(NL);
    for (int i=0; i < l; ++i)     tb.add("  ");
    tb.add(toString() + ' ');
    for (int i=0; i < vals.length; i+=2) {
      tb.add("xmlns");
      final byte[] p=ns.prefix(vals[i]);
      if (p.length != 0)       tb.add(':');
      tb.add(p).add("=\"").add(ns.uri(vals[i + 1])).add("\" ");
    }
  }
  for (int c=0; c < size; ++c)   ch[c].print(tb,l + 1,ns,s,e);
}
