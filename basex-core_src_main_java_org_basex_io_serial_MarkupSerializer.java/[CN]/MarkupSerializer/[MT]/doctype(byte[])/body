{
  if (level != 0 || docsys == null && docpub == null)   return false;
  if (sep)   indent();
  out.print(DOCTYPE);
  if (type == null)   out.print(HTML);
 else   out.print(type);
  if (docpub != null)   out.print(' ' + PUBLIC + " \""+ docpub+ '"');
 else   out.print(' ' + SYSTEM);
  if (docsys != null)   out.print(" \"" + docsys + '"');
  out.print(ELEM_C);
  sep=true;
  return true;
}
