{
  if (ct != 1)   return false;
  ct++;
  if (empty)   finishOpen();
  level++;
  startOpen(META);
  attribute(HTTPEQUIV,CONTENT_TYPE,false);
  attribute(CONTENT,new TokenBuilder(media.isEmpty() ? MediaType.TEXT_HTML.toString() : media).add("; ").add(CHARSET).add('=').addExt(out.encoding()).finish(),false);
  if (html) {
    out.print(ELEM_C);
  }
 else {
    out.print(' ');
    out.print(ELEM_SC);
  }
  level--;
  if (empty)   finishClose();
  return true;
}
