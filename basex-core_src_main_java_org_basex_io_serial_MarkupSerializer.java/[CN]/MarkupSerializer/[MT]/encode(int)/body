{
  if (cp < ' ' && cp != '\n' && cp != '\t' || cp >= 0x7F && cp < 0xA0 || webdav && cp == 0xA0) {
    hex(cp);
  }
 else   if (cp == '&') {
    out.print(E_AMP);
  }
 else   if (cp == '>') {
    out.print(E_GT);
  }
 else   if (cp == '<') {
    out.print(E_LT);
  }
 else   if (cp == 0x2028) {
    out.print(E_2028);
  }
 else {
    printChar(cp);
  }
}
