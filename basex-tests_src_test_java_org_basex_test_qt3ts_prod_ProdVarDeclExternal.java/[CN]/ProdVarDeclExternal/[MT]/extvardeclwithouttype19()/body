{
  final XQuery query=new XQuery("declare variable $x external; xs:dateTime($x)",ctx);
  try {
    query.bind("x",new XQuery("avg((1,2,4))",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
