{
  final XQuery query=new XQuery("\n" + "        declare variable $i := false(); \n" + "        declare variable $t := false(); \n"+ "        some $i in (true(), true()), $t in (true(), true()) satisfies $i eq $t",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertBoolean(true));
}
