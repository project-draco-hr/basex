{
  final XQuery query=new XQuery("\n" + "        declare variable $i := false(); \n" + "        some $i in (true(), true(), true()) satisfies $i",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
