{
  final XQuery query=new XQuery("\"untyped a \" cast as xs:untypedAtomic eq xs:untypedAtomic(\"untyped a \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
