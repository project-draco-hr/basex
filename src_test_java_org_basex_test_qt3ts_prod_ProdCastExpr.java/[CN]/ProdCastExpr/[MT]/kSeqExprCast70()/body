{
  final XQuery query=new XQuery("(xs:QName(\"local-name\") cast as xs:QName) eq xs:QName(\"local-name\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
