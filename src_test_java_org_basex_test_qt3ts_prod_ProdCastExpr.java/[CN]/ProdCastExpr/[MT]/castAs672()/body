{
  final XQuery query=new XQuery("let $var := (\"a\",\"b\",\"c\") cast as xs:ENTITY return count($var) lt 10",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
