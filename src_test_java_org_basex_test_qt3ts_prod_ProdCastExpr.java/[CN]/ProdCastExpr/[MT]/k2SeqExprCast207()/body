{
  final XQuery query=new XQuery("xs:anyURI(\"http:\\\\invalid>URI\\someURI\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertStringValue(false,"http:\\\\invalid>URI\\someURI") || error("FORG0001")));
}
