{
  final XQuery query=new XQuery("let $var := \"abc\" cast as xs:ENTITY return $var instance of xs:ENTITY",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
