{
  final XQuery query=new XQuery("xs:string(\"-P1Y1M1DT1H1M1.123S\") cast as xs:duration",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-P1Y1M1DT1H1M1.123S"));
}
