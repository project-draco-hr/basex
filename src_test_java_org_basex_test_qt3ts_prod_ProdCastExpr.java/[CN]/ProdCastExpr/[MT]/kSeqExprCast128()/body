{
  final XQuery query=new XQuery("xs:string(xs:base64Binary(\"frfhforlksid7453\")) eq \"frfhforlksid7453\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
