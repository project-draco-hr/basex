{
  final XQuery query=new XQuery("\"START\", \n" + "      \t\tQName(\"http://example.com\", \"prefix:localName\"), \n" + "      \t\tQName(\"http://example.com\", \"localName\"), \n"+ "      \t\tQName(\"http://example.com\", \"localName\"), \n"+ "      \t\tQName(\"\", \"localName\"), QName((), \"localName\"), \n"+ "      \t\t\"END\"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("START prefix:localName localName localName localName localName END",false));
}
