{
  final XQuery query=new XQuery("<e xmlns:prefix=\"http://example.com/\"> { xs:QName(\"prefix:localName\") } </e>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<e xmlns:prefix=\"http://example.com/\">prefix:localName</e>",false));
}
