{
  final XQuery query=new XQuery("xs:anyURI(\" http://example.com/ foo.xml \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"http://example.com/ foo.xml"));
}
