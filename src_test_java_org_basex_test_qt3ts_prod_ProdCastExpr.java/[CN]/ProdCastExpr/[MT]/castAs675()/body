{
  final XQuery query=new XQuery("\n" + "        declare function local:clarkname($q as xs:QName) as xs:string { \n" + "          concat('{', namespace-uri-from-QName($q), '}', local-name-from-QName($q)) \n"+ "        }; \n"+ "        let $var := <e>xml:space</e> \n"+ "        return (local:clarkname(node-name($var)), local:clarkname(xs:untypedAtomic($var)))\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XPTY0004"));
}
