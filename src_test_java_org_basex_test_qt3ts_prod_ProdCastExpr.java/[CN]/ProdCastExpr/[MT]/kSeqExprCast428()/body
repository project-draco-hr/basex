{
  final XQuery query=new XQuery("xs:string(\" an arbitrary string \") eq xs:string(\" an arbitrary string \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
