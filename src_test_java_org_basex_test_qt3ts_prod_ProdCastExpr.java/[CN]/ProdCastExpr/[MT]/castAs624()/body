{
  final XQuery query=new XQuery("xs:hexBinary(\"d74d35d35d35\") cast as xs:hexBinary",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"D74D35D35D35"));
}
