{
  final XQuery query=new XQuery("(xs:integer(12678967543233) cast as xs:float) eq xs:float(12678967543233)",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
