{
  Performance p=new Performance();
  Prop.textindex=true;
  Prop.attrindex=true;
  Prop.chop=true;
  boolean ok=true;
  if (ALL) {
    for (int a=0; a < 2; a++) {
      Prop.ftindex=a == 0;
      for (int b=0; b < 2; b++) {
        Prop.ftfuzzy=b == 0;
        for (int c=0; c < 2; c++) {
          Prop.ftst=c == 0;
          for (int d=0; d < 2; d++) {
            Prop.ftdc=d == 0;
            for (int e=0; e < 2; e++) {
              Prop.ftcs=e == 0;
              ok&=test();
            }
          }
        }
      }
    }
  }
 else {
    Prop.ftindex=true;
    Prop.ftfuzzy=false;
    Prop.ftst=false;
    Prop.ftdc=false;
    Prop.ftcs=false;
    ok&=test();
  }
  System.out.println(ok ? "All tests correct." : wrong + " Wrong results...");
  System.out.println(counter + " queries, " + p);
}
