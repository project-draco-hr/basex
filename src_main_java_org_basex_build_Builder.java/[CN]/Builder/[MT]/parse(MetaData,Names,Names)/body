{
  final Performance perf=Util.debug ? new Performance() : null;
  Util.debug(tit() + DOTS);
  meta=md;
  tags=ta;
  atts=at;
  parser.parse(this);
  if (level != 0)   error(DOCOPEN,parser.detail(),tags.key(tstack.get(level)));
  if (meta.size == 0) {
    startDoc(token(name));
    endDoc();
    setSize(0,meta.size);
  }
  meta.lastid=meta.size - 1;
  Util.memory(perf);
}
