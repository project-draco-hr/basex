{
  ok(new CreateDB(NAME));
  ok(new CreateBackup(NAME));
  ok(new DropBackup(NAME));
  ok(new CreateDB(NAME));
  ok(new CreateBackup(NAME));
  final String[] b=context.databases().listBackups(NAME).toArray();
  ok(new DropBackup(b[0]));
  ok(new CreateDB(NAME));
  ok(new CreateDB(NAME2));
  ok(new CreateBackup(NAME));
  ok(new CreateBackup(NAME2));
  ok(new DropBackup(NAME));
}
