{
  final Data data=CONTEXT.data();
  data.startUpdate();
  data.update(10,Data.TEXT,JUNIT);
  data.finishUpdate();
  assertEquals(Data.TEXT,data.kind(10));
  assertArraysEquals(JUNIT,data.text(10,true));
  reload();
  assertEquals(Data.TEXT,data.kind(10));
  assertArraysEquals(JUNIT,data.text(10,true));
}
