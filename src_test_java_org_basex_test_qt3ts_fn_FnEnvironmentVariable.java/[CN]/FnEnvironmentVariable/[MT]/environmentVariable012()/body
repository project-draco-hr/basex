{
  final XQuery query=new XQuery("\n" + "\tlet $var := string-join(\n" + "\t  for $i in (1 to (1024 * 1024)) return \"x\"\n"+ "\t) \n"+ "\treturn empty(fn:environment-variable($var))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
