{
  final XQuery query=new XQuery("<out>{ (<a xmlns:ex=\"http://www.example.com/ns?p='23'\"><ex:b>93.7</ex:b></a>) /Q{http://www.example.com/ns?p='23'}b }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><ex:b xmlns:ex=\"http://www.example.com/ns?p='23'\">93.7</ex:b></out>",false));
}
