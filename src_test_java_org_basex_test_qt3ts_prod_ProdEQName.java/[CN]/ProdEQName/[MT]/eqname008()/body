{
  final XQuery query=new XQuery("<out>{ (<a xmlns:ex=\"http://www.example.com/ns\"><ex:b>93.7</ex:b></a>) /Q{http://www&#x2e;example&#x2E;com/ns}b }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out><ex:b xmlns:ex=\"http://www.example.com/ns\">93.7</ex:b></out>",false));
}
