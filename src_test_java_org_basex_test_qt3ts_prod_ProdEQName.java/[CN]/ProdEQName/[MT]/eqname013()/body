{
  final XQuery query=new XQuery("<out xmlns=\"http://www.example.com/one\">{ (<my:a xmlns:my=\"http://www.example.com/ns/my\"><my:b>42</my:b><b xmlns=\"\">93</b></my:a>) / Q{}* + 5 }</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out xmlns=\"http://www.example.com/one\">98</out>",false));
}
