{
  final XQuery query=new XQuery("\n" + "            declare variable $Q{http://www.example.com/ns/my}var := 12; \n" + "            <out>{$Q{http://www.example.com/ns/my}var}</out>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>12</out>",false));
}
