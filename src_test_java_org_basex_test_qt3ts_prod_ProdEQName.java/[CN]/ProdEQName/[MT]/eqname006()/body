{
  final XQuery query=new XQuery("(# Q{http://www.example.com/ns}pragma ignore me #) {<a/>}",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<a/>",false));
}
