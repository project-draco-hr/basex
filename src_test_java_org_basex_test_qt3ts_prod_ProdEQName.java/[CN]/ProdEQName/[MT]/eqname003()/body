{
  final XQuery query=new XQuery("\n" + "        declare function Q{http://www.example.com/ns/my}fn ($a as xs:integer) as xs:integer { $a + 2 }; \n" + "        <out>{Q{http://www.example.com/ns/my}fn(12)}</out>",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<out>14</out>",false));
}
