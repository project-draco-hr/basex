{
  final XQuery query=new XQuery("normalize-space(iri-to-uri((\"example.com\", current-time())[1] treat as xs:string))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"example.com"));
}
