{
  return new Iter(){
    final Iter ir=expr[0].iter(ctx);
    final byte[] label=checkStr(expr[1],ctx);
    boolean empty=true;
    @Override public Item next() throws QueryException {
      final Item it=ir.next();
      if (it != null) {
        dump(it,label,info,ctx);
        empty=false;
      }
 else       if (empty) {
        dump(null,label,info,ctx);
      }
      return it;
    }
  }
;
}
