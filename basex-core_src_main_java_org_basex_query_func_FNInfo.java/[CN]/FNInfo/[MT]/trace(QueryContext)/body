{
  return new Iter(){
    final Iter ir=exprs[0].iter(qc);
    final byte[] label=toToken(exprs[1],qc);
    boolean empty=true;
    @Override public Item next() throws QueryException {
      final Item it=ir.next();
      if (it != null) {
        dump(it,label,info,qc);
        empty=false;
      }
 else       if (empty) {
        dump(null,label,info,qc);
      }
      return it;
    }
  }
;
}
