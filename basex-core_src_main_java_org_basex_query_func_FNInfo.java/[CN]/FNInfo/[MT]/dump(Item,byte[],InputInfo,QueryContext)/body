{
  try {
    final byte[] value;
    if (it == null) {
      value=Token.token(SeqType.EMP.toString());
    }
 else     if (it.type == NodeType.ATT || it.type == NodeType.NSP) {
      value=Token.token(it.toString());
    }
 else {
      value=it.serialize().toArray();
    }
    dump(value,label,qc);
  }
 catch (  final QueryIOException ex) {
    throw ex.getCause(info);
  }
}
