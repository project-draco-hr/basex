{
  ops=new IntList();
  vals=new StringList();
  final Args arg=new Args(args,this,sa() ? S_LOCALINFO : S_CLIENTINFO,Util.info(S_CONSOLE,sa() ? S_STANDALONE : S_CLIENT));
  while (arg.more()) {
    final char c;
    String v=null;
    if (arg.dash()) {
      c=arg.next();
      if (c == 'b' || c == 'c' || c == 'C' || c == 'i' || c == 'o' || c == 'q' || c == 'r' || c == 's') {
        v=arg.string();
      }
 else       if (c == 'd' || c == 'D' && sa() || c == 'L' || c == 'u' || c == 'R' || c == 'v' || c == 'V' || c == 'w' || c == 'W' || c == 'x' || c == 'X' || c == 'z') {
        v="";
      }
 else       if (!sa()) {
        if (c == 'n') {
          context.globalopts.set(GlobalOptions.HOST,arg.string());
        }
 else         if (c == 'p') {
          context.globalopts.set(GlobalOptions.PORT,arg.number());
        }
 else         if (c == 'P') {
          context.globalopts.set(GlobalOptions.PASSWORD,arg.string());
        }
 else         if (c == 'U') {
          context.globalopts.set(GlobalOptions.USER,arg.string());
        }
 else {
          throw arg.usage();
        }
      }
 else {
        throw arg.usage();
      }
    }
 else {
      v=arg.string().trim();
      c=v.endsWith(IO.BXSSUFFIX) ? 'c' : 'Q';
    }
    if (v != null) {
      ops.add(c);
      vals.add(v);
    }
  }
}
