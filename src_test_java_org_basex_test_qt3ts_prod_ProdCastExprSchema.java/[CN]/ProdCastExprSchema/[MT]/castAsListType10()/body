{
  final XQuery query=new XQuery("\n" + "         let $f := function-lookup(QName('http://www.w3.org/2001/XMLSchema', 'IDREFS'), 1)\n" + "         return $f(\"a b c\")\n"+ "       ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertDeepEq("'a', 'b', 'c'") && assertType("xs:IDREF*")));
}
