{
  final XQuery query=new XQuery("\n" + "         let $f as function(xs:anyAtomicType) as xs:NMTOKEN* := xs:NMTOKENS#1\n" + "         let $v as xs:NMTOKEN* := $f(\"a b c\")\n"+ "         return count($v)\n"+ "       ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("3"));
}
