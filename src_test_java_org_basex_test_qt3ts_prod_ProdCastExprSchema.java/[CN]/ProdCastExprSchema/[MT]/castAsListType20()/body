{
  final XQuery query=new XQuery("\n" + "         let $v as xs:NMTOKENS := xs:NMTOKENS(\"a b c\")\n" + "         return count($v)\n"+ "       ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0051"));
}
