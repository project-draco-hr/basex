{
  final XQuery query=new XQuery("\n" + "         declare namespace eg = \"http://example.com\";\n" + "         %eg:sequential(true())  function () { \"bar\" } ()\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0003"));
}
