{
  final XQuery query=new XQuery("\n" + "         declare namespace eg = \"http://example.com\";\n" + "         () instance of %eg:x(\"abc\", 12e34, 567) function(*)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
