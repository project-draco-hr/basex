{
  final XQuery query=new XQuery("\n" + "         declare namespace math = \"http://www.w3.org/2005/xpath-functions/math\";\n" + "         () instance of %math:x function(*) \n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XQST0045"));
}
