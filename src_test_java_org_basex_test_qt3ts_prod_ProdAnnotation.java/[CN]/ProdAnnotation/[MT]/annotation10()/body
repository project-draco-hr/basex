{
  final XQuery query=new XQuery("\n" + "         declare %Q{http://example.com}bar variable $foo := 0;\n" + "\n"+ "         declare %Q{http://example.com}bar function local:foo($arg) { $arg }; \n"+ "\n"+ "         local:foo($foo)\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("0",false));
}
