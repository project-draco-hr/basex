{
  final XQuery query=new XQuery("\n" + "         declare namespace eg = \"http://example.com\";\n" + "\n"+ "         declare %eg:integer(1+2) function local:foo() { 0 }; \n"+ "\n"+ "         local:foo()\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XPST0003"));
}
