{
  final XQuery query=new XQuery("\n" + "         declare namespace math = \"http://www.w3.org/2005/xpath-functions/math\";\n" + "         declare %math:x function local:foo() {\n"+ "            \"bar\"\n"+ "         };\n"+ "         local:foo()\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(error("XQST0045"));
}
