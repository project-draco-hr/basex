{
  final XQuery query=new XQuery("\n" + "         declare namespace eg = \"http://example.com\";\n" + "         declare %eg:sequential function local:foo() {\n"+ "            \"bar\"\n"+ "         };\n"+ "         local:foo()\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("bar",false));
}
