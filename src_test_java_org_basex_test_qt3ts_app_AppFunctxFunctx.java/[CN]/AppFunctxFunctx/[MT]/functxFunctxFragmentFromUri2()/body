{
  final XQuery query=new XQuery("declare namespace functx = \"http://www.example.com/\";\n" + "                    (:~ : Returns the fragment from a URI : : @author Priscilla Walmsley, Datypic : @version 1.0 : @see http://www.xqueryfunctions.com/xq/functx_fragment-from-uri.html : @param $uri the URI :)\n" + "         declare function functx:fragment-from-uri ( $uri as xs:string? ) as xs:string? { substring-after($uri,'#') } ;\n"+ "                    (functx:fragment-from-uri( 'http://datypic.com/index.htm'))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,""));
}
