{
  final XQuery query=new XQuery("\n" + "         declare namespace functx = \"http://www.example.com/\";\n" + "         declare function functx:left-trim ( $arg as xs:string? ) as xs:string { replace($arg,'^\\s+','') } ;\n"+ "                    (functx:left-trim(' xyz'), functx:left-trim(' xyz    x'), functx:left-trim('xyz'))",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertStringValue(false,"xyz xyz    x xyz"));
}
