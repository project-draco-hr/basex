{
  int low=0;
  int high=rows - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    final int midValMin=pres[mid];
    final int midValMax=midValMin + nids[mid] - fids[mid];
    if (midValMax < pre)     low=mid + 1;
 else     if (midValMin > pre)     high=mid - 1;
 else     return mid;
  }
  return -(low + 1);
}
