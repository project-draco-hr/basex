{
  if (rows == 0 || id < pres[0])   return id;
  if (id > baseid) {
    for (int i=0; i < rows; ++i) {
      if (fids[i] == id)       return pres[i];
      if (fids[i] < id && id <= nids[i])       return pres[i] + id - fids[i];
    }
  }
  final int i=sortedLastIndexOf(oids,id);
  return id + incs[i < 0 ? -i - 2 : i];
}
