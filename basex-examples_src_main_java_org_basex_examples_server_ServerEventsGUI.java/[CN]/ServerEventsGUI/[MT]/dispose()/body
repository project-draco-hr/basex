{
  try {
    session.close();
    if (--open == 0) {
      try (ClientSession css=new ClientSession(server.context,S_ADMIN,S_ADMIN)){
        css.execute("drop event " + NAME);
        css.execute("drop db " + NAME);
      }
       server.stop();
    }
    super.dispose();
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
}
