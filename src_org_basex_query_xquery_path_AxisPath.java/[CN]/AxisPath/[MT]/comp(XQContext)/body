{
  super.comp(ctx);
  final Item ci=ctx.item;
  if (root != null) {
    if (root instanceof Root) {
      if (ctx.item != null)       ctx.item=ctx.iter(root).next();
    }
 else {
      ctx.item=null;
      if (root.i())       ctx.item=(Item)root;
    }
  }
  final Expr e=c(ctx);
  ctx.item=ci;
  return e;
}
