{
  final Item it=root != null ? ctx.iter(root).finish() : ctx.item;
  if (cache && citem != null && litem == it && it.type == Type.DOC) {
    return citem.iter();
  }
  litem=it;
  final Item c=ctx.item;
  final int cs=ctx.size;
  final int cp=ctx.pos;
  ctx.item=it;
  citem=eval(ctx);
  ctx.item=c;
  ctx.size=cs;
  ctx.pos=cp;
  return citem.iter();
}
