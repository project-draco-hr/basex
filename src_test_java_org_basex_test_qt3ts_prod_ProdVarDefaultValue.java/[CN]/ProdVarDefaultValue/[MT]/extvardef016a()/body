{
  final XQuery query=new XQuery("\n" + "        declare variable $y := (<a>1</a>,<a>2</a>,<a>3</a>,<a>4</a>,<a>5</a>,<a>6</a>,<a>7</a>,<a>8</a>,<a>9</a>,<a>10</a>);\n" + "        declare context item := $y[3];\n"+ "        declare variable $x external := fn:position();\n"+ "        $x\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("3"));
}
