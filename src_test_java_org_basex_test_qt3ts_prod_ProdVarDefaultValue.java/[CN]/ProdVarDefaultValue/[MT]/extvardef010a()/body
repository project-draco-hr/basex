{
  final XQuery query=new XQuery("\n" + "        declare variable $a external := 6 + local:foo();\n" + "        declare variable $b external := 12;\n"+ "        declare function local:foo() { $b + 10 };\n"+ "        $a\n"+ "      ",ctx);
  final QT3Result res=result(query);
  result=res;
  test(assertEq("28"));
}
