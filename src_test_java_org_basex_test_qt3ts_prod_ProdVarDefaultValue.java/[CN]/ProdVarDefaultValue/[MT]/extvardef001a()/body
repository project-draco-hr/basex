{
  final XQuery query=new XQuery("declare variable $ext external := 0; $ext",ctx);
  try {
    query.bind("ext",new XQuery("5",ctx).value());
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("5"));
}
