{
  final XQuery query=new XQuery("\n" + "        declare function local:aaa() { 1 };\n" + "        declare variable $x external := local:bbb() + local:aaa();\n"+ "        declare function local:bbb() { 2 };\n"+ "        $x \n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("3"));
}
