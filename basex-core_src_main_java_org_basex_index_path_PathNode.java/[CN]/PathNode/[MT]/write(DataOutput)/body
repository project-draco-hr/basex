{
  out.writeNum(name);
  out.write1(kind);
  out.writeNum(0);
  out.writeNum(children.length);
  out.writeDouble(1);
  boolean leaf=stats.isLeaf();
  for (  final PathNode child : children) {
    leaf&=child.kind == Data.TEXT || child.kind == Data.ATTR;
  }
  stats.setLeaf(leaf);
  stats.write(out);
  for (  final PathNode child : children)   child.write(out);
}
