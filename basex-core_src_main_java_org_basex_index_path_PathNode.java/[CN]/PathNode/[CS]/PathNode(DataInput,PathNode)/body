{
  name=(short)in.readNum();
  kind=(byte)in.read();
  final int s=in.readNum();
  ch=new PathNode[in.readNum()];
  if (in.readDouble() == 1) {
    stats=new Stats(in);
  }
 else {
    stats=new Stats();
    stats.count=s;
  }
  par=p;
  for (int i=0; i < ch.length; ++i)   ch[i]=new PathNode(in,this);
}
