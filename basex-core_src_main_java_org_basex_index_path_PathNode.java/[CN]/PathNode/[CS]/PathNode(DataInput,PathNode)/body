{
  name=(short)in.readNum();
  kind=(byte)in.read();
  final int count=in.readNum();
  children=new PathNode[in.readNum()];
  if (in.readDouble() == 1) {
    stats=new Stats(in);
  }
 else {
    stats=new Stats();
    stats.count=count;
  }
  parent=node;
  for (int c=0; c < children.length; ++c)   children[c]=new PathNode(in,this);
}
