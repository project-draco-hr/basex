{
  return df.newXMLGregorianCalendar(yea != Long.MAX_VALUE ? BigInteger.valueOf(yea > 0 ? yea : yea - 1) : null,mon >= 0 ? mon + 1 : Integer.MIN_VALUE,day >= 0 ? day + 1 : Integer.MIN_VALUE,hou >= 0 ? hou : Integer.MIN_VALUE,min >= 0 ? min : Integer.MIN_VALUE,sec != null ? sec.intValue() : Integer.MIN_VALUE,sec != null ? sec.remainder(BigDecimal.ONE) : null,zon != Short.MAX_VALUE ? zon : Integer.MIN_VALUE);
}
