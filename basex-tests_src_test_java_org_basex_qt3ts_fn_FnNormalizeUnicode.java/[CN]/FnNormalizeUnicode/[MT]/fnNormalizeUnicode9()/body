{
  final XQuery query=new XQuery("string-to-codepoints(fn:normalize-unicode('e?????e?????e??'))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertDeepEq("234, 64976, 234, 64976, 234"));
}
