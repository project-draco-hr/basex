{
  final XQuery query=new XQuery(queryFile(file("fn/normalize-unicode/fn-normalize-unicode-11.xq")),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out/>",false));
}
