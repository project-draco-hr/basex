{
  final XQuery query=new XQuery("fn:upper-case(fn:normalize-unicode(\"normalizedstring\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"NORMALIZEDSTRING"));
}
