{
  final XQuery query=new XQuery("fn:substring-after(\"substring-after\",\"refta-gnirtsbus\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertEq("\"\"") && assertCount(1)));
}
