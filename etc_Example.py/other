import BaseX, sys, time
start = time.clock()
cmd = 'xquery 1 to 10'
try:
    cs = BaseX.Session('localhost', 1984, 'admin', 'admin')
    if (not cs.execute(cmd)):
        print cs.info()
    else:
        print cs.result()
    print '\n', ((time.clock() - start) * 1000), 'ms.'
    cs.close()
except IOError as e:
    print e
