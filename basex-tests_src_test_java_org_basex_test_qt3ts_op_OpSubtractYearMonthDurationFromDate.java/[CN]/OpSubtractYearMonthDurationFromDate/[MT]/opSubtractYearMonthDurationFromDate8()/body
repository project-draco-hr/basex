{
  final XQuery query=new XQuery("(xs:date(\"0001-01-01Z\") - xs:yearMonthDuration(\"-P20Y07M\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"0021-08-01Z"));
}
