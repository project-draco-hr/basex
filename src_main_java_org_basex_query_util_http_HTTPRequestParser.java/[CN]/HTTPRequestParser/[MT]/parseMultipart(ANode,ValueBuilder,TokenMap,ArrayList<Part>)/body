{
  parseAttrs(multipart,attrs);
  if (attrs.get(MEDIA_TYPE) == null)   HC_REQ.thrw(info,"Attribute media-type of http:multipart is mandatory");
  final AxisMoreIter i=multipart.children();
  if (contItems == null) {
    for (ANode n; (n=i.next()) != null; )     parts.add(parsePart(n,null));
  }
 else {
    for (ANode n; (n=i.next()) != null; )     parts.add(parsePart(n,contItems.next()));
  }
}
