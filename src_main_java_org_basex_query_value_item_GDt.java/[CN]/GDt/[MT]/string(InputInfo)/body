{
  final TokenBuilder tb=new TokenBuilder();
  if (yea != Long.MAX_VALUE) {
    if (yea <= 0)     tb.add('-');
    prefix(tb,Math.abs(yea()),4);
  }
 else {
    tb.add('-');
  }
  if (mon >= 0 || day >= 0)   tb.add('-');
  if (mon >= 0)   prefix(tb,mon + 1,2);
  if (day >= 0)   tb.add('-');
  if (day >= 0)   prefix(tb,day + 1,2);
  zone(tb);
  return tb.finish();
}
