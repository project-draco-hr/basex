{
  if (!stack.isEmpty()) {
    if (attName == null && elem != null && stack.peek() == null) {
      stack.pop();
      stack.push(elem);
      elem=null;
    }
 else     if (attName != null || elem != null || stack.peek() == null) {
      error("No array allowed at this stage");
    }
  }
  stack.push(null);
  elem=null;
}
