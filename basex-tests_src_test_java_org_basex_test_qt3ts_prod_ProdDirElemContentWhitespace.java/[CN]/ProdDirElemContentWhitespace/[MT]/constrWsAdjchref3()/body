{
  final XQuery query=new XQuery(queryFile(file("prod/DirElemContent.whitespace/Constr-ws-adjchref-3.xq")),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertQuery("deep-equal(string-to-codepoints(string($result)), (9, 48, 9))"));
}
