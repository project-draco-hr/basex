{
  final XQuery query=new XQuery(queryFile(file("prod/DirElemContent.whitespace/Constr-ws-enclexpr-23.xq")),ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<elem>\r\n\r\n\r\n<a/>\r\n\r\n\r\n<b/>\r\n\r\n\r\n</elem>",false));
}
