{
  final XQuery query=new XQuery("declare boundary-space preserve; string(<e xml:space=\"default\"> </e>) eq \" \"",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
