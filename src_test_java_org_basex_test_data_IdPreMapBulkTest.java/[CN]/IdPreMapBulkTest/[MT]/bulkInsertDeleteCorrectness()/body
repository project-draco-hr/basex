{
  for (int k=0; k < iterations; ++k) {
    setUp();
    for (int i=0, n=baseid, id=baseid + 1; i < opcount; ++i) {
      final int pre=RANDOM.nextInt(n + 1);
      final int c;
      if (RANDOM.nextBoolean() || n == 0) {
        c=RANDOM.nextInt(bulkcount) + 1;
        insert(pre,id,c);
        id+=c;
      }
 else {
        c=-Math.min(n - pre,RANDOM.nextInt(bulkcount) + 1);
        delete(pre,c);
      }
      check();
      n+=c;
    }
  }
}
