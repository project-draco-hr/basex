{
  try {
    if ((len - p) < BUFFERSIZE) {
      mbytebuffer=rwChannel.map(FileChannel.MapMode.READ_WRITE,len - p,BUFFERSIZE);
      off=len - p - 1;
    }
 else {
      mbytebuffer=rwChannel.map(FileChannel.MapMode.READ_WRITE,p,BUFFERSIZE);
      off=p;
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
