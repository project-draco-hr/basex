{
  cursor(p);
  int l=readNum();
  final byte[] b=new byte[l];
  if (l > mbytebuffer.remaining()) {
    int ll=0;
    while (ll < l) {
      mbytebuffer.get(b,ll,mbytebuffer.remaining());
      ll=+mbytebuffer.remaining();
      moveWindow(off + BUFFERSIZE);
    }
  }
 else {
    mbytebuffer.get(b);
  }
  return b;
}
