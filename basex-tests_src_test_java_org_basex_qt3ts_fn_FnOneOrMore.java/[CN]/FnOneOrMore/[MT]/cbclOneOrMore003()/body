{
  final XQuery query=new XQuery("\n" + "      boolean(one-or-more(for $x in 1 to month-from-date(current-date()) return <a/>))\n" + "   ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
