{
  final XQuery query=new XQuery("fn:escape-html-uri(\"example?????example\")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"example%C3%A9%E2%82%ACexample"));
}
