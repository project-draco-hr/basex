{
  final XQuery query=new XQuery("fn:escape-html-uri(codepoints-to-string((9, 65, 128)))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"%09A%C2%80"));
}
