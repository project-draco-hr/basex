{
  final XQuery query=new XQuery(" for $h in (/works/employee[2]/@name) return fn:name($h/parent::node())",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"employee"));
}
