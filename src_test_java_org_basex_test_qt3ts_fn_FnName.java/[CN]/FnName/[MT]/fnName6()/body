{
  final XQuery query=new XQuery("for $h in ./works[1]/employee[2] return\n" + "         fn:string-length(fn:name($h/child::text()[last()]))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("0"));
}
