{
  final XQuery query=new XQuery("fn:string-length(fn:name(./works[1]/employee[2]/@name))",ctx);
  try {
    query.context(node(file("docs/works-mod.xml")));
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"4"));
}
