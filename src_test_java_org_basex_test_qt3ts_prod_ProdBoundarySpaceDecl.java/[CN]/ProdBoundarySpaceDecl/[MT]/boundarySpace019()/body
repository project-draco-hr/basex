{
  final XQuery query=new XQuery("\n" + "declare boundary-space strip;\n" + "<A> (a), (b), (c) </A>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<A> (a), (b), (c) </A>",false));
}
