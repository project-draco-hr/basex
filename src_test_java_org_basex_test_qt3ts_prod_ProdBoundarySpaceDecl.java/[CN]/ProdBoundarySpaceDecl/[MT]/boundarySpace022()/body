{
  final XQuery query=new XQuery("declare boundary-space preserve; (\" \",10, 20, 30, 40,\" \")",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"  10 20 30 40  "));
}
