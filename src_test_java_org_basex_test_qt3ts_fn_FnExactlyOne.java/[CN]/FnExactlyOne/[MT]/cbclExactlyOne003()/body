{
  final XQuery query=new XQuery("\n" + "      \tdeclare function local:generate($arg as xs:integer?) { if ($arg = 0) then () else if ($arg = 1) then $arg else ($arg, $arg) }; \n" + "      \t1 + fn:exactly-one(fn:one-or-more( local:generate( 1 ) ))\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2"));
}
