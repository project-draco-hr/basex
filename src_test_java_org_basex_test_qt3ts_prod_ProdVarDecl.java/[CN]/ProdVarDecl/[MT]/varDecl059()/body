{
  final XQuery query=new XQuery("declare variable $x := (//book/price, (), (1)) ; $x",ctx);
  query.context(node(file("docs/bib.xml")));
  final QT3Result res=result(query);
  result=res;
  test(assertSerialization("<price>65.95</price><price>65.95</price><price>39.95</price><price>129.95</price>1",false));
}
