{
  final XQuery query=new XQuery("declare variable $prefix:var1 := 2; declare namespace prefix = \"http://example.com/myNamespace\"; true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((error("XPST0003") || error("XPST0081")));
}
