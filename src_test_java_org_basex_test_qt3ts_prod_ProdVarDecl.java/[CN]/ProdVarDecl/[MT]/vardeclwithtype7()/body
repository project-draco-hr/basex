{
  final XQuery query=new XQuery("declare variable $var as xs:double := xs:double(1267.43233E12); declare variable $input-context1 external; $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"1.26743233E15"));
}
