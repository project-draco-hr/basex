{
  final XQuery query=new XQuery("declare variable $x := -1.7976931348623157E308 ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-1.7976931348623157E308"));
}
