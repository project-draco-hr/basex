{
  final XQuery query=new XQuery("declare variable $x := \"a \"\" or a ' delimits a string literal\" ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"a \" or a ' delimits a string literal"));
}
