{
  final XQuery query=new XQuery("declare variable $myVar as xs:gYear := 2006; true()",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((assertBoolean(true) || error("XPTY0004")));
}
