{
  final XQuery query=new XQuery("declare variable $x := xs:dateTime(\"2030-12-31T23:59:59Z\") ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"2030-12-31T23:59:59Z"));
}
