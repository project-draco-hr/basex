{
  final XQuery query=new XQuery("declare variable $x := xs:float(\"-3.4028235E38\") ; $x",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"-3.4028235E38"));
}
