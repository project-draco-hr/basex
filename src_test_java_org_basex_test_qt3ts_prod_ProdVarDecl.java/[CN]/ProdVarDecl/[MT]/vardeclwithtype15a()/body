{
  final XQuery query=new XQuery("\n" + "      declare variable $var as xs:integer := $e +1; \n" + "      declare variable $e as xs:integer := 10;  \n"+ "      $var",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPST0008"));
}
