{
  try {
    if ((len - p) < BUFFERSIZE) {
      mbytebuffer=rwChannel.map(FileChannel.MapMode.READ_WRITE,p,len - p);
    }
 else {
      mbytebuffer=rwChannel.map(FileChannel.MapMode.READ_WRITE,p,BUFFERSIZE);
    }
    off=p;
    position=p;
    mbytebuffer.position(0);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
