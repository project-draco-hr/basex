{
  cursor(p);
  int l=readNum();
  final byte[] b=new byte[l];
  if (l > mbytebuffer.remaining()) {
    int ll=0;
    int tmp=0;
    while (ll < l) {
      tmp=mbytebuffer.remaining();
      if (tmp >= l - ll) {
        mbytebuffer.get(b,ll,l - ll);
        ll=l;
      }
 else {
        mbytebuffer.get(b,ll,tmp);
        ll=+tmp;
        moveWindow(off + BUFFERSIZE);
      }
    }
  }
 else {
    mbytebuffer.get(b);
  }
  return b;
}
