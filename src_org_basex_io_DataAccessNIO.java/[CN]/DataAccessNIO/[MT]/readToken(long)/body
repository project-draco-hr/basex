{
  cursor(p);
  int l=readNum();
  final byte[] b=new byte[l];
  if (l > mbytebuffer.remaining()) {
    for (int i=0; i < l; i++) {
      try {
        b[i]=mbytebuffer.get();
      }
 catch (      BufferUnderflowException e) {
        moveWindow(off + BUFFERSIZE);
      }
    }
  }
 else {
    mbytebuffer.get(b);
  }
  return b;
}
