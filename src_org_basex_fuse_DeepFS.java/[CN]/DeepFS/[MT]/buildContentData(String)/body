{
  MemData md=new MemData(64,data.tags,data.atts,data.ns,data.path);
  try {
    MemBuilder mb=new MemBuilder();
    mb.init(md);
    Prop.fscont=true;
    Prop.fsmeta=true;
    String bpath=Prop.backingpath + "/" + data.meta.dbname+ "/"+ path;
    BaseX.errln("[DataFS_parse_single_file] FIX ME? bpath : " + bpath);
    FSParser p=new FSParser(bpath);
    mb.build(p,"tmp_memdata4file");
    return mb.finish();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return md;
}
