{
  final MemData md=new MemData(64,data.tags,data.atts,data.ns,data.path);
  try {
    final MemBuilder mb=new MemBuilder();
    mb.init(md);
    Prop.fscont=true;
    Prop.fsmeta=true;
    final String bpath=data.meta.backingpath + path;
    BaseX.debug("[DataFS_parse_file] path : " + path + " -> "+ bpath);
    final Parser p=Prop.newfsparser ? new NewFSParser(bpath) : new FSParser(bpath);
    return (MemData)mb.build(p,"tmp_memdata4file");
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
  return md;
}
