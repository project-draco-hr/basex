{
  try {
    final EditorArea edit=getEditor();
    file.write(edit.getText());
    edit.file(file);
    refreshHistory(file);
    refreshControls(true);
    edit.release(Action.PARSE);
    return true;
  }
 catch (  final IOException ex) {
    BaseXDialog.error(gui,FILE_NOT_SAVED);
    return false;
  }
}
