{
  final DialogLine dl=new DialogLine(gui);
  if (!dl.ok())   return;
  final EditorArea edit=getEditor();
  final int el=dl.line();
  final int ll=edit.last.length;
  int p=0;
  for (int l=1, e=0; e < ll && l < el; e+=cl(edit.last,e)) {
    if (edit.last[e] != '\n')     continue;
    p=e + 1;
    ++l;
  }
  edit.setCursor(p);
  posCode.invokeLater();
}
