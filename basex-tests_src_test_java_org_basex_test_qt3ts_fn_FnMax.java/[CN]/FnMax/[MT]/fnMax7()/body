{
  final XQuery query=new XQuery("max(for $x in 1 to 10 return xs:yearMonthDuration(concat(\"P\",$x,\"M\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"P10M"));
}
