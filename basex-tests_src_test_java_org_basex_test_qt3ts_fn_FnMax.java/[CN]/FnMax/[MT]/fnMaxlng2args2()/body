{
  final XQuery query=new XQuery("fn:max((xs:long(\"-47175562203048468\"),xs:long(\"-92233720368547758\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("-47175562203048468"));
}
