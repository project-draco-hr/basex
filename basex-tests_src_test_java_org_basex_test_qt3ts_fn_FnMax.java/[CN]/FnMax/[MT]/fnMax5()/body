{
  final XQuery query=new XQuery("max(for $x in 1 to 10 return xs:dayTimeDuration(concat(\"PT\",$x,\"H\")))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertStringValue(false,"PT10H"));
}
