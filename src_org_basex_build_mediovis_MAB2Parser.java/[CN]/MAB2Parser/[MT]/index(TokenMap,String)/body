{
  try {
    final DataAccess in=new DataAccess(new File(fn));
    while (true) {
      final byte[] key=find(in,(byte)'\t');
      final byte[] val=find(in,(byte)'\n');
      if (key == null)       break;
      hash.add(key,val);
    }
  }
 catch (  final IOException ex) {
    BaseX.debug(new File(fn).getAbsolutePath() + " not found.");
  }
}
