{
  if (compiled)   return this;
  compiled=true;
  for (  final Entry<Var,Expr> e : scope.closure().entrySet())   e.setValue(e.getValue().compile(ctx,scp));
  final StaticContext cs=ctx.sc;
  ctx.sc=sc;
  final int fp=scope.enter(ctx);
  try {
    for (    final Entry<Var,Expr> e : staticBindings())     ctx.set(e.getKey(),e.getValue().value(ctx),info);
    expr=expr.compile(ctx,scope);
  }
 catch (  final QueryException qe) {
    expr=FNInfo.error(qe);
  }
 finally {
    scope.cleanUp(this);
    scope.exit(ctx,fp);
    ctx.sc=cs;
  }
  ctx.compInfo(OPTTCE,this);
  expr=expr.markTailCalls();
  return optimize(ctx,scp);
}
