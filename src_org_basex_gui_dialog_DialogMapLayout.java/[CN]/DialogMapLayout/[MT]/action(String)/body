{
  GUIProp.maplayout=choice.getIndex();
  final int prp=prop.value();
  GUIProp.mapprop=prp;
  GUIProp.mapsimple=simple.isSelected();
  GUIProp.mapatts=atts.isSelected();
  propLabel.setText(MAPPROP + " " + (prp > 2 && prp < 7 ? "Centered" : prp < 3 ? "Vertical" : "Horizontal"));
  gui.notify.layout();
}
