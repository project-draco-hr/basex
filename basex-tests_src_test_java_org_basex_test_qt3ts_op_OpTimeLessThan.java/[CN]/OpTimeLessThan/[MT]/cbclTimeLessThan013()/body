{
  final XQuery query=new XQuery("\n" + "        declare function local:time($time as xs:time, $null as xs:boolean) { if ($null) then () else $time };\n" + "        exists(local:time(xs:time(\"23:58:00\"), fn:true()) ge xs:time(\"23:58:00\"))",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(false));
}
