{
  final XQuery query=new XQuery("<a>&#xD;&#x85;&#x2028;</a>",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test((serializationMatches("&#xD;&#x85;&#x2028;","i") || serializationMatches("&#13;&#133;&#8232;","")));
}
