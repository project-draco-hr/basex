{
  final XQuery query=new XQuery("\n" + "         declare function local:hof($s, $f as function(*)) {\n" + "           $f($s[1], $s[2])\n"+ "         };\n"+ "         \n"+ "         local:hof(('1', '2'), concat#2)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertEq("\"12\""));
}
