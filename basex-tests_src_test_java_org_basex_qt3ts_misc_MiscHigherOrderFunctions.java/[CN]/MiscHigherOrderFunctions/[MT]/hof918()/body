{
  final XQuery query=new XQuery("\n" + "        declare function local:round($x as xs:double, $algorithm as (function(xs:double) as xs:double)) as xs:double \n" + "        { $algorithm($x) }; \n"+ "        declare variable $roundToCeiling := local:round(?, upper-case#1); \n"+ "        $roundToCeiling(12.4)\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPTY0004"));
}
