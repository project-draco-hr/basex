{
  final XQuery query=new XQuery("function($a as node()+) { $a + 5 } instance of function(node()) as item()*",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertBoolean(true));
}
