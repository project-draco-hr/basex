{
  final XQuery query=new XQuery("\n" + "        declare function local:tf($i as xs:integer) as function () as xs:boolean { \n" + "            if ($i) then true#0 else false#0 \n"+ "        }; \n"+ "        <out>{(local:tf(0)(), local:tf(1)())}</out>\n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(assertSerialization("<out>false true</out>",false));
}
