{
  final XQuery query=new XQuery("\n" + "         let $f := function($a) { node-name(.), $a }\n" + "         return <a/>/$f(5)\n"+ "      \n"+ "      ",ctx);
  try {
    result=new QT3Result(query.value());
  }
 catch (  final Throwable trw) {
    result=new QT3Result(trw);
  }
 finally {
    query.close();
  }
  test(error("XPDY0002"));
}
